                    container mac-sources {
                        description "Information about packet streams that violate the cpu protection policy per-source rules";

                        container sap-sources {
                            description
                                "MAC source packet streams that violate the cpu protection policy per-source rules
                                 on a SAP";

                            list mac-source {
                                key "service-id sap-id mac-address";
                                description "MAC source packet stream that violates the cpu protection policy per-source rules";

                                leaf service-id {
                                    type types-services:external-service-id;
                                    description "The service index.";
                                }

                                leaf sap-id {
                                    type types-sros:sap;
                                    description "The SAP identifier.";
                                }

                                leaf mac-address {
                                    type yang:mac-address;
                                    description "MAC address.";
                                }

                                leaf sap-type {
                                    type types-services:service-type;
                                    description "The type of the service containing the SAP or the spoke SDP.";
                                }

