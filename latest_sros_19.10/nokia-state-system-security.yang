                        list port {
                            key "port-id";
                            description "Ports where the link-specific rate limit or the overall rate limit is violated";

                            leaf port-id {
                                type types-sros:port;
                                description "The identifier that uniquely identifies the port.";
                            }

                            container link-specific-rate {
                                description "Information about violations of the link-specific rate limit on this port";

                                leaf periods {
                                    type yang:counter32;
                                    description
                                        "The number of times the rate limit violation was detected.
