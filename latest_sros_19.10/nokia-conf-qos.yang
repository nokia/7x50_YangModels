                                    type decimal64 {
                                        range "0.00..100.00";
                                        fraction-digits 2;
                                    }
                                    default "20.00";
                                    description
                                        "Specifies the percentage of delta (over the current H-QoS below CIR or above
                                         CIR pass) consumed aggregate rate that can be given to a queue.";
                                }

                            } // container delta-consumed-agg-rate

                            container unconsumed-higher-tier-rate {
                                description
                                    "Configures the percentage of the unconsumed higher-tier rate that can be giveni
                                     to a queue at the end of an H-QoS below CIR, then above CIR, pass.
