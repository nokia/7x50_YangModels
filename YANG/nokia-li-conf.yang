    revision "2023-05-24";

    ///nokia-li-conf/li
    container li {
        description "Configure lawful intercept";


        ///nokia-li-conf/li/li-filter
        container li-filter {
            description "Enter the li-filter context";

            ///nokia-li-conf/li/li-filter/lock-filter
            leaf lock-filter {
                type enumeration {
                    enum lock                           { value 0; }
                    enum unlock                         { value 2; }
                }
                default "lock";
                description "Lock state of LI filters";
            }

            ///nokia-li-conf/li/li-filter/li-ip-filter
            list li-ip-filter {
                key "li-filter-name";
                description "Enter the li-ip-filter list instance";

                ///nokia-li-conf/li/li-filter/li-ip-filter/li-filter-name
                leaf li-filter-name {
                    type types-sros:named-item {
                        pattern "((([^d_ ]|d($|[^e]|e($|[^f]|f($|[^a]|a($|[^u]|u($|[^l]|l($|[^t]|t\\P{C})))))))\\P{C}*))";
                    }
                    description "LI filter name";
                }

                ///nokia-li-conf/li/li-filter/li-ip-filter/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-li-conf/li/li-filter/li-ip-filter/entry
                list entry {
                    key "li-entry-id";
                    description "Enter the entry list instance";

                    ///nokia-li-conf/li/li-filter/li-ip-filter/entry/li-entry-id
                    leaf li-entry-id {
                        type types-filter:limited-entry-id;
                        description "LI filter entry ID";
                    }

                    ///nokia-li-conf/li/li-filter/li-ip-filter/entry/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match
                    container match {
                        description "Enter the match context";

                        ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/protocol
                        leaf protocol {
                            type types-sros:match-ip-protocol;
                            description "IP protocol to match";
                        }

                        ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/fragment
                        leaf fragment {
                            type enumeration {
                                enum false                          { value 2; }
                                enum true                           { value 3; }
                            }
                            description "Fragmentation state as the match criterion";
                        }

                        ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-port
                        container dst-port {
                            description "Enter the dst-port context";

                            ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-port/port
                            choice port {
                                ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-port/port/eq
                                case eq {

                                    ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-port/port/eq/eq
                                    leaf eq {
                                        type uint16 {
                                            range "0..65535";
                                        }
                                        description "Condition on equality to specified value";
                                    }

                                }
                                ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-port/port/lt
                                case lt {

                                    ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-port/port/lt/lt
                                    leaf lt {
                                        type uint16 {
                                            range "1..65535";
                                        }
                                        description "Condition on being less than the specified value";
                                    }

                                }
                                ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-port/port/gt
                                case gt {

                                    ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-port/port/gt/gt
                                    leaf gt {
                                        type uint16 {
                                            range "0..65534";
                                        }
                                        description "Condition on being greater than the specified value";
                                    }

                                }
                                ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-port/port/range
                                case range {

                                    ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-port/port/range/range
                                    container range {
                                        presence "Enable the range context";
                                        description "Enable the range context";

                                        ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-port/port/range/range/start
                                        leaf start {
                                            type uint16 {
                                                range "0..65534";
                                            }
                                            mandatory true;
                                            description "Lower bound port to match";
                                        }

                                        ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-port/port/range/range/end
                                        leaf end {
                                            type uint16 {
                                                range "1..65535";
                                            }
                                            mandatory true;
                                            description "Upper bound port to match";
                                        }

                                    } // container range

                                }
                            }
                        } // container dst-port

                        ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-port
                        container src-port {
                            description "Enter the src-port context";

                            ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-port/port
                            choice port {
                                ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-port/port/eq
                                case eq {

                                    ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-port/port/eq/eq
                                    leaf eq {
                                        type uint16 {
                                            range "0..65535";
                                        }
                                        description "Condition on equality to specified value";
                                    }

                                }
                                ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-port/port/lt
                                case lt {

                                    ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-port/port/lt/lt
                                    leaf lt {
                                        type uint16 {
                                            range "1..65535";
                                        }
                                        description "Condition on being less than the specified value";
                                    }

                                }
                                ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-port/port/gt
                                case gt {

                                    ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-port/port/gt/gt
                                    leaf gt {
                                        type uint16 {
                                            range "0..65534";
                                        }
                                        description "Condition on being greater than the specified value";
                                    }

                                }
                                ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-port/port/range
                                case range {

                                    ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-port/port/range/range
                                    container range {
                                        presence "Enable the range context";
                                        description "Enable the range context";

                                        ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-port/port/range/range/start
                                        leaf start {
                                            type uint16 {
                                                range "0..65534";
                                            }
                                            mandatory true;
                                            description "Lower bound port to match";
                                        }

                                        ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-port/port/range/range/end
                                        leaf end {
                                            type uint16 {
                                                range "1..65535";
                                            }
                                            mandatory true;
                                            description "Upper bound port to match";
                                        }

                                    } // container range

                                }
                            }
                        } // container src-port

                        ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-ip
                        container dst-ip {
                            description "Enter the dst-ip context";

                            ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-ip/address
                            leaf address {
                                type union {
                                    type types-sros:ipv4-prefix-with-host-bits;
                                    type types-sros:ipv4-address;
                                }
                                description "IP address to match";
                            }

                            ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/dst-ip/mask
                            leaf mask {
                                type types-sros:ipv4-address;
                                description "Mask that is applied as an AND to the IP address";
                            }

                        } // container dst-ip

                        ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-ip
                        container src-ip {
                            description "Enter the src-ip context";

                            ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-ip/address
                            leaf address {
                                type union {
                                    type types-sros:ipv4-prefix-with-host-bits;
                                    type types-sros:ipv4-address;
                                }
                                description "IP address to match";
                            }

                            ///nokia-li-conf/li/li-filter/li-ip-filter/entry/match/src-ip/mask
                            leaf mask {
                                type types-sros:ipv4-address;
                                description "Mask that is applied as an AND to the IP address";
                            }

                        } // container src-ip

                    } // container match

                } // list entry

            } // list li-ip-filter

            ///nokia-li-conf/li/li-filter/li-ipv6-filter
            list li-ipv6-filter {
                key "li-filter-name";
                description "Enter the li-ipv6-filter list instance";

                ///nokia-li-conf/li/li-filter/li-ipv6-filter/li-filter-name
                leaf li-filter-name {
                    type types-sros:named-item {
                        pattern "((([^d_ ]|d($|[^e]|e($|[^f]|f($|[^a]|a($|[^u]|u($|[^l]|l($|[^t]|t\\P{C})))))))\\P{C}*))";
                    }
                    description "LI filter name";
                }

                ///nokia-li-conf/li/li-filter/li-ipv6-filter/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry
                list entry {
                    key "li-entry-id";
                    description "Enter the entry list instance";

                    ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/li-entry-id
                    leaf li-entry-id {
                        type types-filter:limited-entry-id;
                        description "LI filter entry ID";
                    }

                    ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match
                    container match {
                        description "Enter the match context";

                        ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/next-header
                        leaf next-header {
                            type types-sros:match-ip-protocol;
                            description "IP protocol to match";
                        }

                        ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-port
                        container dst-port {
                            description "Enter the dst-port context";

                            ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-port/port
                            choice port {
                                ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-port/port/eq
                                case eq {

                                    ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-port/port/eq/eq
                                    leaf eq {
                                        type uint16 {
                                            range "0..65535";
                                        }
                                        description "Condition on equality to specified value";
                                    }

                                }
                                ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-port/port/lt
                                case lt {

                                    ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-port/port/lt/lt
                                    leaf lt {
                                        type uint16 {
                                            range "1..65535";
                                        }
                                        description "Condition on being less than the specified value";
                                    }

                                }
                                ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-port/port/gt
                                case gt {

                                    ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-port/port/gt/gt
                                    leaf gt {
                                        type uint16 {
                                            range "0..65534";
                                        }
                                        description "Condition on being greater than the specified value";
                                    }

                                }
                                ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-port/port/range
                                case range {

                                    ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-port/port/range/range
                                    container range {
                                        presence "Enable the range context";
                                        description "Enable the range context";

                                        ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-port/port/range/range/start
                                        leaf start {
                                            type uint16 {
                                                range "0..65534";
                                            }
                                            mandatory true;
                                            description "Lower bound port to match";
                                        }

                                        ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-port/port/range/range/end
                                        leaf end {
                                            type uint16 {
                                                range "1..65535";
                                            }
                                            mandatory true;
                                            description "Upper bound port to match";
                                        }

                                    } // container range

                                }
                            }
                        } // container dst-port

                        ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-port
                        container src-port {
                            description "Enter the src-port context";

                            ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-port/port
                            choice port {
                                ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-port/port/eq
                                case eq {

                                    ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-port/port/eq/eq
                                    leaf eq {
                                        type uint16 {
                                            range "0..65535";
                                        }
                                        description "Condition on equality to specified value";
                                    }

                                }
                                ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-port/port/lt
                                case lt {

                                    ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-port/port/lt/lt
                                    leaf lt {
                                        type uint16 {
                                            range "1..65535";
                                        }
                                        description "Condition on being less than the specified value";
                                    }

                                }
                                ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-port/port/gt
                                case gt {

                                    ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-port/port/gt/gt
                                    leaf gt {
                                        type uint16 {
                                            range "0..65534";
                                        }
                                        description "Condition on being greater than the specified value";
                                    }

                                }
                                ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-port/port/range
                                case range {

                                    ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-port/port/range/range
                                    container range {
                                        presence "Enable the range context";
                                        description "Enable the range context";

                                        ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-port/port/range/range/start
                                        leaf start {
                                            type uint16 {
                                                range "0..65534";
                                            }
                                            mandatory true;
                                            description "Lower bound port to match";
                                        }

                                        ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-port/port/range/range/end
                                        leaf end {
                                            type uint16 {
                                                range "1..65535";
                                            }
                                            mandatory true;
                                            description "Upper bound port to match";
                                        }

                                    } // container range

                                }
                            }
                        } // container src-port

                        ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-ip
                        container dst-ip {
                            description "Enter the dst-ip context";

                            ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-ip/address
                            leaf address {
                                type union {
                                    type types-sros:ipv6-prefix-with-host-bits;
                                    type types-sros:ipv6-address;
                                }
                                description "IP address to match";
                            }

                            ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/dst-ip/mask
                            leaf mask {
                                type types-sros:ipv6-address;
                                description "Mask that is applied as an AND to the IPv6 address";
                            }

                        } // container dst-ip

                        ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-ip
                        container src-ip {
                            description "Enter the src-ip context";

                            ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-ip/address
                            leaf address {
                                type union {
                                    type types-sros:ipv6-prefix-with-host-bits;
                                    type types-sros:ipv6-address;
                                }
                                description "IP address to match";
                            }

                            ///nokia-li-conf/li/li-filter/li-ipv6-filter/entry/match/src-ip/mask
                            leaf mask {
                                type types-sros:ipv6-address;
                                description "Mask that is applied as an AND to the IPv6 address";
                            }

                        } // container src-ip

                    } // container match

                } // list entry

            } // list li-ipv6-filter

            ///nokia-li-conf/li/li-filter/li-mac-filter
            list li-mac-filter {
                key "li-filter-name";
                description "Enter the li-mac-filter list instance";

                ///nokia-li-conf/li/li-filter/li-mac-filter/li-filter-name
                leaf li-filter-name {
                    type types-sros:named-item {
                        pattern "((([^d_ ]|d($|[^e]|e($|[^f]|f($|[^a]|a($|[^u]|u($|[^l]|l($|[^t]|t\\P{C})))))))\\P{C}*))";
                    }
                    description "LI filter name";
                }

                ///nokia-li-conf/li/li-filter/li-mac-filter/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-li-conf/li/li-filter/li-mac-filter/entry
                list entry {
                    key "li-entry-id";
                    description "Enter the entry list instance";

                    ///nokia-li-conf/li/li-filter/li-mac-filter/entry/li-entry-id
                    leaf li-entry-id {
                        type types-filter:limited-entry-id;
                        description "LI filter entry ID";
                    }

                    ///nokia-li-conf/li/li-filter/li-mac-filter/entry/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-li-conf/li/li-filter/li-mac-filter/entry/match
                    container match {
                        description "Enter the match context";

                        ///nokia-li-conf/li/li-filter/li-mac-filter/entry/match/frame-type
                        leaf frame-type {
                            type types-filter:mac-frame-type;
                            default "802dot3";
                            description "Frame type for MAC filter match";
                        }

                        ///nokia-li-conf/li/li-filter/li-mac-filter/entry/match/dst-mac
                        container dst-mac {
                            presence "Enable the dst-mac context";
                            description "Enable the dst-mac context";

                            ///nokia-li-conf/li/li-filter/li-mac-filter/entry/match/dst-mac/address
                            leaf address {
                                type yang:mac-address;
                                mandatory true;
                                description "MAC address for filter match";
                            }

                            ///nokia-li-conf/li/li-filter/li-mac-filter/entry/match/dst-mac/mask
                            leaf mask {
                                type yang:mac-address;
                                default "ff:ff:ff:ff:ff:ff";
                                description "MAC address mask for filter match";
                            }

                        } // container dst-mac

                        ///nokia-li-conf/li/li-filter/li-mac-filter/entry/match/src-mac
                        container src-mac {
                            presence "Enable the src-mac context";
                            description "Enable the src-mac context";

                            ///nokia-li-conf/li/li-filter/li-mac-filter/entry/match/src-mac/address
                            leaf address {
                                type yang:mac-address;
                                mandatory true;
                                description "MAC address for filter match";
                            }

                            ///nokia-li-conf/li/li-filter/li-mac-filter/entry/match/src-mac/mask
                            leaf mask {
                                type yang:mac-address;
                                default "ff:ff:ff:ff:ff:ff";
                                description "MAC address mask for filter match";
                            }

                        } // container src-mac

                    } // container match

                } // list entry

            } // list li-mac-filter

            ///nokia-li-conf/li/li-filter/associations
            container associations {
                description "Enter the associations context";

                ///nokia-li-conf/li/li-filter/associations/li-ip-filter
                list li-ip-filter {
                    key "li-filter-name";
                    description "Enter the li-ip-filter list instance";

                    ///nokia-li-conf/li/li-filter/associations/li-ip-filter/li-filter-name
                    leaf li-filter-name {
                        type leafref {
                            path "../../../li-ip-filter/li-filter-name";
                        }
                        description "Name of an existing LI IP filter";
                    }

                    ///nokia-li-conf/li/li-filter/associations/li-ip-filter/ip-filter
                    list ip-filter {
                        key "filter-name";
                        min-elements 1;
                        max-elements 1;
                        description "Add a list entry for ip-filter";

                        ///nokia-li-conf/li/li-filter/associations/li-ip-filter/ip-filter/filter-name
                        leaf filter-name {
                            type types-filter:filter-name;
                            description "IP filter to be associated with specified LI IP filter";
                        }

                    } // list ip-filter

                } // list li-ip-filter

                ///nokia-li-conf/li/li-filter/associations/li-ipv6-filter
                list li-ipv6-filter {
                    key "li-filter-name";
                    description "Enter the li-ipv6-filter list instance";

                    ///nokia-li-conf/li/li-filter/associations/li-ipv6-filter/li-filter-name
                    leaf li-filter-name {
                        type leafref {
                            path "../../../li-ipv6-filter/li-filter-name";
                        }
                        description "LI IPv6 filter name";
                    }

                    ///nokia-li-conf/li/li-filter/associations/li-ipv6-filter/ipv6-filter
                    list ipv6-filter {
                        key "filter-name";
                        min-elements 1;
                        max-elements 1;
                        description "Add a list entry for ipv6-filter";

                        ///nokia-li-conf/li/li-filter/associations/li-ipv6-filter/ipv6-filter/filter-name
                        leaf filter-name {
                            type types-filter:filter-name;
                            description "IP filter to be associated with specified LI IP filter";
                        }

                    } // list ipv6-filter

                } // list li-ipv6-filter

                ///nokia-li-conf/li/li-filter/associations/li-mac-filter
                list li-mac-filter {
                    key "li-filter-name";
                    description "Enter the li-mac-filter list instance";

                    ///nokia-li-conf/li/li-filter/associations/li-mac-filter/li-filter-name
                    leaf li-filter-name {
                        type leafref {
                            path "../../../li-mac-filter/li-filter-name";
                        }
                        description "LI MAC filter name";
                    }

                    ///nokia-li-conf/li/li-filter/associations/li-mac-filter/mac-filter
                    list mac-filter {
                        key "filter-name";
                        min-elements 1;
                        max-elements 1;
                        description "Add a list entry for mac-filter";

                        ///nokia-li-conf/li/li-filter/associations/li-mac-filter/mac-filter/filter-name
