                    }

                    ///nokia-li-conf/li/li-source/li-ipv6-filter/entry/session-id
                    leaf session-id {
                        type types-services:li-session-identifier;
                        description "Session ID of the traffic flow";
                    }

                } // list entry

            } // list li-ipv6-filter

            ///nokia-li-conf/li/li-source/li-mac-filter
            list li-mac-filter {
                key "li-filter-name";
                description "Enter the li-mac-filter list instance";

                ///nokia-li-conf/li/li-source/li-mac-filter/li-filter-name
                leaf li-filter-name {
                    type leafref {
                        path "../../../li-filter/li-mac-filter/li-filter-name";
                    }
                    description "A unique ID for an li-mac-filter";
                }

                ///nokia-li-conf/li/li-source/li-mac-filter/entry
                list entry {
                    key "li-entry-id";
                    min-elements 1;
                    description "Enter the entry list instance";

                    ///nokia-li-conf/li/li-source/li-mac-filter/entry/li-entry-id
                    leaf li-entry-id {
                        type leafref {
                            path "../../../../li-filter/li-mac-filter[li-filter-name=current()/../../li-filter-name]/entry/li-entry-id";
                        }
                        description "A unique ID for an li-mac-filter entry";
                    }

                    ///nokia-li-conf/li/li-source/li-mac-filter/entry/intercept-id
                    leaf intercept-id {
                        type types-services:li-interception-identifier;
                        description "Intercept ID of the traffic flow";
                    }

                    ///nokia-li-conf/li/li-source/li-mac-filter/entry/session-id
                    leaf session-id {
                        type types-services:li-session-identifier;
                        description "Session ID of the traffic flow";
                    }

                } // list entry

