            ///nokia-li-conf/li/li-source/nat
            container nat {
                description "Enter the nat context";

                ///nokia-li-conf/li/li-source/nat/ethernet-header
                container ethernet-header {
                    description "Enter the ethernet-header context";

                    ///nokia-li-conf/li/li-source/nat/ethernet-header/source-address
                    leaf source-address {
                        type types-sros:mac-address;
                        description "Source MAC address field of the Ethernet encapsulation";
                    }

                    ///nokia-li-conf/li/li-source/nat/ethernet-header/destination-address
                    leaf destination-address {
                        type types-sros:mac-address;
                        description "Destination MAC address field of Ethernet encapsulation";
                    }

                    ///nokia-li-conf/li/li-source/nat/ethernet-header/type
                    leaf type {
                        type uint16 {
                            range "1536..65535";
                        }
                        default "1792";
                        description "Ethertype of the ethernet encapsulation";
                    }

                } // container ethernet-header

                ///nokia-li-conf/li/li-source/nat/nat44
                list nat44 {
                    key "router-instance ip";
                    description "Enter the nat44 list instance";

                    ///nokia-li-conf/li/li-source/nat/nat44/router-instance
                    leaf router-instance {
                        type types-services:service-name;
                        description "Name of the service";
                    }

                    ///nokia-li-conf/li/li-source/nat/nat44/ip
                    leaf ip {
                        type types-sros:ipv4-address;
                        description "IP address of the subscriber";
                    }

                    ///nokia-li-conf/li/li-source/nat/nat44/intercept-id
                    leaf intercept-id {
                        type types-services:li-interception-identifier-wide;
                        description "The intercept id of the traffic flow";
                    }

                    ///nokia-li-conf/li/li-source/nat/nat44/session-id
                    leaf session-id {
                        type types-services:li-session-identifier;
                        description "The session id of the traffic flow";
                    }

                } // list nat44

                ///nokia-li-conf/li/li-source/nat/dslite
                list dslite {
                    key "router-instance b4";
                    description "Enter the dslite list instance";

                    ///nokia-li-conf/li/li-source/nat/dslite/router-instance
                    leaf router-instance {
                        type types-services:service-name;
                        description "Name of the service";
                    }

                    ///nokia-li-conf/li/li-source/nat/dslite/b4
                    leaf b4 {
                        type types-sros:ipv6-prefix;
                        description "B4 prefix of the subscriber";
                    }

                    ///nokia-li-conf/li/li-source/nat/dslite/intercept-id
                    leaf intercept-id {
                        type types-services:li-interception-identifier-wide;
                        description "The intercept id of the traffic flow";
                    }

                    ///nokia-li-conf/li/li-source/nat/dslite/session-id
                    leaf session-id {
                        type types-services:li-session-identifier;
                        description "The session id of the traffic flow";
                    }

                } // list dslite

                ///nokia-li-conf/li/li-source/nat/nat64
                list nat64 {
                    key "router-instance ip";
                    description "Enter the nat64 list instance";

                    ///nokia-li-conf/li/li-source/nat/nat64/router-instance
                    leaf router-instance {
                        type types-services:service-name;
                        description "Name of the service";
                    }

                    ///nokia-li-conf/li/li-source/nat/nat64/ip
                    leaf ip {
                        type types-sros:ipv6-prefix;
                        description "IP prefix of the subscriber";
                    }

                    ///nokia-li-conf/li/li-source/nat/nat64/intercept-id
                    leaf intercept-id {
                        type types-services:li-interception-identifier-wide;
                        description "The intercept id of the traffic flow";
                    }

                    ///nokia-li-conf/li/li-source/nat/nat64/session-id
                    leaf session-id {
                        type types-services:li-session-identifier;
                        description "The session id of the traffic flow";
                    }

                } // list nat64

                ///nokia-li-conf/li/li-source/nat/l2-aware
                list l2-aware {
                    key "subscriber-id";
                    description "Enter the l2-aware list instance";

                    ///nokia-li-conf/li/li-source/nat/l2-aware/subscriber-id
                    leaf subscriber-id {
                        type types-submgt:subscriber-id;
                        description "The string identifying the subscribe";
                    }

                    ///nokia-li-conf/li/li-source/nat/l2-aware/intercept-id
                    leaf intercept-id {
                        type types-services:li-interception-identifier-wide;
                        description "The intercept id of the traffic flow";
                    }

                    ///nokia-li-conf/li/li-source/nat/l2-aware/session-id
                    leaf session-id {
                        type types-services:li-session-identifier;
                        description "The session id of the traffic flow";
                    }

                } // list l2-aware

            } // container nat

            ///nokia-li-conf/li/li-source/wlan-gw-dsm-ue
            list wlan-gw-dsm-ue {
                key "mac";
                description "Enter the wlan-gw-dsm-ue list instance";

                ///nokia-li-conf/li/li-source/wlan-gw-dsm-ue/mac
                leaf mac {
                    type yang:mac-address;
                    description "MAC address of the DSM UE";
                }

                ///nokia-li-conf/li/li-source/wlan-gw-dsm-ue/intercept-id
                leaf intercept-id {
                    type types-services:li-interception-identifier-wide;
                    description "Intercept identifier";
                }

                ///nokia-li-conf/li/li-source/wlan-gw-dsm-ue/session-id
                leaf session-id {
                    type types-services:li-session-identifier;
                    description "Session identifier";
                }

            } // list wlan-gw-dsm-ue

            ///nokia-li-conf/li/li-source/subscriber
            list subscriber {
                key "subscriber-id";
                description "Enter the subscriber list instance";

                ///nokia-li-conf/li/li-source/subscriber/subscriber-id
                leaf subscriber-id {
                    type types-submgt:subscriber-id;
                    description "Subscriber ID";
                }

                ///nokia-li-conf/li/li-source/subscriber/ingress
                leaf ingress {
                    type boolean;
                    default "false";
                    description "Perform lawful intercept on ingress traffic";
                }

                ///nokia-li-conf/li/li-source/subscriber/egress
                leaf egress {
                    type boolean;
                    default "false";
                    description "Perform lawful intercept on egress traffic";
                }

                ///nokia-li-conf/li/li-source/subscriber/fc
                leaf-list fc {
                    type enumeration {
                        enum be                             { value 0; }
                        enum l2                             { value 1; }
                        enum af                             { value 2; }
                        enum l1                             { value 3; }
                        enum h2                             { value 4; }
                        enum ef                             { value 5; }
                        enum h1                             { value 6; }
                        enum nc                             { value 7; }
                    }
                    max-elements 8;
                    description "Forwarding classes to be mirrored";
                }

                ///nokia-li-conf/li/li-source/subscriber/host-type
                leaf host-type {
                    type enumeration {
                        enum ipoe                           { value 2; }
                        enum ppp                            { value 3; }
                    }
                    description "Subscriber host type to be monitored";
                }

                ///nokia-li-conf/li/li-source/subscriber/ip-family
                leaf ip-family {
                    type enumeration {
                        enum ipv4                           { value 2; }
                        enum ipv6                           { value 3; }
                    }
                    description "IP family for LI";
                }

                ///nokia-li-conf/li/li-source/subscriber/intercept-id
                leaf intercept-id {
                    type types-services:li-interception-identifier;
                    description "Intercept ID of the traffic flow";
                }

                ///nokia-li-conf/li/li-source/subscriber/session-id
                leaf session-id {
                    type types-services:li-session-identifier;
                    description "Session ID of the traffic flow";
                }

                ///nokia-li-conf/li/li-source/subscriber/host-identification
                choice host-identification {
                    ///nokia-li-conf/li/li-source/subscriber/host-identification/sap-id
                    case sap-id {

                        ///nokia-li-conf/li/li-source/subscriber/host-identification/sap-id/sap-id
                        leaf sap-id {
                            type types-sros:sap;
                            description "SAP ID";
                        }

                        ///nokia-li-conf/li/li-source/subscriber/host-identification/sap-id/mac-address
                        leaf mac-address {
                            type yang:mac-address;
                            description "The source MAC address";
                        }

                        ///nokia-li-conf/li/li-source/subscriber/host-identification/sap-id/ip-address
                        leaf ip-address {
                            type types-sros:ipv4-address;
                            description "IP address of the subscriber";
                        }

                    }
                    ///nokia-li-conf/li/li-source/subscriber/host-identification/sla-profile
                    case sla-profile {

                        ///nokia-li-conf/li/li-source/subscriber/host-identification/sla-profile/sla-profile
                        leaf sla-profile {
                            type types-sros:external-named-item;
                            description "SLA profile";
                        }

                    }
                }
            } // list subscriber

        } // list li-source

        ///nokia-li-conf/li/log
        container log {
            description "Enable the log context";

            ///nokia-li-conf/li/log/log-id
            list log-id {
                key "name";
                max-elements 30;
                description "Enter the log-id list instance";

                ///nokia-li-conf/li/log/log-id/name
                leaf name {
                    type types-log:li-log-name;
                    description "Log ID";
                }

                ///nokia-li-conf/li/log/log-id/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "enable";
                    description "The administrative state of the log";
                }

                ///nokia-li-conf/li/log/log-id/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-li-conf/li/log/log-id/netconf-stream
                leaf netconf-stream {
                    type types-sros:named-item;
                    description "Destination NETCONF stream name";
                }

                ///nokia-li-conf/li/log/log-id/time-format
                leaf time-format {
                    type enumeration {
                        enum utc                            { value 1; }
                        enum local                          { value 2; }
                    }
                    default "utc";
                    description "Time zone to display date and time";
                }

                ///nokia-li-conf/li/log/log-id/filter
                leaf filter {
                    type types-log:log-filter-name;
                    description "Event filter ID to associate with log ID configuration";
                }

                ///nokia-li-conf/li/log/log-id/source
                container source {
                    description "Enter the source context";

                    ///nokia-li-conf/li/log/log-id/source/li
                    leaf li {
                        type boolean;
                        default "false";
                        description "Event stream for events configured for LI activities";
                    }

                } // container source

                ///nokia-li-conf/li/log/log-id/destination
                container destination {
                    description "Enter the destination context";

                    ///nokia-li-conf/li/log/log-id/destination/log-id-destination
                    choice log-id-destination {
                        ///nokia-li-conf/li/log/log-id/destination/log-id-destination/memory
                        case memory {

                            ///nokia-li-conf/li/log/log-id/destination/log-id-destination/memory/memory
                            container memory {
                                presence "Enable the memory context";
                                description "Enable the memory context";

                                ///nokia-li-conf/li/log/log-id/destination/log-id-destination/memory/memory/max-entries
                                leaf max-entries {
                                    type uint32 {
                                        range "50..1024";
                                    }
                                    sros-ext:immutable;
                                    default "100";
                                    description "Number of events stored in this memory log";
                                }

                            } // container memory

                        }
                        ///nokia-li-conf/li/log/log-id/destination/log-id-destination/snmp
                        case snmp {

                            ///nokia-li-conf/li/log/log-id/destination/log-id-destination/snmp/snmp
                            container snmp {
                                presence "Enable the snmp context";
                                description "Enable the snmp context";

                                ///nokia-li-conf/li/log/log-id/destination/log-id-destination/snmp/snmp/max-entries
                                leaf max-entries {
                                    type uint32 {
                                        range "50..1024";
                                    }
                                    sros-ext:immutable;
                                    default "100";
                                    description "Number of events stored in this snmp log";
                                }

                            } // container snmp

                        }
                        ///nokia-li-conf/li/log/log-id/destination/log-id-destination/netconf
                        case netconf {

                            ///nokia-li-conf/li/log/log-id/destination/log-id-destination/netconf/netconf
                            container netconf {
                                presence "Enable the netconf context";
                                description "Enable the netconf context";

                                ///nokia-li-conf/li/log/log-id/destination/log-id-destination/netconf/netconf/max-entries
                                leaf max-entries {
                                    type uint32 {
                                        range "50..1024";
                                    }
                                    sros-ext:immutable;
                                    default "100";
                                    description "Maximum number of events stored in the NETCONF log";
                                }

                            } // container netconf

                        }
                    }
                } // container destination

            } // list log-id

        } // container log

        ///nokia-li-conf/li/mirror-dest-reservation
        container mirror-dest-reservation {
            description "Enter the mirror-dest-reservation context";

            ///nokia-li-conf/li/mirror-dest-reservation/start
            leaf start {
                type uint32 {
                    range "1..2147483647";
                }
                description "Lower bound of the mirror destination ID";
            }

            ///nokia-li-conf/li/mirror-dest-reservation/end
            leaf end {
                type uint32 {
                    range "1..2147483647";
                }
                description "Upper bound of the mirror destination ID";
            }

        } // container mirror-dest-reservation

        ///nokia-li-conf/li/mirror-dest-template
        list mirror-dest-template {
            key "name";
            max-elements 8;
            description "Enter the mirror-dest-template list instance";

            ///nokia-li-conf/li/mirror-dest-template/name
            leaf name {
                type types-sros:named-item;
                description "Mirror destination template name";
            }

            ///nokia-li-conf/li/mirror-dest-template/type
            leaf type {
                type types-services:mirror-type;
                sros-ext:immutable;
                default "ether";
                description "Encapsulation type supported by the mirror service";
            }

            ///nokia-li-conf/li/mirror-dest-template/layer-3-encap
            container layer-3-encap {
                description "Enter the layer-3-encap context";

                ///nokia-li-conf/li/mirror-dest-template/layer-3-encap/router-instance
                leaf router-instance {
                    type types-sros:router-instance;
                    default "Base";
                    description "Virtual router instance to inject mirrored packets";
                }

                ///nokia-li-conf/li/mirror-dest-template/layer-3-encap/encap-type
                leaf encap-type {
                    type enumeration {
                        enum ip-udp-shim                    { value 1; }
                        enum ip-gre                         { value 2; }
                    }
                    sros-ext:immutable;
                    default "ip-udp-shim";
                    description "The header type of the layer 3 encapsulation.";
                }

                ///nokia-li-conf/li/mirror-dest-template/layer-3-encap/direction-bit
                leaf direction-bit {
                    type boolean;
                    default "false";
                    description "Use highest bit in interception ID for flow direction";
                }

                ///nokia-li-conf/li/mirror-dest-template/layer-3-encap/ip-source
                leaf ip-source {
                    type types-sros:ipv4-unicast-address;
                    description "Source IPv4 address for Layer 3 encapsulation";
                }

                ///nokia-li-conf/li/mirror-dest-template/layer-3-encap/udp
                container udp {
                    description "Enter the udp context";

                    ///nokia-li-conf/li/mirror-dest-template/layer-3-encap/udp/source
                    leaf source {
                        when "../../encap-type = 'ip-udp-shim'" {
                            description "header type must be ip-udp-shim";
                        }
                        type types-sros:tcp-udp-port-non-zero;
                        description "Source UDP port";
                    }

                    ///nokia-li-conf/li/mirror-dest-template/layer-3-encap/udp/destination
                    leaf destination {
                        when "../../encap-type = 'ip-udp-shim'" {
                            description "header type must be ip-udp-shim";
                        }
                        type types-sros:tcp-udp-port-non-zero;
                        description "Destination UDP port";
                    }

                } // container udp

            } // container layer-3-encap

        } // list mirror-dest-template

        ///nokia-li-conf/li/nat
        container nat {
            description "Enter the nat context";

            ///nokia-li-conf/li/nat/use-outside-ip-address
            leaf use-outside-ip-address {
                type boolean;
                default "false";
                description "Report outside IP address for L2-Aware NAT subscribers";
            }

        } // container nat

        ///nokia-li-conf/li/radius
        container radius {
            description "Enter the radius context";

            ///nokia-li-conf/li/radius/mirror-dest-template
            leaf mirror-dest-template {
                type leafref {
                    path "../../mirror-dest-template/name";
                }
                description "RADIUS triggered mirror destination template";
            }

        } // container radius



        ///nokia-li-conf/li/x-interfaces
        container x-interfaces {
            description "Enter the x-interfaces context";

            ///nokia-li-conf/li/x-interfaces/admin-state
            leaf admin-state {
                type types-sros:admin-state;
                default "disable";
                description "Administrative state of the x-interfaces";
            }

            ///nokia-li-conf/li/x-interfaces/ine-identifier
            leaf ine-identifier {
                type types-sros:named-item;
                description "Intercepting Network Element (INE) ID";
            }

            ///nokia-li-conf/li/x-interfaces/user-db
            leaf user-db {
                type types-sros:named-item;
                description "Location of the data-trigger host for the LIC";
            }

            ///nokia-li-conf/li/x-interfaces/correlation-id
            container correlation-id {
                description "Enter the correlation-id context";

                ///nokia-li-conf/li/x-interfaces/correlation-id/ipoe
                leaf ipoe {
                    type enumeration {
                        enum radius-host-acct-id            { value 1; }
                        enum radius-queue-acct-id           { value 2; }
                        enum radius-session-acct-id         { value 3; }
                    }
                    default "radius-host-acct-id";
                    description "RADIUS accounting ID type for subscriber correlation";
                }

                ///nokia-li-conf/li/x-interfaces/correlation-id/pppoe
                leaf pppoe {
                    type enumeration {
                        enum radius-host-acct-id            { value 1; }
                        enum radius-queue-acct-id           { value 2; }
                        enum radius-session-acct-id         { value 3; }
                    }
                    default "radius-host-acct-id";
                    description "RADIUS accounting ID type for subscriber correlation";
                }

            } // container correlation-id

            ///nokia-li-conf/li/x-interfaces/lic
            list lic {
                key "name";
                max-elements 18;
                description "Enter the lic list instance";

                ///nokia-li-conf/li/x-interfaces/lic/name
                leaf name {
                    type types-sros:named-item;
                    description "LIC name";
                }

                ///nokia-li-conf/li/x-interfaces/lic/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-li-conf/li/x-interfaces/lic/identifier
                leaf identifier {
                    type types-sros:named-item;
                    description "LIC ID";
                }

                ///nokia-li-conf/li/x-interfaces/lic/port
                leaf port {
                    type inet:port-number;
                    description "TCP port associated with the LIC";
                }

                ///nokia-li-conf/li/x-interfaces/lic/router-instance
                leaf router-instance {
                    type types-sros:named-item-64-or-empty;
                    default "";
                    description "Virtual router instance used by the X-interfaces";
                }

                ///nokia-li-conf/li/x-interfaces/lic/ipv4
                container ipv4 {
                    description "Enter the ipv4 context";

                    ///nokia-li-conf/li/x-interfaces/lic/ipv4/ip-address
                    leaf ip-address {
                        type types-sros:ip-address;
                        description "IP address of the LIC";
                    }

                } // container ipv4

                ///nokia-li-conf/li/x-interfaces/lic/authentication
                container authentication {
                    description "Enter the authentication context";

                    ///nokia-li-conf/li/x-interfaces/lic/authentication/password
                    leaf password {
                        type types-sros:li-static-encrypted-leaf-hex {
                            length "1..50";
                        }
                        description "Password for the X1 and X2 interfaces";
                    }

                    ///nokia-li-conf/li/x-interfaces/lic/authentication/private-ki
                    leaf private-ki {
                        type types-sros:li-static-encrypted-leaf {
                            length "1..50";
                        }
                        description "Private Ki string for the X1 and X2 interfaces";
                    }

                    ///nokia-li-conf/li/x-interfaces/lic/authentication/sequence-group
                    leaf sequence-group {
                        type types-sros:li-static-encrypted-leaf {
                            length "1..42";
                        }
                        description "Sequence group for the X1 and X2 interfaces";
                    }

                } // container authentication

            } // list lic

            ///nokia-li-conf/li/x-interfaces/x1
            container x1 {
                description "Enter the x1 context";

                ///nokia-li-conf/li/x-interfaces/x1/lic-peer
                leaf lic-peer {
                    type leafref {
                        path "../../lic/name";
                    }
                    description "Peer LIC for the X1 interface";
                }

                ///nokia-li-conf/li/x-interfaces/x1/local-tcp-port
                leaf local-tcp-port {
                    type types-sros:tcp-udp-port-non-zero {
                        range "1..65535";
                    }
                    description "Local TCP port used for the X1 interface";
                }

                ///nokia-li-conf/li/x-interfaces/x1/ipv4
                container ipv4 {
                    description "Enter the ipv4 context";

                    ///nokia-li-conf/li/x-interfaces/x1/ipv4/local-address
                    leaf local-address {
                        type types-sros:ip-address;
                        description "X1 interface IP address";
                    }

                } // container ipv4

                ///nokia-li-conf/li/x-interfaces/x1/timeouts
                container timeouts {
                    description "Enter the timeouts context";

                    ///nokia-li-conf/li/x-interfaces/x1/timeouts/message-timeout
                    leaf message-timeout {
                        type uint32 {
                            range "180..300";
                        }
                        units "seconds";
                        default "180";
                        description "Maximum time for the LIC to reply to an X1 message";
                    }

                } // container timeouts

            } // container x1

            ///nokia-li-conf/li/x-interfaces/x2
            container x2 {
                description "Enter the x2 context";

                ///nokia-li-conf/li/x-interfaces/x2/lic-peer
                leaf lic-peer {
                    type leafref {
                        path "../../lic/name";
                    }
                    description "Peer LIC for the X2 interface";
                }

                ///nokia-li-conf/li/x-interfaces/x2/ipv4
                container ipv4 {
                    description "Enter the ipv4 context";

                    ///nokia-li-conf/li/x-interfaces/x2/ipv4/local-address
                    leaf local-address {
                        type types-sros:ip-address;
                        description "X2 interface IP address";
                    }

                } // container ipv4

                ///nokia-li-conf/li/x-interfaces/x2/timeouts
                container timeouts {
                    description "Enter the timeouts context";

                    ///nokia-li-conf/li/x-interfaces/x2/timeouts/keep-alive
                    leaf keep-alive {
                        type uint32 {
                            range "300..600";
                        }
                        units "seconds";
                        default "300";
                        description "Keep-alive timeout";
                    }

                    ///nokia-li-conf/li/x-interfaces/x2/timeouts/request
                    leaf request {
                        type uint32 {
                            range "5..30";
                        }
                        units "seconds";
                        default "5";
                        description "Tx2-normal timeout";
                    }

                } // container timeouts

            } // container x2

            ///nokia-li-conf/li/x-interfaces/x3
            container x3 {
                description "Enter the x3 context";

                ///nokia-li-conf/li/x-interfaces/x3/li-group
                leaf li-group {
                    type uint32 {
                        range "1..4";
                    }
                    description "ISA group of the X3 interface";
                }

                ///nokia-li-conf/li/x-interfaces/x3/session-limit
                leaf session-limit {
                    type uint32 {
                        range "1..32";
                    }
                    default "32";
                    description "Maximum number of X3 sessions initiated to the LIC";
                }

                ///nokia-li-conf/li/x-interfaces/x3/ipv4
                container ipv4 {
                    description "Enter the ipv4 context";

                    ///nokia-li-conf/li/x-interfaces/x3/ipv4/local-address-range
                    container local-address-range {
                        description "Enter the local-address-range context";

                        ///nokia-li-conf/li/x-interfaces/x3/ipv4/local-address-range/start
                        leaf start {
                            type types-sros:ipv4-unicast-address;
                            description "Lower bound of the range";
                        }

                        ///nokia-li-conf/li/x-interfaces/x3/ipv4/local-address-range/end
                        leaf end {
                            type types-sros:ipv4-unicast-address;
                            description "Upper bound of the range";
                        }

                    } // container local-address-range

                } // container ipv4

                ///nokia-li-conf/li/x-interfaces/x3/alarms
                container alarms {
                    description "Enter the alarms context";

                    ///nokia-li-conf/li/x-interfaces/x3/alarms/cpu-alarm
                    container cpu-alarm {
                        description "Enter the cpu-alarm context";

                        ///nokia-li-conf/li/x-interfaces/x3/alarms/cpu-alarm/low-threshold
                        leaf low-threshold {
                            type uint32 {
                                range "0..99";
                            }
                            units "percent";
                            default "0";
                            description "Low threshold value of the CPU usage alarm";
                        }

                        ///nokia-li-conf/li/x-interfaces/x3/alarms/cpu-alarm/high-threshold
                        leaf high-threshold {
                            type uint32 {
                                range "1..100";
                            }
                            units "percent";
                            description "High threshold value of the CPU usage alarm";
                        }

                    } // container cpu-alarm

                    ///nokia-li-conf/li/x-interfaces/x3/alarms/memory-alarm
                    container memory-alarm {
                        description "Enter the memory-alarm context";

                        ///nokia-li-conf/li/x-interfaces/x3/alarms/memory-alarm/low-threshold
                        leaf low-threshold {
                            type uint32 {
                                range "0..99";
                            }
                            units "percent";
                            default "0";
                            description "Low threshold of the packet buffer memory usage alarm";
                        }

                        ///nokia-li-conf/li/x-interfaces/x3/alarms/memory-alarm/high-threshold
                        leaf high-threshold {
                            type uint32 {
                                range "1..100";
                            }
                            units "percent";
                            description "High threshold of the packet buffer memory usage alarm";
                        }

                    } // container memory-alarm

                    ///nokia-li-conf/li/x-interfaces/x3/alarms/throughput-alarm
                    container throughput-alarm {
                        description "Enter the throughput-alarm context";

                        ///nokia-li-conf/li/x-interfaces/x3/alarms/throughput-alarm/low-threshold
                        leaf low-threshold {
                            type uint32 {
                                range "1..4294967295";
                            }
                            units "megabps";
                            description "Low threshold value of the throughput alarm";
                        }

                        ///nokia-li-conf/li/x-interfaces/x3/alarms/throughput-alarm/high-threshold
                        leaf high-threshold {
                            type uint32 {
                                range "1..4294967295";
                            }
                            units "megabps";
                            description "High threshold value of the throughput alarm";
                        }

                    } // container throughput-alarm

                } // container alarms

                ///nokia-li-conf/li/x-interfaces/x3/lic-peers
                list lic-peers {
                    key "name";
                    max-elements 16;
                    description "Add a list entry for lic-peers";

                    ///nokia-li-conf/li/x-interfaces/x3/lic-peers/name
                    leaf name {
                        type leafref {
                            path "../../../lic/name";
                        }
                        description "Peer LIC for the X3 interface";
                    }

                } // list lic-peers

                ///nokia-li-conf/li/x-interfaces/x3/timeouts
                container timeouts {
                    description "Enter the timeouts context";

                    ///nokia-li-conf/li/x-interfaces/x3/timeouts/keep-alive
                    leaf keep-alive {
                        type uint32 {
                            range "300..600";
                        }
                        units "seconds";
                        default "300";
                        description "Keep-alive timeout";
                    }

                    ///nokia-li-conf/li/x-interfaces/x3/timeouts/request
                    leaf request {
                        type uint32 {
                            range "5..30";
                        }
                        units "seconds";
                        default "5";
                        description "Tx3-normal timeout";
                    }

                    ///nokia-li-conf/li/x-interfaces/x3/timeouts/target-retry-wait
                    leaf target-retry-wait {
                        type uint32 {
                            range "300..1200";
                        }
                        units "seconds";
                        default "300";
                        description "Retry interval for target tunnel setup";
                    }

                } // container timeouts

            } // container x3

        } // container x-interfaces

    } // container li

}
