                                    presence "Enable the accounting context";
                                    sros-ext:auto-restart-to-modify;
                                    description "Enable the accounting context";

                                    ///nokia-conf/configure/aaa/radius/isa-policy/servers/server/purpose/accounting/udp-port
                                    leaf udp-port {
                                        type types-sros:tcp-udp-port-non-zero;
                                        sros-ext:auto-restart-to-modify;
                                        default "1813";
                                        description "ISA RADIUS server accounting UDP port";
                                    }

                                } // container accounting

                                ///nokia-conf/configure/aaa/radius/isa-policy/servers/server/purpose/authentication
                                container authentication {
                                    presence "Enable the authentication context";
                                    sros-ext:auto-restart-to-modify;
                                    description "Enable the authentication context";

                                    ///nokia-conf/configure/aaa/radius/isa-policy/servers/server/purpose/authentication/udp-port
                                    leaf udp-port {
                                        type types-sros:tcp-udp-port-non-zero;
                                        sros-ext:auto-restart-to-modify;
                                        default "1812";
                                        description "ISA RADIUS server authentication UDP port";
                                    }

                                } // container authentication

                                ///nokia-conf/configure/aaa/radius/isa-policy/servers/server/purpose/coa
                                container coa {
                                    presence "Enable the coa context";
                                    sros-ext:auto-restart-to-modify;
                                    description "Enable the coa context";

                                    ///nokia-conf/configure/aaa/radius/isa-policy/servers/server/purpose/coa/udp-port
                                    leaf udp-port {
                                        type types-sros:tcp-udp-port-non-zero;
                                        sros-ext:auto-restart-to-modify;
                                        default "3799";
                                        description "ISA RADIUS server change of authorization UDP port";
                                    }

                                } // container coa

                            } // container purpose

                        } // list server

                    } // container servers

                } // list isa-policy

            } // container radius

            ///nokia-conf/configure/aaa/diameter
            container diameter {
                description "Enter the diameter context";

                ///nokia-conf/configure/aaa/diameter/node
                list node {
                    key "origin-host";
                    max-elements 32;
                    description "Enter the node list instance";

                    ///nokia-conf/configure/aaa/diameter/node/origin-host
                    leaf origin-host {
                        type types-diam:diam-fqdn;
                        description "Origin-Host AVP";
                    }

                    ///nokia-conf/configure/aaa/diameter/node/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/aaa/diameter/node/origin-realm
                    leaf origin-realm {
                        type types-diam:diam-fqdn;
                        sros-ext:immutable;
                        description "Origin-realm name";
                    }

                    ///nokia-conf/configure/aaa/diameter/node/python-policy
                    leaf python-policy {
                        type leafref {
                            path "../../../../python/python-policy/name";
                        }
                        description "Python policy for received or sent Diameter messages";
                    }

                    ///nokia-conf/configure/aaa/diameter/node/router-instance
                    leaf router-instance {
                        type string;
                        default "Base";
                        description "Router in which this node connects to its peers";
                    }

                    ///nokia-conf/configure/aaa/diameter/node/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/aaa/diameter/node/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/aaa/diameter/node/connection
                    container connection {
                        description "Enter the connection context";

                        ///nokia-conf/configure/aaa/diameter/node/connection/timer
                        leaf timer {
                            type types-sros:time-duration {
                                range "1..1000";
                            }
                            units "seconds";
                            default "30";
                            description "Wait time before attempting reconnection to peer";
                        }

                        ///nokia-conf/configure/aaa/diameter/node/connection/ipv4
                        container ipv4 {
                            description "Enter the ipv4 context";

                            ///nokia-conf/configure/aaa/diameter/node/connection/ipv4/local-address
                            leaf local-address {
                                type types-sros:ipv4-unicast-address;
                                description "IPv4 source address for the TCP peer connection";
                            }

                            ///nokia-conf/configure/aaa/diameter/node/connection/ipv4/allow-connections
                            leaf allow-connections {
                                type boolean;
                                default "false";
                                description "Listen on local address for incoming peer connections";
                            }

                        } // container ipv4

                        ///nokia-conf/configure/aaa/diameter/node/connection/ipv6
                        container ipv6 {
                            description "Enter the ipv6 context";

                            ///nokia-conf/configure/aaa/diameter/node/connection/ipv6/local-address
                            leaf local-address {
                                type types-sros:ipv6-unicast-address;
                                description "IPv6 source address for the TCP peer connection";
                            }

                            ///nokia-conf/configure/aaa/diameter/node/connection/ipv6/allow-connections
                            leaf allow-connections {
                                type boolean;
                                default "false";
                                description "Listen on local address for incoming peer connections";
                            }

                        } // container ipv6

                    } // container connection

                    ///nokia-conf/configure/aaa/diameter/node/peer
                    list peer {
                        key "index";
                        max-elements 5;
                        description "Enter the peer list instance";

                        ///nokia-conf/configure/aaa/diameter/node/peer/index
                        leaf index {
                            type uint32 {
                                range "1..5";
                            }
                            description "Index of a peer within the node";
                        }

                        ///nokia-conf/configure/aaa/diameter/node/peer/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the Diameter peer";
                        }

                        ///nokia-conf/configure/aaa/diameter/node/peer/address
                        leaf address {
                            type types-sros:ip-unicast-address;
                            description "Diameter peer address";
                        }

                        ///nokia-conf/configure/aaa/diameter/node/peer/destination-host
                        leaf destination-host {
                            type types-diam:diam-fqdn;
                            sros-ext:immutable;
                            mandatory true;
                            description "Destination-Host AVP string for Diameter messages";
                        }

                        ///nokia-conf/configure/aaa/diameter/node/peer/connection-timer
                        leaf connection-timer {
                            type types-sros:time-duration {
                                range "1..1000";
                            }
                            units "seconds";
                            description "Wait time before attempting reconnection to peer";
                        }

                        ///nokia-conf/configure/aaa/diameter/node/peer/preference
                        leaf preference {
                            type uint32 {
                                range "1..100";
                            }
                            default "50";
                            description "Diameter routing preference for a peer";
                        }

                        ///nokia-conf/configure/aaa/diameter/node/peer/watchdog-timer
                        leaf watchdog-timer {
                            type types-sros:time-duration {
                                range "1..1000";
                            }
                            units "seconds";
                            description "Time between consecutive watchdog messages";
                        }

                        ///nokia-conf/configure/aaa/diameter/node/peer/default-peer
                        leaf default-peer {
                            type boolean;
                            default "false";
                            description "Use the peer as default route for realm-based routing";
                        }

                        ///nokia-conf/configure/aaa/diameter/node/peer/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/aaa/diameter/node/peer/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/aaa/diameter/node/peer/route
                        list route {
                            key "index";
                            max-elements 15;
                            description "Enter the route list instance";

                            ///nokia-conf/configure/aaa/diameter/node/peer/route/index
                            leaf index {
                                type uint32 {
                                    range "1..15";
                                }
                                description "Static Diameter route ID";
                            }

                            ///nokia-conf/configure/aaa/diameter/node/peer/route/realm
                            leaf realm {
                                type types-diam:diam-fqdn;
                                sros-ext:immutable;
                                mandatory true;
                                description "Destination realm reachable via the static route";
                            }

                            ///nokia-conf/configure/aaa/diameter/node/peer/route/application
                            leaf application {
                                type enumeration {
                                    enum nasreq                         { value 1; }
                                    enum gy                             { value 4; }
                                    enum gx                             { value 16777238; }
                                }
                                sros-ext:immutable;
                                mandatory true;
                                description "Diameter application to which the route applies";
                            }

                            ///nokia-conf/configure/aaa/diameter/node/peer/route/preference
                            leaf preference {
                                type uint32 {
                                    range "1..100";
                                }
                                default "50";
                                description "Static route preference; lower value is preferred";
                            }

                            ///nokia-conf/configure/aaa/diameter/node/peer/route/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/aaa/diameter/node/peer/route/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list route

                    } // list peer

                } // list node

                ///nokia-conf/configure/aaa/diameter/peer-policy
                list peer-policy {
                    status obsolete;
                    key "name";
                    max-elements 32;
                    description "Enter the peer-policy list instance";

                    ///nokia-conf/configure/aaa/diameter/peer-policy/name
                    leaf name {
                        status obsolete;
                        type types-sros:named-item;
                        description "Diameter peer policy name";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/description
                    leaf description {
                        status obsolete;
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/origin-host
                    leaf origin-host {
                        status obsolete;
                        type string {
                            length "1..80";
                        }
                        description "Origin-Host AVP sent in all Diameter messages";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/origin-realm
                    leaf origin-realm {
                        status obsolete;
                        type string {
                            length "1..80";
                        }
                        description "Origin-Realm AVP sent in all Diameter messages";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/router-instance
                    leaf router-instance {
                        status obsolete;
                        type string;
                        default "Base";
                        description "Diameter peer policy router";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/ipv4-source-address
                    leaf ipv4-source-address {
                        status obsolete;
                        type types-sros:ipv4-unicast-address;
                        description "IPv4 source address for peering connection";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/ipv6-source-address
                    leaf ipv6-source-address {
                        status obsolete;
                        type types-sros:ipv6-unicast-address;
                        description "IPv6 source address for IPv6 peering connection";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/watchdog-timer
                    leaf watchdog-timer {
                        status obsolete;
                        type types-sros:time-duration {
                            range "1..1000";
                        }
                        units "seconds";
                        default "30";
                        description "Time between consecutive watchdog messages";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/connection-timer
                    leaf connection-timer {
                        status obsolete;
                        type types-sros:time-duration {
                            range "1..1000";
                        }
                        units "seconds";
                        default "30";
                        description "Wait time before attempting reconnection to peer";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/transaction-timer
                    leaf transaction-timer {
                        status obsolete;
                        type types-sros:time-duration {
                            range "1..1000";
                        }
                        units "seconds";
                        default "30";
                        description "Timeout for base Diameter messages (DWR, CER, DPR)";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/vendor-support
                    leaf vendor-support {
                        status obsolete;
                        type types-diam:diam-vendor-support;
                        default "three-gpp";
                        description "Vendor support announced in the capability exchange";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/python-policy
                    leaf python-policy {
                        status obsolete;
                        type leafref {
                            path "../../../../python/python-policy/name";
                        }
                        description "Name of the python policy for Diameter processing";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/role
                    leaf role {
                        status obsolete;
                        type enumeration {
                            enum client                         { value 0; }
                            enum proxy                          { value 1; }
                        }
                        sros-ext:immutable;
                        default "client";
                        description "Client or proxy role of a Diameter peer policy";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/apply-groups
                    leaf-list apply-groups {
                        status obsolete;
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        status obsolete;
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/aaa/diameter/peer-policy/applications
                    container applications {
                        status obsolete;
                        description "Enter the applications context";

                        ///nokia-conf/configure/aaa/diameter/peer-policy/applications/gx
                        leaf gx {
                            status obsolete;
                            type boolean;
                            default "false";
                            description "Advertise Gx application support in CER messages";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/applications/gy
                        leaf gy {
                            status obsolete;
                            type boolean;
                            default "false";
                            description "Advertise Gy application support in CER messages";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/applications/nasreq
                        leaf nasreq {
                            status obsolete;
                            type boolean;
                            default "false";
                            description "Advertise NASREQ application support in CER messages";
                        }

                    } // container applications

                    ///nokia-conf/configure/aaa/diameter/peer-policy/peer
                    list peer {
                        status obsolete;
                        key "peer-name";
                        max-elements 5;
                        description "Enter the peer list instance";

                        ///nokia-conf/configure/aaa/diameter/peer-policy/peer/peer-name
                        leaf peer-name {
                            status obsolete;
                            type types-sros:named-item;
                            description "Diameter peer name";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/peer/admin-state
                        leaf admin-state {
                            status obsolete;
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the peer";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/peer/address
                        leaf address {
                            status obsolete;
                            type types-sros:ip-unicast-address;
                            sros-ext:auto-restart-to-modify;
                            description "Diameter peer address";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/peer/destination-host
                        leaf destination-host {
                            status obsolete;
                            type types-sros:string-not-all-spaces {
                                length "1..80";
                            }
                            sros-ext:auto-restart-to-modify;
                            description "Destination-Host AVP string for Diameter messages";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/peer/destination-realm
                        leaf destination-realm {
                            status obsolete;
                            type types-sros:string-not-all-spaces {
                                length "1..80";
                            }
                            sros-ext:auto-restart-to-modify;
                            description "Destination-Realm AVP string for Diameter messages";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/peer/watchdog-timer
                        leaf watchdog-timer {
                            status obsolete;
                            type types-sros:time-duration {
                                range "1..1000";
                            }
                            units "seconds";
                            description "Time between consecutive watchdog messages";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/peer/connection-timer
                        leaf connection-timer {
                            status obsolete;
                            type types-sros:time-duration {
                                range "1..1000";
                            }
                            units "seconds";
                            description "Wait time before attempting reconnection to a peer";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/peer/transaction-timer
                        leaf transaction-timer {
                            status obsolete;
                            type types-sros:time-duration {
                                range "1..1000";
                            }
                            units "seconds";
                            description "Timeout for base Diameter messages (DWR, CER, DPR)";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/peer/preference
                        leaf preference {
                            status obsolete;
                            type uint32 {
                                range "1..100";
                            }
                            default "50";
                            description "Peer preference of the Diameter peer policy";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/peer/apply-groups
                        leaf-list apply-groups {
                            status obsolete;
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/peer/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            status obsolete;
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/peer/transport
                        container transport {
                            status obsolete;
                            description "Enter the transport context";

                            ///nokia-conf/configure/aaa/diameter/peer-policy/peer/transport/port-number
                            leaf port-number {
                                status obsolete;
                                type uint32 {
                                    range "1..65535";
                                }
                                sros-ext:auto-restart-to-modify;
                                default "3868";
                                description "Destination TCP port number for the policy peer";
                            }

                        } // container transport

                        ///nokia-conf/configure/aaa/diameter/peer-policy/peer/statistics
                        container statistics {
                            status obsolete;
                            description "Enter the statistics context";

                        } // container statistics

                    } // list peer

                    ///nokia-conf/configure/aaa/diameter/peer-policy/proxy
                    container proxy {
                        status obsolete;
                        when "../role = 'proxy'" {
                            description "'role' must be proxy";
                        }
                        description "Enter the proxy context";

                        ///nokia-conf/configure/aaa/diameter/peer-policy/proxy/admin-state
                        leaf admin-state {
                            status obsolete;
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of Diameter proxy";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/proxy/router-instance
                        leaf router-instance {
                            status obsolete;
                            type string;
                            sros-ext:auto-restart-to-modify;
                            description "Routing context associated with Diameter proxy";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/proxy/local-address
                        leaf local-address {
                            status obsolete;
                            type types-sros:ip-unicast-address;
                            sros-ext:auto-restart-to-modify;
                            description "Source IP address on which the Diameter proxy listens";
                        }

                        ///nokia-conf/configure/aaa/diameter/peer-policy/proxy/mcs-peer
                        container mcs-peer {
                            status obsolete;
                            presence "Enable the mcs-peer context";
                            description "Enable the mcs-peer context";

                            ///nokia-conf/configure/aaa/diameter/peer-policy/proxy/mcs-peer/address
                            leaf address {
                                status obsolete;
                                type leafref {
                                    path "../../../../../../redundancy/multi-chassis/peer/ip-address";
                                }
                                sros-ext:immutable;
                                mandatory true;
                                description "MCS peer address";
                            }

                            ///nokia-conf/configure/aaa/diameter/peer-policy/proxy/mcs-peer/sync-tag
                            leaf sync-tag {
                                status obsolete;
                                type types-sros:named-item;
                                sros-ext:immutable;
                                mandatory true;
                                description "Synchronization tag shared by MCS peers";
                            }

                            ///nokia-conf/configure/aaa/diameter/peer-policy/proxy/mcs-peer/apply-groups
                            leaf-list apply-groups {
                                status obsolete;
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/aaa/diameter/peer-policy/proxy/mcs-peer/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                status obsolete;
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // container mcs-peer

                    } // container proxy

                } // list peer-policy

            } // container diameter

            ///nokia-conf/configure/aaa/wpp
            container wpp {
                description "Enter the wpp context";

                ///nokia-conf/configure/aaa/wpp/system-name
                leaf system-name {
                    type string {
                        length "1..16";
                    }
                    description "System name used in WPP protocol messages";
                }

                ///nokia-conf/configure/aaa/wpp/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/aaa/wpp/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/aaa/wpp/portal-group
                list portal-group {
                    key "group-name";
                    description "Enter the portal-group list instance";

                    ///nokia-conf/configure/aaa/wpp/portal-group/group-name
                    leaf group-name {
                        type types-sros:named-item;
                        description "Portal group name";
                    }

                    ///nokia-conf/configure/aaa/wpp/portal-group/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/aaa/wpp/portal-group/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the portal group";
                    }

                    ///nokia-conf/configure/aaa/wpp/portal-group/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/aaa/wpp/portal-group/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/aaa/wpp/portal-group/portal
                    list portal {
                        key "router-instance name";
