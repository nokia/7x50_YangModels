submodule nokia-conf-aaa {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import ietf-inet-types                { prefix "inet"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-diameter           { prefix "types-diam"; }
    import nokia-types-nat                { prefix "types-nat"; }
    import nokia-types-radius             { prefix "types-radius"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-groups;
    include nokia-conf-python;
    include nokia-conf-redundancy;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure authentication, authorization and accounting on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-aaa YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2022-11-14";

    grouping conf-aaa {
        ///nokia-conf/configure/aaa
        container aaa {
            description "Enable the aaa context";

            ///nokia-conf/configure/aaa/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/aaa/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/aaa/radius
            container radius {
                description "Enter the radius context";

                ///nokia-conf/configure/aaa/radius/coa-port
                leaf coa-port {
                    type inet:port-number {
                        range "1647|1700|1812|3799";
                    }
                    default "3799";
                    description "RADIUS listening port for CoA and Disconnect messages";
                }

                ///nokia-conf/configure/aaa/radius/acct-on-off-group
                list acct-on-off-group {
                    key "name";
                    max-elements 32;
                    description "Enter the acct-on-off-group list instance";

                    ///nokia-conf/configure/aaa/radius/acct-on-off-group/name
                    leaf name {
                        type types-sros:named-item;
                        description "Group name for accounting on/off";
                    }

                    ///nokia-conf/configure/aaa/radius/acct-on-off-group/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/aaa/radius/acct-on-off-group/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/aaa/radius/acct-on-off-group/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list acct-on-off-group

                ///nokia-conf/configure/aaa/radius/server-policy
                list server-policy {
                    key "name";
                    max-elements 32;
                    description "Enter the server-policy list instance";

                    ///nokia-conf/configure/aaa/radius/server-policy/name
                    leaf name {
                        type types-sros:named-item;
                        description "RADIUS server policy name";
                    }

                    ///nokia-conf/configure/aaa/radius/server-policy/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/aaa/radius/server-policy/python-policy
                    leaf python-policy {
                        type leafref {
                            path "../../../../python/python-policy/name";
                        }
                        description "Python policy for RADIUS packets to/from RADIUS servers";
                    }

                    ///nokia-conf/configure/aaa/radius/server-policy/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/aaa/radius/server-policy/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/aaa/radius/server-policy/servers
                    container servers {
                        description "Enter the servers context";

                        ///nokia-conf/configure/aaa/radius/server-policy/servers/timeout
                        leaf timeout {
                            type types-sros:time-duration {
                                range "1..340";
                            }
                            units "seconds";
                            default "5";
                            description "Time until the next retry to the RADIUS server";
                        }

                        ///nokia-conf/configure/aaa/radius/server-policy/servers/retry-count
                        leaf retry-count {
                            type uint32 {
                                range "1..256";
                            }
                            default "3";
                            description "Number of retries for contacting the RADIUS server";
                        }

                        ///nokia-conf/configure/aaa/radius/server-policy/servers/hold-down-time
                        leaf hold-down-time {
                            type types-sros:time-duration {
                                range "30..86400";
                            }
                            units "seconds";
                            default "30";
                            description "Hold time before reusing a RADIUS server that was down";
                        }

                        ///nokia-conf/configure/aaa/radius/server-policy/servers/router-instance
                        leaf router-instance {
                            type string;
                            description "RADIUS routing instance";
                        }

                        ///nokia-conf/configure/aaa/radius/server-policy/servers/source-address
                        leaf source-address {
                            type types-sros:ipv4-address;
                            description "Source address of RADIUS messages";
                        }

                        ///nokia-conf/configure/aaa/radius/server-policy/servers/ipv6-source-address
                        leaf ipv6-source-address {
                            type types-sros:ipv6-address;
                            description "Source address for IPv6 RADIUS datagrams";
                        }

                        ///nokia-conf/configure/aaa/radius/server-policy/servers/access-algorithm
                        leaf access-algorithm {
                            type types-radius:server-selection-algo;
                            description "Algorithm to select a RADIUS server from the pool";
                        }

                        ///nokia-conf/configure/aaa/radius/server-policy/servers/stickiness
                        leaf stickiness {
                            type boolean;
                            default "true";
                            description "Allow stickiness in a multi-server application";
                        }

                        ///nokia-conf/configure/aaa/radius/server-policy/servers/server
                        list server {
                            key "server-index";
                            max-elements 32;
                            description "Enter the server list instance";

                            ///nokia-conf/configure/aaa/radius/server-policy/servers/server/server-index
                            leaf server-index {
                                type uint32 {
                                    range "1..16";
                                }
                                description "RADIUS server index";
                            }

                            ///nokia-conf/configure/aaa/radius/server-policy/servers/server/server-name
                            leaf server-name {
                                type types-sros:named-item;
                                mandatory true;
                                description "RADIUS server name";
                            }

                            ///nokia-conf/configure/aaa/radius/server-policy/servers/server/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/aaa/radius/server-policy/servers/server/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list server

                        ///nokia-conf/configure/aaa/radius/server-policy/servers/buffering
                        container buffering {
                            description "Enter the buffering context";

                            ///nokia-conf/configure/aaa/radius/server-policy/servers/buffering/acct-start
                            container acct-start {
                                presence "Enable the acct-start context";
                                description "Enable the acct-start context";

                                ///nokia-conf/configure/aaa/radius/server-policy/servers/buffering/acct-start/min
                                leaf min {
                                    type types-sros:time-duration {
                                        range "1..3600";
                                    }
                                    units "seconds";
                                    mandatory true;
                                    description "Minimum time between accounting message resend attempts";
                                }

                                ///nokia-conf/configure/aaa/radius/server-policy/servers/buffering/acct-start/max
                                leaf max {
                                    type types-sros:time-duration {
                                        range "1..3600";
                                    }
                                    units "seconds";
                                    mandatory true;
                                    description "Maximum time between accounting message resend attempts";
                                }

                                ///nokia-conf/configure/aaa/radius/server-policy/servers/buffering/acct-start/lifetime
                                leaf lifetime {
                                    type types-sros:time-duration {
                                        range "1..25";
                                    }
                                    units "hours";
                                    mandatory true;
                                    description "Time accounting message can be in retransmission buffer";
                                }

                            } // container acct-start

                            ///nokia-conf/configure/aaa/radius/server-policy/servers/buffering/acct-interim
                            container acct-interim {
                                presence "Enable the acct-interim context";
                                description "Enable the acct-interim context";

                                ///nokia-conf/configure/aaa/radius/server-policy/servers/buffering/acct-interim/min
                                leaf min {
                                    type types-sros:time-duration {
                                        range "1..3600";
                                    }
                                    units "seconds";
                                    mandatory true;
                                    description "Minimum time between accounting message resend attempts";
                                }

                                ///nokia-conf/configure/aaa/radius/server-policy/servers/buffering/acct-interim/max
                                leaf max {
                                    type types-sros:time-duration {
                                        range "1..3600";
                                    }
                                    units "seconds";
                                    mandatory true;
                                    description "Maximum time between accounting message resend attempts";
                                }

                                ///nokia-conf/configure/aaa/radius/server-policy/servers/buffering/acct-interim/lifetime
                                leaf lifetime {
                                    type types-sros:time-duration {
                                        range "1..25";
                                    }
                                    units "hours";
                                    mandatory true;
                                    description "Time accounting message can be in retransmission buffer";
                                }

                            } // container acct-interim

                            ///nokia-conf/configure/aaa/radius/server-policy/servers/buffering/acct-stop
                            container acct-stop {
                                presence "Enable the acct-stop context";
                                description "Enable the acct-stop context";

                                ///nokia-conf/configure/aaa/radius/server-policy/servers/buffering/acct-stop/min
                                leaf min {
                                    type types-sros:time-duration {
                                        range "1..3600";
                                    }
                                    units "seconds";
                                    mandatory true;
                                    description "Minimum time between accounting message resend attempts";
                                }

                                ///nokia-conf/configure/aaa/radius/server-policy/servers/buffering/acct-stop/max
                                leaf max {
                                    type types-sros:time-duration {
                                        range "1..3600";
                                    }
                                    units "seconds";
                                    mandatory true;
                                    description "Maximum time between accounting message resend attempts";
                                }

                                ///nokia-conf/configure/aaa/radius/server-policy/servers/buffering/acct-stop/lifetime
                                leaf lifetime {
                                    type types-sros:time-duration {
                                        range "1..25";
                                    }
                                    units "hours";
                                    mandatory true;
                                    description "Time accounting message can be in retransmission buffer";
                                }

                            } // container acct-stop

                        } // container buffering

                        ///nokia-conf/configure/aaa/radius/server-policy/servers/health-check
                        container health-check {
                            description "Enter the health-check context";

                            ///nokia-conf/configure/aaa/radius/server-policy/servers/health-check/down-timeout
                            leaf down-timeout {
                                type types-sros:time-duration {
                                    range "1..340";
                                }
                                units "seconds";
                                description "Wait time before declaring RADIUS server out-of-service";
                            }

                            ///nokia-conf/configure/aaa/radius/server-policy/servers/health-check/test-account
                            container test-account {
                                description "Enter the test-account context";

                                ///nokia-conf/configure/aaa/radius/server-policy/servers/health-check/test-account/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    default "disable";
                                    description "Administrative state of the health check";
                                }

                                ///nokia-conf/configure/aaa/radius/server-policy/servers/health-check/test-account/interval
                                leaf interval {
                                    type types-sros:time-duration {
                                        range "1..60";
                                    }
                                    units "seconds";
                                    default "3";
                                    description "Time for test account to check health of RADIUS servers";
                                }

                                ///nokia-conf/configure/aaa/radius/server-policy/servers/health-check/test-account/user-name
                                leaf user-name {
                                    type types-sros:display-string {
                                        length "1..64";
                                    }
                                    description "Username to send access requests to probe RADIUS server";
                                }

                                ///nokia-conf/configure/aaa/radius/server-policy/servers/health-check/test-account/password
                                leaf password {
                                    type types-sros:encrypted-leaf {
                                        length "1..115";
                                    }
                                    description "Test account password for RADIUS server health check";
                                }

                            } // container test-account

                        } // container health-check

                    } // container servers

                    ///nokia-conf/configure/aaa/radius/server-policy/acct-on-off
                    container acct-on-off {
                        presence "Enable the acct-on-off context";
                        description "Enable the acct-on-off context";

                        ///nokia-conf/configure/aaa/radius/server-policy/acct-on-off/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/aaa/radius/server-policy/acct-on-off/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/aaa/radius/server-policy/acct-on-off/mode
                        choice mode {
                            ///nokia-conf/configure/aaa/radius/server-policy/acct-on-off/mode/monitor
                            case monitor {

                                ///nokia-conf/configure/aaa/radius/server-policy/acct-on-off/mode/monitor/monitor
                                leaf monitor {
                                    type leafref {
                                        path "../../../acct-on-off-group/name";
                                    }
                                    sros-ext:immutable;
                                    description "Accounting on/off group name";
                                }

                            }
                            ///nokia-conf/configure/aaa/radius/server-policy/acct-on-off/mode/oper-state-change
                            case oper-state-change {

                                ///nokia-conf/configure/aaa/radius/server-policy/acct-on-off/mode/oper-state-change/oper-state-change
                                container oper-state-change {
                                    presence "Enable the oper-state-change context";
                                    description "Enable the oper-state-change context";

                                    ///nokia-conf/configure/aaa/radius/server-policy/acct-on-off/mode/oper-state-change/oper-state-change/group
                                    leaf group {
                                        type leafref {
                                            path "../../../../acct-on-off-group/name";
                                        }
                                        sros-ext:immutable;
                                        description "Operational state for a group";
                                    }

                                } // container oper-state-change

                            }
                        }
                    } // container acct-on-off

                } // list server-policy

                ///nokia-conf/configure/aaa/radius/route-downloader
                list route-downloader {
                    key "name";
                    max-elements 1;
                    description "Enter the route-downloader list instance";

                    ///nokia-conf/configure/aaa/radius/route-downloader/name
                    leaf name {
                        type types-sros:named-item;
                        description "RADIUS route downloader name";
                    }

                    ///nokia-conf/configure/aaa/radius/route-downloader/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/aaa/radius/route-downloader/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of this route downloader";
                    }

                    ///nokia-conf/configure/aaa/radius/route-downloader/radius-server-policy
                    leaf radius-server-policy {
                        type leafref {
                            path "../../server-policy/name";
                        }
                        sros-ext:auto-restart-to-modify;
                        description "RADIUS server policy referenced";
                    }

                    ///nokia-conf/configure/aaa/radius/route-downloader/download-interval
                    leaf download-interval {
                        type uint32 {
                            range "1..1440";
                        }
                        units "minutes";
                        default "720";
                        description "Wait time between consecutive runs of the process";
                    }

                    ///nokia-conf/configure/aaa/radius/route-downloader/default-metric
                    leaf default-metric {
                        type uint32 {
                            range "0..254";
                        }
                        default "2";
                        description "Default metric that RTM imported routes acquire";
                    }

                    ///nokia-conf/configure/aaa/radius/route-downloader/default-tag
                    leaf default-tag {
                        type uint32 {
                            range "0..4294967295";
                        }
                        default "0";
                        description "Default tag of this route downloader";
                    }

                    ///nokia-conf/configure/aaa/radius/route-downloader/max-routes
                    leaf max-routes {
                        type uint32 {
                            range "1..200000";
                        }
                        default "200000";
                        description "Maximum routes imported by this route downloader";
                    }

                    ///nokia-conf/configure/aaa/radius/route-downloader/base-user-name
                    leaf base-user-name {
                        type types-sros:named-item;
                        description "Prefix of the username used as access requests";
                    }

                    ///nokia-conf/configure/aaa/radius/route-downloader/password
                    leaf password {
                        type types-sros:encrypted-leaf {
                            length "1..71";
                        }
                        description "Route downloader password for RADIUS access requests";
                    }

                    ///nokia-conf/configure/aaa/radius/route-downloader/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/aaa/radius/route-downloader/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/aaa/radius/route-downloader/retry-interval
                    container retry-interval {
                        description "Enter the retry-interval context";

                        ///nokia-conf/configure/aaa/radius/route-downloader/retry-interval/min
                        leaf min {
                            type uint32 {
                                range "1..1440";
                            }
                            units "minutes";
                            default "10";
                            description "Minimum duration of the retry interval";
                        }

                        ///nokia-conf/configure/aaa/radius/route-downloader/retry-interval/max
                        leaf max {
                            type uint32 {
                                range "1..1440";
                            }
                            units "minutes";
                            default "20";
                            description "Maximum duration of the retry interval";
                        }

                    } // container retry-interval

                } // list route-downloader

                ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy
                list l2tp-accounting-policy {
                    key "name";
                    max-elements 32;
                    description "Enter the l2tp-accounting-policy list instance";

                    ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/name
                    leaf name {
                        type types-sros:named-item;
                        description "L2TP RADIUS accounting policy name";
                    }

                    ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/acct-tunnel-connection-fmt
                    leaf acct-tunnel-connection-fmt {
                        type types-sros:display-string {
                            length "1..253";
                            pattern "([^%]+|%[nsStTcC%])+";
                        }
                        default "%n";
                        description "Accounting tunnel connection ASCII specification";
                    }

                    ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/radius-server-policy
                    leaf radius-server-policy {
                        type leafref {
                            path "../../server-policy/name";
                        }
                        description "RADIUS server policy referenced";
                    }

                    ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/accounting-type
                    container accounting-type {
                        description "Enter the accounting-type context";

                        ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/accounting-type/session
                        leaf session {
                            type boolean;
                            default "true";
                            description "Enable per-session accounting";
                        }

                        ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/accounting-type/tunnel
                        leaf tunnel {
                            type boolean;
                            default "true";
                            description "Enable per-tunnel accounting";
                        }

                    } // container accounting-type

                    ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/include-radius-attribute
                    container include-radius-attribute {
                        description "Enter the include-radius-attribute context";

                        ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/include-radius-attribute/calling-station-id
                        leaf calling-station-id {
                            type boolean;
                            default "false";
                            description "Include the Calling-Station-Id attribute";
                        }

                        ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/include-radius-attribute/nas-identifier
                        leaf nas-identifier {
                            type boolean;
                            default "false";
                            description "Include the NAS-Identifier attribute";
                        }

                        ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/include-radius-attribute/nas-port
                        container nas-port {
                            presence "Enable the nas-port context";
                            description "Enable the nas-port context";

                            ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/include-radius-attribute/nas-port/bit-spec
                            leaf bit-spec {
                                type types-sros:binary-specification {
                                    pattern "([01]|(\\*[0123456789]+)?[oismpvc])+";
                                }
                                mandatory true;
                                description "RADIUS NAS-Port attribute";
                            }

                        } // container nas-port

                        ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/include-radius-attribute/nas-port-id
                        container nas-port-id {
                            presence "Enable the nas-port-id context";
                            description "Enable the nas-port-id context";

                            ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/include-radius-attribute/nas-port-id/prefix-string
                            leaf prefix-string {
                                type types-sros:string-not-all-spaces {
                                    length "1..8";
                                }
                                description "Prefix string added to RADIUS NAS-Port attribute";
                            }

                            ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/include-radius-attribute/nas-port-id/suffix
                            leaf suffix {
                                type enumeration {
                                    enum circuit-id                     { value 1; }
                                    enum remote-id                      { value 2; }
                                }
                                description "NAS-Port-Id suffix type";
                            }

                        } // container nas-port-id

                        ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/include-radius-attribute/nas-port-type
                        container nas-port-type {
                            presence "Enable the nas-port-type context";
                            description "Enable the nas-port-type context";

                            ///nokia-conf/configure/aaa/radius/l2tp-accounting-policy/include-radius-attribute/nas-port-type/type
                            leaf type {
                                type union {
                                    type enumeration {
                                        enum rfc-aligned                    { value 1000; }
                                    }
                                    type uint32 {
                                        range "0..255";
                                    }
                                }
                                default "rfc-aligned";
