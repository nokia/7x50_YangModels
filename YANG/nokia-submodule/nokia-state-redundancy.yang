submodule nokia-state-redundancy {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-mcr                { prefix "types-mcr"; }
    import nokia-types-redundancy         { prefix "types-redundancy"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-system             { prefix "types-system"; }

    include nokia-state-isa;
    include nokia-state-lag;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for redundancy configuration on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-redundancy YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-01-09";

    grouping state-redundancy {
        ///nokia-state/state/redundancy
        container redundancy {
            description "Enable the redundancy context";

            ///nokia-state/state/redundancy/switchover-exec-status
            leaf switchover-exec-status {
                type types-system:script-state;
                description
                    "The results of executing the CLI script file specified
                     in 'switchover-exec'.";
            }

            ///nokia-state/state/redundancy/snmp-index-sync-mismatch
            leaf snmp-index-sync-mismatch {
                type boolean;
                description
                    "If SNMP index persistency is enabled on redundant CPMs and
                     the system is not configured to synchronize files between CPMs,
                     this parameter is true. It is false otherwise.";
            }

            ///nokia-state/state/redundancy/mgmt-ethernet
            container mgmt-ethernet {
                description "Enable the mgmt-ethernet context";

                ///nokia-state/state/redundancy/mgmt-ethernet/redundancy-standby
                leaf redundancy-standby {
                    type boolean;
                    description
                        "Indicates whether or not the management of the node
                         is operating from the standby CPM's management Ethernet port.";
                }

            } // container mgmt-ethernet

            ///nokia-state/state/redundancy/multi-chassis
            container multi-chassis {
                description "Enter the multi-chassis context";

                ///nokia-state/state/redundancy/multi-chassis/ipsec-domain
                list ipsec-domain {
                    key "id";
                    description "Enter the ipsec-domain list instance";

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/id
                    leaf id {
                        type uint32 {
                            range "1..255";
                        }
                        description "IPsec domain ID";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/activity-state
                    leaf activity-state {
                        type types-redundancy:mc-ipsec-activity-state;
                        description
                            "Indicates the result of the activity election between this ipsec domain entity
                             and its peers.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/protection-status
                    leaf protection-status {
                        type types-redundancy:tunnel-group-protection-status;
                        description
                            "Indicates the readiness of the ipsec domain to perform a forced switchover
                             of active/standby state from the current active chassis to the standby chassis.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/current-active
                    leaf current-active {
                        type types-sros:ip-address;
                        description "Indicates the router id of the current active chassis for this domain.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/local-router-id
                    leaf local-router-id {
                        type types-sros:ip-address;
                        description "Indicates local router id of the domain.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/dynamic-installed
                    leaf dynamic-installed {
                        type yang:gauge32;
                        description
                            "Indicates the number of dynamic multi-chassis tunnels installed
                             on this ipsec-domain.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/dynamic-installing
                    leaf dynamic-installing {
                        type yang:gauge32;
                        description
                            "Indicates the number of dynamic multi-chassis tunnels being
                             installed on this ipsec-domain.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/dynamic-standby-dormant
                    leaf dynamic-standby-dormant {
                        type yang:gauge32;
                        description
                            "Indicates the number of dynamic multi-chassis tunnels that
                             are in the standby dormant state on this ipsec-domain.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/dynamic-await-conf
                    leaf dynamic-await-conf {
                        type yang:gauge32;
                        description
                            "Indicates the number of dynamic multi-chassis tunnels awaiting
                             configuration on this ipsec-domain.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/dynamic-failed
                    leaf dynamic-failed {
                        type yang:gauge32;
                        description
                            "Indicates the number of dynamic multi-chassis tunnels failed
                             to install on this ipsec-domain.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/installed
                    leaf installed {
                        type yang:gauge32;
                        description
                            "Indicates the number of static multi-chassis tunnels installed
                             on this ipsec-domain.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/installing
                    leaf installing {
                        type yang:gauge32;
                        description
                            "Indicates the number of static multi-chassis tunnels being
                             installed on this ipsec-domain.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/standby-dormant
                    leaf standby-dormant {
                        type yang:gauge32;
                        description
                            "Indicates the number of static multi-chassis tunnels that
                             are in the standby dormant state on this ipsec-domain.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/await-conf
                    leaf await-conf {
                        type yang:gauge32;
                        description
                            "Indicates the number of static multi-chassis tunnels awaiting
                             configuration on this ipsec-domain.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/ipsec-domain/failed
                    leaf failed {
                        type yang:gauge32;
                        description
                            "Indicates the number of static multi-chassis tunnels failed
                             to install on this ipsec-domain.";
                    }

                } // list ipsec-domain

                ///nokia-state/state/redundancy/multi-chassis/mc-endpoint
                container mc-endpoint {
                    description "Enter the mc-endpoint context";

                    ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics
                    container statistics {
                        description "Enter the statistics context";

                        ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/received
                        container received {
                            description "Enter the received context";

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/received/packets
                            leaf packets {
                                type yang:counter32;
                                description "The number of control packets with valid authentication were received.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/received/keep-alive
                            leaf keep-alive {
                                type yang:counter32;
                                description "The number of keep-alive messages received.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/received/config
                            leaf config {
                                type yang:counter32;
                                description "The number of configuration packets received.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/received/peer-config
                            leaf peer-config {
                                type yang:counter32;
                                description "The number of peer configuration packets received.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/received/state
                            leaf state {
                                type yang:counter32;
                                description "The number of packets indicating the lag state received.";
                            }

                        } // container received

                        ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/dropped
                        container dropped {
                            description "Enter the dropped context";

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/dropped/too-short
                            leaf too-short {
                                type yang:counter32;
                                description "The number of control packets dropped because packet was too small.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/dropped/tlv-invalid-size
                            leaf tlv-invalid-size {
                                type yang:counter32;
                                description "The number of control packets dropped because of invalid size.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/dropped/out-of-sequence
                            leaf out-of-sequence {
                                type yang:counter32;
                                description "The number of control packets dropped because they were out of sequence.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/dropped/unknown-tlv
                            leaf unknown-tlv {
                                type yang:counter32;
                                description "The number of control packets dropped because of unknown TLV.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/dropped/invalid-endpoint-id
                            leaf invalid-endpoint-id {
                                type yang:counter32;
                                description
                                    "The number of control packets dropped because they refer to an invalid
                                     endpoint id.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/dropped/failed-md5
                            leaf failed-md5 {
                                type yang:counter32;
                                description "The number of control packets dropped because they failed MD5 authentication.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/dropped/peer-not-found
                            leaf peer-not-found {
                                type yang:counter32;
                                description "The number of control packets dropped because no peer was assigned or peer did not match.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/dropped/keep-alive-task
                            leaf keep-alive-task {
                                type yang:counter32;
                                description "The number of control packets dropped with invalid size, authentication or unknown peer.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/dropped/verification-failed
                            leaf verification-failed {
                                type yang:counter32;
                                description "The number of control packets dropped because of invalid formatting.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/dropped/unknown-peer
                            leaf unknown-peer {
                                type yang:counter32;
                                description "The number of control packets dropped because they are coming from an unknown peer.";
                            }

                        } // container dropped

                        ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/transmitted
                        container transmitted {
                            description "Enter the transmitted context";

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/transmitted/packets
                            leaf packets {
                                type yang:counter32;
                                description "The number of control packets transmitted.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/transmitted/keep-alive
                            leaf keep-alive {
                                type yang:counter32;
                                description "The number of keep-alive messages transmitted.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/transmitted/peer-config
                            leaf peer-config {
                                type yang:counter32;
                                description "The number of peer configuration packets transmitted.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/transmitted/failed
                            leaf failed {
                                type yang:counter32;
                                description "The number of packets failed to be transmitted.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/transmitted/config
                            leaf config {
                                type yang:counter32;
                                description "The number of configuration packets transmitted.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/statistics/transmitted/state
                            leaf state {
                                type yang:counter32;
                                description "The number of packets indicating lag state transmitted.";
                            }

                        } // container transmitted

                    } // container statistics

                    ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/endpoint
                    list endpoint {
                        key "id";
                        description "Enter the endpoint list instance";

                        ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/endpoint/id
                        leaf id {
                            type uint32 {
                                range "1..4294967295";
                            }
                            description "Multi-chassis end-point identifier.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/endpoint/statistics
                        container statistics {
                            description "Enter the statistics context";

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/endpoint/statistics/received
                            container received {
                                description "Enter the received context";

                                ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/endpoint/statistics/received/config
                                leaf config {
                                    type yang:counter32;
                                    description "The number of endpoint config packets received on this system for this endpoint.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/endpoint/statistics/received/state
                                leaf state {
                                    type yang:counter32;
                                    description "The number of endpoint state packets received on this system for this endpoint.";
                                }

                            } // container received

                            ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/endpoint/statistics/transmitted
                            container transmitted {
                                description "Enter the transmitted context";

                                ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/endpoint/statistics/transmitted/config
                                leaf config {
                                    type yang:counter32;
                                    description "The number of endpoint config packets transmitted on this system for this endpoint.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/endpoint/statistics/transmitted/state
                                leaf state {
                                    type yang:counter32;
                                    description "The number of endpoint state packets transmitted on this system for this endpoint.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/mc-endpoint/endpoint/statistics/transmitted/failed
                                leaf failed {
                                    type yang:counter32;
                                    description "The number of endpoint control packets failed to be transmitted on this system for this endpoint.";
                                }

                            } // container transmitted

                        } // container statistics

                    } // list endpoint

                } // container mc-endpoint

                ///nokia-state/state/redundancy/multi-chassis/mc-ring
                container mc-ring {
                    description "Enter the mc-ring context";

                    ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics
                    container statistics {
                        description "Enter the statistics context";

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/tx-packets
                        leaf tx-packets {
                            type yang:counter32;
                            description "The number of signalling packets transmitted by this system.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/tx-no-buffer
                        leaf tx-no-buffer {
                            type yang:counter32;
                            description "The number of signalling packets that could not be transmitted by this system due to lack of packet buffers.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/tx-failures
                        leaf tx-failures {
                            type yang:counter32;
                            description "The number of signalling packets that could not be transmitted by this system due to transmission failure.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/tx-unknown-dest
                        leaf tx-unknown-dest {
                            type yang:counter32;
                            description "The number of signalling packets that could not be transmitted by this system because the destination was unknown.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/rx-packets
                        leaf rx-packets {
                            type yang:counter32;
                            description "The number of signalling packets received by this system.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/rx-too-short
                        leaf rx-too-short {
                            type yang:counter32;
                            description "The number of signalling packets received by this system that were too short.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/rx-wrong-auth
                        leaf rx-wrong-auth {
                            type yang:counter32;
                            description "The number of signalling packets received by this system with invalid authentication.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/rx-invalid-tlv
                        leaf rx-invalid-tlv {
                            type yang:counter32;
                            description "The number of signalling packets received by thsi system with invalid TLV.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/rx-incomplete
                        leaf rx-incomplete {
                            type yang:counter32;
                            description "The number of signalling packets received by this system that were incomplete.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/rx-unknown-type
                        leaf rx-unknown-type {
                            type yang:counter32;
                            description "The number of signalling packets received by this system that were of unknown type.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/rx-unknown-peer
                        leaf rx-unknown-peer {
                            type yang:counter32;
                            description "The number of signalling packets received by this system that were related to an unknown peer.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/rx-unknown-ring
                        leaf rx-unknown-ring {
                            type yang:counter32;
                            description "The number of signalling packets received by this system that were related to an unknown ring.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/rx-unknown-ring-node
                        leaf rx-unknown-ring-node {
                            type yang:counter32;
                            description "The number of signalling packets received by this system that were related to an unknown ring node.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/rx-delivered-to-peer
                        leaf rx-delivered-to-peer {
                            type yang:counter32;
                            description "The number of signalling packets received by this system that were correctly delivered to their peer.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/rx-delivered-to-ring
                        leaf rx-delivered-to-ring {
                            type yang:counter32;
                            description "The number of signalling packets received by this system that were correctly delivered to their ring.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/rx-delivered-to-ring-node
                        leaf rx-delivered-to-ring-node {
                            type yang:counter32;
                            description "The number of signalling packets received by this system that were correctly delivered to their ring node.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/missed-config-events
                        leaf missed-config-events {
                            type yang:counter32;
                            description "The number of missed configuration events on this system.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/mc-ring/statistics/missed-bfd-events
                        leaf missed-bfd-events {
                            type yang:counter32;
                            description "The number of missed BFD events on this system.";
                        }

                    } // container statistics

                } // container mc-ring

                ///nokia-state/state/redundancy/multi-chassis/peer
                list peer {
                    key "ip-address";
                    description "Enter the peer list instance";

                    ///nokia-state/state/redundancy/multi-chassis/peer/ip-address
                    leaf ip-address {
                        type types-sros:ip-unicast-address;
                        description "Peer IP address";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/peer/source-oper-address
                    leaf source-oper-address {
                        type types-sros:ip-address;
                        description
                            "Indicates the operational source address in use
                             to communicate with the multi-chassis peer.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/peer/rings-oper-state
                    leaf rings-oper-state {
                        type types-sros:oper-state;
                        description
                            "Indicates the operational state of this multi-chassis peer
                             with respect to multi-chassis ring operation.";
                    }

                    ///nokia-state/state/redundancy/multi-chassis/peer/options
                    container options {
                        description "Enter the options context";

                        ///nokia-state/state/redundancy/multi-chassis/peer/options/sub-mgmt
                        container sub-mgmt {
                            description "Enter the sub-mgmt context";

                            ///nokia-state/state/redundancy/multi-chassis/peer/options/sub-mgmt/dhcp-leasetime-threshold
                            container dhcp-leasetime-threshold {
                                description "Enter the dhcp-leasetime-threshold context";

                                ///nokia-state/state/redundancy/multi-chassis/peer/options/sub-mgmt/dhcp-leasetime-threshold/active
                                leaf active {
                                    type types-sros:time-duration;
                                    description
                                        "The active dhcp-leasetime-threshold of this multi-chassis peer.
                                         It is the minimum between the local configured dhcp-leasetime-threshold
                                         and the received remote dhcp-leatime-threshold.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/options/sub-mgmt/dhcp-leasetime-threshold/remote
                                leaf remote {
                                    type types-sros:time-duration;
                                    description "The dhcp-leasetime-threshold recieved from this multi-chassis peer.";
                                }

                            } // container dhcp-leasetime-threshold

                        } // container sub-mgmt

                    } // container options

                    ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint
                    container mc-endpoint {
                        description "Enable the mc-endpoint context";

                        ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/oper-state
                        leaf oper-state {
                            type types-sros:oper-state;
                            description "Operational status of the multi-chassis endpoint peer.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/passive-mode-operation
                        leaf passive-mode-operation {
                            type boolean;
                            description
                                "Indicates whether passive-mode behavior is operationally enabled on this multi-chassis
                                 endpoint peer.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/peer-reference-counter
                        leaf peer-reference-counter {
                            type uint32;
                            description "The number of service endpoints referencing this multi-chassis endpoint.";
                        }

                        ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/statistics
                        container statistics {
                            description "Enter the statistics context";

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/statistics/received
                            container received {
                                description "Enter the received context";

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/statistics/received/packets
                                leaf packets {
                                    type yang:counter32;
                                    description "The number of control packets with valid authentication were received.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/statistics/received/keep-alive
                                leaf keep-alive {
                                    type yang:counter32;
                                    description "The number of keep-alive messages received.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/statistics/received/config
                                leaf config {
                                    type yang:counter32;
                                    description "The number of configuration packets received.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/statistics/received/peer-config
                                leaf peer-config {
                                    type yang:counter32;
                                    description "The number of peer configuration packets received.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/statistics/received/state
                                leaf state {
                                    type yang:counter32;
                                    description "The number of packets indicating the lag state received.";
                                }

                            } // container received

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/statistics/dropped
                            container dropped {
                                description "Enter the dropped context";

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/statistics/dropped/too-short
                                leaf too-short {
                                    type yang:counter32;
                                    description "The number of control packets dropped because packet was too small.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/statistics/dropped/tlv-invalid-size
                                leaf tlv-invalid-size {
                                    type yang:counter32;
                                    description "The number of control packets dropped because of invalid size.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/statistics/dropped/out-of-sequence
                                leaf out-of-sequence {
                                    type yang:counter32;
                                    description "The number of control packets dropped because they were out of sequence.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/statistics/dropped/unknown-tlv
                                leaf unknown-tlv {
                                    type yang:counter32;
                                    description "The number of control packets dropped because of unknown TLV.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/statistics/dropped/invalid-endpoint-id
                                leaf invalid-endpoint-id {
                                    type yang:counter32;
                                    description
                                        "The number of control packets dropped because they refer to an invalid
                                         endpoint id.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-endpoint/statistics/dropped/failed-md5
                                leaf failed-md5 {
