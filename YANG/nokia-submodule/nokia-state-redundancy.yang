                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/statistics/tx-signalling-msg
                            leaf tx-signalling-msg {
                                type yang:counter32;
                                description "The number of signalling messages transmitted to the peer.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/statistics/rx-signalling-msg
                            leaf rx-signalling-msg {
                                type yang:counter32;
                                description "The number of valid signalling messages received from the peer.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/statistics/tx-mcs-id-requests
                            leaf tx-mcs-id-requests {
                                type yang:counter32;
                                description "The number of MCS ID requests transmitted to the peer.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/statistics/rx-mcs-id-requests
                            leaf rx-mcs-id-requests {
                                type yang:counter32;
                                description "The number of valid MCS ID requests received from the peer.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/statistics/tx-mcs-id-responses
                            leaf tx-mcs-id-responses {
                                type yang:counter32;
                                description "The number of MCS ID responses transmitted to the peer.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/statistics/rx-mcs-id-responses
                            leaf rx-mcs-id-responses {
                                type yang:counter32;
                                description "The number of valid MCS ID responses received from the peer.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/statistics/tx-ring-exists-requests
                            leaf tx-ring-exists-requests {
                                type yang:counter32;
                                description "The number of ring-exists requests transmitted to the peer.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/statistics/rx-ring-exists-requests
                            leaf rx-ring-exists-requests {
                                type yang:counter32;
                                description "The number of valid ring-exists requests received from the peer.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/statistics/tx-ring-exists-responses
                            leaf tx-ring-exists-responses {
                                type yang:counter32;
                                description "The number of ring-exists responses transmitted to the peer.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/statistics/rx-ring-exists-responses
                            leaf rx-ring-exists-responses {
                                type yang:counter32;
                                description "The number of valid ring-exists responses received from the peer.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/statistics/tx-keep-alive
                            leaf tx-keep-alive {
                                type yang:counter32;
                                description "The number of keep-alive messages transmitted to the peer.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/statistics/rx-keep-alive
                            leaf rx-keep-alive {
                                type yang:counter32;
                                description "The number of valid keep-alive messages received from the peer.";
                            }

                        } // container statistics

                        ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring
                        list ring {
                            key "sync-tag";
                            description "Enter the ring list instance";

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/sync-tag
                            leaf sync-tag {
                                type types-sros:named-item;
                                description "Tag for synchronizing with the multi-chassis peer";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/oper-state
                            leaf oper-state {
                                type enumeration {
                                    enum unknown                        { value 1; }
                                    enum shutdown                       { value 2; }
                                    enum config-error                   { value 3; }
                                    enum no-peer                        { value 4; }
                                    enum connected                      { value 5; }
                                    enum broken                         { value 6; }
                                    enum local-broken                   { value 7; }
                                    enum conflict                       { value 8; }
                                    enum testing-ring                   { value 9; }
                                    enum half-broken                    { value 10; }
                                    enum waiting-for-peer               { value 11; }
                                }
                                description "The operational state of the multi-chassis ring";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/failure-reason
                            leaf failure-reason {
                                type types-sros:display-string {
                                    length "1..80";
                                }
                                description "The reason why the multi-chassis ring is in one of the failed states.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/in-band-control-path
                            container in-band-control-path {
                                description "Enter the in-band-control-path context";

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/in-band-control-path/oper-state
                                leaf oper-state {
                                    type types-mcr:ring-oper-state;
                                    description "The operational state of the in-band control connection.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/in-band-control-path/port-id
                                leaf port-id {
                                    type types-sros:port;
                                    description "The port associated with this ring.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/in-band-control-path/src-ip
                                leaf src-ip {
                                    type types-sros:ipv4-unicast-address;
                                    description "The source address used in the in-band control connection.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/in-band-control-path/debounce-start
                                leaf debounce-start {
                                    type yang:date-and-time;
                                    description
                                        "The last time the debounce timer was started after the in-band control
                                         connection transitioned from connected to broken.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/in-band-control-path/debounce-timeout
                                leaf debounce-timeout {
                                    type types-sros:time-interval;
                                    description
                                        "The value of the debounce timer at the time indicated by
                                         debounce-start. Data traffic may have been dropped starting from
                                         debounce-start during the interval indicated by debounce-timeout.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/in-band-control-path/debounce-guard-time
                                leaf debounce-guard-time {
                                    type uint32;
                                    units "seconds";
                                    description
                                        "The value of the debounce guard time. If no transitions of the
                                         operational state of the in-band control connection occur during the 
                                         debounce guard time, the debounce mechanism is reset.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/in-band-control-path/controls-path-b
                                leaf controls-path-b {
                                    type boolean;
                                    description "Indicates if the current node controls path-b or not.";
                                }

                            } // container in-band-control-path

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/oper-path-b
                            container oper-path-b {
                                description "Enter the oper-path-b context";

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/oper-path-b/wildcard-saps
                                leaf wildcard-saps {
                                    type boolean;
                                    description "Include the SAPs starting with a wildcard '*'";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/oper-path-b/range
                                list range {
                                    key "start end";
                                    description "Enter the range list instance";

                                    ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/oper-path-b/range/start
                                    leaf start {
                                        type uint32 {
                                            range "0..4094";
                                        }
                                        description "Lower bound of the VLAN range";
                                    }

                                    ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/oper-path-b/range/end
                                    leaf end {
                                        type uint32 {
                                            range "0..4094";
                                        }
                                        description "Upper bound of the VLAN range";
                                    }

                                } // list range

                            } // container oper-path-b

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/oper-path-excl
                            container oper-path-excl {
                                description "Enter the oper-path-excl context";

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/oper-path-excl/wildcard-saps
                                leaf wildcard-saps {
                                    type boolean;
                                    description "Include the SAPs starting with a wildcard '*'";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/oper-path-excl/range
                                list range {
                                    key "start end";
                                    description "Enter the range list instance";

                                    ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/oper-path-excl/range/start
                                    leaf start {
                                        type uint32 {
                                            range "0..4094";
                                        }
                                        description "Lower bound of the VLAN range";
                                    }

                                    ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/oper-path-excl/range/end
                                    leaf end {
                                        type uint32 {
                                            range "0..4094";
                                        }
                                        description "Upper bound of the VLAN range";
                                    }

                                } // list range

                            } // container oper-path-excl

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/statistics
                            container statistics {
                                description "Enter the statistics context";

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/statistics/tx-saps-changed
                                leaf tx-saps-changed {
                                    type yang:counter32;
                                    description "The number of SAPS CHANGED messages transmitted to the peer.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/statistics/rx-saps-changed
                                leaf rx-saps-changed {
                                    type yang:counter32;
                                    description "The number of valid SAPS CHANGED messages received from the peer.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/statistics/tx-opaque
                                leaf tx-opaque {
                                    type yang:counter32;
                                    description "The number of OPAQUE messages transmitted to the peer.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/statistics/rx-opaque
                                leaf rx-opaque {
                                    type yang:counter32;
                                    description
                                        "The number of valid OPAQUE messages received from the peer and delivered
                                         to the multi-chassis ring.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/statistics/rx-opaque-no-dest
                                leaf rx-opaque-no-dest {
                                    type yang:counter32;
                                    description
                                        "The number of valid OPAQUE messages received from the peer for which no
                                         valid destination was found.";
                                }

                            } // container statistics

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/ring-node
                            list ring-node {
                                key "name";
                                description "Enter the ring-node list instance";

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/name
                                leaf name {
                                    type types-sros:named-item;
                                    description "Name of the multi-chassis ring access node";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/local-oper-state
                                leaf local-oper-state {
                                    type types-mcr:ring-node-oper-state;
                                    description "The operational state of the ring node as monitored by the local chassis.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/remote-oper-state
                                leaf remote-oper-state {
                                    type types-mcr:ring-node-oper-state;
                                    description "The operational state of the ring node as monitored by the remote chassis.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/in-use
                                leaf in-use {
                                    type boolean;
                                    description
                                        "Indicates if this ring node is currently used by any subscriber or configured
                                         as ring-node on any SAP.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/failure-reason
                                leaf failure-reason {
                                    type types-sros:display-string {
                                        length "1..80";
                                    }
                                    description "The reason why the ring-node is in one of the failed states.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/statistics
                                container statistics {
                                    description "Enter the statistics context";

                                    ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/statistics/tx-detected
                                    leaf tx-detected {
                                        type yang:counter32;
                                        description "The number of DETECTED RING-NODE signalling messages transmitted by this system.";
                                    }

                                    ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/statistics/rx-detected
                                    leaf rx-detected {
                                        type yang:counter32;
                                        description "The number of valid DETECTED RING-NODE signalling messages received from the peer.";
                                    }

                                    ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/statistics/tx-detected-ack
                                    leaf tx-detected-ack {
                                        type yang:counter32;
                                        description "The number of DETECTED RING-NODE signalling messages that were acknowledged by this system.";
                                    }

                                    ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/statistics/rx-detected-ack
                                    leaf rx-detected-ack {
                                        type yang:counter32;
                                        description "The number of DETECTED RING-NODE signalling messages that were acked by the peer.";
                                    }

                                    ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/statistics/tx-rncv-requests
                                    leaf tx-rncv-requests {
                                        type yang:counter32;
                                        description "The number of connectivity verification requests transmitted for this ring-node.";
                                    }

                                    ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/statistics/rx-rncv-responses
                                    leaf rx-rncv-responses {
                                        type yang:counter32;
                                        description "The number of valid connectivity verification responses received for this ring-node.";
                                    }

                                    ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/statistics/last-rncv-delay
                                    leaf last-rncv-delay {
                                        type types-sros:time-interval;
                                        description "The round-trip time of the last successful connectivity verification for this ring-node";
                                    }

                                } // container statistics

                            } // list ring-node

                            ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/debounce-event
                            list debounce-event {
                                key "index";
                                description "Enter the debounce-event list instance";

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/debounce-event/index
                                leaf index {
                                    type uint32;
                                    description "A unique value that identifies the debounce event.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/debounce-event/start
                                leaf start {
                                    type types-sros:date-time;
                                    description
                                        "The time that the debounce timer was started after the in-band control
                                         connection transitioned from connected to broken.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/debounce-event/debounce-time
                                leaf debounce-time {
                                    type uint32;
                                    units "seconds";
                                    description "The value of the debounce timer at the start of the debounce event.";
                                }

                                ///nokia-state/state/redundancy/multi-chassis/peer/mc-ring/ring/debounce-event/oper-state
                                leaf oper-state {
                                    type types-mcr:ring-oper-state;
                                    description "The operational state of the multi-chassis ring after debouncing.";
                                }

                            } // list debounce-event

                        } // list ring

                    } // container mc-ring

                    ///nokia-state/state/redundancy/multi-chassis/peer/wpp
                    container wpp {
                        description "Enter the wpp context";

                        ///nokia-state/state/redundancy/multi-chassis/peer/wpp/statistics
                        list statistics {
                            key "id";
                            description "Enter the statistics list instance";

                            ///nokia-state/state/redundancy/multi-chassis/peer/wpp/statistics/id
                            leaf id {
                                type uint32 {
                                    range "0..40";
                                }
                                description "The counter id.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/wpp/statistics/description
                            leaf description {
                                type types-sros:display-string {
                                    length "1..64";
                                }
                                description "Describes the statistic.";
                            }

                            ///nokia-state/state/redundancy/multi-chassis/peer/wpp/statistics/value
                            leaf value {
                                type yang:counter32;
                                description "The value of the counter.";
                            }

                        } // list statistics

                    } // container wpp

                } // list peer

            } // container multi-chassis

            ///nokia-state/state/redundancy/synchronization
            container synchronization {
                description "Enter the synchronization context";

                ///nokia-state/state/redundancy/synchronization/standby-status
                leaf standby-status {
                    type enumeration {
                        enum failed {
                            value -1;
                            description "The problem occurs.";
                        }
                        enum standby {
                            value 3;
                            description "The standby CPM in the redundant pair.";
                        }
                        enum disabled {
                            value 5;
                            description "CPM is not available.";
                        }
                        enum syncing {
                            value 6;
                            description "The redundant pair is syncing.";
                        }
                    }
                    description "Indicates the synchronization status of the standby CPM card.";
                }

                ///nokia-state/state/redundancy/synchronization/standby-failure
                leaf standby-failure {
                    type types-sros:display-string;
                    description "Indicates the reason why a hardware component 'failed'.";
                }

                ///nokia-state/state/redundancy/synchronization/standby-uptime
                leaf standby-uptime {
                    type yang:date-and-time;
                    description "The date and time of the last reboot.";
                }

                ///nokia-state/state/redundancy/synchronization/standby-version
                leaf standby-version {
                    type types-sros:display-string;
                    description
                        "The software product release version number for
                         the software image currently running on this card.";
                }

                ///nokia-state/state/redundancy/synchronization/failover-time
                leaf failover-time {
                    type types-redundancy:date-and-time-or-never;
                    description
                        "The time when the last redundancy failover occurred
                         causing a switchover from active to standby CPM.";
                }

                ///nokia-state/state/redundancy/synchronization/failover-reason
                leaf failover-reason {
                    type types-sros:description;
                    description
                        "A text string giving an explanation of the cause of
                         the last redundancy failover.";
                }

                ///nokia-state/state/redundancy/synchronization/sync-status
                leaf sync-status {
                    type enumeration {
                        enum unknown {
                            value 0;
                            description "No synchronization.";
                        }
                        enum config-only {
                            value 1;
                            description "Config only synchronized.";
                        }
                        enum boot-env {
                            value 2;
                            description "All boot environment synchronized.";
                        }
                        enum config-fail {
                            value 3;
                            description "Config synchronize failed.";
                        }
                        enum boot-env-fail {
                            value 4;
                            description "Boot environment synchronize failed.";
                        }
                        enum config-in-progress {
                            value 5;
                            description "Config synchronize in progress.";
                        }
                        enum boot-env-in-progress {
                            value 6;
                            description "Boot environment synchronize in progress.";
                        }
                    }
                    description
                        "The results of the last synchronization operation between
                         the primary and secondary CPMs.";
                }

                ///nokia-state/state/redundancy/synchronization/config-file-sync-time
                leaf config-file-sync-time {
                    type types-redundancy:date-and-time-or-never;
                    description
                        "The time of the last successful synchronization
                         of the configuration files.";
                }

                ///nokia-state/state/redundancy/synchronization/boot-file-sync-time
                leaf boot-file-sync-time {
                    type types-redundancy:date-and-time-or-never;
                    description
                        "The time of the last successful synchronization
                         of the boot files.";
                }

                ///nokia-state/state/redundancy/synchronization/rollback-sync-status
                leaf rollback-sync-status {
                    type types-redundancy:sync-status;
                    description
                        "The results of the last rollback synchronization operation
                         between the primary and secondary CPMs.";
                }

                ///nokia-state/state/redundancy/synchronization/rollback-sync-time
                leaf rollback-sync-time {
                    type types-redundancy:date-and-time-or-never;
                    description
                        "The time of the last successful synchronization
                         of the rollback files.";
                }

                ///nokia-state/state/redundancy/synchronization/cert-file-sync-status
                leaf cert-file-sync-status {
                    type types-redundancy:sync-status;
                    description
                        "The results of the last certificate synchronization
                         operation between the primary and secondary CPMs.";
                }

                ///nokia-state/state/redundancy/synchronization/cert-file-sync-time
                leaf cert-file-sync-time {
                    type types-redundancy:date-and-time-or-never;
                    description
                        "The time of the last successful synchronization
                         of the certificate files.";
                }

            } // container synchronization

            ///nokia-state/state/redundancy/bgp-evpn
            container bgp-evpn {
                description "Enter the bgp-evpn context";

                ///nokia-state/state/redundancy/bgp-evpn/ethernet-segment
                container ethernet-segment {
                    description "Enter the ethernet-segment context";

                    ///nokia-state/state/redundancy/bgp-evpn/ethernet-segment/boot-timer-remaining
                    leaf boot-timer-remaining {
                        type uint32;
                        units "seconds";
                        description
                            "specifies the time remaining for the system to wait after a node
                             reboot before running the BGP EVPN multi-homing DF (designated-forwarder) 
                             election algorithm for BGP EVPN services.";
                    }

                } // container ethernet-segment

            } // container bgp-evpn

        } // container redundancy

    } // grouping state-redundancy

}
