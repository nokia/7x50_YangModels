submodule nokia-conf-router-ospf3 {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-igp                { prefix "types-igp"; }
    import nokia-types-ospf               { prefix "types-ospf"; }
    import nokia-types-router             { prefix "types-router"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-groups;
    include nokia-conf-ipsec;
    include nokia-conf-policy-options;
    include nokia-conf-router;
    include nokia-conf-routing-options;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure router ospf protocol attributes on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-router-ospf3 YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-01-04";

    grouping conf-router-ospf3 {
        ///nokia-conf/configure/router/ospf3
        list ospf3 {
            key "ospf-instance";
            max-elements 32;
            description "Enter the ospf3 list instance";

            ///nokia-conf/configure/router/ospf3/ospf-instance
            leaf ospf-instance {
                type uint32 {
                    range "0..31|64..95";
                }
                description "Value for the integrated OSPF instance";
            }

            ///nokia-conf/configure/router/ospf3/admin-state
            leaf admin-state {
                type types-sros:admin-state;
                default "disable";
                description "Administrative state of the OSPF instance";
            }

            ///nokia-conf/configure/router/ospf3/router-id
            leaf router-id {
                type types-igp:router-id;
                sros-ext:auto-restart-to-modify;
                description "Unique router ID for the OSPF instance";
            }

            ///nokia-conf/configure/router/ospf3/advertise-router-capability
            leaf advertise-router-capability {
                type types-ospf:advertise-router-capability-type;
                default "false";
                description "Allow router advertisement capabilities";
            }

            ///nokia-conf/configure/router/ospf3/external-preference
            leaf external-preference {
                type int32 {
                    range "1..255";
                }
                default "150";
                description "Preference for OSPF external routes";
            }

            ///nokia-conf/configure/router/ospf3/export-policy
            leaf-list export-policy {
                type leafref {
                    path "../../../policy-options/policy-statement/name";
                }
                max-elements 5;
                ordered-by user;
                description "Export policies that determine exported routes";
            }

            ///nokia-conf/configure/router/ospf3/import-policy
            leaf-list import-policy {
                type leafref {
                    path "../../../policy-options/policy-statement/name";
                }
                max-elements 5;
                ordered-by user;
                description "Import policy names for routes from IGP to route table";
            }

            ///nokia-conf/configure/router/ospf3/ldp-sync
            leaf ldp-sync {
                type boolean;
                default "true";
                description "Configure IGP-LDP synchronization for interfaces";
            }

            ///nokia-conf/configure/router/ospf3/multicast-import
            leaf multicast-import {
                type boolean;
                default "false";
                description "Submit routes into the multicast Route Table Manager";
            }

            ///nokia-conf/configure/router/ospf3/overload-include-ext-1
            leaf overload-include-ext-1 {
                type boolean;
                default "false";
                description "Advertise routes with maximum metric value for overload";
            }

            ///nokia-conf/configure/router/ospf3/overload-include-ext-2
            leaf overload-include-ext-2 {
                type boolean;
                default "false";
                description "Advertise routes with maximum metric value for overload";
            }

            ///nokia-conf/configure/router/ospf3/overload-include-stub
            leaf overload-include-stub {
                type boolean;
                default "false";
                description "Advertise all stub interfaces with max metric value";
            }

            ///nokia-conf/configure/router/ospf3/overload
            leaf overload {
                type boolean;
                default "false";
                description "Change local router state to appear overloaded";
            }

            ///nokia-conf/configure/router/ospf3/preference
            leaf preference {
                type int32 {
                    range "1..255";
                }
                default "10";
                description "Preference for OSPF internal routes";
            }

            ///nokia-conf/configure/router/ospf3/reference-bandwidth
            leaf reference-bandwidth {
                type types-sros:reference-bandwidth-ext;
                units "kilobps";
                default "100000000";
                description "Bandwidth to reference default costing of interfaces";
            }

            ///nokia-conf/configure/router/ospf3/unicast-import
            leaf unicast-import {
                type boolean;
                default "true";
                description "Submit routes into the unicast Route Table Manager";
            }

            ///nokia-conf/configure/router/ospf3/extended-lsa
            leaf extended-lsa {
                type types-ospf:extended-lsa-mode;
                sros-ext:auto-restart-to-modify;
                default "sparse";
                description "Extended LSA format in an OSPFv3 area";
            }

            ///nokia-conf/configure/router/ospf3/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/router/ospf3/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/router/ospf3/external-db-overflow
            container external-db-overflow {
                presence "Enable the external-db-overflow context";
                description "Enable the external-db-overflow context";

                ///nokia-conf/configure/router/ospf3/external-db-overflow/limit
                leaf limit {
                    type uint32 {
                        range "0..2147483647";
                    }
                    default "0";
                    description "Number of external LSA at which overload is triggered";
                }

                ///nokia-conf/configure/router/ospf3/external-db-overflow/interval
                leaf interval {
                    type uint32 {
                        range "0..2147483647";
                    }
                    units "seconds";
                    default "0";
                    description "Time during which the router operates in overload";
                }

            } // container external-db-overflow

            ///nokia-conf/configure/router/ospf3/export-limit
            container export-limit {
                presence "Enable the export-limit context";
                description "Enable the export-limit context";

                ///nokia-conf/configure/router/ospf3/export-limit/number
                leaf number {
                    type uint32 {
                        range "1..max";
                    }
                    mandatory true;
                    description "Maximum routes or prefixes exported from route table";
                }

                ///nokia-conf/configure/router/ospf3/export-limit/log-percent
                leaf log-percent {
                    type uint32 {
                        range "1..100";
                    }
                    description "Export limit before warning and SNMP notification sent";
                }

            } // container export-limit

            ///nokia-conf/configure/router/ospf3/overload-on-boot
            container overload-on-boot {
                presence "Enable the overload-on-boot context";
                description "Enable the overload-on-boot context";

                ///nokia-conf/configure/router/ospf3/overload-on-boot/timeout
                leaf timeout {
                    type uint32 {
                        range "60..1800";
                    }
                    units "seconds";
                    description "Router interval in overload before normal operations";
                }

            } // container overload-on-boot

            ///nokia-conf/configure/router/ospf3/rib-priority
            container rib-priority {
                description "Enter the rib-priority context";

                ///nokia-conf/configure/router/ospf3/rib-priority/high
                container high {
                    description "Enter the high context";

                    ///nokia-conf/configure/router/ospf3/rib-priority/high/prefix-list
                    leaf prefix-list {
                        type leafref {
                            path "../../../../../policy-options/prefix-list/name";
                        }
                        description "Higher priority list used during OSPF route calculation";
                    }

                } // container high

            } // container rib-priority

            ///nokia-conf/configure/router/ospf3/rtr-adv-lsa-limit
            container rtr-adv-lsa-limit {
                presence "Enable the rtr-adv-lsa-limit context";
                description "Enable the rtr-adv-lsa-limit context";

                ///nokia-conf/configure/router/ospf3/rtr-adv-lsa-limit/max-lsa-count
                leaf max-lsa-count {
                    type uint32 {
                        range "1..max";
                    }
                    mandatory true;
                    description "Max number of LSAs one router can advertise";
                }

                ///nokia-conf/configure/router/ospf3/rtr-adv-lsa-limit/log-only
                leaf log-only {
                    type boolean;
                    default "false";
                    description "Log the event without triggering overload";
                }

                ///nokia-conf/configure/router/ospf3/rtr-adv-lsa-limit/warning-threshold
                leaf warning-threshold {
                    type uint32 {
                        range "0..100";
                    }
                    units "percent";
                    default "0";
                    description "Percentage of the max LSA count that causes a warning";
                }

                ///nokia-conf/configure/router/ospf3/rtr-adv-lsa-limit/overload-timeout
                leaf overload-timeout {
                    type types-igp:overload-timeout;
                    default "forever";
                    description "Maximum time in overload after LSA limit is reached";
                }

            } // container rtr-adv-lsa-limit

            ///nokia-conf/configure/router/ospf3/graceful-restart
            container graceful-restart {
                presence "Enable the graceful-restart context";
                description "Enable the graceful-restart context";

                ///nokia-conf/configure/router/ospf3/graceful-restart/helper-mode
                leaf helper-mode {
                    type boolean;
                    default "true";
                    description "Enable graceful restart helper for OSPF";
                }

                ///nokia-conf/configure/router/ospf3/graceful-restart/strict-lsa-checking
                leaf strict-lsa-checking {
                    type boolean;
                    default "true";
                    description "Perform strict LSA checking during graceful restart";
                }

            } // container graceful-restart

            ///nokia-conf/configure/router/ospf3/timers
            container timers {
                description "Enter the timers context";

                ///nokia-conf/configure/router/ospf3/timers/incremental-spf-wait
                leaf incremental-spf-wait {
                    type uint32 {
                        range "0..1000";
                    }
                    units "milliseconds";
                    default "1000";
                    description "Delay time before an incremental SPF calculation starts";
                }

                ///nokia-conf/configure/router/ospf3/timers/lsa-accumulate
                leaf lsa-accumulate {
                    type uint32 {
                        range "0..1000";
                    }
                    units "milliseconds";
                    default "1000";
                    description "Delay to gather LSAs before advertising to neighbors";
                }

                ///nokia-conf/configure/router/ospf3/timers/lsa-arrival
                leaf lsa-arrival {
                    type uint32 {
                        range "0..600000";
                    }
                    units "milliseconds";
                    default "1000";
                    description "Min delay between receipt of same LSAs from neighbors";
                }

                ///nokia-conf/configure/router/ospf3/timers/redistribute-delay
                leaf redistribute-delay {
                    type uint32 {
                        range "0..1000";
                    }
                    units "milliseconds";
                    default "1000";
                    description "Hold down timer for external routes into OSPF";
                }

                ///nokia-conf/configure/router/ospf3/timers/lsa-generate
                container lsa-generate {
                    description "Enter the lsa-generate context";

                    ///nokia-conf/configure/router/ospf3/timers/lsa-generate/max-lsa-wait
                    leaf max-lsa-wait {
                        type uint32 {
                            range "10..600000";
                        }
                        units "milliseconds";
                        default "5000";
                        description "Max time between two LSAs being generated";
                    }

                    ///nokia-conf/configure/router/ospf3/timers/lsa-generate/lsa-initial-wait
                    leaf lsa-initial-wait {
                        type uint32 {
                            range "10..600000";
                        }
                        units "milliseconds";
                        default "5000";
                        description "First wait period between OSPF LSA generation";
                    }

                    ///nokia-conf/configure/router/ospf3/timers/lsa-generate/lsa-second-wait
                    leaf lsa-second-wait {
                        type uint32 {
                            range "10..600000";
                        }
                        units "milliseconds";
                        default "5000";
                        description "Hold time between the first and second LSA generation";
                    }

                } // container lsa-generate

                ///nokia-conf/configure/router/ospf3/timers/spf-wait
                container spf-wait {
                    description "Enter the spf-wait context";

                    ///nokia-conf/configure/router/ospf3/timers/spf-wait/spf-max-wait
                    leaf spf-max-wait {
                        type uint32 {
                            range "10..120000";
                        }
                        units "milliseconds";
                        default "10000";
                        description "Max interval between two consecutive SPF calculations";
                    }

                    ///nokia-conf/configure/router/ospf3/timers/spf-wait/spf-initial-wait
                    leaf spf-initial-wait {
                        type uint32 {
                            range "10..100000";
                        }
                        units "milliseconds";
                        default "1000";
                        description "Initial SPF calculation delay after a topology change";
                    }

                    ///nokia-conf/configure/router/ospf3/timers/spf-wait/spf-second-wait
                    leaf spf-second-wait {
                        type uint32 {
                            range "10..100000";
                        }
                        units "milliseconds";
                        default "1000";
                        description "Hold time between the first and second SPF calculation";
                    }

                } // container spf-wait

            } // container timers

            ///nokia-conf/configure/router/ospf3/loopfree-alternate
            container loopfree-alternate {
                presence "Enable the loopfree-alternate context";
                description "Enable the loopfree-alternate context";

                ///nokia-conf/configure/router/ospf3/loopfree-alternate/exclude
                container exclude {
                    description "Enter the exclude context";

                    ///nokia-conf/configure/router/ospf3/loopfree-alternate/exclude/prefix-policy
                    leaf-list prefix-policy {
                        type leafref {
                            path "../../../../../policy-options/policy-statement/name";
                        }
                        max-elements 5;
                        ordered-by user;
                        description "Policy to exclude prefixes from LFA SPF calculation";
                    }

                } // container exclude

                ///nokia-conf/configure/router/ospf3/loopfree-alternate/remote-lfa
                container remote-lfa {
                    presence "Enable the remote-lfa context";
                    description "Enable the remote-lfa context";

                    ///nokia-conf/configure/router/ospf3/loopfree-alternate/remote-lfa/max-pq-cost
                    leaf max-pq-cost {
                        type uint32;
                        default "65535";
                        description "Destination max cost for reverse SPF calculation";
                    }

                    ///nokia-conf/configure/router/ospf3/loopfree-alternate/remote-lfa/node-protect
                    container node-protect {
                        presence "Enable the node-protect context";
                        description "Enable the node-protect context";

                        ///nokia-conf/configure/router/ospf3/loopfree-alternate/remote-lfa/node-protect/max-pq-nodes
                        leaf max-pq-nodes {
                            type uint32 {
                                range "1..32";
                            }
                            default "16";
                            description "Maximum number of PQ nodes found in the LFA SPFs";
                        }

                    } // container node-protect

                } // container remote-lfa

                ///nokia-conf/configure/router/ospf3/loopfree-alternate/ti-lfa
                container ti-lfa {
                    presence "Enable the ti-lfa context";
                    description "Enable the ti-lfa context";

                    ///nokia-conf/configure/router/ospf3/loopfree-alternate/ti-lfa/max-sr-frr-labels
                    leaf max-sr-frr-labels {
                        type uint32 {
                            range "0..3";
                        }
                        default "2";
                        description "Maximum number of labels the TI-LFA backup path can use";
                    }

                    ///nokia-conf/configure/router/ospf3/loopfree-alternate/ti-lfa/node-protect
                    container node-protect {
                        presence "Enable the node-protect context";
                        description "Enable the node-protect context";

                    } // container node-protect

                } // container ti-lfa

            } // container loopfree-alternate

            ///nokia-conf/configure/router/ospf3/database-export
            container database-export {
                presence "Enable the database-export context";
                description "Enable the database-export context";

                ///nokia-conf/configure/router/ospf3/database-export/igp-identifier
                leaf igp-identifier {
                    type uint64;
                    description "IGP instance in the BGP-LS NLRI";
                }

                ///nokia-conf/configure/router/ospf3/database-export/bgp-ls-identifier
                container bgp-ls-identifier {
                    presence "Enable the bgp-ls-identifier context";
                    description "Enable the bgp-ls-identifier context";

                    ///nokia-conf/configure/router/ospf3/database-export/bgp-ls-identifier/value
                    leaf value {
                        type uint32;
                        default "0";
                        description "BGP-LS identifier sent in the BGP-LS NLRI";
                    }

                } // container bgp-ls-identifier

            } // container database-export

            ///nokia-conf/configure/router/ospf3/asbr
            container asbr {
                presence "Enable the asbr context";
                description "Enable the asbr context";

            } // container asbr

            ///nokia-conf/configure/router/ospf3/segment-routing
            container segment-routing {
                description "Enter the segment-routing context";

                ///nokia-conf/configure/router/ospf3/segment-routing/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of segment routing";
                }

                ///nokia-conf/configure/router/ospf3/segment-routing/adj-sid-hold
                leaf adj-sid-hold {
                    type types-ospf:adj-sid-hold-type;
                    units "seconds";
                    default "15";
                    description "Adjacency SID hold time";
                }

                ///nokia-conf/configure/router/ospf3/segment-routing/tunnel-mtu
                leaf tunnel-mtu {
                    type types-router:ip-mtu-ext;
                    description "Tunnel MTU size";
                }

                ///nokia-conf/configure/router/ospf3/segment-routing/tunnel-table-pref
                leaf tunnel-table-pref {
                    type uint32 {
                        range "1..255";
                    }
                    default "10";
                    description "Preference of SR tunnels created by the IGP instance";
                }

                ///nokia-conf/configure/router/ospf3/segment-routing/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/ospf3/segment-routing/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }


                ///nokia-conf/configure/router/ospf3/segment-routing/prefix-sid-range
                container prefix-sid-range {
                    presence "Enable the prefix-sid-range context";
                    description "Enable the prefix-sid-range context";

                    ///nokia-conf/configure/router/ospf3/segment-routing/prefix-sid-range/type
                    choice type {
                        ///nokia-conf/configure/router/ospf3/segment-routing/prefix-sid-range/type/global
                        case global {

                            ///nokia-conf/configure/router/ospf3/segment-routing/prefix-sid-range/type/global/global
                            leaf global {
                                type empty;
                                description "BGP global SR range allocation";
                            }

                        }
                        ///nokia-conf/configure/router/ospf3/segment-routing/prefix-sid-range/type/local
                        case local {

                            ///nokia-conf/configure/router/ospf3/segment-routing/prefix-sid-range/type/local/start-label
                            leaf start-label {
                                type uint32 {
                                    range "0..524287";
                                }
                                sros-ext:auto-restart-to-modify;
                                default "0";
                                description "Lower bound value for the local label offset";
                            }

                            ///nokia-conf/configure/router/ospf3/segment-routing/prefix-sid-range/type/local/max-index
                            leaf max-index {
                                type uint32 {
                                    range "0..524287";
                                }
                                sros-ext:auto-restart-to-modify;
                                default "1";
                                description "Upper bound value for the local SID index";
                            }

                        }
                    }
                } // container prefix-sid-range

                ///nokia-conf/configure/router/ospf3/segment-routing/adjacency-sid
                container adjacency-sid {
                    description "Enter the adjacency-sid context";

                    ///nokia-conf/configure/router/ospf3/segment-routing/adjacency-sid/allocate-dual-sids
                    leaf allocate-dual-sids {
                        type boolean;
                        default "false";
                        description "Allocate dual adjacency SIDs per interface";
                    }

                } // container adjacency-sid

                ///nokia-conf/configure/router/ospf3/segment-routing/egress-statistics
                container egress-statistics {
                    description "Enter the egress-statistics context";

                    ///nokia-conf/configure/router/ospf3/segment-routing/egress-statistics/adj-sid
                    leaf adj-sid {
                        type boolean;
                        default "false";
                        description "Enable the allocation of statistics on adjacency SIDs";
                    }

                    ///nokia-conf/configure/router/ospf3/segment-routing/egress-statistics/node-sid
                    leaf node-sid {
                        type boolean;
                        default "false";
                        description "Enable the allocation of statistics on node SIDs";
                    }

                } // container egress-statistics

                ///nokia-conf/configure/router/ospf3/segment-routing/ingress-statistics
                container ingress-statistics {
                    description "Enter the ingress-statistics context";

                    ///nokia-conf/configure/router/ospf3/segment-routing/ingress-statistics/adj-sid
                    leaf adj-sid {
                        type boolean;
                        default "false";
                        description "Enable the allocation of statistics on adjacency SIDs";
                    }

                    ///nokia-conf/configure/router/ospf3/segment-routing/ingress-statistics/node-sid
                    leaf node-sid {
                        type boolean;
                        default "false";
                        description "Enable the allocation of statistics on node SIDs";
                    }

                } // container ingress-statistics



            } // container segment-routing

            ///nokia-conf/configure/router/ospf3/igp-shortcut
            container igp-shortcut {
                description "Enable the igp-shortcut context";

                ///nokia-conf/configure/router/ospf3/igp-shortcut/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of the IGP shortcuts";
                }

                ///nokia-conf/configure/router/ospf3/igp-shortcut/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/ospf3/igp-shortcut/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/router/ospf3/igp-shortcut/tunnel-next-hop
                container tunnel-next-hop {
                    description "Enter the tunnel-next-hop context";

                    ///nokia-conf/configure/router/ospf3/igp-shortcut/tunnel-next-hop/family
                    list family {
                        key "family-type";
                        description "Enter the family list instance";

                        ///nokia-conf/configure/router/ospf3/igp-shortcut/tunnel-next-hop/family/family-type
                        leaf family-type {
                            type types-igp:tunn-next-hop-family-type;
                            description "Address family type for the tunnel next hop";
                        }

                        ///nokia-conf/configure/router/ospf3/igp-shortcut/tunnel-next-hop/family/resolution
                        leaf resolution {
                            type types-sros:resolution-type;
                            description "Resolution state for IGP shortcut tunnels";
                        }

                        ///nokia-conf/configure/router/ospf3/igp-shortcut/tunnel-next-hop/family/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/ospf3/igp-shortcut/tunnel-next-hop/family/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/ospf3/igp-shortcut/tunnel-next-hop/family/resolution-filter
                        container resolution-filter {
                            description "Enter the resolution-filter context";

                            ///nokia-conf/configure/router/ospf3/igp-shortcut/tunnel-next-hop/family/resolution-filter/rsvp
                            leaf rsvp {
                                type boolean;
                                default "false";
                                description "Use RSVP tunneling for next-hop resolution";
                            }

                            ///nokia-conf/configure/router/ospf3/igp-shortcut/tunnel-next-hop/family/resolution-filter/sr-te
                            leaf sr-te {
                                type boolean;
                                default "false";
                                description "Use SR-TE tunneling for next-hop resolution";
                            }

                        } // container resolution-filter

                    } // list family

                } // container tunnel-next-hop

            } // container igp-shortcut

            ///nokia-conf/configure/router/ospf3/area
            list area {
                key "area-id";
                description "Enter the area list instance";

                ///nokia-conf/configure/router/ospf3/area/area-id
                leaf area-id {
                    type types-sros:ipv4-address;
                    description "Area-ID attribute";
                }

                ///nokia-conf/configure/router/ospf3/area/advertise-router-capability
                leaf advertise-router-capability {
                    type boolean;
                    default "true";
                    description "Allow router advertisement capabilities";
                }

                ///nokia-conf/configure/router/ospf3/area/blackhole-aggregate
                leaf blackhole-aggregate {
                    type boolean;
                    default "true";
                    description "Install a low priority blackhole route to avoid loops";
                }

                ///nokia-conf/configure/router/ospf3/area/export-policy
                leaf-list export-policy {
                    type leafref {
                        path "../../../../policy-options/policy-statement/name";
                    }
                    max-elements 5;
                    ordered-by user;
                    description "Type 3 Summary-LSA/OSPFv3 inter-area-prefix-LSA route";
                }

                ///nokia-conf/configure/router/ospf3/area/import-policy
                leaf-list import-policy {
                    type leafref {
                        path "../../../../policy-options/policy-statement/name";
                    }
                    max-elements 5;
                    ordered-by user;
                    description "Route imported as Summary Type 3/Inter-Area-Prefix-LSA";
                }

                ///nokia-conf/configure/router/ospf3/area/loopfree-alternate-exclude
                leaf loopfree-alternate-exclude {
                    type boolean;
                    default "false";
                    description "Exclude interfaces in OSPF areas in SPF LFA computation";
                }

                ///nokia-conf/configure/router/ospf3/area/database-export-exclude
                leaf database-export-exclude {
                    type boolean;
                    default "false";
                    description "Exclude IGP link-state OSPF area info into TE-DB";
                }

                ///nokia-conf/configure/router/ospf3/area/extended-lsa
                leaf extended-lsa {
                    type types-ospf:area-extended-lsa-mode;
                    description "Extended LSA format in an OSPFv3 area";
                }

                ///nokia-conf/configure/router/ospf3/area/key-rollover-interval
                leaf key-rollover-interval {
                    type uint32 {
                        range "10..300";
                    }
                    units "seconds";
                    default "10";
                    description "Key rollover interval";
                }

                ///nokia-conf/configure/router/ospf3/area/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/ospf3/area/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/router/ospf3/area/stub
                container stub {
                    presence "Enable the stub context";
                    description "Enable the stub context";

                    ///nokia-conf/configure/router/ospf3/area/stub/default-metric
                    leaf default-metric {
                        type uint32 {
                            range "1..16777214";
                        }
                        default "1";
                        description "Metric used by ABR for default route into the stub area";
                    }

                    ///nokia-conf/configure/router/ospf3/area/stub/summaries
                    leaf summaries {
                        type boolean;
                        default "true";
                        description "Send summary (Type 3) LSAs into the stub area on an ABR";
                    }

                } // container stub

                ///nokia-conf/configure/router/ospf3/area/nssa
                container nssa {
                    presence "Enable the nssa context";
                    description "Enable the nssa context";

                    ///nokia-conf/configure/router/ospf3/area/nssa/redistribute-external
                    leaf redistribute-external {
                        type boolean;
                        default "true";
                        description "Redistribute external routes into the NSSA";
                    }

                    ///nokia-conf/configure/router/ospf3/area/nssa/summaries
                    leaf summaries {
                        type boolean;
                        default "true";
                        description "Send summary (Type 3) LSAs into the NSSA on an ABR";
                    }

                    ///nokia-conf/configure/router/ospf3/area/nssa/area-range
                    list area-range {
                        key "ip-prefix-mask";
                        description "Enter the area-range list instance";

                        ///nokia-conf/configure/router/ospf3/area/nssa/area-range/ip-prefix-mask
                        leaf ip-prefix-mask {
                            type types-sros:ip-unicast-prefix;
                            description "Addresses on ABR for route summarization or suppression";
                        }

                        ///nokia-conf/configure/router/ospf3/area/nssa/area-range/advertise
                        leaf advertise {
                            type boolean;
                            default "true";
                            description "Advertise summarized range of addresses to other areas";
                        }

                        ///nokia-conf/configure/router/ospf3/area/nssa/area-range/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/ospf3/area/nssa/area-range/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list area-range

                    ///nokia-conf/configure/router/ospf3/area/nssa/originate-default-route
                    container originate-default-route {
                        presence "Enable the originate-default-route context";
                        description "Enable the originate-default-route context";

                        ///nokia-conf/configure/router/ospf3/area/nssa/originate-default-route/type-nssa
                        leaf type-nssa {
                            type boolean;
                            default "false";
                            description "Generate a default route using NSSA-LSA type";
                        }

                        ///nokia-conf/configure/router/ospf3/area/nssa/originate-default-route/adjacency-check
                        leaf adjacency-check {
                            type boolean;
                            default "true";
                            description "Default route to remove if there is no adjacency";
                        }

                    } // container originate-default-route

                } // container nssa

                ///nokia-conf/configure/router/ospf3/area/area-range
                list area-range {
                    key "ip-prefix-mask";
                    description "Enter the area-range list instance";

                    ///nokia-conf/configure/router/ospf3/area/area-range/ip-prefix-mask
                    leaf ip-prefix-mask {
                        type types-sros:ip-unicast-prefix;
                        description "Address ranges to create on an ABR for route summarization or suppression";
                    }

                    ///nokia-conf/configure/router/ospf3/area/area-range/advertise
                    leaf advertise {
                        type boolean;
                        default "true";
                        description "Advertise summarized range of addresses to other areas";
                    }

                    ///nokia-conf/configure/router/ospf3/area/area-range/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/ospf3/area/area-range/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list area-range

                ///nokia-conf/configure/router/ospf3/area/interface
                list interface {
                    key "interface-name";
                    description "Enter the interface list instance";

                    ///nokia-conf/configure/router/ospf3/area/interface/interface-name
                    leaf interface-name {
                        type types-sros:interface-name;
                        description "Router interface name";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/advertise-router-capability
                    leaf advertise-router-capability {
                        type boolean;
                        default "true";
                        description "Allow router advertisement capabilities";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/interface-type
                    leaf interface-type {
                        type types-ospf:interface-type;
                        sros-ext:immutable;
                        description "Interface type";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/lsa-filter-out
                    leaf lsa-filter-out {
                        type types-ospf:lsa-filter-out;
                        default "none";
                        description "LSA flooding reduction";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/metric
                    leaf metric {
                        type uint32 {
                            range "1..65535";
                        }
                        description "Route cost metric for the interface";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/mtu
                    leaf mtu {
                        type types-router:ip-mtu-ext;
                        description "MTU for the OSPF to use on the interface";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/passive
                    leaf passive {
                        type boolean;
                        description "Advertise passive interfaces as OSPF interfaces";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/poll-interval
                    leaf poll-interval {
                        type uint32;
                        units "seconds";
                        default "120";
                        description "Interval for Hellos to non-adjacent OSPF NBMA neighbor";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/priority
                    leaf priority {
                        type uint16 {
                            range "0..255";
                        }
                        default "1";
                        description "Interface priority in the DR election on the subnet";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/rib-priority
                    leaf rib-priority {
                        type types-ospf:rib-priority;
                        description "RIB priority for OSPF";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/load-balancing-weight
                    leaf load-balancing-weight {
                        type uint32 {
                            range "1..max";
                        }
                        description "Load balancing weight for an OSPF3 interface";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "enable";
                        description "Administrative state of the OSPF interface";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/hello-interval
                    leaf hello-interval {
                        type uint32 {
                            range "1..65535";
                        }
                        units "seconds";
                        default "10";
                        description "Time between OSPF Hellos of this interface";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/dead-interval
                    leaf dead-interval {
                        type uint32 {
                            range "2..65535";
                        }
                        units "seconds";
                        description "OSPF wait time for Hellos before neighbor declared down";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/retransmit-interval
                    leaf retransmit-interval {
                        type uint32 {
                            range "1..1800";
                        }
                        units "seconds";
                        default "5";
                        description "Time before OSPF retransmits an unacknowledged LSA";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/transit-delay
                    leaf transit-delay {
                        type uint32 {
                            range "1..1800";
                        }
                        units "seconds";
                        default "1";
                        description "Required LSA transmit time";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/sid-protection
                    leaf sid-protection {
                        type boolean;
                        default "true";
                        description "Allow adjacency SID protection by LFA and remote LFA";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/ospf3/area/interface/bfd-liveness
                    container bfd-liveness {
                        presence "Enable the bfd-liveness context";
                        description "Enable the bfd-liveness context";

                        ///nokia-conf/configure/router/ospf3/area/interface/bfd-liveness/remain-down-on-failure
                        leaf remain-down-on-failure {
                            type boolean;
                            default "false";
                            description "Force adjacency down on failure until session returns";
                        }

                        ///nokia-conf/configure/router/ospf3/area/interface/bfd-liveness/strict
                        leaf strict {
                            type boolean;
                            default "false";
                            description "Enable BFD strict mode";
                        }

                        ///nokia-conf/configure/router/ospf3/area/interface/bfd-liveness/strict-mode-holddown
                        leaf strict-mode-holddown {
                            type int32 {
                                range "1..600";
                            }
                            units "seconds";
                            description "Adjacency up time delay after BFD session establishment";
                        }

                    } // container bfd-liveness

                    ///nokia-conf/configure/router/ospf3/area/interface/loopfree-alternate
                    container loopfree-alternate {
                        description "Enter the loopfree-alternate context";

                        ///nokia-conf/configure/router/ospf3/area/interface/loopfree-alternate/exclude
                        leaf exclude {
                            type boolean;
                            default "false";
                            description "Enable fast reroute at OSPF primary interface level";
                        }

                        ///nokia-conf/configure/router/ospf3/area/interface/loopfree-alternate/policy-map
                        container policy-map {
                            presence "Enable the policy-map context";
                            description "Enable the policy-map context";

                            ///nokia-conf/configure/router/ospf3/area/interface/loopfree-alternate/policy-map/route-nh-template
                            leaf route-nh-template {
                                type leafref {
                                    path "../../../../../../../routing-options/route-next-hop-policy/template/template-name";
                                }
                                mandatory true;
                                description "Route next hop policy template name";
                            }

                        } // container policy-map

                    } // container loopfree-alternate

                    ///nokia-conf/configure/router/ospf3/area/interface/node-sid
                    container node-sid {
                        presence "Enable the node-sid context";
                        description "Enable the node-sid context";

                        ///nokia-conf/configure/router/ospf3/area/interface/node-sid/clear-n-flag
                        leaf clear-n-flag {
                            type boolean;
                            default "false";
                            description "Clear the N-flag in an OSPF3 prefix";
                        }

                        ///nokia-conf/configure/router/ospf3/area/interface/node-sid/type
                        choice type {
                            ///nokia-conf/configure/router/ospf3/area/interface/node-sid/type/label
                            case label {

                                ///nokia-conf/configure/router/ospf3/area/interface/node-sid/type/label/label
                                leaf label {
                                    type int64 {
                                        range "1..4294967295";
                                    }
                                    description "Label value for the node SID";
                                }

                            }
                            ///nokia-conf/configure/router/ospf3/area/interface/node-sid/type/index
                            case index {

                                ///nokia-conf/configure/router/ospf3/area/interface/node-sid/type/index/index
                                leaf index {
                                    type int64 {
                                        range "0..4294967295";
                                    }
                                    description "Node SID index for this interface";
                                }

                            }
                        }
                    } // container node-sid


                    ///nokia-conf/configure/router/ospf3/area/interface/authentication
                    container authentication {
                        presence "Enable the authentication context";
                        description "Enable the authentication context";

                        ///nokia-conf/configure/router/ospf3/area/interface/authentication/inbound
                        leaf inbound {
                            type leafref {
                                path "../../../../../../ipsec/static-sa/name";
                            }
                            mandatory true;
                            description "sa-name";
                        }

                        ///nokia-conf/configure/router/ospf3/area/interface/authentication/outbound
                        leaf outbound {
                            type leafref {
                                path "../../../../../../ipsec/static-sa/name";
                            }
                            mandatory true;
                            description "sa-name";
                        }

                    } // container authentication

                    ///nokia-conf/configure/router/ospf3/area/interface/neighbor
                    list neighbor {
                        key "address";
                        description "Add a list entry for neighbor";

                        ///nokia-conf/configure/router/ospf3/area/interface/neighbor/address
                        leaf address {
                            type types-sros:ip-address;
                            description "Neighbor link local address";
                        }

                    } // list neighbor


                } // list interface

                ///nokia-conf/configure/router/ospf3/area/virtual-link
                list virtual-link {
                    key "router-id transit-area";
                    description "Enter the virtual-link list instance";

                    ///nokia-conf/configure/router/ospf3/area/virtual-link/router-id
                    leaf router-id {
                        type types-sros:ipv4-address;
                        description "Router ID of the virtual link neighbor";
                    }

                    ///nokia-conf/configure/router/ospf3/area/virtual-link/transit-area
                    leaf transit-area {
                        type leafref {
                            path "../../../area/area-id";
                        }
                        description "Transit area linking backbone and non-connected area";
                    }

                    ///nokia-conf/configure/router/ospf3/area/virtual-link/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "enable";
                        description "Administrative state of the OSPF interface";
                    }

                    ///nokia-conf/configure/router/ospf3/area/virtual-link/hello-interval
                    leaf hello-interval {
                        type uint32 {
                            range "1..65535";
                        }
                        units "seconds";
                        default "10";
                        description "Time between OSPF Hellos of this interface";
                    }

                    ///nokia-conf/configure/router/ospf3/area/virtual-link/dead-interval
                    leaf dead-interval {
                        type uint32 {
                            range "2..65535";
                        }
                        units "seconds";
                        description "OSPF wait time for Hellos before neighbor declared down";
                    }

                    ///nokia-conf/configure/router/ospf3/area/virtual-link/retransmit-interval
                    leaf retransmit-interval {
                        type uint32 {
                            range "1..1800";
                        }
                        units "seconds";
                        default "5";
                        description "Time before OSPF retransmits an unacknowledged LSA";
                    }

                    ///nokia-conf/configure/router/ospf3/area/virtual-link/transit-delay
                    leaf transit-delay {
                        type uint32 {
                            range "1..1800";
                        }
                        units "seconds";
                        default "1";
                        description "Required LSA transmit time";
                    }

                    ///nokia-conf/configure/router/ospf3/area/virtual-link/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/ospf3/area/virtual-link/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/ospf3/area/virtual-link/authentication
                    container authentication {
                        presence "Enable the authentication context";
                        description "Enable the authentication context";

                        ///nokia-conf/configure/router/ospf3/area/virtual-link/authentication/inbound
                        leaf inbound {
                            type leafref {
                                path "../../../../../../ipsec/static-sa/name";
                            }
                            mandatory true;
                            description "sa-name";
                        }

                        ///nokia-conf/configure/router/ospf3/area/virtual-link/authentication/outbound
                        leaf outbound {
                            type leafref {
                                path "../../../../../../ipsec/static-sa/name";
                            }
                            mandatory true;
                            description "sa-name";
                        }

                    } // container authentication

                } // list virtual-link

            } // list area

        } // list ospf3

    } // grouping conf-router-ospf3

}
