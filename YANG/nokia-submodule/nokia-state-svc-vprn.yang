                                            ///nokia-state/state/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/dh-group
                                            leaf dh-group {
                                                type types-ipsec:dh-group;
                                                description "The Diffie-Hellman (DH) group of the IKE SA for this IPsec tunnel.";
