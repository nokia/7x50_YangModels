                                    description "Source-class index";
                                }

                                ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/source-class/forwarded-packets
                                leaf forwarded-packets {
                                    type yang:counter64;
                                    units "packets";
                                    description
                                        "Indicates the total number of packets received
                                         for this class that were forwarded.";
                                }

                                ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/source-class/forwarded-bytes
                                leaf forwarded-bytes {
                                    type yang:counter64;
                                    units "bytes";
                                    description
