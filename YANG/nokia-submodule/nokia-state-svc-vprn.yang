                            container bfd-liveness {
                                description "Enable the bfd-liveness context";

                                ///nokia-state/state/service/vprn/interface/ipsec/ipsec-tunnel/bfd/bfd-liveness/src-ip
                                leaf src-ip {
                                    type types-sros:ip-address;
                                    description "The source IP address on the interface running BFD.";
                                }

                                ///nokia-state/state/service/vprn/interface/ipsec/ipsec-tunnel/bfd/bfd-liveness/oper-state
                                leaf oper-state {
                                    type types-ipsec:bfd-session-state;
                                    description
