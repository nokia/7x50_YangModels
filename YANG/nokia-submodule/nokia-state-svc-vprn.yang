                        ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route
                        list route {
                            key "ipv4-prefix";
                            description "Enter the route list instance";

                            ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/ipv4-prefix
                            leaf ipv4-prefix {
                                type types-sros:ipv4-prefix;
                                description "Indicates the IPv4 prefix for this route table entry.";
                            }

                            ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/age
                            leaf age {
                                type uint32 {
                                    range "0..4294967295";
                                }
                                description "The number of seconds since this route was last updated.";
                            }

                            ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/instance
                            leaf instance {
                                type uint8;
                                description "The protocol instance the route belongs to.";
                            }

                            ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/flags
                            leaf flags {
                                type bits {
                                    bit sticky-flag                    { position 0; }
                                }
                                description
                                    "Bit flags that indicate different properties of this route.

                                     If the 'stickyFlag' bit is set, it indicates the ECMP
                                     sticky flag is set on this route.";
                            }

                            ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/preference
                            leaf preference {
                                type uint8;
                                description
                                    "Indicates the priority of this route versus the routes
                                     from different sources such as BGP or OSPF etc.";
                            }

                            ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/protocol
                            leaf protocol {
                                type types-router:protocol-type;
                                description "Identifies the routing protocol.";
                            }

                            ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop
                            list nexthop {
                                key "nexthop-index";
                                description "Enter the nexthop list instance";

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/nexthop-index
                                leaf nexthop-index {
                                    type uint32;
                                    description "Index of ECMP next-hop or backup next-hop.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/flags
                                leaf flags {
                                    type bits {
                                        bit nh-is-indirect                 { position 0; }
                                        bit nh-is-tunneled                 { position 1; }
                                        bit nh-is-lfa                      { position 2; }
                                        bit nh-is-backup                   { position 3; }
                                        bit nh-is-unnumbered               { position 4; }
                                    }
                                    description
                                        "Bit flags that indicate different properties of this next-hop.

                                         If the 'nh-is-indirect' bit is set, it indicates the next-hop is
                                         an indirect next-hop IP. An additional lookup will have to be
                                         made to map the indirect IP address to the resolving IP
                                         address(es) The 'if-index' leaf will be 0 in this case.

                                         If the 'nh-is-tunneled' bit is set, it indicates the next-hop is
                                         a tunnel destination (the 'nexthop-ip' leaf is a far-end tunnel
                                         destination). The 'if-index' leaf will be 0 in this case.

                                         If the 'nh-is-lfa' bit is set, it indicates the next-hop is a
                                         loop-free alternate (LFA) of the next-hop immediately preceding
                                         this next-hop.

                                         If the 'nh-is-backup' bit is set, it indicates the next-hop is
                                         a BGP backup next-hop.

                                         If the 'nh-is-unnumbered' bit is set, it indicates the next-hop is
                                         unnumbered.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/if-index
                                leaf if-index {
                                    type uint32;
                                    description
                                        "This is the index of the egress interface where the
                                         next-hop resides.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/if-vrtr-id
                                leaf if-vrtr-id {
                                    type uint32;
                                    description
                                        "Indicates the virtual router instance of the egress interface
                                         defined by 'if-index'.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/metric
                                leaf metric {
                                    type uint32;
                                    description "Indicates the routing metric for the next-hop.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/nexthop-ip
                                leaf nexthop-ip {
                                    type types-sros:ip-address;
                                    description
                                        "Indicates the next-hop IP address of the route entry.

                                         The nexthop-ip is a tunnel far-end IP if the 'flags' leaf
                                         has the 'nh-is-tunneled' bit set.

                                         The nexthop-ip is an indirect IP if the 'flags' leaf has
                                         the 'nh-is-indirect' bit set.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/nexthop-ip-mask-len
                                leaf nexthop-ip-mask-len {
                                    type uint8;
                                    description "Indicates the netmask of the next-hop IP address of the route entry.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/nexthop-tunnel-id
                                leaf nexthop-tunnel-id {
                                    type uint32;
                                    description
                                        "This indicates the next-hop tunnel ID when there are
                                         multiple tunnels to a destination or SDP ID.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/nexthop-tunnel-type
                                leaf nexthop-tunnel-type {
                                    type types-router:tunnel-type;
                                    description
                                        "This indicates the next-hop tunnel type if the
                                         'flags' leaf has the 'nh-is-tunneled' bit set.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/nexthop-tunnel-type-instance
                                leaf nexthop-tunnel-type-instance {
                                    type uint8;
                                    description
                                        "This indicates the next-hop tunnel type instance if
                                         the 'flags' leaf has the 'nh-is-tunneled' bit set.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/nexthop-label
                                leaf nexthop-label {
                                    type uint32 {
                                        range "0..1048575";
                                    }
                                    description "This is a VPRN or route label associated with the route entry.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/forwarding-class
                                leaf forwarding-class {
                                    type types-sros:fc-name;
                                    description "This is the forwarding class associated with the next-hop.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/priority
                                leaf priority {
                                    type types-router:nexthop-priority;
                                    description "This is the priority associated with packets to the next-hop.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/source-class-index
                                leaf source-class-index {
                                    type uint8 {
                                        range "1..255";
                                    }
                                    description "This is the source class index associated with the next-hop.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/dest-class-index
                                leaf dest-class-index {
                                    type uint8 {
                                        range "1..255";
                                    }
                                    description "This is the destination class index associated with the next-hop.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/weight
                                leaf weight {
                                    type types-router:ip-ecmp;
                                    description
                                        "The value of this leaf indicates the weight given to traffic using this
                                         particular next-hop among the ECMP set of next-hops.";
                                }

                                ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/resolving-nexthop
                                list resolving-nexthop {
                                    key "resolving-nexthop-index";
                                    description "Enter the resolving-nexthop list instance";

                                    ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/resolving-nexthop/resolving-nexthop-index
                                    leaf resolving-nexthop-index {
                                        type uint32;
                                        description "Index of ECMP resolving next-hop or LFA next-hop.";
                                    }

                                    ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/resolving-nexthop/flags
                                    leaf flags {
                                        type bits {
                                            bit nh-is-indirect                 { position 0; }
                                            bit nh-is-tunneled                 { position 1; }
                                            bit nh-is-lfa                      { position 2; }
                                            bit nh-is-backup                   { position 3; }
                                            bit nh-is-unnumbered               { position 4; }
                                        }
                                        description
                                            "Bit flags that indicate different properties of this next-hop.

                                             If the 'nh-is-indirect' bit is set, it indicates the next-hop is
                                             an indirect next-hop IP. An additional lookup will have to be
                                             made to map the indirect IP address to the resolving IP
                                             address(es) The 'if-index' leaf will be 0 in this case.

                                             If the 'nh-is-tunneled' bit is set, it indicates the next-hop is
                                             a tunnel destination (the 'nexthop-ip' leaf is a far-end tunnel
                                             destination). The 'if-index' leaf will be 0 in this case.

                                             If the 'nh-is-lfa' bit is set, it indicates the next-hop is a
                                             loop-free alternate (LFA) of the next-hop immediately preceding
                                             this next-hop.

                                             If the 'nh-is-backup' bit is set, it indicates the next-hop is
                                             a BGP backup next-hop.

                                             If the 'nh-is-unnumbered' bit is set, it indicates the next-hop is
                                             unnumbered.";
                                    }

                                    ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/resolving-nexthop/if-index
                                    leaf if-index {
                                        type uint32;
                                        description
                                            "This is the index of the egress interface where the
                                             next-hop resides.";
                                    }

                                    ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/resolving-nexthop/if-vrtr-id
                                    leaf if-vrtr-id {
                                        type uint32;
                                        description
                                            "Indicates the virtual router instance of the egress interface
                                             defined by 'if-index'.";
                                    }

                                    ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/resolving-nexthop/metric
                                    leaf metric {
                                        type uint32;
                                        description "Indicates the routing metric for the next-hop.";
                                    }

                                    ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/resolving-nexthop/nexthop-ip
                                    leaf nexthop-ip {
                                        type types-sros:ip-address;
                                        description
                                            "Indicates the next-hop IP address of the route entry.

                                             The nexthop-ip is a tunnel far-end IP if the 'flags' leaf
                                             has the 'nh-is-tunneled' bit set.

                                             The nexthop-ip is an indirect IP if the 'flags' leaf has
                                             the 'nh-is-indirect' bit set.";
                                    }

                                    ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/resolving-nexthop/nexthop-ip-mask-len
                                    leaf nexthop-ip-mask-len {
                                        type uint8;
                                        description "Indicates the netmask of the next-hop IP address of the route entry.";
                                    }

                                    ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/resolving-nexthop/nexthop-tunnel-id
                                    leaf nexthop-tunnel-id {
                                        type uint32;
                                        description
                                            "This indicates the next-hop tunnel ID when there are
                                             multiple tunnels to a destination or SDP ID.";
                                    }

                                    ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/resolving-nexthop/nexthop-tunnel-type
                                    leaf nexthop-tunnel-type {
                                        type types-router:tunnel-type;
                                        description
                                            "This indicates the next-hop tunnel type if the
                                             'flags' leaf has the 'nh-is-tunneled' bit set.";
                                    }

                                    ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/resolving-nexthop/nexthop-tunnel-type-instance
                                    leaf nexthop-tunnel-type-instance {
                                        type uint8;
                                        description
                                            "This indicates the next-hop tunnel type instance if
                                             the 'flags' leaf has the 'nh-is-tunneled' bit set.";
                                    }

                                    ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/resolving-nexthop/nexthop-label
                                    leaf nexthop-label {
                                        type uint32 {
                                            range "0..1048575";
                                        }
                                        description "This is a VPRN or route label associated with the route entry.";
                                    }

                                    ///nokia-state/state/service/vprn/route-table/unicast/ipv4/route/nexthop/resolving-nexthop/forwarding-class
                                    leaf forwarding-class {
                                        type types-sros:fc-name;
                                        description "This is the forwarding class associated with the next-hop.";
