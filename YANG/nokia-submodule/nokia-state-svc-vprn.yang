                                                type uint32;
                                                description "The matching certificate profile entry id used for this tunnel";
                                            }

                                            ///nokia-state/state/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/cert-profile-name
                                            leaf cert-profile-name {
                                                type types-sros:named-item;
                                                description "The IPsec tunnel certificate profile name used for this tunnel";
                                            }

                                            ///nokia-state/state/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/statistics
                                            container statistics {
                                                description "Enter the statistics context";

                                                ///nokia-state/state/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/statistics/ctrl-packets-sent
                                                leaf ctrl-packets-sent {
                                                    type yang:counter32;
                                                    description "The number of control packets this IPsec Tunnel has sent";
                                                }

                                                ///nokia-state/state/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/statistics/ctrl-packets-received
                                                leaf ctrl-packets-received {
                                                    type yang:counter32;
                                                    description "The number of control packets this IPsec Tunnel has received";
                                                }

                                                ///nokia-state/state/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/statistics/ctrl-packets-sent-errors
                                                leaf ctrl-packets-sent-errors {
                                                    type yang:counter32;
