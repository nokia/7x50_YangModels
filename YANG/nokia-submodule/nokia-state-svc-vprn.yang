                                    ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/dh-group
                                    leaf dh-group {
                                        type types-ipsec:dh-group;
                                        description "The Diffie-Hellman (DH) group of the IKE SA for this IPsec tunnel.";
                                    }

                                    ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/cert-profile-entry-id
                                    leaf cert-profile-entry-id {
                                        type uint32;
                                        description "The matching certificate profile entry id used for this tunnel";
                                    }

                                    ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/cert-profile-name
                                    leaf cert-profile-name {
                                        type types-sros:named-item;
                                        description "The IPsec tunnel certificate profile name used for this tunnel";
                                    }

                                    ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/statistics
                                    container statistics {
                                        description "Enter the statistics context";

                                        ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/statistics/ctrl-packets-sent
                                        leaf ctrl-packets-sent {
                                            type yang:counter32;
