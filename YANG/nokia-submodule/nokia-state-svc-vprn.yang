                        leaf tunnel-name {
                            type types-sros:interface-name;
                            description "IP tunnel name";
                        }

                        ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/host-mda
                        leaf host-mda {
                            type types-isa:slot-mda;
                            description "Active ISA that is being used to host this tunnel.";
                        }

                        ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/last-oper-changed
                        leaf last-oper-changed {
                            type types-sros:date-time;
                            description "Last operational status change time.";
                        }

                        ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/oper-state
                        leaf oper-state {
                            type types-services:ip-tunnel-oper-state;
                            description "Operational state of the tunnel.";
                        }

                        ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/oper-flags
                        leaf oper-flags {
                            type bits {
                                bit unresolved-local-ip            { position 0; }
