                    } // list ipsec-gateway

                    ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel
                    list ipsec-tunnel {
                        key "name";
                        description "Enter the ipsec-tunnel list instance";

                        ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/name
                        leaf name {
                            type types-sros:named-item;
                            description "IPsec tunnel name";
                        }

                        ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/host-mda
                        leaf host-mda {
                            type types-isa:slot-mda;
                            description "Active ISA that is being used to host this tunnel.";
                        }

                        ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/matched-trust-anchor
                        leaf matched-trust-anchor {
                            type types-sros:named-item;
                            description
                                "The matched Certificate Authority (CA) profile name
                                 associated with the IPSec tunnel certificate.";
                        }

                        ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/oper-flags
                        leaf oper-flags {
                            type bits {
                                bit unresolved-local-ip            { position 0; }
                                bit tunnel-admin-down              { position 1; }
                                bit sap-down                       { position 2; }
                                bit unresolved-delivery-svc        { position 3; }
                                bit bfd-session-down               { position 4; }
                                bit reserved-1                     { position 5; }
                                bit unresolved-dst-ip              { position 6; }
                                bit invalid-cert-file              { position 7; }
                                bit invalid-key-file               { position 8; }
                                bit trust-anchors-down             { position 9; }
                                bit cert-profile-down              { position 10; }
                                bit invalid-cert-key-combo         { position 11; }
                            }
                            description "The reason why the IPsec tunnel is operationally down.";
                        }

                        ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/oper-state
                        leaf oper-state {
                            type types-ipsec:oper-state;
                            description "The operational status of this IPsec tunnel.";
                        }

                        ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/last-oper-changed
                        leaf last-oper-changed {
                            type yang:date-and-time;
                            description "Uptime at the time of the last operational status change of this IPsec tunnel";
                        }

                        ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/bfd
                        container bfd {
                            description "Enter the bfd context";

                            ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/bfd/bfd-liveness
                            container bfd-liveness {
                                description "Enable the bfd-liveness context";

                                ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/bfd/bfd-liveness/src-ip
                                leaf src-ip {
                                    type types-sros:ip-address;
                                    description "The source IP address on the interface running BFD.";
                                }

                                ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/bfd/bfd-liveness/oper-state
                                leaf oper-state {
                                    type types-ipsec:bfd-session-state;
                                    description
                                        "The operational state of the BFD session the IPsec tunnel
                                         is relying upon for its fast triggering mechanism.";
                                }

                            } // container bfd-liveness

                        } // container bfd

                        ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange
                        container key-exchange {
                            description "Enter the key-exchange context";

                            ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type
                            choice type {
                                ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic
                                case dynamic {

                                    ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic
                                    container dynamic {
                                        description "Enable the dynamic context";

                                        ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa
                                        container ike-sa {
                                            description "Enter the ike-sa context";

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/state
                                            leaf state {
                                                type enumeration {
                                                    enum up                             { value 1; }
                                                    enum down                           { value 2; }
                                                }
                                                description "The state of IKE SA";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/established-time
                                            leaf established-time {
                                                type yang:date-and-time;
                                                description "The time the IKE SA completed";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/negotiated-lifetime
                                            leaf negotiated-lifetime {
                                                type types-sros:time-duration;
                                                description "The lifetime negotiated for the IKE SA";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/expiration-time
                                            leaf expiration-time {
                                                type yang:date-and-time;
                                                description "The time when the IKE SA will expire";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/path-mtu
                                            leaf path-mtu {
                                                type uint32;
                                                units "bytes";
                                                description "The path MTU used between the local and remote endpoints for this IPsec tunnel";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/authentication-algorithm
                                            leaf authentication-algorithm {
                                                type types-ipsec:auth-algorithms;
                                                description "The authentication algorithm of the IKE SA for this IPsec tunnel";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/encryption-algorithm
                                            leaf encryption-algorithm {
