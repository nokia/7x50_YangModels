                                description "Enter the statistics context";

                                ///nokia-state/state/service/vprn/interface/sap/ipsec-gateway/dynamic-tunnel/statistics/traffic-forward
                                container traffic-forward {
                                    description "Enter the traffic-forward context";

                                    ///nokia-state/state/service/vprn/interface/sap/ipsec-gateway/dynamic-tunnel/statistics/traffic-forward/current
                                    container current {
                                        description "Enter the current context";

                                        ///nokia-state/state/service/vprn/interface/sap/ipsec-gateway/dynamic-tunnel/statistics/traffic-forward/current/start-time
                                        leaf start-time {
                                            type yang:date-and-time;
                                            description "The time when the sampling interval started.";
                                        }

                                        ///nokia-state/state/service/vprn/interface/sap/ipsec-gateway/dynamic-tunnel/statistics/traffic-forward/current/sampling-duration
                                        leaf sampling-duration {
                                            type types-sros:time-duration;
                                            description "The duration of the corresponding sampling interval.";
                                        }

                                        ///nokia-state/state/service/vprn/interface/sap/ipsec-gateway/dynamic-tunnel/statistics/traffic-forward/current/packets
                                        container packets {
                                            description "Enter the packets context";

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-gateway/dynamic-tunnel/statistics/traffic-forward/current/packets/encrypted
                                            leaf encrypted {
                                                type yang:counter64;
                                                units "packets";
                                                description "Number of encrypted IPsec packets.";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-gateway/dynamic-tunnel/statistics/traffic-forward/current/packets/decrypted
                                            leaf decrypted {
                                                type yang:counter64;
                                                units "packets";
                                                description "Number of decrypted IPsec packets.";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-gateway/dynamic-tunnel/statistics/traffic-forward/current/packets/total
                                            leaf total {
                                                type yang:counter64;
                                                units "packets";
                                                description "Number of encrypted and decrypted IPsec packets.";
                                            }

                                        } // container packets

                                        ///nokia-state/state/service/vprn/interface/sap/ipsec-gateway/dynamic-tunnel/statistics/traffic-forward/current/bytes
                                        container bytes {
                                            description "Enter the bytes context";

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-gateway/dynamic-tunnel/statistics/traffic-forward/current/bytes/encrypted
                                            leaf encrypted {
                                                type yang:counter64;
                                                units "kilobytes";
                                                description "Number of kilobytes in the encrypted IPsec packets.";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-gateway/dynamic-tunnel/statistics/traffic-forward/current/bytes/decrypted
                                            leaf decrypted {
                                                type yang:counter64;
                                                units "kilobytes";
                                                description "Number of kilobytes in the decrypted IPsec packets.";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-gateway/dynamic-tunnel/statistics/traffic-forward/current/bytes/total
                                            leaf total {
                                                type yang:counter64;
                                                units "kilobytes";
                                                description "Number of kilobytes in the encrypted and decrypted IPsec packets.";
                                            }

                                        } // container bytes

                                    } // container current

                                } // container traffic-forward

                            } // container statistics

