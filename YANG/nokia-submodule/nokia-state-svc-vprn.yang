                                    container post-encapsulation {
                                        description "Enter the post-encapsulation context";

                                        ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/ipsec-sa/isa/fragmentation/post-encapsulation/last-packet-size
                                        leaf last-packet-size {
                                            type uint32;
                                            units "bytes";
                                            description
