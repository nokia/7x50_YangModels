                                }

                                ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/source-class/incomplete-counts
                                leaf incomplete-counts {
                                    type boolean;
                                    description "Indicates if the counts are incomplete.";
                                }

                            } // list source-class

                            ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/destination-class
                            list destination-class {
                                key "index";
                                description "Enter the destination-class list instance";

                                ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/destination-class/index
                                leaf index {
                                    type types-sros:class-index;
                                    description "Destination-class index";
                                }

                                ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/destination-class/policer
                                leaf policer {
                                    type uint32 {
                                        range "1..63";
                                    }
                                    description "Indicates the policer associated with this destination class.";
                                }

                                ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/destination-class/forwarded-packets
                                leaf forwarded-packets {
                                    type yang:counter64;
                                    units "packets";
                                    description
                                        "Indicates the total number of packets received
                                         for this class that were forwarded.";
                                }

                                ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/destination-class/forwarded-bytes
                                leaf forwarded-bytes {
                                    type yang:counter64;
                                    units "bytes";
                                    description
                                        "Indicates the total number of bytes received
                                         for this class that were forwarded.";
                                }

                                ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/destination-class/incomplete-counts
                                leaf incomplete-counts {
                                    type boolean;
                                    description "Indicates if the counts are incomplete.";
                                }

                            } // list destination-class

                            ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/policer
                            list policer {
                                key "index";
                                description "Enter the policer list instance";

                                ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/policer/index
                                leaf index {
                                    type uint32 {
                                        range "1..63";
                                    }
                                    description "Policer index";
                                }

                                ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/policer/dropped-packets
                                leaf dropped-packets {
                                    type yang:counter64;
                                    units "packets";
                                    description
                                        "Indicates the total number of packets
                                         that were dropped by this policer.";
                                }

                                ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/policer/dropped-bytes
                                leaf dropped-bytes {
                                    type yang:counter64;
                                    units "bytes";
                                    description
                                        "Indicates the total number of bytes
                                         that were dropped by this policer.";
                                }

                                ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/policer/forwarded-packets
                                leaf forwarded-packets {
                                    type yang:counter64;
                                    units "packets";
                                    description
                                        "Indicates the total number of packets
                                         that were forwarded by this policer.";
                                }

                                ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/policer/forwarded-bytes
                                leaf forwarded-bytes {
                                    type yang:counter64;
                                    units "bytes";
                                    description
                                        "Indicates the total number of bytes
                                         that were forwarded by this policer.";
                                }

                                ///nokia-state/state/service/vprn/interface/ingress/statistics/policy-accounting/policer/incomplete-counts
                                leaf incomplete-counts {
                                    type boolean;
                                    description "Indicates if the counts are incomplete.";
                                }

                            } // list policer

                        } // container policy-accounting

                    } // container statistics

                } // container ingress

                ///nokia-state/state/service/vprn/interface/hold-time
                container hold-time {
                    description "Enter the hold-time context";

                    ///nokia-state/state/service/vprn/interface/hold-time/ipv4
                    container ipv4 {
