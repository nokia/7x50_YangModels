                                            ///nokia-state/state/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/expiration-time
                                            leaf expiration-time {
                                                type yang:date-and-time;
                                                description "The time when the IKE SA will expire";
                                            }

                                            ///nokia-state/state/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/path-mtu
                                            leaf path-mtu {
                                                type uint32;
                                                units "bytes";
                                                description "The path MTU used between the local and remote endpoints for this IPsec tunnel";
