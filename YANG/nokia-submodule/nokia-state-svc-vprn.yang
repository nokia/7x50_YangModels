                                                description "The encryption algorithm of the IKE SA for this IPsec tunnel";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/prf-algorithm
                                            leaf prf-algorithm {
                                                type types-ipsec:prf-algorithms;
                                                description "The pseudo-random function (PRF)";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/dh-group
                                            leaf dh-group {
                                                type types-ipsec:dh-group;
                                                description "The Diffie-Hellman (DH) group of the IKE SA for this IPsec tunnel.";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/cert-profile-entry-id
                                            leaf cert-profile-entry-id {
                                                type uint32;
                                                description "The matching certificate profile entry id used for this tunnel";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/cert-profile-name
                                            leaf cert-profile-name {
                                                type types-sros:named-item;
                                                description "The IPsec tunnel certificate profile name used for this tunnel";
                                            }

                                            ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/statistics
                                            container statistics {
                                                description "Enter the statistics context";

                                                ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/statistics/ctrl-packets-sent
                                                leaf ctrl-packets-sent {
                                                    type yang:counter32;
                                                    description "The number of control packets this IPsec Tunnel has sent";
                                                }

                                                ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/statistics/ctrl-packets-received
                                                leaf ctrl-packets-received {
                                                    type yang:counter32;
                                                    description "The number of control packets this IPsec Tunnel has received";
                                                }

                                                ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/statistics/ctrl-packets-sent-errors
                                                leaf ctrl-packets-sent-errors {
                                                    type yang:counter32;
                                                    description "The number of control packet transmit errors";
                                                }

                                                ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/statistics/ctrl-packets-received-errors
                                                leaf ctrl-packets-received-errors {
                                                    type yang:counter32;
                                                    description "The number of control packet receive errors";
                                                }

                                                ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/statistics/dpd-packets-sent
                                                leaf dpd-packets-sent {
                                                    type yang:counter32;
                                                    description "The number of Dead-Peer-Detection packets transmitted";
                                                }

                                                ///nokia-state/state/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/statistics/dpd-packets-received
                                                leaf dpd-packets-received {
                                                    type yang:counter32;
                                                    description "The number of Dead-Peer-Detection packets received";
                                                }

