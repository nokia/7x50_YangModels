                                        ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/statistics/dpd-packets-received
                                        leaf dpd-packets-received {
                                            type yang:counter32;
                                            description "The number of Dead-Peer-Detection packets received";
                                        }

                                        ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/statistics/dpd-ack-packets-sent
                                        leaf dpd-ack-packets-sent {
                                            type yang:counter32;
                                            description "The number of Dead-Peer-Detection acknowledgement packets transmitted";
                                        }

                                        ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/statistics/dpd-ack-packets-received
                                        leaf dpd-ack-packets-received {
                                            type yang:counter32;
                                            description "The number of Dead-Peer-Detection acknowledgement packets received";
                                        }

