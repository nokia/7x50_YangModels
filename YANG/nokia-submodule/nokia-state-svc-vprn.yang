                            ///nokia-state/state/service/vprn/pim/ipv4/sg/interface/interface-flags
                            leaf interface-flags {
                                type types-pim:sg-flags;
                                description "Set of lists to which this interface belongs";
                            }

                            ///nokia-state/state/service/vprn/pim/ipv4/sg/interface/spmsi-interface-name
