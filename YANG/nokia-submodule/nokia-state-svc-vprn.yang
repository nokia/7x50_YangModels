                                bit sap-down                       { position 2; }
                                bit unresolved-delivery-svc        { position 3; }
                                bit unregistered-local-ip          { position 4; }
                                bit remote-ip-unreachable          { position 5; }
                                bit unresolved-dst-ip              { position 6; }
                                bit trust-anchors-down             { position 7; }
                                bit cert-profile-down              { position 8; }
                            }
                            description "Reason why the tunnel is operationally down";
                        }

                        ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/oper-remote-ip-address
                        leaf oper-remote-ip-address {
                            type types-sros:ip-address;
                            description "Operational remote IP address.";
                        }

                        ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/matched-trust-anchor
                        leaf matched-trust-anchor {
                            type types-sros:named-item;
                            description
                                "The name for matched Certificate-Authority Profile name associated
                                 with this SAP IPSec SA certificate.";
                        }

                        ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange
                        container key-exchange {
                            description "Enter the key-exchange context";

                            ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic
                            container dynamic {
                                description "Enter the dynamic context";

                                ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa
                                container ike-sa {
                                    description "Enter the ike-sa context";

                                    ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/state
                                    leaf state {
                                        type enumeration {
                                            enum up                             { value 1; }
                                            enum down                           { value 2; }
                                        }
                                        description "The state of IKE SA";
                                    }

                                    ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/established-time
                                    leaf established-time {
                                        type yang:date-and-time;
                                        description "The time the IKE SA completed";
                                    }

                                    ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/negotiated-lifetime
                                    leaf negotiated-lifetime {
                                        type types-sros:time-duration;
                                        description "The lifetime negotiated for the IKE SA";
                                    }

                                    ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/expiration-time
                                    leaf expiration-time {
                                        type yang:date-and-time;
                                        description "The time when the IKE SA will expire";
