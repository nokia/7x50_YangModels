                                    ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/path-mtu
                                    leaf path-mtu {
                                        type uint32;
                                        units "bytes";
                                        description "The path MTU used between the local and remote endpoints for this IPsec tunnel";
                                    }

                                    ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/authentication-algorithm
                                    leaf authentication-algorithm {
                                        type types-ipsec:auth-algorithms;
                                        description "The authentication algorithm of the IKE SA for this IPsec tunnel";
                                    }

                                    ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/encryption-algorithm
                                    leaf encryption-algorithm {
                                        type types-ipsec:encryption-algorithms;
                                        description "The encryption algorithm of the IKE SA for this IPsec tunnel";
                                    }

                                    ///nokia-state/state/service/vprn/interface/sap/ip-tunnel/key-exchange/dynamic/ike-sa/prf-algorithm
                                    leaf prf-algorithm {
                                        type types-ipsec:prf-algorithms;
                                        description "The pseudo-random function (PRF)";
