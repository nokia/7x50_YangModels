                            description "Enable delay TCAs for the measurement interval";
                        }

                        ///nokia-conf/configure/oam-pm/session/measurement-interval/threshold-cross-alerts/loss-events
                        leaf loss-events {
                            type boolean;
                            default "false";
                            description "Enable loss TCAs for the measurement interval";
                        }

                    } // container threshold-cross-alerts

                } // list measurement-interval

            } // list session

            ///nokia-conf/configure/oam-pm/streaming
            container streaming {
                description "Enter the streaming context";

                ///nokia-conf/configure/oam-pm/streaming/delay-template
                list delay-template {
                    key "delay-template-name";
                    description "Enter the delay-template list instance";

                    ///nokia-conf/configure/oam-pm/streaming/delay-template/delay-template-name
                    leaf delay-template-name {
                        type types-sros:named-item-64;
                        description "Streaming delay template name";
                    }

                    ///nokia-conf/configure/oam-pm/streaming/delay-template/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the delay template";
                    }

                    ///nokia-conf/configure/oam-pm/streaming/delay-template/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/oam-pm/streaming/delay-template/sample-window
                    leaf sample-window {
                        type uint32 {
                            range "10..60";
                        }
                        units "seconds";
                        sros-ext:auto-restart-to-modify;
                        default "60";
                        description "Sample window duration for the template";
                    }

                    ///nokia-conf/configure/oam-pm/streaming/delay-template/window-integrity
                    leaf window-integrity {
                        type uint32 {
                            range "1..100";
                        }
                        units "percent";
                        sros-ext:auto-restart-to-modify;
                        default "50";
                        description "Measurements for the sample window validity";
                    }

                    ///nokia-conf/configure/oam-pm/streaming/delay-template/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        sros-ext:auto-restart-to-modify;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/oam-pm/streaming/delay-template/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        sros-ext:auto-restart-to-modify;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/oam-pm/streaming/delay-template/fd-avg
                    list fd-avg {
                        key "direction";
                        description "Add a list entry for fd-avg";

                        ///nokia-conf/configure/oam-pm/streaming/delay-template/fd-avg/direction
                        leaf direction {
                            type types-oam:direction;
                            description "Traffic flow direction of the OAM-PM test or metric";
                        }

                    } // list fd-avg

                    ///nokia-conf/configure/oam-pm/streaming/delay-template/ifdv-avg
                    list ifdv-avg {
                        key "direction";
                        description "Add a list entry for ifdv-avg";

                        ///nokia-conf/configure/oam-pm/streaming/delay-template/ifdv-avg/direction
                        leaf direction {
                            type types-oam:direction;
                            description "Traffic flow direction of the OAM-PM test or metric";
                        }

                    } // list ifdv-avg

                } // list delay-template

            } // container streaming

        } // container oam-pm

    } // grouping conf-oam-pm

}
