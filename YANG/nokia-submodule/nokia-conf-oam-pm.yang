                            ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/twamp-light
                            container twamp-light {
                                presence "Enable the twamp-light context";
                                description "Enable the twamp-light context";

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/twamp-light/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    default "disable";
                                    description "Administrative state of the test";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/twamp-light/test-id
                                leaf test-id {
                                    type uint32 {
                                        range "0..2147483647";
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Test ID";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/twamp-light/test-duration
                                leaf test-duration {
                                    type uint32 {
                                        range "1..86400";
                                    }
                                    units "seconds";
                                    sros-ext:auto-restart-to-modify;
                                    description "Duration of an on-demand test";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/twamp-light/interval
                                leaf interval {
                                    type uint32 {
                                        range "50|100|200|300|400|500|600|700|800|900|1000|10000";
                                    }
                                    units "milliseconds";
                                    sros-ext:auto-restart-to-modify;
                                    description "Time between transmission PDUs for the session's test";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/twamp-light/allow-ipv6-udp-checksum-zero
                                leaf allow-ipv6-udp-checksum-zero {
                                    type boolean;
                                    sros-ext:auto-restart-to-modify;
                                    default "false";
                                    description "Process IPv6 packets with a zero UDP checksum";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/twamp-light/pad-size
                                leaf pad-size {
                                    type uint32 {
                                        range "0..2000";
                                    }
                                    units "octets";
                                    sros-ext:auto-restart-to-modify;
                                    default "0";
                                    description "Amount of packet padding sent for the TWAMP-Light test";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/twamp-light/pad-tlv-size
                                leaf pad-tlv-size {
                                    type uint32 {
                                        range "4..2000";
                                    }
                                    units "octets";
                                    sros-ext:auto-restart-to-modify;
                                    description "Pad TLV size in each sent STAMP packet";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/twamp-light/record-stats
                                leaf record-stats {
                                    type enumeration {
                                        enum delay                          { value 1; }
                                        enum loss                           { value 2; }
                                        enum delay-and-loss                 { value 3; }
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    default "delay";
                                    description "Type of statistics recorded for the TWAMP-Light test";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/twamp-light/session-sender-type
                                leaf session-sender-type {
                                    type enumeration {
                                        enum twamp-light                    { value 1; }
                                        enum stamp                          { value 2; }
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    default "twamp-light";
                                    description "Packet type sent by the test";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/twamp-light/timestamp-format
                                leaf timestamp-format {
                                    type enumeration {
                                        enum ntp                            { value 0; }
                                        enum ptp                            { value 1; }
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    default "ntp";
                                    description "TWAMP Light or STAMP test packet timestamp format";
                                }

