                                                    container sr-te {
                                                        presence "Enable the sr-te context";
                                                        description "Enable the sr-te context";

                                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/tunnel/tunnel-types/tunnel-type-mpls/mpls/mpls-tunnel-types/mpls-sr-te/sr-te/lsp
                                                        leaf lsp {
                                                            type types-sros:named-item-64;
                                                            description "Name of LSP that carries the test packets";
                                                        }

                                                    } // container sr-te

                                                }
                                            }
                                        } // container mpls

