                                    leaf hli-force-count {
                                        type boolean;
                                        sros-ext:auto-restart-to-modify;
                                        default "false";
                                        description "Increment HLI/CHLI counters regardless of availability";
                                    }

                                    ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/twamp-light/loss/timing
                                    container timing {
                                        sros-ext:auto-restart-to-modify;
                                        description "Enter the timing context";

                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/twamp-light/loss/timing/frames-per-delta-t
                                        leaf frames-per-delta-t {
                                            type uint32 {
                                                range "1..50";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            description "Number of frames sent in one delta-t";
