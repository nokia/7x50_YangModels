                                                        leaf lsp-template {
                                                            type types-sros:named-item;
                                                            description "LSP template used to identify the LSP to be tested";
                                                        }

                                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/tunnel/tunnel-types/tunnel-type-mpls/mpls/mpls-tunnel-types/mpls-rsvp-te-auto/rsvp-te-auto/from
                                                        leaf from {
                                                            type types-sros:ipv4-unicast-address;
                                                            description "IPv4 address used to identify the LSP to be tested";
                                                        }

                                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/tunnel/tunnel-types/tunnel-type-mpls/mpls/mpls-tunnel-types/mpls-rsvp-te-auto/rsvp-te-auto/to
                                                        leaf to {
                                                            type types-sros:ipv4-unicast-address;
                                                            description "IPv4 unicast address to identify the LSP to be tested";
                                                        }

                                                    } // container rsvp-te-auto

                                                }
                                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/tunnel/tunnel-types/tunnel-type-mpls/mpls/mpls-tunnel-types/mpls-sr-isis
                                                case mpls-sr-isis {

                                                    ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/tunnel/tunnel-types/tunnel-type-mpls/mpls/mpls-tunnel-types/mpls-sr-isis/sr-isis
                                                    container sr-isis {
                                                        presence "Enable the sr-isis context";
                                                        description "Enable the sr-isis context";

                                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/tunnel/tunnel-types/tunnel-type-mpls/mpls/mpls-tunnel-types/mpls-sr-isis/sr-isis/prefix
                                                        leaf prefix {
                                                            type types-sros:ip-unicast-prefix;
                                                            description "IP prefix to tunnel IP packets for the session tests";
                                                        }

                                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/tunnel/tunnel-types/tunnel-type-mpls/mpls/mpls-tunnel-types/mpls-sr-isis/sr-isis/igp-instance
                                                        leaf igp-instance {
                                                            type uint32 {
                                                                range "0..127";
                                                            }
                                                            default "0";
                                                            description "IGP instance used to tunnel IP packets";
                                                        }

                                                    } // container sr-isis

                                                }
                                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/tunnel/tunnel-types/tunnel-type-mpls/mpls/mpls-tunnel-types/mpls-sr-ospf
                                                case mpls-sr-ospf {

                                                    ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/tunnel/tunnel-types/tunnel-type-mpls/mpls/mpls-tunnel-types/mpls-sr-ospf/sr-ospf
                                                    container sr-ospf {
