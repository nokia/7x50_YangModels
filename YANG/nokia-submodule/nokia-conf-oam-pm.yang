                                                        leaf prefix {
                                                            type types-sros:ipv4-unicast-prefix;
                                                            description "IP prefix to tunnel IP packets for the session tests";
                                                        }

                                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/tunnel/tunnel-types/tunnel-type-mpls/mpls/mpls-tunnel-types/mpls-sr-ospf/sr-ospf/igp-instance
                                                        leaf igp-instance {
                                                            type uint32 {
                                                                range "0..31";
                                                            }
                                                            default "0";
                                                            description "IGP instance used to tunnel IP packets";
                                                        }

                                                    } // container sr-ospf

                                                }
                                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/tunnel/tunnel-types/tunnel-type-mpls/mpls/mpls-tunnel-types/mpls-sr-ospf3
                                                case mpls-sr-ospf3 {

                                                    ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/tunnel/tunnel-types/tunnel-type-mpls/mpls/mpls-tunnel-types/mpls-sr-ospf3/sr-ospf3
                                                    container sr-ospf3 {
