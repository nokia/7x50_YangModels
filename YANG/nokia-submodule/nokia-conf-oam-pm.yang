                                                        leaf prefix {
                                                            type types-sros:ip-unicast-prefix;
                                                            description "IP prefix to tunnel IP packets for the session tests";
                                                        }

                                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/tunnel/tunnel-types/tunnel-type-mpls/mpls/mpls-tunnel-types/mpls-sr-ospf3/sr-ospf3/igp-instance
                                                        leaf igp-instance {
                                                            type uint32 {
