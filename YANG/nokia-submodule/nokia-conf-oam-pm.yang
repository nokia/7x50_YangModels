                                                    } // container rsvp-te

                                                }
                                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/tunnel/tunnel-types/tunnel-type-mpls/mpls/mpls-tunnel-types/mpls-rsvp-te-auto
                                                case mpls-rsvp-te-auto {

