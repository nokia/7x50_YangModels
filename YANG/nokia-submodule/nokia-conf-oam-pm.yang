                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/forwarding/forwarding-choice
                                choice forwarding-choice {
                                    mandatory true;
                                    ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/forwarding/forwarding-choice/next-hop-case
                                    case next-hop-case {

                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/forwarding/forwarding-choice/next-hop-case/next-hop
                                        leaf next-hop {
                                            type types-sros:ip-unicast-address;
                                            description "IP address for forwarding";
                                        }

                                    }
                                    ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/forwarding/forwarding-choice/interface-case
                                    case interface-case {

                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case/ip/forwarding/forwarding-choice/interface-case/interface
                                        leaf interface {
                                            type types-sros:interface-name;
                                            description "Interface name";
