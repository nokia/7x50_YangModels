submodule nokia-conf-oam-pm {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-eth-cfm            { prefix "types-eth-cfm"; }
    import nokia-types-oam                { prefix "types-oam"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-groups;
    include nokia-conf-log;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure OAM-PM configuration on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-oam-pm YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-06-16";

    grouping conf-oam-pm {
        ///nokia-conf/configure/oam-pm
        container oam-pm {
            description "Enable the oam-pm context";

            ///nokia-conf/configure/oam-pm/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/oam-pm/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/oam-pm/bin-group
            list bin-group {
                key "bin-group-id";
                description "Enter the bin-group list instance";

                ///nokia-conf/configure/oam-pm/bin-group/bin-group-id
                leaf bin-group-id {
                    type uint32 {
                        range "1..255";
                    }
                    description "Bin group ID";
                }

                ///nokia-conf/configure/oam-pm/bin-group/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    description "Administrative state of the bin group";
                }

                ///nokia-conf/configure/oam-pm/bin-group/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/oam-pm/bin-group/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    sros-ext:auto-restart-to-modify;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/oam-pm/bin-group/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    sros-ext:auto-restart-to-modify;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/oam-pm/bin-group/bin-type
                list bin-type {
                    key "bin-metric";
                    description "Enter the bin-type list instance";

                    ///nokia-conf/configure/oam-pm/bin-group/bin-type/bin-metric
                    leaf bin-metric {
                        type enumeration {
                            enum fd                             { value 1; }
                            enum fdr                            { value 2; }
                            enum ifdv                           { value 3; }
                        }
                        description "Delay metric bin structure";
                    }

                    ///nokia-conf/configure/oam-pm/bin-group/bin-type/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/oam-pm/bin-group/bin-type/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/oam-pm/bin-group/bin-type/delay-event
                    list delay-event {
                        key "direction";
                        description "Enter the delay-event list instance";

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/delay-event/direction
                        leaf direction {
                            type types-oam:direction;
                            description "Traffic flow direction of the OAM-PM test or metric";
                        }

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/delay-event/lowest-bin
                        leaf lowest-bin {
                            type uint32 {
                                range "0..9";
                            }
                            mandatory true;
                            description "Lowest delay bin used when applying a delay threshold";
                        }

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/delay-event/raise-threshold
                        leaf raise-threshold {
                            type int32 {
                                range "1..864000";
                            }
                            mandatory true;
                            description "Raise threshold for excessive delay";
                        }

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/delay-event/clear-threshold
                        leaf clear-threshold {
                            type int32 {
                                range "0..863999";
                            }
                            description "OAM-PM clear threshold for an excessive delay";
                        }

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/delay-event/exclude-lowest-bin
                        leaf exclude-lowest-bin {
                            type int32 {
                                range "1..9";
                            }
                            description "Lowest bin excluded from the TCA count";
                        }

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/delay-event/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/delay-event/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list delay-event

                    ///nokia-conf/configure/oam-pm/bin-group/bin-type/exclude-from-avg
                    list exclude-from-avg {
                        key "direction";
                        description "Enter the exclude-from-avg list instance";

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/exclude-from-avg/direction
                        leaf direction {
                            type types-oam:direction;
                            description "Traffic flow direction of the OAM-PM test or metric";
                        }

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/exclude-from-avg/bins
                        leaf bins {
                            type string {
                                length "1..39";
                                pattern "(all|(([0-9]|([0-9]-[0-9]))(,([0-9]|([0-9]-[0-9])))*))";
                            }
                            mandatory true;
                            description "Bin numbers excluded from the average calculation";
                        }

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/exclude-from-avg/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/exclude-from-avg/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list exclude-from-avg

                    ///nokia-conf/configure/oam-pm/bin-group/bin-type/bin
                    list bin {
                        key "bin-number";
                        description "Enter the bin list instance";

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/bin/bin-number
                        leaf bin-number {
                            type uint32 {
                                range "0..9";
                            }
                            description "Bin number";
                        }

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/bin/lower-bound
                        leaf lower-bound {
                            type int64 {
                                range "0|1..4294967295";
                            }
                            units "microseconds";
                            description "Lower bound for the bin";
                        }

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/bin/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/oam-pm/bin-group/bin-type/bin/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list bin

                } // list bin-type

            } // list bin-group

            ///nokia-conf/configure/oam-pm/session
            list session {
                key "session-name";
                description "Enter the session list instance";

                ///nokia-conf/configure/oam-pm/session/session-name
                leaf session-name {
                    type types-sros:named-item;
                    description "OAM-PM session name";
                }

                ///nokia-conf/configure/oam-pm/session/session-type
                leaf session-type {
                    type enumeration {
                        enum proactive                      { value 1; }
                        enum on-demand                      { value 2; }
                    }
                    sros-ext:immutable;
                    default "proactive";
                    description "Session type";
                }

                ///nokia-conf/configure/oam-pm/session/bin-group
                leaf bin-group {
                    type leafref {
                        path "../../bin-group/bin-group-id";
                    }
                    description "Bin group for the session";
                }

                ///nokia-conf/configure/oam-pm/session/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/oam-pm/session/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/oam-pm/session/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/oam-pm/session/test-family-choice
                choice test-family-choice {
                    mandatory true;
                    ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case
                    case ethernet-case {

                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet
                        container ethernet {
                            presence "Enable the ethernet context";
                            description "Enable the ethernet context";

                            ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/dest-mac
                            leaf dest-mac {
                                type types-sros:mac-unicast-address;
                                description "Destination MAC address for the session";
                            }

                            ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/remote-mep
                            leaf remote-mep {
                                type uint32 {
                                    range "1..8191";
                                }
                                description "Remote MEP for the session";
                            }

                            ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/priority
                            leaf priority {
                                type uint32 {
                                    range "0..7";
                                }
                                default "0";
                                description "Priority and forwarding class for the session";
                            }

                            ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/source
                            container source {
                                presence "Enable the source context";
                                description "Enable the source context";

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/source/mep
                                leaf mep {
                                    type uint32 {
                                        range "1..8191";
                                    }
                                    mandatory true;
                                    description "Source Ethernet MEP ID";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/source/md-admin-name
                                leaf md-admin-name {
                                    type types-eth-cfm:admin-name;
                                    mandatory true;
                                    description "Source Ethernet MD name";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/source/ma-admin-name
                                leaf ma-admin-name {
                                    type types-eth-cfm:admin-name;
                                    mandatory true;
                                    description "Source Ethernet MA name";
                                }

                            } // container source

                            ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm
                            container dmm {
                                presence "Enable the dmm context";
                                description "Enable the dmm context";

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    default "disable";
                                    description "Administrative state of the test";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/test-id
                                leaf test-id {
                                    type uint32 {
                                        range "0..2147483647";
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Test ID";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/test-duration
                                leaf test-duration {
                                    type uint32 {
                                        range "1..86400";
                                    }
                                    units "seconds";
                                    sros-ext:auto-restart-to-modify;
                                    description "Duration of an on-demand test";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/interval
                                leaf interval {
                                    type uint32 {
                                        range "50|100|200|300|400|500|600|700|800|900|1000|10000";
                                    }
                                    units "milliseconds";
                                    sros-ext:auto-restart-to-modify;
                                    description "Time between transmission PDUs for the session's test";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/data-tlv-size
                                leaf data-tlv-size {
                                    type uint32 {
                                        range "0|3..2000";
                                    }
                                    units "octets";
                                    sros-ext:auto-restart-to-modify;
                                    default "0";
                                    description "Size of the pad TLV in the frames sent for the test";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/delay-template
                                leaf delay-template {
                                    type types-sros:named-item-64;
                                    sros-ext:auto-restart-to-modify;
                                    description "Reference to a streaming delay template for the test";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    sros-ext:auto-restart-to-modify;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    sros-ext:auto-restart-to-modify;
                                    description "Exclude a configuration group at this level";
                                }

                            } // container dmm

                            ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm
                            container lmm {
                                presence "Enable the lmm context";
                                description "Enable the lmm context";

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    default "disable";
                                    description "Administrative state of the test";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/test-id
                                leaf test-id {
                                    type uint32 {
                                        range "0..2147483647";
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Test ID";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/test-duration
                                leaf test-duration {
                                    type uint32 {
                                        range "1..86400";
                                    }
                                    units "seconds";
                                    sros-ext:auto-restart-to-modify;
                                    description "Duration of an on-demand test";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/interval
                                leaf interval {
                                    type uint32 {
                                        range "100|1000|10000";
                                    }
                                    units "milliseconds";
                                    sros-ext:auto-restart-to-modify;
                                    description "The elapsed time between transmission PDUs for the specified test";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/fc-collection
                                leaf fc-collection {
                                    type boolean;
                                    sros-ext:auto-restart-to-modify;
                                    default "false";
                                    description "Enable collection of per-forwarding class counters";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    sros-ext:auto-restart-to-modify;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    sros-ext:auto-restart-to-modify;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/availability
                                container availability {
                                    sros-ext:auto-restart-to-modify;
                                    description "Enter the availability context";

                                    ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/availability/admin-state
                                    leaf admin-state {
                                        type types-sros:admin-state;
                                        sros-ext:auto-restart-to-modify;
                                        default "disable";
                                        description "Administrative state of availability statistics";
                                    }

                                    ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/availability/flr-threshold
                                    leaf flr-threshold {
                                        type types-sros:percent;
                                        sros-ext:auto-restart-to-modify;
                                        default "50";
                                        description "FLR threshold for the test";
                                    }

                                    ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/availability/hli-force-count
                                    leaf hli-force-count {
                                        type boolean;
                                        sros-ext:auto-restart-to-modify;
                                        default "false";
                                        description "Increment HLI/CHLI counters regardless of availability";
                                    }

                                    ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/availability/timing
                                    container timing {
                                        sros-ext:auto-restart-to-modify;
                                        description "Enter the timing context";

                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/availability/timing/frames-per-delta-t
                                        leaf frames-per-delta-t {
                                            type uint32 {
                                                range "1..50";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            description "Number of frames sent in one delta-t";
                                        }

                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/availability/timing/consec-delta-t
                                        leaf consec-delta-t {
                                            type uint32 {
                                                range "2..10";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            default "10";
                                            description "Number of consecutive delta-t intervals";
                                        }

                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/availability/timing/chli-threshold
                                        leaf chli-threshold {
                                            type uint32 {
                                                range "1..9";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            default "5";
                                            description "CHLI threshold";
                                        }

                                    } // container timing

                                } // container availability

                                ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events
                                container loss-events {
                                    sros-ext:auto-restart-to-modify;
                                    description "Enter the loss-events context";

                                    ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/avg-flr-event
                                    list avg-flr-event {
                                        key "direction";
                                        description "Enter the avg-flr-event list instance";

                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/avg-flr-event/direction
                                        leaf direction {
                                            type enumeration {
                                                enum forward                        { value 1; }
                                                enum backward                       { value 2; }
                                            }
                                            description "Traffic flow direction";
                                        }

                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/avg-flr-event/raise-threshold
                                        leaf raise-threshold {
                                            type decimal64 {
                                                range "0.000..100.000";
                                                fraction-digits 3;
                                            }
                                            units "percent";
                                            mandatory true;
                                            description "Raise threshold for the average FLR";
                                        }

                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/avg-flr-event/clear-threshold
                                        leaf clear-threshold {
                                            type decimal64 {
                                                range "0.000..99.999";
                                                fraction-digits 3;
                                            }
                                            units "percent";
                                            description "Clear threshold for the average FLR";
                                        }

                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/avg-flr-event/apply-groups
                                        leaf-list apply-groups {
                                            type leafref {
                                                path "../../../../../../../groups/group/name";
                                            }
                                            max-elements 8;
                                            ordered-by user;
                                            description "Apply a configuration group at this level";
                                        }

                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/avg-flr-event/apply-groups-exclude
                                        leaf-list apply-groups-exclude {
                                            type leafref {
                                                path "../../../../../../../groups/group/name";
                                            }
                                            max-elements 8;
                                            ordered-by user;
                                            description "Exclude a configuration group at this level";
                                        }

                                    } // list avg-flr-event

                                    ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/chli-event
                                    list chli-event {
                                        key "direction";
                                        description "Enter the chli-event list instance";

                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/chli-event/direction
                                        leaf direction {
                                            type enumeration {
                                                enum forward                        { value 1; }
                                                enum backward                       { value 2; }
                                                enum aggregate                      { value 3; }
                                            }
                                            description "Traffic flow direction";
                                        }

