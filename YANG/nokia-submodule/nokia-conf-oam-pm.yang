                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/slm/loss-events/undet-unavailability-event/apply-groups
                                        leaf-list apply-groups {
                                            type leafref {
                                                path "../../../../../../../groups/group/name";
                                            }
                                            max-elements 8;
                                            ordered-by user;
                                            description "Apply a configuration group at this level";
                                        }

                                        ///nokia-conf/configure/oam-pm/session/test-family-choice/ethernet-case/ethernet/slm/loss-events/undet-unavailability-event/apply-groups-exclude
                                        leaf-list apply-groups-exclude {
                                            type leafref {
                                                path "../../../../../../../groups/group/name";
                                            }
                                            max-elements 8;
                                            ordered-by user;
                                            description "Exclude a configuration group at this level";
                                        }

                                    } // list undet-unavailability-event

                                } // container loss-events

                            } // container slm

                        } // container ethernet

                    }
                    ///nokia-conf/configure/oam-pm/session/test-family-choice/ip-case
