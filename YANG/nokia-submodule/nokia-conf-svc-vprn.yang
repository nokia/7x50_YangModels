                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container ping-template

                ///nokia-conf/configure/service/vprn/interface/ipsec
                container ipsec {
                    presence "Enable the ipsec context";
                    description "Enable the ipsec context";

                    ///nokia-conf/configure/service/vprn/interface/ipsec/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of IPsec secured interface";
                    }

                    ///nokia-conf/configure/service/vprn/interface/ipsec/tunnel-group
                    leaf tunnel-group {
                        type leafref {
                            path "../../../../../isa/tunnel-group/id";
                        }
                        sros-ext:immutable;
                        mandatory true;
                        description "Tunnel group ID";
                    }

                    ///nokia-conf/configure/service/vprn/interface/ipsec/public-sap
                    leaf public-sap {
                        type uint32 {
                            range "0..4094";
                        }
                        sros-ext:immutable;
                        mandatory true;
                        description "Public SAP ID";
                    }

                    ///nokia-conf/configure/service/vprn/interface/ipsec/ip-exception
                    leaf ip-exception {
                        type leafref {
                            path "../../../../../filter/ip-exception/filter-name";
                        }
                        description "IP exception filter";
                    }

                    ///nokia-conf/configure/service/vprn/interface/ipsec/ipv6-exception
                    leaf ipv6-exception {
                        type leafref {
                            path "../../../../../filter/ipv6-exception/filter-name";
                        }
                        description "IPv6 filter exception used to bypass encryption";
                    }

                    ///nokia-conf/configure/service/vprn/interface/ipsec/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/interface/ipsec/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel
                    list ipsec-tunnel {
                        key "name";
                        description "Enter the ipsec-tunnel list instance";

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/name
                        leaf name {
                            type types-sros:named-item {
                                pattern "[a-zA-Z]+.*" {
                                    error-message "IPSec tunnel name cannot start with a number";
                                }
                            }
                            description "IPsec tunnel name";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/private-sap
                        leaf private-sap {
                            type uint32 {
                                range "0..4094";
                            }
                            sros-ext:immutable;
                            sros-ext:auto-restart-to-modify;
                            mandatory true;
                            description "Private SAP ID";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the IPsec tunnel";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/clear-df-bit
                        leaf clear-df-bit {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "false";
                            description "Reset the DF bit to 0 in all payload IP packets";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/copy-traffic-class-upon-decapsulation
                        leaf copy-traffic-class-upon-decapsulation {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "false";
                            description "Enable traffic class copy upon decapsulation";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/encapsulated-ip-mtu
                        leaf encapsulated-ip-mtu {
                            type uint32 {
                                range "512..9000";
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "Maximum size of the encapsulated tunnel packet";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/ip-mtu
                        leaf ip-mtu {
                            type uint32 {
                                range "512..9000";
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "Private MTU of the IPsec tunnel";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/replay-window
                        leaf replay-window {
                            type uint32 {
                                range "32|64|128|256|512";
                            }
                            units "packets";
                            sros-ext:auto-restart-to-modify;
                            description "Anti-replay window size";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/public-tcp-mss-adjust
                        leaf public-tcp-mss-adjust {
                            type union {
                                type uint32 {
                                    range "512..9000";
                                }
                                type enumeration {
                                    enum auto                           { value 0; }
                                }
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "TCP maximum segment size (MSS) on the public network";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/private-tcp-mss-adjust
                        leaf private-tcp-mss-adjust {
                            type int32 {
                                range "512..9000";
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "TCP maximum segment size (MSS) adjustment";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/local-gateway-address-override
                        leaf local-gateway-address-override {
                            type types-sros:ip-address;
                            sros-ext:auto-restart-to-modify;
                            description "Local IPsec tunnel endpoint address";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/remote-gateway-address
                        leaf remote-gateway-address {
                            type types-sros:ip-address;
                            sros-ext:auto-restart-to-modify;
                            description "Remote IPsec tunnel endpoint address";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/private-service
                        leaf private-service {
                            type types-services:service-name;
                            sros-ext:immutable;
                            sros-ext:auto-restart-to-modify;
                            description "Private service name";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/propagate-pmtu-v4
                        leaf propagate-pmtu-v4 {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "true";
                            description "Enable propagation of the path MTU to IPv4 hosts";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/propagate-pmtu-v6
                        leaf propagate-pmtu-v6 {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "true";
                            description "Enable propagation of the path MTU to IPv6 hosts";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/pmtu-discovery-aging
                        leaf pmtu-discovery-aging {
                            type uint32 {
                                range "900..3600";
                            }
                            units "seconds";
                            sros-ext:auto-restart-to-modify;
                            default "900";
                            description "Aging out time of the learned path MTU";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            sros-ext:auto-restart-to-modify;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            sros-ext:auto-restart-to-modify;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/bfd
                        container bfd {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the bfd context";

                            ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/bfd/bfd-designate
                            leaf bfd-designate {
                                type boolean;
                                sros-ext:auto-restart-to-modify;
                                default "false";
                                description "Designate IPsec tunnel to carry BFD traffic";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/bfd/bfd-liveness
                            container bfd-liveness {
                                presence "Enable the bfd-liveness context";
                                sros-ext:auto-restart-to-modify;
                                description "Enable the bfd-liveness context";

                                ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/bfd/bfd-liveness/dest-ip
                                leaf dest-ip {
                                    type types-sros:ipv4-unicast-address;
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Destination address used for the BFD session";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/bfd/bfd-liveness/interface
                                leaf interface {
                                    type types-sros:interface-name;
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Name of the interface used by the BFD session";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/bfd/bfd-liveness/service-name
                                leaf service-name {
                                    type types-services:service-name;
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Administrative service name";
                                }

                            } // container bfd-liveness

                        } // container bfd

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/icmp6-generation
                        container icmp6-generation {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the icmp6-generation context";

                            ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/icmp6-generation/packet-too-big
                            container packet-too-big {
                                sros-ext:auto-restart-to-modify;
                                description "Enter the packet-too-big context";

                                ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/icmp6-generation/packet-too-big/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    sros-ext:auto-restart-to-modify;
                                    default "enable";
                                    description "Administrative state of Packet Too Big message sends";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/icmp6-generation/packet-too-big/message-count
                                leaf message-count {
                                    type uint32 {
                                        range "10..1000";
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    default "100";
                                    description "Maximum number of ICMPv6 PTB messages that can be sent";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/icmp6-generation/packet-too-big/interval
                                leaf interval {
                                    type uint32 {
                                        range "1..60";
                                    }
                                    units "seconds";
                                    sros-ext:auto-restart-to-modify;
                                    default "10";
                                    description "Interval for sending Packet Too Big messages";
                                }

                            } // container packet-too-big

                        } // container icmp6-generation

                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange
                        container key-exchange {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the key-exchange context";

                            ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type
                            choice type {
                                ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic
                                case dynamic {

                                    ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic
                                    container dynamic {
                                        presence "Enable the dynamic context";
                                        sros-ext:auto-restart-to-modify;
                                        description "Enable the dynamic context";

                                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/auto-establish
                                        leaf auto-establish {
                                            type boolean;
                                            sros-ext:auto-restart-to-modify;
                                            default "false";
                                            description "Attempt to establish a phase 1 exchange automatically";
                                        }

                                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-policy
                                        leaf ike-policy {
                                            type leafref {
                                                path "../../../../../../../../ipsec/ike-policy/id";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            description "IKE policy ID";
                                        }

                                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ipsec-transform
                                        leaf-list ipsec-transform {
                                            type leafref {
                                                path "../../../../../../../../ipsec/ipsec-transform/id";
                                            }
                                            max-elements 4;
                                            sros-ext:auto-restart-to-modify;
                                            description "IPsec transform IDs used by the dynamic key";
                                        }

                                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/pre-shared-key
                                        leaf pre-shared-key {
                                            type types-sros:encrypted-leaf {
                                                length "1..115";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            description "Pre-shared key for authentication";
                                        }

                                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert
                                        container cert {
                                            sros-ext:auto-restart-to-modify;
                                            description "Enter the cert context";

                                            ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/cert-profile
                                            leaf cert-profile {
                                                type leafref {
                                                    path "../../../../../../../../../ipsec/cert-profile/name";
                                                }
                                                description "Certificate profile name";
                                            }

                                            ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/trust-anchor-profile
                                            leaf trust-anchor-profile {
                                                type leafref {
                                                    path "../../../../../../../../../ipsec/trust-anchor-profile/name";
                                                }
                                                description "Trust anchor profile name";
                                            }

                                            ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify
                                            container status-verify {
                                                sros-ext:auto-restart-to-modify;
                                                description "Enter the status-verify context";

                                                ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify/default-result
                                                leaf default-result {
                                                    type enumeration {
                                                        enum revoked                        { value 0; }
                                                        enum good                           { value 1; }
                                                    }
                                                    default "revoked";
                                                    description "Default result for Certificate Status Verification";
                                                }

                                                ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify/primary
                                                leaf primary {
                                                    type types-ipsec:cert-revocation-status;
                                                    default "crl";
                                                    description "Primary method of CSV to verify the revocation status";
                                                }

                                                ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify/secondary
                                                leaf secondary {
                                                    type types-ipsec:cert-revocation-status-with-none;
                                                    default "none";
                                                    description "Secondary method used to verify certificate revocation";
                                                }

                                            } // container status-verify

                                        } // container cert

                                        ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id
                                        container id {
                                            sros-ext:auto-restart-to-modify;
                                            description "Enter the id context";

                                            ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id
                                            choice id {
                                                ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv4
                                                case ipv4 {

                                                    ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv4/ipv4
                                                    leaf ipv4 {
                                                        type types-sros:ipv4-unicast-address;
                                                        sros-ext:auto-restart-to-modify;
                                                        description "IPv4 as the local ID type";
                                                    }

                                                }
                                                ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv6
                                                case ipv6 {

                                                    ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv6/ipv6
                                                    leaf ipv6 {
                                                        type types-sros:ip-address;
                                                        sros-ext:auto-restart-to-modify;
                                                        description "IPv6 used as the local IKE ID type";
                                                    }

                                                }
                                                ///nokia-conf/configure/service/vprn/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/fqdn
                                                case fqdn {

