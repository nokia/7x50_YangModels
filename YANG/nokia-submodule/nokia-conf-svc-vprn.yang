                                description "Enter the severity context";

                                ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/severity/severity-match
                                choice severity-match {
                                    ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/severity/severity-match/eq
                                    case eq {

                                        ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/severity/severity-match/eq/eq
                                        leaf eq {
                                            type types-log:severity-level;
                                            description "Log event severity level to match";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/severity/severity-match/neq
                                    case neq {

                                        ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/severity/severity-match/neq/neq
                                        leaf neq {
                                            type types-log:severity-level;
                                            description "Log event severity level to filter out";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/severity/severity-match/lt
                                    case lt {

                                        ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/severity/severity-match/lt/lt
                                        leaf lt {
                                            type types-log:severity-level;
                                            description "Log event severity level";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/severity/severity-match/lte
                                    case lte {

                                        ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/severity/severity-match/lte/lte
                                        leaf lte {
                                            type types-log:severity-level;
                                            description "Log event severity level";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/severity/severity-match/gt
                                    case gt {

                                        ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/severity/severity-match/gt/gt
                                        leaf gt {
                                            type types-log:severity-level;
                                            description "Log event severity level";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/severity/severity-match/gte
                                    case gte {

                                        ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/severity/severity-match/gte/gte
                                        leaf gte {
                                            type types-log:severity-level;
                                            description "Log event severity level";
                                        }

                                    }
                                }
                            } // container severity

                            ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/subject
                            container subject {
                                description "Enter the subject context";

                                ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/subject/regexp
                                leaf regexp {
                                    type boolean;
                                    default "false";
                                    description "String comparison to determine if the log event matches the value of subject";
                                }

                                ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/subject/subject-match
                                choice subject-match {
                                    ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/subject/subject-match/eq
                                    case eq {

                                        ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/subject/subject-match/eq/eq
                                        leaf eq {
                                            type types-sros:named-item;
                                            description "Log event subject string to match";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/subject/subject-match/neq
                                    case neq {

                                        ///nokia-conf/configure/service/vprn/log/filter/named-entry/match/subject/subject-match/neq/neq
                                        leaf neq {
                                            type types-sros:named-item;
                                            description "Log event subject string to filter out";
                                        }

                                    }
                                }
                            } // container subject

                        } // container match

                    } // list named-entry

                } // list filter

                ///nokia-conf/configure/service/vprn/log/log-id
                list log-id {
                    key "name";
                    max-elements 30;
                    description "Enter the log-id list instance";

                    ///nokia-conf/configure/service/vprn/log/log-id/name
                    leaf name {
                        type types-log:li-log-name;
                        description "Log ID";
                    }

                    ///nokia-conf/configure/service/vprn/log/log-id/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "enable";
                        description "Administrative state of the log";
                    }

                    ///nokia-conf/configure/service/vprn/log/log-id/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/service/vprn/log/log-id/netconf-stream
                    leaf netconf-stream {
                        type types-sros:named-item;
                        description "Destination NETCONF stream name";
                    }

                    ///nokia-conf/configure/service/vprn/log/log-id/time-format
                    leaf time-format {
                        type enumeration {
                            enum utc                            { value 1; }
                            enum local                          { value 2; }
                        }
                        default "utc";
                        description "Time zone output for file log contents and syslog";
                    }

                    ///nokia-conf/configure/service/vprn/log/log-id/filter
                    leaf filter {
                        type leafref {
                            path "../../filter/filter-name";
                        }
                        description "Event filter policy with the log destination";
                    }

                    ///nokia-conf/configure/service/vprn/log/log-id/python-policy
                    leaf python-policy {
                        type leafref {
                            path "../../../../../python/python-policy/name";
                        }
                        description "Python policy name";
                    }

                    ///nokia-conf/configure/service/vprn/log/log-id/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/log/log-id/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/log/log-id/source
                    container source {
                        description "Enter the source context";

                        ///nokia-conf/configure/service/vprn/log/log-id/source/main
                        leaf main {
                            type boolean;
                            default "false";
                            description "Collect log events from the main event stream";
                        }

                        ///nokia-conf/configure/service/vprn/log/log-id/source/security
                        leaf security {
                            type boolean;
                            default "false";
                            description "Collect log events from the security event stream";
                        }

                        ///nokia-conf/configure/service/vprn/log/log-id/source/change
                        leaf change {
                            type boolean;
                            default "false";
                            description "Collect log events from the change event stream";
                        }

                        ///nokia-conf/configure/service/vprn/log/log-id/source/debug
                        leaf debug {
                            type boolean;
                            default "false";
                            description "Collect log events from the debug event stream";
                        }

                    } // container source

                    ///nokia-conf/configure/service/vprn/log/log-id/destination
                    container destination {
                        description "Enter the destination context";

                        ///nokia-conf/configure/service/vprn/log/log-id/destination/log-id-destination
                        choice log-id-destination {
                            ///nokia-conf/configure/service/vprn/log/log-id/destination/log-id-destination/syslog
                            case syslog {

                                ///nokia-conf/configure/service/vprn/log/log-id/destination/log-id-destination/syslog/syslog
                                leaf syslog {
                                    type leafref {
                                        path "../../../syslog/syslog-name";
                                    }
                                    sros-ext:immutable;
                                    description "Index with the information to format event messages sent to a specific SYSLOG collector";
                                }

                            }
                            ///nokia-conf/configure/service/vprn/log/log-id/destination/log-id-destination/snmp
                            case snmp {

                                ///nokia-conf/configure/service/vprn/log/log-id/destination/log-id-destination/snmp/snmp
                                container snmp {
                                    presence "Enable the snmp context";
                                    description "Enable the snmp context";

                                    ///nokia-conf/configure/service/vprn/log/log-id/destination/log-id-destination/snmp/snmp/max-entries
                                    leaf max-entries {
                                        type uint32 {
                                            range "50..3000";
                                        }
                                        sros-ext:immutable;
                                        default "100";
                                        description "Number of events stored in the memory log";
                                    }

                                } // container snmp

                            }
                            ///nokia-conf/configure/service/vprn/log/log-id/destination/log-id-destination/netconf
                            case netconf {

                                ///nokia-conf/configure/service/vprn/log/log-id/destination/log-id-destination/netconf/netconf
                                container netconf {
                                    presence "Enable the netconf context";
                                    description "Enable the netconf context";

                                    ///nokia-conf/configure/service/vprn/log/log-id/destination/log-id-destination/netconf/netconf/max-entries
                                    leaf max-entries {
                                        type uint32 {
                                            range "50..3000";
                                        }
                                        sros-ext:immutable;
                                        default "100";
                                        description "Number of events stored in the NETCONF log";
                                    }

                                } // container netconf

                            }
                        }
                    } // container destination

                } // list log-id

                ///nokia-conf/configure/service/vprn/log/snmp-trap-group
                list snmp-trap-group {
                    key "log-name";
                    description "Enter the snmp-trap-group list instance";

                    ///nokia-conf/configure/service/vprn/log/snmp-trap-group/log-name
                    leaf log-name {
                        type types-log:svc-vprn-snmp-trap-group-name;
                        description "Log ID";
                    }

                    ///nokia-conf/configure/service/vprn/log/snmp-trap-group/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/service/vprn/log/snmp-trap-group/log-id
                    leaf log-id {
                        status obsolete;
                        type int32 {
                            range "1..100";
                        }
                        sros-ext:immutable;
                        description "SNMP notification group associated with the event log";
                    }

                    ///nokia-conf/configure/service/vprn/log/snmp-trap-group/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/log/snmp-trap-group/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/log/snmp-trap-group/trap-target
                    list trap-target {
                        key "name";
                        max-elements 25;
                        description "Enter the trap-target list instance";

                        ///nokia-conf/configure/service/vprn/log/snmp-trap-group/trap-target/name
                        leaf name {
                            type string {
                                length "1..28";
                            }
                            description "Trap target name";
                        }

                        ///nokia-conf/configure/service/vprn/log/snmp-trap-group/trap-target/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/service/vprn/log/snmp-trap-group/trap-target/address
                        leaf address {
                            type types-sros:ip-unicast-address;
                            mandatory true;
                            description "IP address of the trap receiver";
                        }

                        ///nokia-conf/configure/service/vprn/log/snmp-trap-group/trap-target/port
                        leaf port {
                            type types-qos:tcp-udp-match-port;
                            default "162";
                            description "UDP port number to send messages to this remote SNMP notification collector";
                        }

                        ///nokia-conf/configure/service/vprn/log/snmp-trap-group/trap-target/version
                        leaf version {
                            type enumeration {
                                enum snmpv1                         { value 0; }
                                enum snmpv2c                        { value 1; }
                                enum snmpv3                         { value 3; }
                            }
                            default "snmpv3";
                            description "SNMP version to format notification messages sent to this SNMP notification collector";
                        }

                        ///nokia-conf/configure/service/vprn/log/snmp-trap-group/trap-target/notify-community
                        leaf notify-community {
                            type string {
                                length "1..31";
                            }
                            mandatory true;
                            description "SNMPv1 or SNMPv2c community name string, or SNMPv3 security name, for sending a notification";
                        }

                        ///nokia-conf/configure/service/vprn/log/snmp-trap-group/trap-target/security-level
                        leaf security-level {
                            type enumeration {
                                enum no-auth-no-privacy             { value 1; }
                                enum auth-no-privacy                { value 2; }
                                enum privacy                        { value 3; }
                            }
                            default "no-auth-no-privacy";
                            description "Security level at which SNMP notification messages are sent to SNMP notification collector";
                        }

                        ///nokia-conf/configure/service/vprn/log/snmp-trap-group/trap-target/replay
                        leaf replay {
                            type boolean;
                            default "false";
                            description "Retransmit missed notifications";
                        }

                        ///nokia-conf/configure/service/vprn/log/snmp-trap-group/trap-target/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/log/snmp-trap-group/trap-target/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list trap-target

                } // list snmp-trap-group

                ///nokia-conf/configure/service/vprn/log/syslog
                list syslog {
                    key "syslog-name";
                    max-elements 30;
                    description "Enter the syslog list instance";

                    ///nokia-conf/configure/service/vprn/log/syslog/syslog-name
                    leaf syslog-name {
                        type types-log:log-vprn-syslog-name;
                        description "Syslog name";
                    }

                    ///nokia-conf/configure/service/vprn/log/syslog/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/service/vprn/log/syslog/address
                    leaf address {
                        type types-sros:ip-unicast-address;
                        description "IP address of the Syslog target host";
                    }

                    ///nokia-conf/configure/service/vprn/log/syslog/facility
                    leaf facility {
                        type enumeration {
                            enum kernel                         { value 0; }
                            enum user                           { value 1; }
                            enum mail                           { value 2; }
                            enum systemd                        { value 3; }
                            enum auth                           { value 4; }
                            enum syslogd                        { value 5; }
                            enum printer                        { value 6; }
                            enum netnews                        { value 7; }
                            enum uucp                           { value 8; }
                            enum cron                           { value 9; }
                            enum authpriv                       { value 10; }
                            enum ftp                            { value 11; }
                            enum ntp                            { value 12; }
                            enum logaudit                       { value 13; }
                            enum logalert                       { value 14; }
                            enum cron2                          { value 15; }
                            enum local0                         { value 16; }
                            enum local1                         { value 17; }
                            enum local2                         { value 18; }
                            enum local3                         { value 19; }
                            enum local4                         { value 20; }
                            enum local5                         { value 21; }
                            enum local6                         { value 22; }
                            enum local7                         { value 23; }
                        }
                        default "local7";
                        description "Facility code for messages";
                    }

                    ///nokia-conf/configure/service/vprn/log/syslog/severity
                    leaf severity {
                        type enumeration {
                            enum emergency                      { value 0; }
                            enum alert                          { value 1; }
                            enum critical                       { value 2; }
                            enum error                          { value 3; }
                            enum warning                        { value 4; }
                            enum notice                         { value 5; }
                            enum info                           { value 6; }
                            enum debug                          { value 7; }
                        }
                        default "info";
                        description "Severity level threshold for the syslog message";
                    }

                    ///nokia-conf/configure/service/vprn/log/syslog/log-prefix
                    leaf log-prefix {
                        type union {
                            type enumeration {
                                enum no-prefix                      { value -1; }
                            }
                            type string {
                                length "1..32";
                                pattern "([!-9;<>-~])+" {
                                    error-message "String must contain only printable characters between 33 - 126 ASCII except ':' and '='";
                                }
                            }
                        }
                        default "TMNX";
                        description "Prefix string to log message sent to target syslog host";
                    }

                    ///nokia-conf/configure/service/vprn/log/syslog/tls-client-profile
                    leaf tls-client-profile {
                        type leafref {
                            path "../../../../../system/security/tls/client-tls-profile/client-profile-name";
                        }
                        sros-ext:immutable;
                        description "TLS client profile used to encrypt syslog communication";
                    }

                    ///nokia-conf/configure/service/vprn/log/syslog/port
                    leaf port {
                        when "not(../tls-client-profile)" {
                            description "Log syslog TLS profile can not be defined together with port. TLS is using default port 6514";
                        }
                        type types-qos:tcp-udp-match-port;
                        default "514";
                        description "Destination port when sending syslog over UDP";
                    }

                    ///nokia-conf/configure/service/vprn/log/syslog/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/log/syslog/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/log/syslog/hostname
                    container hostname {
                        description "Enter the hostname context";

                        ///nokia-conf/configure/service/vprn/log/syslog/hostname/hostname
                        choice hostname {
                            ///nokia-conf/configure/service/vprn/log/syslog/hostname/hostname/value
                            case value {

                                ///nokia-conf/configure/service/vprn/log/syslog/hostname/hostname/value/value
                                leaf value {
                                    type types-sros:named-item-255;
                                    description "Syslog HOSTNAME field value";
                                }

                            }
                            ///nokia-conf/configure/service/vprn/log/syslog/hostname/hostname/use-system-name
                            case use-system-name {

                                ///nokia-conf/configure/service/vprn/log/syslog/hostname/hostname/use-system-name/use-system-name
                                container use-system-name {
                                    presence "Enable the use-system-name context";
                                    description "Enable the use-system-name context";

                                } // container use-system-name

                            }
                            ///nokia-conf/configure/service/vprn/log/syslog/hostname/hostname/use-vprn-name
                            case use-vprn-name {

                                ///nokia-conf/configure/service/vprn/log/syslog/hostname/hostname/use-vprn-name/use-vprn-name
                                container use-vprn-name {
                                    presence "Enable the use-vprn-name context";
                                    description "Enable the use-vprn-name context";

                                } // container use-vprn-name

                            }
                        }
                    } // container hostname

                } // list syslog

            } // container log

            ///nokia-conf/configure/service/vprn/igmp-host-tracking
            container igmp-host-tracking {
                description "Enable the igmp-host-tracking context";

                ///nokia-conf/configure/service/vprn/igmp-host-tracking/admin-state
