                        leaf timeout {
                            type uint32 {
                                range "0..65535";
                            }
                            units "seconds";
                            default "14400";
                            description "Timeout for an ARP entry learned on the interface";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/retry-timer
                        leaf retry-timer {
                            type uint32 {
                                range "1..300";
                            }
                            units "deciseconds";
                            default "50";
                            description "ARP retry interval";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/learn-unsolicited
                        leaf learn-unsolicited {
                            type boolean;
                            default "false";
                            description "Learn new entries from any received NA message";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/proactive-refresh
                        leaf proactive-refresh {
                            type boolean;
                            default "false";
                            description "Send a single refresh message before entry timeout";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/populate
                        leaf populate {
                            type boolean;
                            default "false";
                            description "Allow static and dynamic hosts to be populated in system ARP cache";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/populate-host
                        leaf populate-host {
                            status obsolete;
                            type boolean;
                            default "false";
                            description "Allow addition or deletion of host routes in route table derived from ARP entries in ARP cache";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/route-tag
                        leaf route-tag {
                            status obsolete;
                            type types-router:route-tag;
                            description "ARP route tag";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/local-proxy-arp
                        leaf local-proxy-arp {
                            type boolean;
                            description "Enable local proxy ARP on interface";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/remote-proxy-arp
                        leaf remote-proxy-arp {
                            type boolean;
                            default "false";
                            description "Enable remote proxy ARP on the interface";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/proxy-arp-policy
                        leaf-list proxy-arp-policy {
                            type leafref {
                                path "../../../../../../policy-options/policy-statement/name";
                            }
                            max-elements 5;
                            ordered-by user;
                            description "Proxy ARP policy name";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/host-route
                        container host-route {
                            description "Enter the host-route context";

                            ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/host-route/populate
                            list populate {
                                key "route-type";
                                description "Enter the populate list instance";

                                ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/host-route/populate/route-type
                                leaf route-type {
                                    type enumeration {
                                        enum static                         { value 1; }
                                        enum dynamic                        { value 2; }
                                        enum evpn                           { value 3; }
                                    }
                                    description "Type of ARP or ND entries that generate host routes";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/host-route/populate/route-tag
                                leaf route-tag {
                                    type uint32 {
                                        range "1..255";
                                    }
                                    sros-ext:immutable;
                                    description "Tag value used with the host route from an ARP/ND entry";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/host-route/populate/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/host-route/populate/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                            } // list populate

                        } // container host-route

                        ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/limit
                        container limit {
                            description "Enter the limit context";

                            ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/limit/max-entries
                            leaf max-entries {
                                type types-router:ipv4-neighbor-limit;
                                description "Maximum number of entries learned on an IP interface";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/limit/log-only
                            leaf log-only {
                                type boolean;
                                default "false";
                                description "Generate log entries only if limit is reached";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/limit/threshold
                            leaf threshold {
                                type uint32 {
                                    range "1..100";
                                }
                                units "percent";
                                default "90";
                                description "Threshold value that triggers a warning message";
                            }

                        } // container limit

                        ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/static-neighbor-unnumbered
                        container static-neighbor-unnumbered {
                            when "../../unnumbered/ip-address or ../../unnumbered/ip-int-name" {
                                description "only allowed in unnumbered interfaces";
                            }
                            presence "Enable the static-neighbor-unnumbered context";
                            description "Enable the static-neighbor-unnumbered context";

                            ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/static-neighbor-unnumbered/mac-address
                            leaf mac-address {
                                type yang:mac-address;
                                mandatory true;
                                description "MAC address for the static neighbor";
                            }

                        } // container static-neighbor-unnumbered

                        ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/static-neighbor
                        list static-neighbor {
                            when "not(../../unnumbered/ip-address) and not(../../unnumbered/ip-int-name)" {
                                description "not allowed in unnumbered interfaces";
                            }
                            key "ipv4-address";
                            description "Enter the static-neighbor list instance";

                            ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/static-neighbor/ipv4-address
                            leaf ipv4-address {
                                type types-sros:ipv4-address;
                                description "IPv4 address that corresponds to the physical address";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/static-neighbor/mac-address
                            leaf mac-address {
                                type yang:mac-address;
                                mandatory true;
                                description "MAC address for the static neighbor";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/static-neighbor/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv4/neighbor-discovery/static-neighbor/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list static-neighbor

                    } // container neighbor-discovery

                    ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp
                    container dhcp {
                        description "Enable the dhcp context";

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            description "Administrative state of DHCP";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/python-policy
                        leaf python-policy {
                            when "../../../tunnel = 'false'" {
                                description "not allowed in a tunnel interface";
                            }
                            type leafref {
                                path "../../../../../../python/python-policy/name";
                            }
                            description "Python policy";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/server
                        leaf-list server {
                            type types-sros:ipv4-unicast-address;
                            max-elements 8;
                            ordered-by user;
                            description "IP addresses for DHCP server requests";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/trusted
                        leaf trusted {
                            type boolean;
                            default "false";
                            description "Relay untrusted packets";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/gi-address
                        leaf gi-address {
                            type types-sros:ipv4-unicast-address;
                            description "GI address for the DHCP relay";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/release-include-gi-address
                        leaf release-include-gi-address {
                            type boolean;
                            default "false";
                            description "Include gateway IP address in DHCP Release messages";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/src-ip-addr
                        leaf src-ip-addr {
                            type enumeration {
                                enum auto                           { value 0; }
                                enum gi-address                     { value 1; }
                            }
                            default "auto";
                            description "Type of source address to use for DHCP relay";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/relay-plain-bootp
                        leaf relay-plain-bootp {
                            when "../../../tunnel = 'false'" {
                                description "not allowed in a tunnel interface";
                            }
                            type boolean;
                            default "false";
                            description "Enable relaying of plain BOOTP packets";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/use-arp
                        leaf use-arp {
                            when "../../../tunnel = 'false'" {
                                description "not allowed in a tunnel interface";
                            }
                            type boolean;
                            default "false";
                            description "Use ARP to determine the destination hardware address";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/proxy-server
                        container proxy-server {
                            description "Enter the proxy-server context";

                            ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/proxy-server/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "disable";
                                description "Administrative state of the DHCP proxy server";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/proxy-server/emulated-server
                            leaf emulated-server {
                                type types-sros:ipv4-unicast-address;
                                description "IP address used as the DHCP server address for the SAP";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/proxy-server/lease-time
                            container lease-time {
                                description "Enter the lease-time context";

                                ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/proxy-server/lease-time/value
                                leaf value {
                                    type types-sros:time-duration {
                                        range "300..315446399";
                                    }
                                    units "seconds";
                                    description "DHCP lease time";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/proxy-server/lease-time/radius-override
                                leaf radius-override {
                                    type boolean;
                                    default "false";
                                    description "Use lease time information provided by RADIUS server";
                                }

                            } // container lease-time

                        } // container proxy-server

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82
                        container option-82 {
                            description "Enter the option-82 context";

                            ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/action
                            leaf action {
                                type enumeration {
                                    enum replace                        { value 1; }
                                    enum drop                           { value 2; }
                                    enum keep                           { value 3; }
                                }
                                default "keep";
                                description "Action to take with received DHCP Option 82";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/circuit-id
                            container circuit-id {
                                description "Enter the circuit-id context";

                                ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/circuit-id/circuit-id
                                choice circuit-id {
                                    default "ascii-tuple";
                                    ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/none
                                    case none {

                                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/none/none
                                        leaf none {
                                            type empty;
                                            description "Do not include the circuit ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ifindex
                                    case ifindex {

                                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ifindex/ifindex
                                        leaf ifindex {
                                            type empty;
                                            description "Use the interface index for the circuit ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ascii-tuple
                                    case ascii-tuple {

                                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ascii-tuple/ascii-tuple
                                        leaf ascii-tuple {
                                            type empty;
                                            description "Use the ASCII-encoded tuple for the circuit ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/sap-id
                                    case sap-id {

                                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/sap-id/sap-id
                                        leaf sap-id {
                                            type empty;
                                            description "Use the SAP ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/vlan-ascii-tuple
                                    case vlan-ascii-tuple {

                                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/vlan-ascii-tuple/vlan-ascii-tuple
                                        leaf vlan-ascii-tuple {
                                            type empty;
                                            description "Include the VLAN ID and dot1p bits in the ASCII tuple";
                                        }

                                    }
                                }
                            } // container circuit-id

                            ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/remote-id
                            container remote-id {
                                description "Enter the remote-id context";

                                ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/remote-id/remote-id
                                choice remote-id {
                                    default "none";
                                    ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/remote-id/remote-id/none
                                    case none {

                                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/remote-id/remote-id/none/none
                                        leaf none {
                                            type empty;
                                            description "Do not include the remote ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/remote-id/remote-id/mac
                                    case mac {

                                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/remote-id/remote-id/mac/mac
                                        leaf mac {
                                            type empty;
                                            description "Use the MAC address for the remote ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/remote-id/remote-id/ascii-string
                                    case ascii-string {

                                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/remote-id/remote-id/ascii-string/ascii-string
                                        leaf ascii-string {
                                            type types-sros:string-not-all-spaces {
                                                length "1..32";
                                            }
                                            description "User-defined ASCII string for the remote ID";
                                        }

                                    }
                                }
                            } // container remote-id

                            ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/vendor-specific-option
                            container vendor-specific-option {
                                description "Enter the vendor-specific-option context";

                                ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/vendor-specific-option/system-id
                                leaf system-id {
                                    type boolean;
                                    default "false";
                                    description "Send the system ID in the VSO";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/vendor-specific-option/client-mac-address
                                leaf client-mac-address {
                                    type boolean;
                                    default "false";
                                    description "Send the MAC address in the VSO";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/vendor-specific-option/pool-name
                                leaf pool-name {
                                    type boolean;
                                    default "false";
                                    description "Send the pool name in the VSO";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/vendor-specific-option/service-id
                                leaf service-id {
                                    type boolean;
                                    default "false";
                                    description "Send the service ID in the Vendor Specific Option";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/vendor-specific-option/sap-id
                                leaf sap-id {
                                    type boolean;
                                    default "false";
                                    description "Send SAP ID in the sub-option of the DHCP relay packet";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/option-82/vendor-specific-option/string
                                leaf string {
                                    type types-sros:string-not-all-spaces {
                                        length "1..32";
                                    }
                                    description "User-defined ASCII string for the VSO";
                                }

                            } // container vendor-specific-option

                        } // container option-82

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/lease-populate
                        container lease-populate {
                            description "Enter the lease-populate context";

                            ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/lease-populate/max-leases
                            leaf max-leases {
                                type uint32 {
                                    range "0..511999";
                                }
                                description "Maximum number of DHCPv4 leases allowed";
                            }

                        } // container lease-populate

                        ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/relay-proxy
                        container relay-proxy {
                            presence "Enable the relay-proxy context";
                            description "Enable the relay-proxy context";

                            ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/relay-proxy/release-update-src-ip
                            leaf release-update-src-ip {
                                type boolean;
                                default "false";
                                description "Update the source IP address of a DHCP RELEASE message";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv4/dhcp/relay-proxy/siaddr-override
                            leaf siaddr-override {
                                type types-sros:ipv4-unicast-address;
                                description "DHCP server IP address for address hiding function";
                            }

                        } // container relay-proxy

                    } // container dhcp

                    ///nokia-conf/configure/service/vprn/interface/ipv4/vrrp
                    list vrrp {
                        key "virtual-router-id";
                        description "Enter the vrrp list instance";

                        ///nokia-conf/configure/service/vprn/interface/ipv4/vrrp/virtual-router-id
