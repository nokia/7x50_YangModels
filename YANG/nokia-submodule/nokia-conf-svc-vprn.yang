                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/clear-df-bit
                        leaf clear-df-bit {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "false";
                            description "Reset the DF bit to 0 in all payload IP packets";
                        }

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/copy-traffic-class-upon-decapsulation
                        leaf copy-traffic-class-upon-decapsulation {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "false";
                            description "Enable traffic class copy upon decapsulation";
                        }

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/encapsulated-ip-mtu
                        leaf encapsulated-ip-mtu {
                            type uint32 {
                                range "512..9000";
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "Maximum size of the encapsulated tunnel packet";
                        }

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/ip-mtu
                        leaf ip-mtu {
                            type uint32 {
                                range "512..9000";
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "Private MTU of the IPsec tunnel";
                        }

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/replay-window
                        leaf replay-window {
                            type uint32 {
                                range "32|64|128|256|512";
                            }
                            units "packets";
                            sros-ext:auto-restart-to-modify;
                            description "Anti-replay window size";
                        }

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/public-tcp-mss-adjust
                        leaf public-tcp-mss-adjust {
                            type union {
                                type uint32 {
                                    range "512..9000";
                                }
                                type enumeration {
                                    enum auto                           { value 0; }
                                }
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "TCP maximum segment size (MSS) on the public network";
                        }

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/private-tcp-mss-adjust
                        leaf private-tcp-mss-adjust {
                            type int32 {
                                range "512..9000";
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "TCP maximum segment size (MSS) adjustment";
                        }

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/propagate-pmtu-v4
                        leaf propagate-pmtu-v4 {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "true";
                            description "Enable propagation of the path MTU to IPv4 hosts";
                        }

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/propagate-pmtu-v6
                        leaf propagate-pmtu-v6 {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "true";
                            description "Enable propagation of the path MTU to IPv6 hosts";
                        }

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/pmtu-discovery-aging
                        leaf pmtu-discovery-aging {
                            type uint32 {
                                range "900..3600";
