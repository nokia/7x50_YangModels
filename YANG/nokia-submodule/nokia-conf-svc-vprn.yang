                            default "disable";
                            description "Administrative state of the L2TP tunnel";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/preference
                        leaf preference {
                            type uint32 {
