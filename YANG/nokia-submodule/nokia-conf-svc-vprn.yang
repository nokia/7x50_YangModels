                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Up hold time for the IP interface";
                            }

                        } // container up

                        ///nokia-conf/configure/service/vprn/interface/hold-time/ipv6/down
                        container down {
                            description "Enter the down context";

                            ///nokia-conf/configure/service/vprn/interface/hold-time/ipv6/down/seconds
                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Down hold time for the IP interface";
                            }

                            ///nokia-conf/configure/service/vprn/interface/hold-time/ipv6/down/init-only
                            leaf init-only {
                                type boolean;
                                default "false";
                                description "Apply delay only at interface configuration or reboot";
                            }

                        } // container down

                    } // container ipv6

