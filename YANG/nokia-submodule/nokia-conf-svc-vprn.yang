                    container link-local-address {
                        description "Enter the link-local-address context";

                        ///nokia-conf/configure/service/vprn/interface/ipv6/link-local-address/address
                        leaf address {
                            type types-sros:ipv6-address;
                            description "IPv6 link-local address";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/link-local-address/duplicate-address-detection
                        leaf duplicate-address-detection {
                            type boolean;
                            default "true";
                            description "Enable Duplicate Address Detection";
                        }

                    } // container link-local-address

                    ///nokia-conf/configure/service/vprn/interface/ipv6/address
                    list address {
                        key "ipv6-address";
                        description "Enter the address list instance";

                        ///nokia-conf/configure/service/vprn/interface/ipv6/address/ipv6-address
                        leaf ipv6-address {
                            type types-sros:ipv6-address;
                            description "IPv6 address assigned to the interface";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/address/prefix-length
                        leaf prefix-length {
                            type uint32 {
                                range "4..128";
                            }
                            mandatory true;
                            description "IPv6 address prefix length";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/address/eui-64
                        leaf eui-64 {
                            type boolean;
                            sros-ext:immutable;
                            default "false";
                            description "Form IPv6 address from prefix and 64-bit interface ID";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/address/duplicate-address-detection
                        leaf duplicate-address-detection {
                            type boolean;
                            sros-ext:immutable;
                            default "true";
                            description "Enable Duplicate Address Detection";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/address/primary-preference
                        leaf primary-preference {
                            type uint32 {
                                range "1..4294967295";
                            }
                            description "Index assigned to the IPv6 address of the interface";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/address/track-srrp
                        leaf track-srrp {
                            type uint32 {
                                range "1..4294967295";
                            }
                            description "SRRP ID whose state is tracked on this IP address";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/address/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/address/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list address

                    ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6
                    container dhcp6 {
                        when "../../tunnel = 'false'" {
                            description "not allowed in a tunnel interface";
                        }
                        description "Enable the dhcp6 context";

                        ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay
                        container relay {
                            description "Enter the relay context";

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/description
                            leaf description {
                                type types-sros:description;
                                description "Text description";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "disable";
                                description "Administrative state of DHCPv6 Relay";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/link-address
                            leaf link-address {
                                type types-sros:ipv6-address;
                                description "Link address of the DHCPv6 relay messages";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/neighbor-resolution
                            leaf neighbor-resolution {
                                type boolean;
                                default "false";
                                description "Enable neighbor resolution via DHCPv6 relay";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/python-policy
                            leaf python-policy {
                                type leafref {
                                    path "../../../../../../../python/python-policy/name";
                                }
                                description "Python policy name";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/source-address
                            leaf source-address {
                                type types-sros:ipv6-unicast-or-linklocal-address;
                                description "Source IPv6 address of the DHCPv6 relay messages";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/user-db
                            leaf user-db {
                                type leafref {
                                    path "../../../../../../../subscriber-mgmt/local-user-db/name";
                                }
                                description "Local user database for authentication";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/server
                            leaf-list server {
                                type types-sros:ipv6-address-with-zone;
                                max-elements 8;
                                ordered-by user;
                                description "DHCPv6 server to which the DHCPv6 requests are forwarded";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/lease-populate
                            container lease-populate {
                                description "Enter the lease-populate context";

                                ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/lease-populate/max-nbr-of-leases
                                leaf max-nbr-of-leases {
                                    type int32 {
                                        range "0..32767";
                                    }
                                    default "0";
                                    description "Maximum lease state entries allowed for the interface";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/lease-populate/route-populate
                                container route-populate {
                                    description "Enter the route-populate context";

                                    ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/lease-populate/route-populate/na
                                    leaf na {
                                        type boolean;
                                        default "false";
                                        description "Create route based on IA_NA prefix option in relay-reply message";
                                    }

                                    ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/lease-populate/route-populate/ta
                                    leaf ta {
                                        type boolean;
                                        default "false";
                                        description "Create route based on IA_TA prefix option in relay-reply message";
                                    }

                                    ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/lease-populate/route-populate/pd
                                    container pd {
                                        presence "Enable the pd context";
                                        description "Enable the pd context";

                                        ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/lease-populate/route-populate/pd/exclude
                                        leaf exclude {
                                            type boolean;
                                            default "false";
                                            description "Create back hole route based on prefix exclude option in relay-reply message";
                                        }

                                    } // container pd

                                } // container route-populate

                            } // container lease-populate

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/option
                            container option {
                                description "Enable the option context";

                                ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/option/remote-id
                                leaf remote-id {
                                    type boolean;
                                    default "false";
                                    description "Send remote ID option in the DHCPv6 relay packet";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/option/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/option/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/option/interface-id
                                container interface-id {
                                    description "Enter the interface-id context";

                                    ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/option/interface-id/interface-id
                                    choice interface-id {
                                        ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/ascii-tuple
                                        case ascii-tuple {

                                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/ascii-tuple/ascii-tuple
                                            leaf ascii-tuple {
                                                type empty;
                                                description "Use ASCII-encoded concatenated tuple";
                                            }

                                        }
                                        ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/sap-id
                                        case sap-id {

                                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/sap-id/sap-id
                                            leaf sap-id {
                                                type empty;
                                                description "Use SAP ID in interface ID option in relay packet";
                                            }

                                        }
                                        ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/if-index
                                        case if-index {

                                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/if-index/if-index
                                            leaf if-index {
                                                type empty;
                                                description "Use interface index in the DHCPv6 relay packet";
                                            }

                                        }
                                        ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/string
                                        case string {

                                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/string/string
                                            leaf string {
                                                type types-sros:string-not-all-spaces {
                                                    length "1..80";
                                                }
                                                description "String for interface ID option in DHCPv6 relay packet";
                                            }

                                        }
                                    }
                                } // container interface-id

                            } // container option

                        } // container relay

                        ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server
                        container server {
                            description "Enter the server context";

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server/max-nbr-of-leases
                            leaf max-nbr-of-leases {
                                type int32 {
                                    range "0..8000";
                                }
                                default "8000";
                                description "DHCPv6 leases allowed";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server/prefix-delegation
                            container prefix-delegation {
                                description "Enter the prefix-delegation context";

                                ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server/prefix-delegation/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    default "disable";
                                    description "Administrative state of the prefix delegation";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server/prefix-delegation/prefix
                                list prefix {
                                    key "ipv6-prefix";
                                    description "Enter the prefix list instance";

                                    ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server/prefix-delegation/prefix/ipv6-prefix
                                    leaf ipv6-prefix {
                                        type types-sros:ipv6-prefix;
                                        description "IPv6 address and prefix";
                                    }

                                    ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server/prefix-delegation/prefix/preferred-lifetime
                                    leaf preferred-lifetime {
                                        type types-services:ipv6-prefix-lifetime;
                                        units "seconds";
                                        default "604800";
                                        description "Preferred lifetime of the prefix";
                                    }

                                    ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server/prefix-delegation/prefix/valid-lifetime
                                    leaf valid-lifetime {
                                        type types-services:ipv6-prefix-lifetime;
                                        units "seconds";
                                        default "2592000";
                                        description "Valid lifetime of the prefix";
                                    }

                                    ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server/prefix-delegation/prefix/apply-groups
                                    leaf-list apply-groups {
                                        type leafref {
                                            path "../../../../../../../../../groups/group/name";
                                        }
                                        max-elements 8;
                                        ordered-by user;
                                        description "Apply a configuration group at this level";
                                    }

                                    ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server/prefix-delegation/prefix/apply-groups-exclude
                                    leaf-list apply-groups-exclude {
                                        type leafref {
                                            path "../../../../../../../../../groups/group/name";
                                        }
                                        max-elements 8;
                                        ordered-by user;
                                        description "Exclude a configuration group at this level";
                                    }

                                    ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server/prefix-delegation/prefix/client-id
                                    container client-id {
                                        description "Enter the client-id context";

                                        ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server/prefix-delegation/prefix/client-id/duid
                                        leaf duid {
                                            type string {
                                                length "1..130";
                                            }
                                            description "Requesting router ID";
                                        }

                                        ///nokia-conf/configure/service/vprn/interface/ipv6/dhcp6/server/prefix-delegation/prefix/client-id/iaid
                                        leaf iaid {
                                            type uint32 {
                                                range "1..4294967295";
                                            }
                                            description "IAID from the requesting router to match";
                                        }

                                    } // container client-id

                                } // list prefix

                            } // container prefix-delegation

                        } // container server

                    } // container dhcp6

                    ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery
                    container neighbor-discovery {
                        when "../../tunnel = 'false'" {
                            description "not allowed in a tunnel interface";
                        }
                        description "Enter the neighbor-discovery context";

                        ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/reachable-time
                        leaf reachable-time {
                            type types-router:neighbor-discovery-reachable-time;
                            description "Neighbor reachability detection timer";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/stale-time
                        leaf stale-time {
                            type types-router:neighbor-discovery-stale-time;
                            description "Time a Neighbor Discovery cache entry remains stale";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/learn-unsolicited
                        leaf learn-unsolicited {
                            type types-router:ipv6-address-type-selection;
                            description "Type of entries learned from unsolicited NA messages";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/proactive-refresh
                        leaf proactive-refresh {
                            type types-router:ipv6-address-type-selection;
                            description "Proactive refresh of neighbor entries";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/populate-host
                        leaf populate-host {
                            status obsolete;
                            type boolean;
                            default "false";
                            description "Allow addition or deletion of host routes in route table derived from ARP entries in ARP cache";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/route-tag
                        leaf route-tag {
                            status obsolete;
                            type types-router:route-tag;
                            description "ARP route tag";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/local-proxy-nd
                        leaf local-proxy-nd {
                            type boolean;
                            default "false";
                            description "Enable local proxy neighbor discovery on the interface";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/proxy-nd-policy
                        leaf-list proxy-nd-policy {
                            type leafref {
                                path "../../../../../../policy-options/policy-statement/name";
                            }
                            max-elements 5;
                            ordered-by user;
                            description "Proxy Neighbor Discovery policy name for the interface";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/host-route
                        container host-route {
                            description "Enter the host-route context";

                            ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/host-route/populate
                            list populate {
                                key "route-type";
                                description "Enter the populate list instance";

                                ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/host-route/populate/route-type
                                leaf route-type {
                                    type enumeration {
                                        enum static                         { value 1; }
                                        enum dynamic                        { value 2; }
                                        enum evpn                           { value 3; }
                                    }
                                    description "Type of ARP or ND entries that generate host routes";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/host-route/populate/route-tag
                                leaf route-tag {
                                    type uint32 {
                                        range "1..255";
                                    }
                                    sros-ext:immutable;
                                    description "Tag value used with the host route from an ARP/ND entry";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/host-route/populate/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/host-route/populate/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                            } // list populate

                        } // container host-route

                        ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/secure-nd
                        container secure-nd {
                            description "Enter the secure-nd context";

                            ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/secure-nd/allow-unsecured-msgs
                            leaf allow-unsecured-msgs {
                                type boolean;
                                default "true";
                                description "Accept unsecured messages";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/secure-nd/public-key-min-bits
                            leaf public-key-min-bits {
                                type uint32 {
                                    range "512..1024";
                                }
                                default "1024";
                                description "Minimum acceptable key length for public keys in CGA";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/secure-nd/security-parameter
                            leaf security-parameter {
                                type uint32 {
                                    range "0..1";
                                }
                                default "1";
                                description "Security parameter used in the generation of a CGA";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/secure-nd/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "disable";
                                description "Administrative state of Secure Neighbor Discovery";
                            }

                        } // container secure-nd

                        ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/limit
                        container limit {
                            description "Enter the limit context";

                            ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/limit/max-entries
                            leaf max-entries {
                                type types-router:ipv6-neighbor-limit;
                                description "Maximum number of entries learned on an IP interface";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/limit/log-only
                            leaf log-only {
                                type boolean;
                                default "false";
                                description "Generate log entries when limit is reached";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/limit/threshold
                            leaf threshold {
                                type uint32 {
                                    range "1..100";
                                }
                                units "percent";
                                default "90";
                                description "Threshold percentage that triggers a warning message";
                            }

                        } // container limit

                        ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/static-neighbor
                        list static-neighbor {
                            key "ipv6-address";
                            description "Enter the static-neighbor list instance";

                            ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/static-neighbor/ipv6-address
                            leaf ipv6-address {
                                type types-sros:ipv6-address;
                                description "IPv6 address corresponding to the physical address";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/static-neighbor/mac-address
                            leaf mac-address {
                                type yang:mac-address;
                                mandatory true;
                                description "MAC address for the static neighbor";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/static-neighbor/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/vprn/interface/ipv6/neighbor-discovery/static-neighbor/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list static-neighbor

                    } // container neighbor-discovery

                    ///nokia-conf/configure/service/vprn/interface/ipv6/vrrp
                    list vrrp {
                        key "virtual-router-id";
                        max-elements 4;
                        description "Enter the vrrp list instance";

                        ///nokia-conf/configure/service/vprn/interface/ipv6/vrrp/virtual-router-id
                        leaf virtual-router-id {
                            type int32 {
                                range "1..255";
                            }
                            description "Virtual Router Identifier (VRID) for the IP interface";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv6/vrrp/backup
                        leaf-list backup {
                            type types-sros:ipv6-address;
                            max-elements 4;
                            description "Virtual router IP addresses for the interface";
                        }

