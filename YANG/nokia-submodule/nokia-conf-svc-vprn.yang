                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            sros-ext:auto-restart-to-modify;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            sros-ext:auto-restart-to-modify;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/bfd
                        container bfd {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the bfd context";

                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/bfd/bfd-designate
                            leaf bfd-designate {
                                type boolean;
                                sros-ext:auto-restart-to-modify;
                                default "false";
                                description "Designate IPsec tunnel to carry BFD traffic";
                            }

                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/bfd/bfd-liveness
                            container bfd-liveness {
                                presence "Enable the bfd-liveness context";
                                sros-ext:auto-restart-to-modify;
                                description "Enable the bfd-liveness context";

                                ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/bfd/bfd-liveness/dest-ip
                                leaf dest-ip {
                                    type types-sros:ipv4-unicast-address;
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Destination address used for the BFD session";
                                }

                                ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/bfd/bfd-liveness/interface
                                leaf interface {
                                    type types-sros:interface-name;
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Name of the interface used by the BFD session";
                                }

                                ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/bfd/bfd-liveness/service-name
                                leaf service-name {
                                    type types-services:service-name;
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Administrative service name";
                                }

                            } // container bfd-liveness

                        } // container bfd

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/icmp6-generation
                        container icmp6-generation {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the icmp6-generation context";

                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/icmp6-generation/packet-too-big
                            container packet-too-big {
                                sros-ext:auto-restart-to-modify;
                                description "Enter the packet-too-big context";

                                ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/icmp6-generation/packet-too-big/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    sros-ext:auto-restart-to-modify;
                                    default "enable";
                                    description "Administrative state of Packet Too Big message sends";
                                }

                                ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/icmp6-generation/packet-too-big/message-count
                                leaf message-count {
                                    type uint32 {
                                        range "10..1000";
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    default "100";
                                    description "Maximum number of ICMPv6 PTB messages that can be sent";
                                }

                                ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/icmp6-generation/packet-too-big/interval
                                leaf interval {
                                    type uint32 {
                                        range "1..60";
                                    }
                                    units "seconds";
                                    sros-ext:auto-restart-to-modify;
                                    default "10";
                                    description "Interval for sending Packet Too Big messages";
                                }

                            } // container packet-too-big

                        } // container icmp6-generation

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange
                        container key-exchange {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the key-exchange context";

                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type
                            choice type {
                                ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic
                                case dynamic {

                                    ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic
                                    container dynamic {
                                        presence "Enable the dynamic context";
                                        sros-ext:auto-restart-to-modify;
                                        description "Enable the dynamic context";

                                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/auto-establish
                                        leaf auto-establish {
                                            type boolean;
                                            sros-ext:auto-restart-to-modify;
                                            default "false";
                                            description "Attempt to establish a phase 1 exchange automatically";
                                        }

                                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-policy
                                        leaf ike-policy {
                                            type leafref {
                                                path "../../../../../../../../ipsec/ike-policy/id";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            description "IKE policy ID";
                                        }

                                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ipsec-transform
                                        leaf-list ipsec-transform {
                                            type leafref {
                                                path "../../../../../../../../ipsec/ipsec-transform/id";
                                            }
                                            max-elements 4;
                                            sros-ext:auto-restart-to-modify;
                                            description "IPsec transform IDs used by the dynamic key";
                                        }

                                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/pre-shared-key
                                        leaf pre-shared-key {
                                            type types-sros:encrypted-leaf {
                                                length "1..115";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            description "Pre-shared key for authentication";
                                        }

                                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert
                                        container cert {
                                            sros-ext:auto-restart-to-modify;
                                            description "Enter the cert context";

                                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/cert-profile
                                            leaf cert-profile {
                                                type leafref {
                                                    path "../../../../../../../../../ipsec/cert-profile/name";
                                                }
                                                description "Certificate profile name";
                                            }

                                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/trust-anchor-profile
                                            leaf trust-anchor-profile {
                                                type leafref {
                                                    path "../../../../../../../../../ipsec/trust-anchor-profile/name";
                                                }
                                                description "Trust anchor profile name";
                                            }

                                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify
                                            container status-verify {
                                                sros-ext:auto-restart-to-modify;
                                                description "Enter the status-verify context";

                                                ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify/default-result
                                                leaf default-result {
                                                    type enumeration {
                                                        enum revoked                        { value 0; }
                                                        enum good                           { value 1; }
                                                    }
                                                    default "revoked";
                                                    description "Default result for Certificate Status Verification";
                                                }

                                                ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify/primary
                                                leaf primary {
                                                    type types-ipsec:cert-revocation-status;
                                                    default "crl";
                                                    description "Primary method of CSV to verify the revocation status";
                                                }

                                                ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify/secondary
                                                leaf secondary {
                                                    type types-ipsec:cert-revocation-status-with-none;
                                                    default "none";
                                                    description "Secondary method used to verify certificate revocation";
                                                }

                                            } // container status-verify

                                        } // container cert

                                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id
                                        container id {
                                            sros-ext:auto-restart-to-modify;
                                            description "Enter the id context";

                                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id
                                            choice id {
                                                ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv4
                                                case ipv4 {

                                                    ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv4/ipv4
                                                    leaf ipv4 {
                                                        type types-sros:ipv4-unicast-address;
                                                        sros-ext:auto-restart-to-modify;
                                                        description "IPv4 as the local ID type";
                                                    }

                                                }
                                                ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv6
                                                case ipv6 {

                                                    ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv6/ipv6
                                                    leaf ipv6 {
                                                        type types-sros:ip-address;
                                                        sros-ext:auto-restart-to-modify;
                                                        description "IPv6 used as the local IKE ID type";
                                                    }

                                                }
                                                ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/fqdn
                                                case fqdn {

                                                    ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/fqdn/fqdn
                                                    leaf fqdn {
                                                        type types-sros:fully-qualified-domain-name;
                                                        sros-ext:auto-restart-to-modify;
                                                        description "FQDN used as the local ID IKE type";
                                                    }

                                                }
                                            }
                                        } // container id

                                    } // container dynamic

