                        leaf local-address {
                            type types-sros:ipv4-unicast-address;
                            description "Local address";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/receive-window-size
                        leaf receive-window-size {
                            type types-l2tp:child-receive-window-size;
                            description "L2TP receive window size";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/hello-interval
                        leaf hello-interval {
                            type types-l2tp:child-hello-interval;
                            description "Hello interval";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/idle-timeout
                        leaf idle-timeout {
                            type types-l2tp:child-idle-timeout;
                            description "Idle timeout";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/destruct-timeout
                        leaf destruct-timeout {
                            type types-l2tp:child-destruct-timeout;
                            description "Destruction timeout";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/max-retries-estab
                        leaf max-retries-estab {
                            type types-l2tp:child-max-retries;
                            description "Maximum retries for established tunnels";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/max-retries-not-estab
                        leaf max-retries-not-estab {
                            type types-l2tp:child-max-retries;
                            description "Maximum retries for unestablished tunnels";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/radius-accounting-policy
                        leaf radius-accounting-policy {
                            type leafref {
                                path "../../../../../../aaa/radius/l2tp-accounting-policy/name";
                            }
                            description "RADIUS accounting policy";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/remote-name
                        leaf remote-name {
                            type types-l2tp:host-name;
                            description "Remote tunnel host name";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/local-name
                        leaf local-name {
                            type types-l2tp:host-name;
                            description "Local host name used to distinguish tunnels";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/password
                        leaf password {
                            type types-sros:encrypted-leaf {
                                length "1..115";
                            }
                            description "Password between L2TP LAC and LNS";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/challenge
                        leaf challenge {
                            type types-sros:bool-with-inherit;
                            description "Enable use of challenge-response authentication";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/avp-hiding
                        leaf avp-hiding {
                            type types-l2tp:child-avp-hiding;
                            description "The AVP hiding algorithm";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/session-limit
                        leaf session-limit {
                            type types-l2tp:child-tunnel-session-limit;
                            description "L2TP session limit for each tunnel of this router";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/group/tunnel/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

