                    }

                    ///nokia-conf/configure/service/vprn/l2tp/group/local-name
                    leaf local-name {
                        type types-l2tp:host-name;
                        description "Local host name used to distinguish tunnels";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/group/password
                    leaf password {
                        type types-sros:encrypted-leaf {
                            length "1..115";
                        }
                        description "Password between L2TP LAC and LNS";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/group/challenge
                    leaf challenge {
                        type types-sros:bool-with-inherit;
                        description "Enable use of challenge-response authentication";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/group/avp-hiding
                    leaf avp-hiding {
                        type types-l2tp:child-avp-hiding;
                        description "The AVP hiding algorithm";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/group/session-limit
                    leaf session-limit {
                        type types-l2tp:child-session-limit;
                        description "Session limit";
