                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Up hold time for the IP interface";
                            }

                        } // container up

                        ///nokia-conf/configure/service/vprn/interface/hold-time/ipv4/down
                        container down {
