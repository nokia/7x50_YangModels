                ///nokia-conf/configure/service/vprn/l2tp/l2tpv3
                container l2tpv3 {
                    description "Enter the l2tpv3 context";

                    ///nokia-conf/configure/service/vprn/l2tp/l2tpv3/password
                    leaf password {
                        type types-sros:encrypted-leaf {
                            length "1..115";
                        }
                        description "L2TPv3 password";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/l2tpv3/cookie-length
                    leaf cookie-length {
                        type types-l2tp:base-v3-cookie-length;
                        description "Cookie field length";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/l2tpv3/digest-type
                    leaf digest-type {
                        type types-l2tp:base-v3-digest-type;
                        description "Hashing algorithm that calculates the message digest";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/l2tpv3/nonce-length
                    leaf nonce-length {
                        type types-l2tp:base-v3-nonce-length;
                        description "Length of the local L2TPv3 nonce (random number)";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/l2tpv3/public-tcp-mss-adjust
                    leaf public-tcp-mss-adjust {
                        type types-l2tp:base-v3-tcp-mss-adjust;
                        description "TCP maximum segment size (MSS) on public network";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/l2tpv3/private-tcp-mss-adjust
                    leaf private-tcp-mss-adjust {
