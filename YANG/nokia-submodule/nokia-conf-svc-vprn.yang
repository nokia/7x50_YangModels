                ///nokia-conf/configure/service/vprn/l2tp/ethernet-tunnel
                container ethernet-tunnel {
                    description "Enter the ethernet-tunnel context";

                    ///nokia-conf/configure/service/vprn/l2tp/ethernet-tunnel/reconnect-timeout
                    leaf reconnect-timeout {
