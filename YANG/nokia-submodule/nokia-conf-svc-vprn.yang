                            description "Multi-chassis peer address";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/failover/track-srrp/sync-tag
                        leaf sync-tag {
                            type types-sros:named-item;
                            mandatory true;
                            description "Synchronization tag on the multi-chassis peer";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/failover/track-srrp/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
