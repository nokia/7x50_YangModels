                            type boolean;
                            default "false";
                            description "Allow non-owner master to reply to ICMP echo requests";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/vrrp/policy
                        leaf policy {
                            type leafref {
                                path "../../../../../../vrrp/policy/policy-id";
                            }
                            description "VRRP priority control policy";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/vrrp/preempt
                        leaf preempt {
                            type boolean;
                            default "true";
                            description "Allow the VRRP to override an existing non-owner master";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/vrrp/telnet-reply
                        leaf telnet-reply {
                            type boolean;
                            default "false";
                            description "Allow non-owner master to reply to Telnet requests";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/vrrp/traceroute-reply
                        leaf traceroute-reply {
                            type boolean;
                            default "false";
                            description "Allow non-owner master to reply to traceroute requests";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/vrrp/standby-forwarding
                        leaf standby-forwarding {
                            type boolean;
                            default "false";
                            description "Allow standby router to forward traffic";
                        }

                        ///nokia-conf/configure/service/vprn/interface/ipv4/vrrp/ssh-reply
                        leaf ssh-reply {
                            type boolean;
                            default "false";
                            description "Allow the non-owner master to reply to SSH requests";
