                    ///nokia-conf/configure/service/vprn/grt-leaking/export-grt/policy-name
                    leaf-list policy-name {
                        type union {
                            type types-sros:policy-expr-string;
                            type string;
                        }
                        max-elements 5;
                        ordered-by user;
                        description "Route policy name or policy logical expression";
                    }

                } // container export-grt

                ///nokia-conf/configure/service/vprn/grt-leaking/import-grt
                container import-grt {
                    description "Enter the import-grt context";

                    ///nokia-conf/configure/service/vprn/grt-leaking/import-grt/policy-name
                    leaf-list policy-name {
                        type union {
                            type types-sros:policy-expr-string;
                            type string;
                        }
                        max-elements 5;
                        ordered-by user;
                        description "Route policy name or policy logical expression";
                    }

                } // container import-grt

            } // container grt-leaking

            ///nokia-conf/configure/service/vprn/gsmp
            container gsmp {
                description "Enable the gsmp context";

                ///nokia-conf/configure/service/vprn/gsmp/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of GSMP";
                }

                ///nokia-conf/configure/service/vprn/gsmp/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vprn/gsmp/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vprn/gsmp/group
                list group {
                    key "name";
                    max-elements 1024;
                    description "Enter the group list instance";

                    ///nokia-conf/configure/service/vprn/gsmp/group/name
                    leaf name {
                        type types-sros:named-item;
                        description "GSMP group name";
                    }

                    ///nokia-conf/configure/service/vprn/gsmp/group/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the GSMP group";
                    }

                    ///nokia-conf/configure/service/vprn/gsmp/group/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/service/vprn/gsmp/group/keepalive
                    leaf keepalive {
                        type types-sros:time-duration {
                            range "1..25";
                        }
                        units "seconds";
                        default "10";
                        description "Keepalive value for the GSMP connections in this group";
                    }

                    ///nokia-conf/configure/service/vprn/gsmp/group/hold-multiplier
                    leaf hold-multiplier {
                        type uint32 {
                            range "1..100";
                        }
                        default "3";
                        description "Hold-multiplier for the GSMP connections in this group";
                    }

                    ///nokia-conf/configure/service/vprn/gsmp/group/persistency
                    leaf persistency {
                        type boolean;
                        default "false";
                        description "Store DSL line information when the GSMP connection terminates";
                    }

                    ///nokia-conf/configure/service/vprn/gsmp/group/idle-filter
                    leaf idle-filter {
                        type boolean;
                        default "false";
                        description "Filter ANCP messages from IDLE DSL lines";
                    }

                    ///nokia-conf/configure/service/vprn/gsmp/group/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/gsmp/group/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/gsmp/group/ancp
                    container ancp {
                        description "Enter the ancp context";

                        ///nokia-conf/configure/service/vprn/gsmp/group/ancp/dynamic-topology-discovery
                        leaf dynamic-topology-discovery {
                            type boolean;
                            default "true";
                            description "Enable the ANCP dynamic topology discovery capability";
                        }

                        ///nokia-conf/configure/service/vprn/gsmp/group/ancp/oam
                        leaf oam {
                            type boolean;
                            default "false";
                            description "Enable GSMP ANCP OAM capability at startup of GSMP connection";
                        }

                    } // container ancp

                    ///nokia-conf/configure/service/vprn/gsmp/group/neighbor
                    list neighbor {
                        key "remote-address";
                        description "Enter the neighbor list instance";

                        ///nokia-conf/configure/service/vprn/gsmp/group/neighbor/remote-address
                        leaf remote-address {
                            type types-sros:ipv4-unicast-address;
                            description "GSMP neighbor remote IP address";
                        }

                        ///nokia-conf/configure/service/vprn/gsmp/group/neighbor/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the GSMP neighbor";
                        }

                        ///nokia-conf/configure/service/vprn/gsmp/group/neighbor/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/service/vprn/gsmp/group/neighbor/local-address
                        leaf local-address {
                            type types-sros:ipv4-unicast-address;
                            sros-ext:auto-restart-to-modify;
                            description "Restrict connections to this local address only within the service running ANCP";
                        }

                        ///nokia-conf/configure/service/vprn/gsmp/group/neighbor/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            sros-ext:auto-restart-to-modify;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/gsmp/group/neighbor/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            sros-ext:auto-restart-to-modify;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/gsmp/group/neighbor/priority-marking
                        container priority-marking {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the priority-marking context";

                            ///nokia-conf/configure/service/vprn/gsmp/group/neighbor/priority-marking/priority-marking
                            choice priority-marking {
                                ///nokia-conf/configure/service/vprn/gsmp/group/neighbor/priority-marking/priority-marking/dscp
                                case dscp {

                                    ///nokia-conf/configure/service/vprn/gsmp/group/neighbor/priority-marking/priority-marking/dscp/dscp
                                    leaf dscp {
                                        type types-qos:dscp-name;
                                        sros-ext:auto-restart-to-modify;
                                        description "DSCP that is used while remarking the in profile packets";
                                    }

                                }
                                ///nokia-conf/configure/service/vprn/gsmp/group/neighbor/priority-marking/priority-marking/prec
                                case prec {

                                    ///nokia-conf/configure/service/vprn/gsmp/group/neighbor/priority-marking/priority-marking/prec/prec
                                    leaf prec {
                                        type types-qos:precedence-value;
                                        sros-ext:auto-restart-to-modify;
                                        description "Precedence priority marking";
                                    }

                                }
                            }
                        } // container priority-marking

                    } // list neighbor

                } // list group

            } // container gsmp

            ///nokia-conf/configure/service/vprn/vxlan
            container vxlan {
                description "Enter the vxlan context";

                ///nokia-conf/configure/service/vprn/vxlan/tunnel-termination
                list tunnel-termination {
                    key "ip-address";
                    description "Enter the tunnel-termination list instance";

                    ///nokia-conf/configure/service/vprn/vxlan/tunnel-termination/ip-address
                    leaf ip-address {
                        type types-sros:ip-unicast-address;
                        description "Non-system IP address that terminates the VXLAN";
                    }

                    ///nokia-conf/configure/service/vprn/vxlan/tunnel-termination/fpe-id
                    leaf fpe-id {
                        type leafref {
                            path "../../../../../fwd-path-ext/fpe/fpe-id";
                        }
                        sros-ext:immutable;
                        mandatory true;
                        description "FPE id for this entry";
                    }

                    ///nokia-conf/configure/service/vprn/vxlan/tunnel-termination/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/vxlan/tunnel-termination/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list tunnel-termination

            } // container vxlan

            ///nokia-conf/configure/service/vprn/ttl-propagate
            container ttl-propagate {
                description "Enter the ttl-propagate context";

                ///nokia-conf/configure/service/vprn/ttl-propagate/local
                leaf local {
                    type types-router:vprn-ttl-propagate;
                    default "use-base";
                    description "Local TTL propagation control for the VPRN";
                }

                ///nokia-conf/configure/service/vprn/ttl-propagate/transit
                leaf transit {
                    type types-router:vprn-ttl-propagate;
                    default "use-base";
                    description "Transit TTL propagation control for the VPRN";
                }

            } // container ttl-propagate

            ///nokia-conf/configure/service/vprn/sgt-qos
            container sgt-qos {
                description "Enter the sgt-qos context";

                ///nokia-conf/configure/service/vprn/sgt-qos/dscp
                container dscp {
                    description "Enter the dscp context";

                    ///nokia-conf/configure/service/vprn/sgt-qos/dscp/application
                    list application {
                        key "dscp-app-name";
                        description "Enter the application list instance";

                        ///nokia-conf/configure/service/vprn/sgt-qos/dscp/application/dscp-app-name
                        leaf dscp-app-name {
                            type types-router:dscp-app-name-vprn;
                            description "DSCP application ID that generates control traffic";
                        }

                        ///nokia-conf/configure/service/vprn/sgt-qos/dscp/application/dscp
                        leaf dscp {
                            type union {
                                type types-qos:dscp-name;
                                type int32 {
                                    range "0..63";
                                }
                            }
                            description "DSCP value to the traffic generated by this application";
                        }

                        ///nokia-conf/configure/service/vprn/sgt-qos/dscp/application/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/sgt-qos/dscp/application/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list application

                    ///nokia-conf/configure/service/vprn/sgt-qos/dscp/dscp-map
                    list dscp-map {
                        key "dscp-name";
                        description "Enter the dscp-map list instance";

                        ///nokia-conf/configure/service/vprn/sgt-qos/dscp/dscp-map/dscp-name
                        leaf dscp-name {
                            type types-qos:dscp-name;
                            description "DSCP name mapped to forwarding class";
                        }

                        ///nokia-conf/configure/service/vprn/sgt-qos/dscp/dscp-map/fc
                        leaf fc {
                            type types-sros:fc-name;
                            description "Value for the forwarding class for this mapping";
                        }

                        ///nokia-conf/configure/service/vprn/sgt-qos/dscp/dscp-map/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/sgt-qos/dscp/dscp-map/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list dscp-map

                } // container dscp

                ///nokia-conf/configure/service/vprn/sgt-qos/dot1p
                container dot1p {
                    description "Enter the dot1p context";

                    ///nokia-conf/configure/service/vprn/sgt-qos/dot1p/application
                    list application {
                        key "dot1p-app-name";
                        description "Enter the application list instance";

                        ///nokia-conf/configure/service/vprn/sgt-qos/dot1p/application/dot1p-app-name
                        leaf dot1p-app-name {
                            type types-router:dot1p-app-name;
                            description "Dot1p application ID that generates control traffic";
                        }

                        ///nokia-conf/configure/service/vprn/sgt-qos/dot1p/application/dot1p
                        leaf dot1p {
                            type union {
                                type types-sros:fc-name;
                                type int32 {
                                    range "0..7";
                                }
                            }
                            description "Dot1p value to the traffic generated by this application";
                        }

                        ///nokia-conf/configure/service/vprn/sgt-qos/dot1p/application/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/sgt-qos/dot1p/application/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list application

                } // container dot1p

            } // container sgt-qos

            ///nokia-conf/configure/service/vprn/snmp
            container snmp {
                description "Enter the snmp context";

                ///nokia-conf/configure/service/vprn/snmp/access
                leaf access {
                    type boolean;
                    default "false";
                    description "Enable SNMP access for the VPRN service";
                }

                ///nokia-conf/configure/service/vprn/snmp/community
                list community {
                    key "community-string";
                    description "Enter the community list instance";

                    ///nokia-conf/configure/service/vprn/snmp/community/community-string
                    leaf community-string {
                        type types-sros:encrypted-leaf {
                            length "1..114";
                        }
                        description "SNMP v1/v2c community name associated with the VPRN";
                    }

                    ///nokia-conf/configure/service/vprn/snmp/community/access-permissions
                    leaf access-permissions {
                        type enumeration {
                            enum r                              { value 1; }
                            enum rw                             { value 2; }
                        }
                        mandatory true;
                        description "Access permissions to MIB objects";
                    }

                    ///nokia-conf/configure/service/vprn/snmp/community/version
                    leaf version {
                        type enumeration {
                            enum v1                             { value 1; }
                            enum v2c                            { value 2; }
                            enum both                           { value 3; }
                        }
                        default "both";
                        description "SNMP version";
                    }

                    ///nokia-conf/configure/service/vprn/snmp/community/source-access-list
                    leaf source-access-list {
                        type leafref {
                            path "../../../../../system/security/snmp/source-access-list/list-name";
                        }
                        description "List name used to validate the source IP address";
                    }

                    ///nokia-conf/configure/service/vprn/snmp/community/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/snmp/community/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list community

            } // container snmp

            ///nokia-conf/configure/service/vprn/network
            container network {
                description "Enter the network context";

                ///nokia-conf/configure/service/vprn/network/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vprn/network/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vprn/network/ingress
                container ingress {
                    description "Enter the ingress context";

                    ///nokia-conf/configure/service/vprn/network/ingress/urpf-check
                    leaf urpf-check {
                        type boolean;
                        default "true";
                        description "Enable unicast RPF check of network ingress traffic";
                    }

                    ///nokia-conf/configure/service/vprn/network/ingress/classification-override
                    leaf classification-override {
                        type leafref {
                            path "../../../../../qos/ingress-classification-policy/ingress-classification-policy-name";
                        }
                        description "Short-pipe mode for CoS propagation";
                    }

                    ///nokia-conf/configure/service/vprn/network/ingress/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-conf/configure/service/vprn/network/ingress/qos/network-policy
                        leaf network-policy {
                            type leafref {
                                path "../../../../../../qos/network/network-policy-name";
                            }
                            description "Network policy name associated with a VPRN";
                        }

                        ///nokia-conf/configure/service/vprn/network/ingress/qos/fp-redirect-group
                        leaf fp-redirect-group {
                            type leafref {
                                path "../../../../../../qos/queue-group-templates/ingress/queue-group/ingress-queue-group-name";
                            }
                            description "The forwarding-plane queue group Policy for this VPRN";
                        }

                        ///nokia-conf/configure/service/vprn/network/ingress/qos/instance
                        leaf instance {
                            type types-services:qos-qgrp-instance-id;
                            description "Forwarding plane ingress queue group instance";
                        }

                    } // container qos

                    ///nokia-conf/configure/service/vprn/network/ingress/filter
                    container filter {
                        description "Enter the filter context";

                        ///nokia-conf/configure/service/vprn/network/ingress/filter/ip
                        leaf ip {
                            type leafref {
                                path "../../../../../../filter/ip-filter/filter-name";
                            }
                            description "IPv4 filter policy name";
                        }

                        ///nokia-conf/configure/service/vprn/network/ingress/filter/ipv6
                        leaf ipv6 {
                            type leafref {
                                path "../../../../../../filter/ipv6-filter/filter-name";
                            }
                            description "IPv6 filter policy name";
                        }

                    } // container filter

                } // container ingress

            } // container network

            ///nokia-conf/configure/service/vprn/management
            container management {
                presence "Enable the management context";
                description "Enable the management context";

                ///nokia-conf/configure/service/vprn/management/allow-telnet
                leaf allow-telnet {
                    type boolean;
                    default "false";
                    description "Allow access to the IPv4 Telnet server";
                }

                ///nokia-conf/configure/service/vprn/management/allow-telnet6
                leaf allow-telnet6 {
                    type boolean;
                    default "false";
                    description "Allow access to the Telnet IPv6 server";
                }

                ///nokia-conf/configure/service/vprn/management/allow-ftp
                leaf allow-ftp {
                    type boolean;
                    default "false";
                    description "Allow access to the FTP server";
                }

                ///nokia-conf/configure/service/vprn/management/allow-ssh
                leaf allow-ssh {
                    type boolean;
                    default "false";
                    description "Allow access to the SSH server";
                }

                ///nokia-conf/configure/service/vprn/management/allow-netconf
                leaf allow-netconf {
                    type boolean;
                    default "false";
                    description "Allow access to the NETCONF server";
                }

                ///nokia-conf/configure/service/vprn/management/allow-grpc
                leaf allow-grpc {
                    type boolean;
                    default "false";
                    description "Allow access to the gRPC server";
                }

                ///nokia-conf/configure/service/vprn/management/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vprn/management/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container management

            uses conf-svc-vprn-dhcp-server;
            uses conf-svc-vprn-isis;
            uses conf-svc-vprn-msdp;
            uses conf-svc-vprn-ospf;
            uses conf-svc-vprn-ospf3;
            uses conf-svc-vprn-radius;
            uses conf-svc-vprn-rip;
            uses conf-svc-vprn-ripng;
            uses conf-svc-vprn-sub-if-subscriber-interface;
