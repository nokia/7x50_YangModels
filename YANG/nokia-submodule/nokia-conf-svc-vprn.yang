                    description "Enable the upf-data-endpoint context";

                    ///nokia-conf/configure/service/vprn/gtp/upf-data-endpoint/interface
                    leaf interface {
                        type leafref {
                            path "../../../../vprn[service-name=current()/../../../service-name]/interface/interface-name";
                        }
                        mandatory true;
                        description "Interface on which the GTP-U packets terminate";
                    }

                    ///nokia-conf/configure/service/vprn/gtp/upf-data-endpoint/fpe
                    leaf fpe {
                        type leafref {
                            path "../../../../../fwd-path-ext/fpe/fpe-id";
                        }
                        mandatory true;
                        description "FPE used to encapsulate and decapsulate GTP-U traffic";
                    }

                    ///nokia-conf/configure/service/vprn/gtp/upf-data-endpoint/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/gtp/upf-data-endpoint/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container upf-data-endpoint

                ///nokia-conf/configure/service/vprn/gtp/uplink
                container uplink {
                    presence "Enable the uplink context";
                    description "Enable the uplink context";

                    ///nokia-conf/configure/service/vprn/gtp/uplink/apn
                    leaf apn {
                        type string {
                            length "1..80";
                            pattern "([a-zA-Z0-9]+-+)*([a-zA-Z0-9]+\\.([a-zA-Z0-9]+-+)*)*[a-zA-Z0-9]+" {
                                error-message "wrong format of apn name";
                            }
                        }
                        description "Network Identifier part of APN";
                    }

                    ///nokia-conf/configure/service/vprn/gtp/uplink/pdn-type
                    leaf pdn-type {
                        type enumeration {
                            enum ipv4                           { value 0; }
                            enum ipv6                           { value 1; }
                            enum ipv4v6                         { value 2; }
                        }
                        default "ipv4";
                        description "Default 3GPP PDN in GTP";
                    }

                    ///nokia-conf/configure/service/vprn/gtp/uplink/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/gtp/uplink/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/gtp/uplink/peer-profile-map
                    container peer-profile-map {
                        description "Enter the peer-profile-map context";

                        ///nokia-conf/configure/service/vprn/gtp/uplink/peer-profile-map/prefix
                        list prefix {
                            key "peer-prefix";
                            description "Enter the prefix list instance";

                            ///nokia-conf/configure/service/vprn/gtp/uplink/peer-profile-map/prefix/peer-prefix
                            leaf peer-prefix {
                                type types-sros:ip-unicast-prefix;
                                description "IP prefix and prefix length of the subnet";
                            }

                            ///nokia-conf/configure/service/vprn/gtp/uplink/peer-profile-map/prefix/peer-profile
                            leaf peer-profile {
                                type leafref {
                                    path "../../../../../../../subscriber-mgmt/gtp/peer-profile/name";
                                }
                                mandatory true;
                                description "Peer profile name";
                            }

                            ///nokia-conf/configure/service/vprn/gtp/uplink/peer-profile-map/prefix/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/vprn/gtp/uplink/peer-profile-map/prefix/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list prefix

                    } // container peer-profile-map

                } // container uplink

            } // container gtp

            ///nokia-conf/configure/service/vprn/l2tp
            container l2tp {
                presence "Enable the l2tp context";
                description "Enable the l2tp context";

                ///nokia-conf/configure/service/vprn/l2tp/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of L2TP";
                }

                ///nokia-conf/configure/service/vprn/l2tp/local-address
                leaf local-address {
                    type types-sros:ipv4-unicast-address;
                    description "Local address";
                }

                ///nokia-conf/configure/service/vprn/l2tp/receive-window-size
                leaf receive-window-size {
                    type types-l2tp:base-receive-window-size;
                    description "L2TP receive window size";
                }

                ///nokia-conf/configure/service/vprn/l2tp/hello-interval
                leaf hello-interval {
                    type types-l2tp:base-hello-interval;
                    description "Hello interval";
                }

                ///nokia-conf/configure/service/vprn/l2tp/idle-timeout
                leaf idle-timeout {
                    type types-l2tp:base-idle-timeout;
                    description "Idle timeout";
                }

                ///nokia-conf/configure/service/vprn/l2tp/destruct-timeout
                leaf destruct-timeout {
                    type types-l2tp:base-destruct-timeout;
                    description "Destruction timeout";
                }

                ///nokia-conf/configure/service/vprn/l2tp/max-retries-estab
                leaf max-retries-estab {
                    type types-l2tp:base-max-retries;
                    description "Maximum retries for established tunnels";
                }

                ///nokia-conf/configure/service/vprn/l2tp/max-retries-not-estab
                leaf max-retries-not-estab {
                    type types-l2tp:base-max-retries;
                    description "Maximum retries for unestablished tunnels";
                }

                ///nokia-conf/configure/service/vprn/l2tp/rtm-debounce-time
                leaf rtm-debounce-time {
                    type types-l2tp:rtm-debounce-time;
                    description "Debounce timer that declares L2TP action for route table management events";
                }

                ///nokia-conf/configure/service/vprn/l2tp/radius-accounting-policy
                leaf radius-accounting-policy {
                    type leafref {
                        path "../../../../aaa/radius/l2tp-accounting-policy/name";
                    }
                    description "RADIUS accounting policy";
                }

                ///nokia-conf/configure/service/vprn/l2tp/peer-address-change-policy
                leaf peer-address-change-policy {
                    type types-l2tp:peer-address-change-policy-option;
                    description "Reaction for L2TP response from a different address";
                }

                ///nokia-conf/configure/service/vprn/l2tp/local-name
                leaf local-name {
                    type types-l2tp:host-name;
                    description "Local host name";
                }

                ///nokia-conf/configure/service/vprn/l2tp/password
                leaf password {
                    type types-sros:encrypted-leaf {
                        length "1..115";
                    }
                    description "L2TP password";
                }

                ///nokia-conf/configure/service/vprn/l2tp/challenge
                leaf challenge {
                    type boolean;
                    default "false";
                    description "Use challenge-response authentication";
                }

                ///nokia-conf/configure/service/vprn/l2tp/avp-hiding
                leaf avp-hiding {
                    type types-l2tp:base-avp-hiding;
                    description "Attribute of the Value Pair (AVP) hiding algorithm";
                }

                ///nokia-conf/configure/service/vprn/l2tp/session-limit
                leaf session-limit {
                    type types-l2tp:base-session-limit;
                    description "L2TP session limit of this router";
                }

                ///nokia-conf/configure/service/vprn/l2tp/group-session-limit
                leaf group-session-limit {
                    type types-l2tp:base-session-limit;
                    description "L2TP session limit for each group of this router";
                }

                ///nokia-conf/configure/service/vprn/l2tp/tunnel-session-limit
                leaf tunnel-session-limit {
                    type types-l2tp:base-tunnel-session-limit;
                    description "L2TP session limit for each tunnel of this router";
                }

                ///nokia-conf/configure/service/vprn/l2tp/session-assign-method
                leaf session-assign-method {
                    type types-l2tp:base-session-assign-method;
                    description "Session assignment method";
                }

                ///nokia-conf/configure/service/vprn/l2tp/next-attempt
                leaf next-attempt {
                    type enumeration {
                        enum same-preference-level          { value 0; }
                        enum next-preference-level          { value 1; }
                    }
                    default "next-preference-level";
                    description "Tunnel that is selected when previous session setup failed";
                }

                ///nokia-conf/configure/service/vprn/l2tp/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vprn/l2tp/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vprn/l2tp/exclude-avps
                container exclude-avps {
                    description "Enter the exclude-avps context";

                    ///nokia-conf/configure/service/vprn/l2tp/exclude-avps/calling-number
                    leaf calling-number {
                        type boolean;
                        default "false";
                        description "Calling Number AVP to exclude";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/exclude-avps/initial-rx-lcp-conf-req
                    leaf initial-rx-lcp-conf-req {
                        type boolean;
                        default "false";
                        description "Exclude the Initial Received LCP CONFREQ AVP";
                    }

                } // container exclude-avps

                ///nokia-conf/configure/service/vprn/l2tp/ignore-avps
                container ignore-avps {
                    description "Enter the ignore-avps context";

                    ///nokia-conf/configure/service/vprn/l2tp/ignore-avps/sequencing-required
                    leaf sequencing-required {
                        type boolean;
                        default "false";
                        description "Ignore Sequencing Required AVP";
                    }

                } // container ignore-avps

                ///nokia-conf/configure/service/vprn/l2tp/replace-result-code
                container replace-result-code {
                    description "Enter the replace-result-code context";

                    ///nokia-conf/configure/service/vprn/l2tp/replace-result-code/cdn-invalid-dst
                    leaf cdn-invalid-dst {
                        type boolean;
                        default "false";
                        description "Replace result code with general error code";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/replace-result-code/cdn-permanent-no-facilities
                    leaf cdn-permanent-no-facilities {
                        type boolean;
                        default "false";
                        description "Replace result code with general error";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/replace-result-code/cdn-temporary-no-facilities
                    leaf cdn-temporary-no-facilities {
                        type boolean;
                        default "false";
                        description "Replace result code with general error";
                    }

                } // container replace-result-code

                ///nokia-conf/configure/service/vprn/l2tp/lac
                container lac {
                    description "Enter the lac context";

                    ///nokia-conf/configure/service/vprn/l2tp/lac/df-bit
                    leaf df-bit {
                        type boolean;
                        default "true";
                        description "Send all L2TP packets with DF bit set to 1";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/lac/calling-number-format
                    leaf calling-number-format {
                        type types-sros:ascii-specification {
                            length "1..255";
                            pattern "([^%]+|%[Scrsl%])+";
                        }
                        default "%S %s";
                        description "Calling Number AVP for L2TP control messages";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/lac/cisco-nas-port
                    container cisco-nas-port {
                        description "Enter the cisco-nas-port context";

                        ///nokia-conf/configure/service/vprn/l2tp/lac/cisco-nas-port/ethernet
                        leaf ethernet {
                            type types-sros:binary-specification {
                                pattern "([01]|(\\*[0123456789]+)?[smpoi])+";
                            }
                            description "L2TP Cisco NAS port AVP with binary patterns for Ethernet";
                        }

                    } // container cisco-nas-port

                } // container lac

                ///nokia-conf/configure/service/vprn/l2tp/tunnel-selection-blacklist
                container tunnel-selection-blacklist {
                    description "Enter the tunnel-selection-blacklist context";

                    ///nokia-conf/configure/service/vprn/l2tp/tunnel-selection-blacklist/max-time
                    leaf max-time {
                        type int32 {
                            range "1..60";
                        }
                        units "minutes";
                        default "5";
                        description "Time that a tunnel or peer can remain in the denylist";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/tunnel-selection-blacklist/max-list-length
                    leaf max-list-length {
                        type union {
                            type int32 {
                                range "1..65535";
                            }
                            type enumeration {
                                enum infinite                       { value -1; }
                            }
                        }
                        default "infinite";
                        description "Number of tunnels or peers in the denylist";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/tunnel-selection-blacklist/timeout-action
                    leaf timeout-action {
                        type enumeration {
                            enum remove-from-blacklist          { value 0; }
                            enum try-one-session                { value 1; }
                        }
                        default "remove-from-blacklist";
                        description "Action when a tunnel or peer exceeds time in denylist";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/tunnel-selection-blacklist/add-tunnel-on
                    container add-tunnel-on {
                        description "Enter the add-tunnel-on context";

                        ///nokia-conf/configure/service/vprn/l2tp/tunnel-selection-blacklist/add-tunnel-on/address-change-timeout
                        leaf address-change-timeout {
                            type boolean;
                            default "false";
                            description "Enable a timed-out tunnel to be forced to the denylist";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/tunnel-selection-blacklist/add-tunnel-on/cdn-err-code
                        leaf cdn-err-code {
                            type boolean;
                            default "false";
                            description "Add to deny if CDN is received with code: General error";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/tunnel-selection-blacklist/add-tunnel-on/cdn-invalid-dst
                        leaf cdn-invalid-dst {
                            type boolean;
                            default "false";
                            description "Force tunnel to the denylist in case of Result Codes 6";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/tunnel-selection-blacklist/add-tunnel-on/cdn-permanent-no-facilities
                        leaf cdn-permanent-no-facilities {
                            type boolean;
                            default "false";
                            description "Force tunnel to the denylist in case of Result Codes 5";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/tunnel-selection-blacklist/add-tunnel-on/cdn-temporary-no-facilities
                        leaf cdn-temporary-no-facilities {
                            type boolean;
                            default "false";
                            description "Force tunnel to the denylist in case of Result Codes 4";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/tunnel-selection-blacklist/add-tunnel-on/stop-ccn-err-code
                        leaf stop-ccn-err-code {
                            type boolean;
                            default "false";
                            description "Add to denylist if received with general error code";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/tunnel-selection-blacklist/add-tunnel-on/stop-ccn-other
                        leaf stop-ccn-other {
                            type boolean;
                            default "false";
                            description "Add to denylist if StopCCN received with general error";
                        }

                        ///nokia-conf/configure/service/vprn/l2tp/tunnel-selection-blacklist/add-tunnel-on/tx-cdn-not-established-in-time
                        leaf tx-cdn-not-established-in-time {
                            type boolean;
                            default "false";
                            description "Add to denylist if CDN transmitted with result code";
                        }

                    } // container add-tunnel-on

                } // container tunnel-selection-blacklist

                ///nokia-conf/configure/service/vprn/l2tp/failover
                container failover {
                    description "Enter the failover context";

                    ///nokia-conf/configure/service/vprn/l2tp/failover/recovery-method
                    leaf recovery-method {
                        type types-l2tp:base-failover-recovery-method;
                        description "Recovery method of the sequence numbers after failover";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/failover/recovery-time
                    leaf recovery-time {
                        type types-l2tp:base-failover-recovery-time;
                        description "Time requested from the L2TP peer before assuming failover as failed";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/failover/recovery-max-session-lifetime
                    leaf recovery-max-session-lifetime {
                        type uint32 {
                            range "2..4294967295";
                        }
                        units "centiseconds";
                        default "2";
                        description "Subset of sessions that this system attempts to synchronize in the Session State Synchronization phase";
                    }

                    ///nokia-conf/configure/service/vprn/l2tp/failover/track-srrp
                    list track-srrp {
