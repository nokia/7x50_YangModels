                                            }

                                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/manual/manual/keys/direction
                                            leaf direction {
                                                type enumeration {
                                                    enum inbound                        { value 1; }
                                                    enum outbound                       { value 2; }
                                                }
                                                description "Direction of the IPsec tunnel";
                                            }

                                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/manual/manual/keys/ipsec-transform
                                            leaf ipsec-transform {
                                                type leafref {
                                                    path "../../../../../../../../../ipsec/ipsec-transform/id";
                                                }
                                                sros-ext:immutable;
                                                mandatory true;
                                                description "Transform entry used by manual SAs";
                                            }

                                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/manual/manual/keys/spi
                                            leaf spi {
                                                type uint32 {
                                                    range "256..16383";
                                                }
                                                sros-ext:immutable;
                                                mandatory true;
                                                description "SPI of inbound and outbound packets";
                                            }

                                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/manual/manual/keys/authentication-key
                                            leaf authentication-key {
                                                type types-sros:hex-string {
                                                    length "1..130";
                                                }
                                                sros-ext:immutable;
                                                description "Key used for the authentication algorithm";
                                            }

                                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/manual/manual/keys/encryption-key
                                            leaf encryption-key {
                                                type types-sros:hex-string {
                                                    length "1..66";
                                                }
                                                sros-ext:immutable;
                                                description "Key used for the encryption algorithm";
                                            }

                                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/manual/manual/keys/apply-groups
                                            leaf-list apply-groups {
                                                type leafref {
                                                    path "../../../../../../../../../groups/group/name";
                                                }
                                                max-elements 8;
                                                ordered-by user;
                                                description "Apply a configuration group at this level";
                                            }

                                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/key-exchange/type/manual/manual/keys/apply-groups-exclude
                                            leaf-list apply-groups-exclude {
                                                type leafref {
                                                    path "../../../../../../../../../groups/group/name";
                                                }
                                                max-elements 8;
                                                ordered-by user;
                                                description "Exclude a configuration group at this level";
                                            }

                                        } // list keys

                                    } // container manual

                                }
                            }
                        } // container key-exchange

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/max-history-key-records
                        container max-history-key-records {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the max-history-key-records context";

                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/max-history-key-records/ike
                            leaf ike {
                                type uint32 {
                                    range "1..3";
                                }
                                sros-ext:auto-restart-to-modify;
                                description "Maximum number of historical IKE key records";
                            }

                            ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/max-history-key-records/esp
                            leaf esp {
                                type uint32 {
                                    range "1..48";
                                }
                                sros-ext:auto-restart-to-modify;
                                description "Maximum number of recent records";
                            }

                        } // container max-history-key-records

                        ///nokia-conf/configure/service/vprn/interface/sap/ipsec-tunnel/tunnel-endpoint
                        container tunnel-endpoint {
