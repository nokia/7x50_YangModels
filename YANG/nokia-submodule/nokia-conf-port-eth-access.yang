                                ///nokia-conf/configure/port/ethernet/access/ingress/queue-group/queue-overrides/queue/rate/pir
                                leaf pir {
                                    type types-qos:queue-pir-rate-override;
                                    units "kilobps";
                                    description "PIR for the queue";
                                }

                            } // container rate

                            ///nokia-conf/configure/port/ethernet/access/ingress/queue-group/queue-overrides/queue/monitor-queue-depth
                            container monitor-queue-depth {
                                presence "Enable the monitor-queue-depth context";
