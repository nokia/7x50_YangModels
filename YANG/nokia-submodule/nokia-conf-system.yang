                    ///nokia-conf/configure/system/grpc/tcp-keepalive/interval
                    leaf interval {
                        type types-sros:time-duration {
                            range "1..100000";
                        }
                        units "seconds";
                        default "15";
                        description "Time between TCP keep-alive probes";
                    }

                    ///nokia-conf/configure/system/grpc/tcp-keepalive/retries
                    leaf retries {
                        type uint32 {
                            range "3..100";
                        }
                        default "4";
                        description "Number of probe retries before closing the connection";
                    }

                } // container tcp-keepalive

                ///nokia-conf/configure/system/grpc/gnmi
                container gnmi {
                    description "Enter the gnmi context";

                    ///nokia-conf/configure/system/grpc/gnmi/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "enable";
                        description "Administrative state of the gNMI service";
                    }

                    ///nokia-conf/configure/system/grpc/gnmi/proto-version
                    leaf proto-version {
                        type enumeration {
                            enum latest {
                                value 0;
                                description "Latest supported version";
                            }
                            enum v070 {
                                value 1;
                                description "gNMI version 0.7.0";
                            }
                        }
                        default "latest";
                        description "gnmi.proto version";
                    }

                    ///nokia-conf/configure/system/grpc/gnmi/auto-config-save
                    leaf auto-config-save {
                        type boolean;
                        default "true";
                        description "Automatically save configuration as part of commit";
                    }

                } // container gnmi

                ///nokia-conf/configure/system/grpc/gnoi
                container gnoi {
                    description "Enter the gnoi context";

                    ///nokia-conf/configure/system/grpc/gnoi/cert-mgmt
                    container cert-mgmt {
                        description "Enter the cert-mgmt context";

                        ///nokia-conf/configure/system/grpc/gnoi/cert-mgmt/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of gNOI CertificateManagement";
                        }

                    } // container cert-mgmt

                    ///nokia-conf/configure/system/grpc/gnoi/file
                    container file {
                        description "Enter the file context";

                        ///nokia-conf/configure/system/grpc/gnoi/file/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the gNOI File service";
                        }

                    } // container file

                    ///nokia-conf/configure/system/grpc/gnoi/system
                    container system {
                        description "Enter the system context";

                        ///nokia-conf/configure/system/grpc/gnoi/system/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the gNOI System service";
                        }

                    } // container system

                } // container gnoi

                ///nokia-conf/configure/system/grpc/md-cli
                container md-cli {
                    description "Enter the md-cli context";

                    ///nokia-conf/configure/system/grpc/md-cli/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the MD-CLI service";
                    }

                } // container md-cli

                ///nokia-conf/configure/system/grpc/rib-api
                container rib-api {
                    description "Enter the rib-api context";

                    ///nokia-conf/configure/system/grpc/rib-api/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the RIB API service";
                    }

                    ///nokia-conf/configure/system/grpc/rib-api/purge-timeout
                    leaf purge-timeout {
                        type types-sros:time-duration {
                            range "1..100000";
                        }
                        units "seconds";
                        description "Time until stale entries are purged";
                    }

                } // container rib-api

            } // container grpc

            ///nokia-conf/configure/system/grpc-tunnel
            container grpc-tunnel {
                description "Enter the grpc-tunnel context";

                ///nokia-conf/configure/system/grpc-tunnel/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/system/grpc-tunnel/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/system/grpc-tunnel/destination-group
                list destination-group {
                    key "name";
                    max-elements 4;
                    description "Enter the destination-group list instance";

                    ///nokia-conf/configure/system/grpc-tunnel/destination-group/name
                    leaf name {
                        type types-sros:named-item;
                        description "Destination group name";
                    }

                    ///nokia-conf/configure/system/grpc-tunnel/destination-group/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/system/grpc-tunnel/destination-group/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/grpc-tunnel/destination-group/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/grpc-tunnel/destination-group/transport
                    choice transport {
                        ///nokia-conf/configure/system/grpc-tunnel/destination-group/transport/tls-client-profile
                        case tls-client-profile {

                            ///nokia-conf/configure/system/grpc-tunnel/destination-group/transport/tls-client-profile/tls-client-profile
                            leaf tls-client-profile {
                                type leafref {
                                    path "../../../security/tls/client-tls-profile/client-profile-name";
                                }
                                description "TLS client profile assigned to the destination group";
                            }

                        }
                        ///nokia-conf/configure/system/grpc-tunnel/destination-group/transport/allow-unsecure-connection
                        case allow-unsecure-connection {

                            ///nokia-conf/configure/system/grpc-tunnel/destination-group/transport/allow-unsecure-connection/allow-unsecure-connection
                            leaf allow-unsecure-connection {
                                type empty;
                                description "Allow unsecured operation of gRPC connections";
                            }

                        }
                    }
                    ///nokia-conf/configure/system/grpc-tunnel/destination-group/tcp-keepalive
                    container tcp-keepalive {
                        description "Enter the tcp-keepalive context";

                        ///nokia-conf/configure/system/grpc-tunnel/destination-group/tcp-keepalive/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the TCP keepalive algorithm";
                        }

                        ///nokia-conf/configure/system/grpc-tunnel/destination-group/tcp-keepalive/idle-time
                        leaf idle-time {
                            type types-sros:time-duration {
                                range "1..100000";
                            }
                            units "seconds";
                            default "600";
                            description "Time until the first TCP keepalive probe is sent";
                        }

                        ///nokia-conf/configure/system/grpc-tunnel/destination-group/tcp-keepalive/interval
                        leaf interval {
                            type types-sros:time-duration {
                                range "1..100000";
                            }
                            units "seconds";
                            default "15";
                            description "Time between TCP keep-alive probes";
                        }

                        ///nokia-conf/configure/system/grpc-tunnel/destination-group/tcp-keepalive/retries
                        leaf retries {
                            type uint32 {
                                range "3..100";
                            }
                            default "4";
                            description "Number of probe retries before closing the connection";
                        }

                    } // container tcp-keepalive

                    ///nokia-conf/configure/system/grpc-tunnel/destination-group/destination
                    list destination {
                        key "address port";
                        max-elements 4;
                        ordered-by user;
                        description "Enter the destination list instance";

                        ///nokia-conf/configure/system/grpc-tunnel/destination-group/destination/address
                        leaf address {
                            type union {
                                type types-sros:ip-address;
                                type types-sros:fully-qualified-domain-name;
                            }
                            description "Address of the destination within the destination group";
                        }

                        ///nokia-conf/configure/system/grpc-tunnel/destination-group/destination/port
                        leaf port {
                            type types-sros:tcp-udp-port-non-zero;
                            description "TCP port number for the destination";
                        }

                        ///nokia-conf/configure/system/grpc-tunnel/destination-group/destination/router-instance
                        leaf router-instance {
                            type string;
                            description "Router instance for the destination group";
                        }

                        ///nokia-conf/configure/system/grpc-tunnel/destination-group/destination/local-source-address
                        leaf local-source-address {
                            type types-sros:ip-address;
                            description "Local IP address of packets sent from the source";
                        }

                        ///nokia-conf/configure/system/grpc-tunnel/destination-group/destination/originated-qos-marking
                        leaf originated-qos-marking {
                            type types-qos:dscp-name;
                            description "QoS marking used for gRPC tunnel packets";
                        }

                        ///nokia-conf/configure/system/grpc-tunnel/destination-group/destination/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/system/grpc-tunnel/destination-group/destination/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list destination

                } // list destination-group

                ///nokia-conf/configure/system/grpc-tunnel/tunnel
                list tunnel {
                    key "name";
                    max-elements 4;
                    description "Enter the tunnel list instance";

                    ///nokia-conf/configure/system/grpc-tunnel/tunnel/name
                    leaf name {
                        type types-sros:named-item;
                        description "Tunnel name";
                    }

                    ///nokia-conf/configure/system/grpc-tunnel/tunnel/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the tunnel";
                    }

                    ///nokia-conf/configure/system/grpc-tunnel/tunnel/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/system/grpc-tunnel/tunnel/destination-group
                    leaf destination-group {
                        type leafref {
                            path "../../destination-group/name";
                        }
                        description "Destination group used in the tunnel";
                    }

                    ///nokia-conf/configure/system/grpc-tunnel/tunnel/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/grpc-tunnel/tunnel/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/grpc-tunnel/tunnel/target-name
                    container target-name {
                        description "Enter the target-name context";

                        ///nokia-conf/configure/system/grpc-tunnel/tunnel/target-name/tunnel-identifier
                        choice tunnel-identifier {
                            ///nokia-conf/configure/system/grpc-tunnel/tunnel/target-name/tunnel-identifier/custom-string
                            case custom-string {

                                ///nokia-conf/configure/system/grpc-tunnel/tunnel/target-name/tunnel-identifier/custom-string/custom-string
                                leaf custom-string {
                                    type types-sros:named-item-64;
                                    description "Custom target name";
                                }

                            }
                            ///nokia-conf/configure/system/grpc-tunnel/tunnel/target-name/tunnel-identifier/user-agent
                            case user-agent {

                                ///nokia-conf/configure/system/grpc-tunnel/tunnel/target-name/tunnel-identifier/user-agent/user-agent
                                leaf user-agent {
                                    type empty;
                                    description "Set the user agent as the target name";
                                }

                            }
                            ///nokia-conf/configure/system/grpc-tunnel/tunnel/target-name/tunnel-identifier/node-name
                            case node-name {

                                ///nokia-conf/configure/system/grpc-tunnel/tunnel/target-name/tunnel-identifier/node-name/node-name
                                leaf node-name {
                                    type empty;
                                    description "Set the node name as target name";
                                }

                            }
                        }
                    } // container target-name

                    ///nokia-conf/configure/system/grpc-tunnel/tunnel/handler
                    list handler {
                        key "name";
                        max-elements 8;
                        description "Enter the handler list instance";

                        ///nokia-conf/configure/system/grpc-tunnel/tunnel/handler/name
                        leaf name {
                            type types-sros:named-item;
                            description "Handler name";
                        }

                        ///nokia-conf/configure/system/grpc-tunnel/tunnel/handler/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the handler";
                        }

                        ///nokia-conf/configure/system/grpc-tunnel/tunnel/handler/port
                        leaf port {
                            type types-sros:tcp-udp-port-non-zero;
                            description "TCP port number the handler listens to internally";
                        }

                        ///nokia-conf/configure/system/grpc-tunnel/tunnel/handler/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/system/grpc-tunnel/tunnel/handler/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/system/grpc-tunnel/tunnel/handler/target-type
                        container target-type {
                            description "Enter the target-type context";

                            ///nokia-conf/configure/system/grpc-tunnel/tunnel/handler/target-type/application
                            choice application {
                                ///nokia-conf/configure/system/grpc-tunnel/tunnel/handler/target-type/application/custom-type
                                case custom-type {

                                    ///nokia-conf/configure/system/grpc-tunnel/tunnel/handler/target-type/application/custom-type/custom-type
                                    leaf custom-type {
                                        type string {
                                            length "1..64";
                                            pattern "[_0-9A-Z]+" {
                                                error-message "must only contain uppercase letters, digits or underscores";
                                            }
                                        }
                                        description "Custom string for target type";
                                    }

                                }
                                ///nokia-conf/configure/system/grpc-tunnel/tunnel/handler/target-type/application/grpc-server
                                case grpc-server {

                                    ///nokia-conf/configure/system/grpc-tunnel/tunnel/handler/target-type/application/grpc-server/grpc-server
                                    leaf grpc-server {
                                        type empty;
                                        description "Target type set to GNMI_GNOI";
                                    }

                                }
                                ///nokia-conf/configure/system/grpc-tunnel/tunnel/handler/target-type/application/ssh-server
                                case ssh-server {

                                    ///nokia-conf/configure/system/grpc-tunnel/tunnel/handler/target-type/application/ssh-server/ssh-server
                                    leaf ssh-server {
                                        type empty;
                                        description "Target type is SSH";
                                    }

                                }
                            }
                        } // container target-type

                    } // list handler

                } // list tunnel

            } // container grpc-tunnel

            ///nokia-conf/configure/system/management-interface
            container management-interface {
                description "Enable the management-interface context";

                ///nokia-conf/configure/system/management-interface/configuration-mode
                leaf configuration-mode {
                    type enumeration {
                        enum classic                        { value 1; }
                        enum model-driven                   { value 2; }
                        enum mixed                          { value 3; }
                    }
                    description "Management interfaces allowed to edit the configuration";
                }

                ///nokia-conf/configure/system/management-interface/schema-path
                leaf schema-path {
                    type types-sros:url;
                    description "Schema path URL";
                }

                ///nokia-conf/configure/system/management-interface/commit-history
                leaf commit-history {
                    type uint32 {
                        range "0..200";
                    }
                    default "50";
                    description "Number of commit history IDs to store";
                }

                ///nokia-conf/configure/system/management-interface/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/system/management-interface/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/system/management-interface/cli
                container cli {
                    description "Enable the cli context";

                    ///nokia-conf/configure/system/management-interface/cli/cli-engine
                    leaf-list cli-engine {
                        type types-system:cli-engine-type;
                        max-elements 2;
                        ordered-by user;
                        description "System-wide CLI engine access";
                    }

                    ///nokia-conf/configure/system/management-interface/cli/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/management-interface/cli/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/management-interface/cli/classic-cli
                    container classic-cli {
                        description "Enter the classic-cli context";

                        ///nokia-conf/configure/system/management-interface/cli/classic-cli/allow-immediate
                        leaf allow-immediate {
                            type boolean;
                            default "true";
                            description "Allow writable access in classic CLI configure branch";
                        }

                        ///nokia-conf/configure/system/management-interface/cli/classic-cli/rollback
                        container rollback {
                            description "Enable the rollback context";

                            ///nokia-conf/configure/system/management-interface/cli/classic-cli/rollback/location
                            leaf location {
                                type types-sros:url;
                                description "Path and filename prefix for rollback checkpoint files";
                            }

                            ///nokia-conf/configure/system/management-interface/cli/classic-cli/rollback/local-checkpoints
                            leaf local-checkpoints {
                                type uint32 {
                                    range "1..50";
                                }
                                default "10";
                                description "Maximum number of rollback files on local storage";
                            }

                            ///nokia-conf/configure/system/management-interface/cli/classic-cli/rollback/remote-checkpoints
                            leaf remote-checkpoints {
                                type uint32 {
                                    range "1..200";
                                }
                                default "10";
                                description "Maximum rollback files saved at a remote location";
                            }

                            ///nokia-conf/configure/system/management-interface/cli/classic-cli/rollback/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/system/management-interface/cli/classic-cli/rollback/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/system/management-interface/cli/classic-cli/rollback/rescue
                            container rescue {
                                description "Enter the rescue context";

                                ///nokia-conf/configure/system/management-interface/cli/classic-cli/rollback/rescue/location
                                leaf location {
                                    type types-sros:url;
                                    description "Location of the rescue configuration file";
                                }

                            } // container rescue

                        } // container rollback

                    } // container classic-cli

                    ///nokia-conf/configure/system/management-interface/cli/md-cli
                    container md-cli {
                        description "Enable the md-cli context";

                        ///nokia-conf/configure/system/management-interface/cli/md-cli/auto-config-save
                        leaf auto-config-save {
                            type boolean;
                            default "true";
                            description "Automatically save configuration as part of commit";
                        }

                        ///nokia-conf/configure/system/management-interface/cli/md-cli/command-accounting-during-load
                        leaf command-accounting-during-load {
                            status obsolete;
                            type boolean;
                            default "true";
                            description "Perform remote command accounting during a load or rollback operation";
                        }

                        ///nokia-conf/configure/system/management-interface/cli/md-cli/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/system/management-interface/cli/md-cli/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/system/management-interface/cli/md-cli/environment
                        container environment {
                            description "Enter the environment context";

                            ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/more
                            leaf more {
                                type boolean;
                                default "true";
                                description "Activate the pager when output is longer than a screen";
                            }

                            ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/time-display
                            leaf time-display {
                                type enumeration {
                                    enum local                          { value 1; }
                                    enum utc                            { value 2; }
                                }
                                default "local";
                                description "Time zone to display time";
                            }

                            ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/time-format
                            leaf time-format {
                                type enumeration {
                                    enum iso-8601                       { value 1; }
                                    enum rfc-1123                       { value 2; }
                                    enum rfc-3339                       { value 3; }
                                }
                                default "rfc-3339";
                                description "Format to display the date and time";
                            }

                            ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-alias
                            container command-alias {
                                description "Enter the command-alias context";

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-alias/alias
                                list alias {
                                    key "alias-name";
                                    description "Enter the alias list instance";

                                    ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-alias/alias/alias-name
                                    leaf alias-name {
                                        type string {
                                            length "1..64";
                                            pattern "[_a-zA-Z0-9][-_a-zA-Z0-9]*" {
                                                error-message "must only contain alpha-numeric characters, underscore or hyphen (not as the first character)";
                                            }
                                        }
                                        description "Alias name";
                                    }

                                    ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-alias/alias/admin-state
                                    leaf admin-state {
                                        type types-sros:admin-state;
                                        default "disable";
                                        description "Administrative state of the alias";
                                    }

                                    ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-alias/alias/description
                                    leaf description {
                                        type types-sros:string-not-all-spaces {
                                            length "1..110";
                                        }
                                        description "Alias description";
                                    }

                                    ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-alias/alias/apply-groups
                                    leaf-list apply-groups {
                                        type leafref {
                                            path "../../../../../../../../groups/group/name";
                                        }
                                        max-elements 8;
                                        ordered-by user;
                                        description "Apply a configuration group at this level";
                                    }

                                    ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-alias/alias/apply-groups-exclude
                                    leaf-list apply-groups-exclude {
                                        type leafref {
                                            path "../../../../../../../../groups/group/name";
                                        }
                                        max-elements 8;
                                        ordered-by user;
                                        description "Exclude a configuration group at this level";
                                    }

                                    ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-alias/alias/alias-expansion
                                    choice alias-expansion {
                                        mandatory true;
                                        ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-alias/alias/alias-expansion/python
                                        case python {

                                            ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-alias/alias/alias-expansion/python/python-script
                                            leaf python-script {
                                                type leafref {
                                                    path "../../../../../../../../python/python-script/name";
                                                }
                                                description "Python script to run when executing the alias";
                                            }

                                        }
                                        ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-alias/alias/alias-expansion/cli
                                        case cli {

                                            ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-alias/alias/alias-expansion/cli/cli-command
                                            leaf cli-command {
                                                type string {
                                                    length "1..255";
                                                }
                                                description "CLI command to run when executing the alias";
                                            }

                                        }
                                    }
                                    ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-alias/alias/mount-point
                                    list mount-point {
                                        key "path";
                                        min-elements 1;
                                        description "Add a list entry for mount-point";

                                        ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-alias/alias/mount-point/path
                                        leaf path {
                                            type union {
                                                type enumeration {
                                                    enum global                         { value 0; }
                                                }
                                                type string {
                                                    length "1..255";
                                                    pattern "/.*" {
                                                        error-message "must start with forward slash";
                                                    }
                                                }
                                            }
                                            description "Mount point where the alias is available";
                                        }

                                    } // list mount-point

                                } // list alias

                            } // container command-alias

                            ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-completion
                            container command-completion {
                                description "Enter the command-completion context";

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-completion/enter
                                leaf enter {
                                    type boolean;
                                    default "true";
                                    description "Complete the command when the Enter key is pressed";
                                }

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-completion/space
                                leaf space {
                                    type boolean;
                                    default "true";
                                    description "Complete the command when the Space key is pressed";
                                }

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/command-completion/tab
                                leaf tab {
                                    type boolean;
                                    default "true";
                                    description "Complete the command when the Tab key is pressed";
                                }

                            } // container command-completion

                            ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/console
                            container console {
                                description "Enter the console context";

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/console/length
                                leaf length {
                                    type uint32 {
                                        range "24..512";
                                    }
                                    default "24";
                                    description "Number of lines displayed on the console";
                                }

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/console/width
                                leaf width {
                                    type uint32 {
                                        range "80..512";
                                    }
                                    default "80";
                                    description "Number of columns displayed on the console";
                                }

                            } // container console

                            ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/info-output
                            container info-output {
                                description "Enter the info-output context";

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/info-output/always-display
                                container always-display {
                                    description "Enter the always-display context";

                                    ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/info-output/always-display/admin-state
                                    leaf admin-state {
                                        type boolean;
                                        default "false";
                                        description "Always display admin-state elements";
                                    }

                                } // container always-display

                            } // container info-output

                            ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/message-severity-level
                            container message-severity-level {
                                description "Enter the message-severity-level context";

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/message-severity-level/cli
                                leaf cli {
                                    type enumeration {
                                        enum warning                        { value 6; }
                                        enum info                           { value 7; }
                                    }
                                    default "info";
                                    description "Message severity threshold for CLI messages";
                                }

                            } // container message-severity-level

                            ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/progress-indicator
                            container progress-indicator {
                                description "Enter the progress-indicator context";

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/progress-indicator/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    default "enable";
                                    description "Administrative state of the progress indicator";
                                }

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/progress-indicator/type
                                leaf type {
                                    type enumeration {
                                        enum dots                           { value 1; }
                                    }
                                    default "dots";
                                    description "Progress indicator output style";
                                }

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/progress-indicator/delay
                                leaf delay {
                                    type uint32 {
                                        range "0..10000";
                                    }
                                    units "milliseconds";
                                    default "1000";
                                    description "Delay before the progress indicator is displayed";
                                }

                            } // container progress-indicator

                            ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/prompt
                            container prompt {
                                description "Enter the prompt context";

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/prompt/context
                                leaf context {
                                    type boolean;
                                    default "true";
                                    description "Show the current command context in the prompt";
                                }

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/prompt/newline
                                leaf newline {
                                    type boolean;
                                    default "true";
                                    description "Add a new line before every prompt line";
                                }

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/prompt/timestamp
                                leaf timestamp {
                                    type boolean;
                                    default "false";
                                    description "Show the timestamp before the first prompt line";
                                }

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/prompt/uncommitted-changes-indicator
                                leaf uncommitted-changes-indicator {
                                    type boolean;
                                    default "true";
                                    description "Show an asterisk (*) when uncommitted changes exist";
                                }

                            } // container prompt

                            ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/python
                            container python {
                                description "Enter the python context";

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/python/memory-reservation
                                leaf memory-reservation {
                                    type uint32 {
                                        range "1..500";
                                    }
                                    units "megabytes";
                                    description "Memory reserved per Python interpreter";
                                }

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/python/minimum-available-memory
                                leaf minimum-available-memory {
                                    type uint32 {
                                        range "5..50";
                                    }
                                    units "percent";
                                    description "Minimum memory requirement to run a Python interpreter";
                                }

                                ///nokia-conf/configure/system/management-interface/cli/md-cli/environment/python/timeout
                                leaf timeout {
                                    type uint32 {
                                        range "30..86400";
                                    }
                                    units "seconds";
                                    default "3600";
                                    description "Maximum run time before a Python application is stopped";
                                }

                            } // container python

                        } // container environment

                    } // container md-cli

                } // container cli

                ///nokia-conf/configure/system/management-interface/configuration-save
                container configuration-save {
                    description "Enable the configuration-save context";

                    ///nokia-conf/configure/system/management-interface/configuration-save/configuration-backups
                    leaf configuration-backups {
                        type uint32 {
                            range "1..200";
                        }
                        default "50";
                        description "Maximum number of configuration versions maintained";
                    }

                    ///nokia-conf/configure/system/management-interface/configuration-save/incremental-saves
                    leaf incremental-saves {
                        type boolean;
                        default "true";
                        description "Use incremental saved configuration files";
                    }

                    ///nokia-conf/configure/system/management-interface/configuration-save/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/management-interface/configuration-save/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container configuration-save

                ///nokia-conf/configure/system/management-interface/netconf
                container netconf {
                    description "Enter the netconf context";

                    ///nokia-conf/configure/system/management-interface/netconf/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of NETCONF";
                    }

                    ///nokia-conf/configure/system/management-interface/netconf/port
                    leaf port {
                        type uint32 {
                            range "22|830";
                        }
                        default "830";
                        description "Port on which NETCONF server listens for connections";
                    }

                    ///nokia-conf/configure/system/management-interface/netconf/auto-config-save
                    leaf auto-config-save {
                        type boolean;
                        default "true";
                        description "Automatically save configuration as part of commit";
                    }

                    ///nokia-conf/configure/system/management-interface/netconf/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/management-interface/netconf/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/management-interface/netconf/capabilities
                    container capabilities {
                        description "Enter the capabilities context";

                        ///nokia-conf/configure/system/management-interface/netconf/capabilities/candidate
                        leaf candidate {
                            type boolean;
                            default "true";
                            description "Allow the NETCONF server to access candidate datastore";
                        }

                        ///nokia-conf/configure/system/management-interface/netconf/capabilities/writable-running
                        leaf writable-running {
                            status obsolete;
                            type boolean;
                            default "false";
                            description "Allow NETCONF server to access the running datastore";
                        }

                    } // container capabilities

                } // container netconf

                ///nokia-conf/configure/system/management-interface/operations
                container operations {
                    description "Enable the operations context";

                    ///nokia-conf/configure/system/management-interface/operations/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/management-interface/operations/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/management-interface/operations/global-timeouts
                    container global-timeouts {
                        description "Enter the global-timeouts context";

                        ///nokia-conf/configure/system/management-interface/operations/global-timeouts/asynchronous-execution
                        leaf asynchronous-execution {
                            type types-operation:operation-timeout;
                            default "3600";
                            description "Timeout for asynchronous operation execution";
                        }

                        ///nokia-conf/configure/system/management-interface/operations/global-timeouts/asynchronous-retention
                        leaf asynchronous-retention {
                            type types-operation:operation-timeout;
                            default "86400";
                            description "Timeout for asynchronous operation data retention";
                        }

                        ///nokia-conf/configure/system/management-interface/operations/global-timeouts/synchronous-execution
                        leaf synchronous-execution {
                            type types-operation:operation-timeout;
                            default "never";
                            description "Timeout for synchronous operation execution";
                        }

                    } // container global-timeouts

                } // container operations

                ///nokia-conf/configure/system/management-interface/remote-management
                container remote-management {
                    description "Enter the remote-management context";

                    ///nokia-conf/configure/system/management-interface/remote-management/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of remote management registration";
                    }

                    ///nokia-conf/configure/system/management-interface/remote-management/connection-timeout
                    leaf connection-timeout {
                        type uint32 {
                            range "1..3600";
                        }
                        units "seconds";
                        default "60";
                        description "Time without a response before manager declared down";
                    }

                    ///nokia-conf/configure/system/management-interface/remote-management/hello-interval
                    leaf hello-interval {
                        type types-sros:time-duration {
                            range "10..216000";
                        }
                        units "seconds";
                        default "600";
                        description "Time between hello messages from SR OS node to manager";
                    }

                    ///nokia-conf/configure/system/management-interface/remote-management/router-instance
                    leaf router-instance {
                        type string;
                        default "management";
                        description "Router name or VPRN service name";
                    }

                    ///nokia-conf/configure/system/management-interface/remote-management/device-label
                    leaf device-label {
                        type types-sros:named-item-64;
                        description "Device label supplied to the remote manager";
                    }

                    ///nokia-conf/configure/system/management-interface/remote-management/device-name
                    leaf device-name {
                        type types-sros:named-item-64;
                        description "Device name supplied to the remote manager";
                    }

                    ///nokia-conf/configure/system/management-interface/remote-management/source-address
                    leaf source-address {
                        type types-sros:ip-address;
                        description "Source IP address for connection to the manager";
                    }

                    ///nokia-conf/configure/system/management-interface/remote-management/source-port
                    leaf source-port {
                        type types-system:remote-mgmt-registration-port;
                        default "grpc-default";
                        description "Source TCP port number to connection to the manager";
                    }

                    ///nokia-conf/configure/system/management-interface/remote-management/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/management-interface/remote-management/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/management-interface/remote-management/transport
                    choice transport {
                        ///nokia-conf/configure/system/management-interface/remote-management/transport/client-tls-profile
                        case client-tls-profile {

                            ///nokia-conf/configure/system/management-interface/remote-management/transport/client-tls-profile/client-tls-profile
                            leaf client-tls-profile {
                                type leafref {
                                    path "../../../security/tls/client-tls-profile/client-profile-name";
                                }
                                description "TLS client profile name";
                            }

                        }
                        ///nokia-conf/configure/system/management-interface/remote-management/transport/allow-unsecure-connection
                        case allow-unsecure-connection {

                            ///nokia-conf/configure/system/management-interface/remote-management/transport/allow-unsecure-connection/allow-unsecure-connection
                            leaf allow-unsecure-connection {
                                type empty;
                                description "Allow connection without secured transport protocol";
                            }

                        }
                    }
                    ///nokia-conf/configure/system/management-interface/remote-management/manager
                    list manager {
                        key "manager-name";
                        max-elements 2;
                        description "Enter the manager list instance";

                        ///nokia-conf/configure/system/management-interface/remote-management/manager/manager-name
                        leaf manager-name {
                            type types-sros:named-item-64;
                            description "Remote management manager name";
                        }

                        ///nokia-conf/configure/system/management-interface/remote-management/manager/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of remote management registration";
                        }

                        ///nokia-conf/configure/system/management-interface/remote-management/manager/connection-timeout
                        leaf connection-timeout {
                            type uint32 {
                                range "1..3600";
                            }
                            units "seconds";
                            description "Time without response before manager is declared down";
                        }

                        ///nokia-conf/configure/system/management-interface/remote-management/manager/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/system/management-interface/remote-management/manager/manager-address
                        leaf manager-address {
                            type union {
                                type types-sros:ip-address;
                                type types-sros:fully-qualified-domain-name;
                            }
                            description "Destination IP address of the manager";
                        }

                        ///nokia-conf/configure/system/management-interface/remote-management/manager/manager-port
                        leaf manager-port {
                            type types-sros:tcp-udp-port-non-zero;
                            default "57400";
                            description "Destination TCP port for gRPC connections to manager";
                        }

                        ///nokia-conf/configure/system/management-interface/remote-management/manager/router-instance
                        leaf router-instance {
                            type string;
                            description "Reference to a router or VPRN service name";
                        }

                        ///nokia-conf/configure/system/management-interface/remote-management/manager/device-label
                        leaf device-label {
                            type types-sros:named-item-64;
                            description "Device label supplied to the remote manager";
                        }

                        ///nokia-conf/configure/system/management-interface/remote-management/manager/device-name
                        leaf device-name {
                            type types-sros:named-item-64;
                            description "Device name supplied to the remote manager";
                        }

                        ///nokia-conf/configure/system/management-interface/remote-management/manager/source-address
                        leaf source-address {
                            type types-sros:ip-address;
                            description "Source IP address for connection to the manager";
                        }

                        ///nokia-conf/configure/system/management-interface/remote-management/manager/source-port
                        leaf source-port {
                            type types-system:remote-mgmt-registration-port;
                            description "Source TCP destination port number";
                        }

                        ///nokia-conf/configure/system/management-interface/remote-management/manager/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/system/management-interface/remote-management/manager/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/system/management-interface/remote-management/manager/transport
                        choice transport {
                            ///nokia-conf/configure/system/management-interface/remote-management/manager/transport/client-tls-profile
                            case client-tls-profile {

                                ///nokia-conf/configure/system/management-interface/remote-management/manager/transport/client-tls-profile/client-tls-profile
                                leaf client-tls-profile {
                                    type leafref {
                                        path "../../../../security/tls/client-tls-profile/client-profile-name";
                                    }
                                    description "TLS client profile name assigned to the remote manager";
                                }

                            }
                            ///nokia-conf/configure/system/management-interface/remote-management/manager/transport/allow-unsecure-connection
                            case allow-unsecure-connection {

                                ///nokia-conf/configure/system/management-interface/remote-management/manager/transport/allow-unsecure-connection/allow-unsecure-connection
                                leaf allow-unsecure-connection {
                                    type empty;
                                    description "Allow connection without secured transport protocol";
                                }

                            }
                        }
                    } // list manager

                } // container remote-management

                ///nokia-conf/configure/system/management-interface/yang-modules
                container yang-modules {
                    description "Enable the yang-modules context";

                    ///nokia-conf/configure/system/management-interface/yang-modules/base-r13-modules
                    leaf base-r13-modules {
                        status obsolete;
                        type boolean;
                        default "false";
                        description "Support Base-R13 YANG models";
                    }

                    ///nokia-conf/configure/system/management-interface/yang-modules/nokia-modules
                    leaf nokia-modules {
                        status obsolete;
                        type boolean;
                        description "Support Nokia YANG models";
                    }

                    ///nokia-conf/configure/system/management-interface/yang-modules/nokia-submodules
                    leaf nokia-submodules {
                        type boolean;
                        description "Support submodule-based packaging of Nokia YANG models";
                    }

                    ///nokia-conf/configure/system/management-interface/yang-modules/openconfig-modules
                    leaf openconfig-modules {
                        type boolean;
                        default "false";
                        description "Support access to OpenConfig YANG models";
                    }

                    ///nokia-conf/configure/system/management-interface/yang-modules/nokia-combined-modules
                    leaf nokia-combined-modules {
                        type boolean;
                        description "Support access to combined Nokia YANG models";
                    }

