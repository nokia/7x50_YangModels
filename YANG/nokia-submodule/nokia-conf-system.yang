                        ///nokia-conf/configure/system/management-interface/yang-modules/nmda/nmda-support
                        leaf nmda-support {
                            type boolean;
                            default "false";
                            description "Advertise NMDA support over NETCONF";
                        }

                    } // container nmda

                } // container yang-modules

                ///nokia-conf/configure/system/management-interface/snmp
                container snmp {
                    description "Enable the snmp context";

                    ///nokia-conf/configure/system/management-interface/snmp/engine-id
                    leaf engine-id {
