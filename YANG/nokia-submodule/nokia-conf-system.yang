                        description "Port number used to send general SNMP messages";
                    }

                    ///nokia-conf/configure/system/management-interface/snmp/max-bulk-duration
                    leaf max-bulk-duration {
                        type uint32 {
                            range "100..5000";
                        }
                        units "milliseconds";
                        description "Maximum process duration before responses are returned";
                    }

                    ///nokia-conf/configure/system/management-interface/snmp/packet-size
                    leaf packet-size {
                        type int32 {
                            range "484..9216";
                        }
                        default "1500";
                        description "Maximum SNMP packet size generated by the node";
                    }

                    ///nokia-conf/configure/system/management-interface/snmp/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "enable";
                        description "Administrative state of the SNMP agent";
                    }

                    ///nokia-conf/configure/system/management-interface/snmp/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/management-interface/snmp/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/management-interface/snmp/streaming
                    container streaming {
                        description "Enable the streaming context";

                        ///nokia-conf/configure/system/management-interface/snmp/streaming/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of SNMP streaming";
                        }

                        ///nokia-conf/configure/system/management-interface/snmp/streaming/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/system/management-interface/snmp/streaming/apply-groups-exclude
                        leaf-list apply-groups-exclude {
