submodule nokia-conf-system {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-cellular           { prefix "types-cellular"; }
    import nokia-types-eth-cfm            { prefix "types-eth-cfm"; }
    import nokia-types-l2tp               { prefix "types-l2tp"; }
    import nokia-types-operation          { prefix "types-operation"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-system             { prefix "types-system"; }

    include nokia-conf-groups;
    include nokia-conf-python;
    include nokia-conf-system-bluetooth;
    include nokia-conf-system-central-frequency-clock;
    include nokia-conf-system-cron;
    include nokia-conf-system-login-control;
    include nokia-conf-system-ned;
    include nokia-conf-system-persistence;
    include nokia-conf-system-ptp;
    include nokia-conf-system-script-control;
    include nokia-conf-system-security;
    include nokia-conf-system-security-tls;
    include nokia-conf-system-telemetry;
    include nokia-conf-system-time;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure system parameters on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-system YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-06-05";

    grouping conf-system {
        ///nokia-conf/configure/system
        container system {
            description "Enter the system context";

            ///nokia-conf/configure/system/contact
            leaf contact {
                type types-sros:description;
                description "Contact information for the managed node";
            }

            ///nokia-conf/configure/system/name
            leaf name {
                type types-sros:named-item-64;
                description "Administrative name assigned to the system";
            }

            ///nokia-conf/configure/system/location
            leaf location {
                type types-sros:description;
                description "Site location of the system";
            }

            ///nokia-conf/configure/system/alarm-contact-in-power
            leaf alarm-contact-in-power {
                type boolean;
                default "false";
                description "Power the output pin on the CPM alarm interface port";
            }

            ///nokia-conf/configure/system/icmp-vse
            leaf icmp-vse {
                type boolean;
                default "false";
                description "Enable vendor-specific extensions to ICMP";
            }

            ///nokia-conf/configure/system/selective-fib
            leaf selective-fib {
                type boolean;
                default "false";
                description "FIB assigned to the system";
            }

            ///nokia-conf/configure/system/coordinates
            leaf coordinates {
                type types-sros:description;
                description "GPS coordinates for the system location";
            }

            ///nokia-conf/configure/system/clli-code
            leaf clli-code {
                type types-sros:clli-description;
                description "CLLI code value for the system";
            }

            ///nokia-conf/configure/system/congestion-management
            leaf congestion-management {
                type boolean;
                default "false";
                description "Enable Virtual Service Router congestion management";
            }

            ///nokia-conf/configure/system/ospf-dynamic-hostnames
            leaf ospf-dynamic-hostnames {
                type boolean;
                default "false";
                description "Process received OSPF dynamic hostname information";
            }

            ///nokia-conf/configure/system/boot-good-exec
            leaf boot-good-exec {
                type types-sros:url;
                description "CLI script file to execute following successful boot-up";
            }

            ///nokia-conf/configure/system/boot-bad-exec
            leaf boot-bad-exec {
                type types-sros:url;
                description "CLI script file to execute following a failed boot-up";
            }

            ///nokia-conf/configure/system/allow-boot-license-violations
            leaf allow-boot-license-violations {
                type boolean;
                default "true";
                description "Allow boot license violations in boot-up configuration";
            }

            ///nokia-conf/configure/system/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/system/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/system/alarms
            container alarms {
                description "Enter the alarms context";

                ///nokia-conf/configure/system/alarms/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "enable";
                    description "Administrative state of the system alarm";
                }

                ///nokia-conf/configure/system/alarms/max-cleared
                leaf max-cleared {
                    type uint32 {
                        range "0..500";
                    }
                    default "500";
                    description "Maximum number of cleared alarms";
                }

                ///nokia-conf/configure/system/alarms/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/system/alarms/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container alarms

            ///nokia-conf/configure/system/cpm-http-redirect
            container cpm-http-redirect {
                description "Enable the cpm-http-redirect context";

                ///nokia-conf/configure/system/cpm-http-redirect/optimized-mode
                leaf optimized-mode {
                    type boolean;
                    default "true";
                    description "Enable optimized mode for CPM HTTP redirect messages";
                }

                ///nokia-conf/configure/system/cpm-http-redirect/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/system/cpm-http-redirect/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container cpm-http-redirect

            ///nokia-conf/configure/system/load-balancing
            container load-balancing {
                description "Enter the load-balancing context";

                ///nokia-conf/configure/system/load-balancing/l2tp-load-balancing
                leaf l2tp-load-balancing {
                    type boolean;
                    default "false";
                    description "Include L2TP header information for load balancing";
                }

                ///nokia-conf/configure/system/load-balancing/l4-load-balancing
                leaf l4-load-balancing {
                    type boolean;
                    description "Use load balancing based on Layer 4 fields";
                }

                ///nokia-conf/configure/system/load-balancing/lsr-load-balancing
                leaf lsr-load-balancing {
                    type enumeration {
                        enum lbl-only                       { value 1; }
                        enum lbl-ip                         { value 2; }
                        enum ip-only                        { value 3; }
                        enum eth-encap-ip                   { value 4; }
                        enum lbl-ip-l4-teid                 { value 5; }
                        enum lbl-eth-ip-l4-teid             { value 6; }
                        enum lbl-ip-or-teid                 { value 7; }
                    }
                    description "Algorithm for system-wide LSR load balancing";
                }

                ///nokia-conf/configure/system/load-balancing/mc-enh-load-balancing
                leaf mc-enh-load-balancing {
                    type boolean;
                    default "false";
                    description "Enable enhanced egress multicast load balancing";
                }

                ///nokia-conf/configure/system/load-balancing/service-id-lag-hashing
                leaf service-id-lag-hashing {
                    type boolean;
                    default "false";
                    description "Enable enhanced VLL LAG service ID hashing";
                }

                ///nokia-conf/configure/system/load-balancing/system-ip-load-balancing
                leaf system-ip-load-balancing {
                    type boolean;
                    description "Use system IP address for ECMP and LAG load balancing";
                }

                ///nokia-conf/configure/system/load-balancing/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/system/load-balancing/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/system/load-balancing/hash-polynomial
                container hash-polynomial {
                    description "Enter the hash-polynomial context";

                    ///nokia-conf/configure/system/load-balancing/hash-polynomial/ecmp
                    leaf ecmp {
                        type types-system:hash-polynomial-type;
                        default "poly1";
                        description "Hashing polynomial for (IP) ECMP";
                    }

                    ///nokia-conf/configure/system/load-balancing/hash-polynomial/lag
                    leaf lag {
                        type types-system:hash-polynomial-type;
                        default "poly2";
                        description "Hashing polynomial for LAG";
                    }

                } // container hash-polynomial

            } // container load-balancing

            ///nokia-conf/configure/system/alarm-contact-input
            list alarm-contact-input {
                key "input-pin-number";
                description "Enter the alarm-contact-input list instance";

                ///nokia-conf/configure/system/alarm-contact-input/input-pin-number
                leaf input-pin-number {
                    type int32 {
                        range "1..4";
                    }
                    description "Alarm contact input pin";
                }

                ///nokia-conf/configure/system/alarm-contact-input/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of the alarm contact input";
                }

                ///nokia-conf/configure/system/alarm-contact-input/description
                leaf description {
                    type types-sros:long-description;
                    description "Text description";
                }

                ///nokia-conf/configure/system/alarm-contact-input/normal-state
                leaf normal-state {
                    type enumeration {
                        enum open                           { value 1; }
                        enum closed                         { value 2; }
                    }
                    default "open";
                    description "Normal state associated with the alarm contact input";
                }

                ///nokia-conf/configure/system/alarm-contact-input/trigger-message
                leaf trigger-message {
                    type types-sros:description;
                    default "Alarm Input Triggered";
                    description "Text message sent in the log event when input changes";
                }

                ///nokia-conf/configure/system/alarm-contact-input/clear-message
                leaf clear-message {
                    type types-sros:description;
                    default "Alarm Input Cleared";
                    description "Text message sent in the log event when an alarm clears";
                }

                ///nokia-conf/configure/system/alarm-contact-input/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/system/alarm-contact-input/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // list alarm-contact-input


            ///nokia-conf/configure/system/dns
            container dns {
                description "Enable the dns context";

                ///nokia-conf/configure/system/dns/address-pref
                leaf address-pref {
                    type enumeration {
                        enum ipv4-only                      { value 1; }
                        enum ipv6-first                     { value 2; }
                    }
                    description "Preference in DNS address resolving order";
                }

                ///nokia-conf/configure/system/dns/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/system/dns/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/system/dns/dnssec
                container dnssec {
                    description "Enter the dnssec context";

                    ///nokia-conf/configure/system/dns/dnssec/ad-validation
                    leaf ad-validation {
                        type enumeration {
                            enum fall-through {
                                value 1;
                                description "Allow non-DNSSEC responses to fall-through to permit resolution in case of validation failure";
                            }
                            enum drop {
                                value 2;
                                description "Drop non-DNSSEC responses in case of validation failure";
                            }
                        }
                        description "Validation of AD-bit presence in DNS server responses";
                    }

                } // container dnssec

            } // container dns

            ///nokia-conf/configure/system/efm-oam
            container efm-oam {
                description "Enable the efm-oam context";

                ///nokia-conf/configure/system/efm-oam/dying-gasp-tx-on-reset
                leaf dying-gasp-tx-on-reset {
                    type boolean;
                    default "false";
                    description "Generate Information OAM PDU on soft reset notification";
                }

                ///nokia-conf/configure/system/efm-oam/grace-tx
                leaf grace-tx {
                    type boolean;
                    default "false";
                    description "Send Grace TLVs for soft reset graceful recovery events";
                }

                ///nokia-conf/configure/system/efm-oam/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/system/efm-oam/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container efm-oam

            ///nokia-conf/configure/system/eth-cfm
            container eth-cfm {
                description "Enter the eth-cfm context";

                ///nokia-conf/configure/system/eth-cfm/grace
                leaf grace {
                    type boolean;
                    default "true";
                    description "Allow system level capability of grace messaging";
                }

                ///nokia-conf/configure/system/eth-cfm/named-display
                leaf named-display {
                    type boolean;
                    default "false";
                    description "Enable administrative name display in CLI show outputs";
                }

                ///nokia-conf/configure/system/eth-cfm/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/system/eth-cfm/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/system/eth-cfm/sender-id
                container sender-id {
                    description "Enter the sender-id context";

                    ///nokia-conf/configure/system/eth-cfm/sender-id/type
                    leaf type {
                        type types-eth-cfm:chassis-id-subtype;
                        description "ETH-CFM sender ID to be used in CFM PDUs";
                    }

                    ///nokia-conf/configure/system/eth-cfm/sender-id/local-name
                    leaf local-name {
                        type string {
                            length "1..45";
                        }
                        description "Local name used in CFM PDUs";
                    }

                } // container sender-id

                ///nokia-conf/configure/system/eth-cfm/redundancy
                container redundancy {
                    description "Enter the redundancy context";

                    ///nokia-conf/configure/system/eth-cfm/redundancy/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/eth-cfm/redundancy/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/eth-cfm/redundancy/mc-lag
                    container mc-lag {
                        description "Enter the mc-lag context";

                        ///nokia-conf/configure/system/eth-cfm/redundancy/mc-lag/propagate-hold-time
                        leaf propagate-hold-time {
                            type union {
                                type int32 {
                                    range "1..60";
                                }
                                type enumeration {
                                    enum none                           { value 0; }
                                }
                            }
                            units "seconds";
                            default "1";
                            description "Delay timer value for the fault propagation";
                        }

                        ///nokia-conf/configure/system/eth-cfm/redundancy/mc-lag/standby-mep
                        leaf standby-mep {
                            type boolean;
                            default "false";
                            description "Allow standby MC-LAG MEPs to act administratively down";
                        }

                    } // container mc-lag

                } // container redundancy

                ///nokia-conf/configure/system/eth-cfm/slm
                container slm {
                    description "Enable the slm context";

                    ///nokia-conf/configure/system/eth-cfm/slm/inactivity-timer
                    leaf inactivity-timer {
                        type uint32 {
                            range "10..100";
                        }
                        units "seconds";
                        default "100";
                        description "SLR inactivity timer to maintain the stale test data";
                    }

                    ///nokia-conf/configure/system/eth-cfm/slm/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/eth-cfm/slm/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container slm

                ///nokia-conf/configure/system/eth-cfm/md-auto-id
                container md-auto-id {
                    description "Enter the md-auto-id context";

                    ///nokia-conf/configure/system/eth-cfm/md-auto-id/md-index-range
                    container md-index-range {
                        presence "Enable the md-index-range context";
                        description "Enable the md-index-range context";

                        ///nokia-conf/configure/system/eth-cfm/md-auto-id/md-index-range/start
                        leaf start {
                            type uint32 {
                                range "1..max";
                            }
                            sros-ext:immutable;
                            mandatory true;
                            description "Lower bound of the range";
                        }

                        ///nokia-conf/configure/system/eth-cfm/md-auto-id/md-index-range/end
                        leaf end {
                            type uint32 {
                                range "1..max";
                            }
                            sros-ext:immutable;
                            mandatory true;
                            description "Upper bound of the range";
                        }

                        ///nokia-conf/configure/system/eth-cfm/md-auto-id/md-index-range/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/system/eth-cfm/md-auto-id/md-index-range/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container md-index-range

                    ///nokia-conf/configure/system/eth-cfm/md-auto-id/ma-index-range
                    container ma-index-range {
                        presence "Enable the ma-index-range context";
                        description "Enable the ma-index-range context";

                        ///nokia-conf/configure/system/eth-cfm/md-auto-id/ma-index-range/start
                        leaf start {
                            type uint32 {
                                range "1..max";
                            }
                            sros-ext:immutable;
                            mandatory true;
                            description "Lower bound of the range";
                        }

                        ///nokia-conf/configure/system/eth-cfm/md-auto-id/ma-index-range/end
                        leaf end {
                            type uint32 {
                                range "1..max";
                            }
                            sros-ext:immutable;
                            mandatory true;
                            description "Upper bound of the range";
                        }

                        ///nokia-conf/configure/system/eth-cfm/md-auto-id/ma-index-range/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/system/eth-cfm/md-auto-id/ma-index-range/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container ma-index-range

                } // container md-auto-id

            } // container eth-cfm

            ///nokia-conf/configure/system/fp
            container fp {
                description "Enter the fp context";

                ///nokia-conf/configure/system/fp/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/system/fp/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/system/fp/options
                container options {
                    description "Enable the options context";

                    ///nokia-conf/configure/system/fp/options/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/fp/options/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/fp/options/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-conf/configure/system/fp/options/qos/dscp-transparency
                        container dscp-transparency {
                            description "Enter the dscp-transparency context";

                            ///nokia-conf/configure/system/fp/options/qos/dscp-transparency/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of the DSCP transparency option";
                            }

                        } // container dscp-transparency

                        ///nokia-conf/configure/system/fp/options/qos/fc-4-profile
                        container fc-4-profile {
                            description "Enter the fc-4-profile context";

                            ///nokia-conf/configure/system/fp/options/qos/fc-4-profile/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "disable";
                                description "Administrative state of the fc-4-profile option";
                            }

                        } // container fc-4-profile

                        ///nokia-conf/configure/system/fp/options/qos/mac-classification-criteria
                        container mac-classification-criteria {
                            description "Enter the mac-classification-criteria context";

                            ///nokia-conf/configure/system/fp/options/qos/mac-classification-criteria/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "disable";
                                description "Administrative state of QoS mac-classification-criteria";
                            }

                        } // container mac-classification-criteria

                        ///nokia-conf/configure/system/fp/options/qos/ipv6-classification-criteria
                        container ipv6-classification-criteria {
                            description "Enter the ipv6-classification-criteria context";

