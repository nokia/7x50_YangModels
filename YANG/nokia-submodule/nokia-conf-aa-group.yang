                        description "Enable the video-template context";

                        ///nokia-conf/configure/application-assurance/group/cflowd/rtp-performance/video-template/field-selection
                        leaf field-selection {
                            type types-aa:aa-grp-cflowd-template-select;
                            default "legacy";
                            description "Field selection method";
                        }

                        ///nokia-conf/configure/application-assurance/group/cflowd/rtp-performance/video-template/apply-groups
