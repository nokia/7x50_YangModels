                    container audio-template {
                        description "Enable the audio-template context";

                        ///nokia-conf/configure/application-assurance/group/cflowd/rtp-performance/audio-template/field-selection
                        leaf field-selection {
                            type types-aa:aa-grp-cflowd-template-select;
                            default "legacy";
