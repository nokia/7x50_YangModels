                        ///nokia-conf/configure/application-assurance/group/cflowd/rtp-performance/audio-template/dynamic-fields
                        container dynamic-fields {
                            description "Enter the dynamic-fields context";

                            ///nokia-conf/configure/application-assurance/group/cflowd/rtp-performance/audio-template/dynamic-fields/admin-state
