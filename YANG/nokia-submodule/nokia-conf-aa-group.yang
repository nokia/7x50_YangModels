                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/application-assurance/group/cflowd/comprehensive/template/dynamic-fields
                        container dynamic-fields {
                            description "Enter the dynamic-fields context";

                            ///nokia-conf/configure/application-assurance/group/cflowd/comprehensive/template/dynamic-fields/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "disable";
                                description "Administrative state of the dynamic fields";
                            }

                            ///nokia-conf/configure/application-assurance/group/cflowd/comprehensive/template/dynamic-fields/field
                            list field {
                                key "field-name";
                                description "Add a list entry for field";

                                ///nokia-conf/configure/application-assurance/group/cflowd/comprehensive/template/dynamic-fields/field/field-name
                                leaf field-name {
                                    type types-sros:named-item;
                                    description "Cflowd template dynamic field name";
                                }

                            } // list field

                        } // container dynamic-fields

                    } // container template

                } // container comprehensive

                ///nokia-conf/configure/application-assurance/group/cflowd/export-override
                container export-override {
                    description "Enter the export-override context";

                    ///nokia-conf/configure/application-assurance/group/cflowd/export-override/mode
                    leaf mode {
                        type types-aa:cflowd-export-override-type;
                        sros-ext:auto-restart-to-modify;
                        description "AA cflowd export override mode";
                    }

                    ///nokia-conf/configure/application-assurance/group/cflowd/export-override/prefix
                    leaf prefix {
                        type types-sros:named-item {
                            length "1..8";
                        }
                        sros-ext:auto-restart-to-modify;
                        description "AA group cflowd export override prefix";
                    }

                } // container export-override


                ///nokia-conf/configure/application-assurance/group/cflowd/rtp-performance
