         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-06-05";

    grouping conf-aa-group {
        ///nokia-conf/configure/application-assurance/group
        list group {
            key "aa-group-id";
            description "Enter the group list instance";

            ///nokia-conf/configure/application-assurance/group/aa-group-id
            leaf aa-group-id {
                type uint32 {
                    range "1..255";
                }
                description "AA group ID";
            }

            ///nokia-conf/configure/application-assurance/group/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/application-assurance/group/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/application-assurance/group/certificate-profile
            list certificate-profile {
                key "cert-prof-name";
                description "Enter the certificate-profile list instance";

                ///nokia-conf/configure/application-assurance/group/certificate-profile/cert-prof-name
                leaf cert-prof-name {
                    type types-sros:named-item;
                    description "AA group certificate profile name";
                }

                ///nokia-conf/configure/application-assurance/group/certificate-profile/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of the AA certificate profile";
                }

                ///nokia-conf/configure/application-assurance/group/certificate-profile/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/application-assurance/group/certificate-profile/file
                leaf file {
                    type types-aa:certificate-file;
                    sros-ext:auto-restart-to-modify;
                    description "Certificate file name";
                }

                ///nokia-conf/configure/application-assurance/group/certificate-profile/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/application-assurance/group/certificate-profile/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // list certificate-profile

            ///nokia-conf/configure/application-assurance/group/cflowd
            container cflowd {
                description "Enable the cflowd context";

                ///nokia-conf/configure/application-assurance/group/cflowd/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of cflowd export";
                }

                ///nokia-conf/configure/application-assurance/group/cflowd/template-retransmit
                leaf template-retransmit {
                    type uint32 {
                        range "10..600";
                    }
                    units "seconds";
                    default "600";
                    description "Template retransmit time";
                }

                ///nokia-conf/configure/application-assurance/group/cflowd/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/application-assurance/group/cflowd/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/application-assurance/group/cflowd/obfuscation
                container obfuscation {
                    description "Enter the obfuscation context";

                    ///nokia-conf/configure/application-assurance/group/cflowd/obfuscation/aes-128-encryption-key
                    leaf aes-128-encryption-key {
                        type types-sros:encrypted-leaf {
                            length "51";
                        }
                        description "AES-128 encryption key";
                    }

                    ///nokia-conf/configure/application-assurance/group/cflowd/obfuscation/aes-256-encryption-key
                    leaf aes-256-encryption-key {
                        type types-sros:encrypted-leaf {
                            length "71";
                        }
                        description "AES-256 encryption key";
                    }

                } // container obfuscation

                ///nokia-conf/configure/application-assurance/group/cflowd/collector
                list collector {
                    key "ip-address port";
                    max-elements 2;
                    description "Enter the collector list instance";

                    ///nokia-conf/configure/application-assurance/group/cflowd/collector/ip-address
                    leaf ip-address {
                        type types-sros:ip-unicast-address;
                        description "IP address of the remote cflowd collector host";
                    }

                    ///nokia-conf/configure/application-assurance/group/cflowd/collector/port
                    leaf port {
                        type types-sros:tcp-udp-port-non-zero;
                        description "Cflowd collector host port number";
                    }

                    ///nokia-conf/configure/application-assurance/group/cflowd/collector/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the cflowd export";
                    }

                    ///nokia-conf/configure/application-assurance/group/cflowd/collector/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/application-assurance/group/cflowd/collector/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/application-assurance/group/cflowd/collector/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list collector

                ///nokia-conf/configure/application-assurance/group/cflowd/direct-export
                container direct-export {
                    description "Enter the direct-export context";

                    ///nokia-conf/configure/application-assurance/group/cflowd/direct-export/vlan-id
                    leaf vlan-id {
                        type types-sros:vlan-id;
                        description "Cflowd direct export VLAN ID";
                    }

                    ///nokia-conf/configure/application-assurance/group/cflowd/direct-export/collector
                    list collector {
                        key "collector-id";
                        max-elements 16;
                        description "Enter the collector list instance";

                        ///nokia-conf/configure/application-assurance/group/cflowd/direct-export/collector/collector-id
                        leaf collector-id {
                            type uint32 {
                                range "1..65535";
                            }
                            description "Cflowd direct export collector ID";
                        }

                        ///nokia-conf/configure/application-assurance/group/cflowd/direct-export/collector/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/application-assurance/group/cflowd/direct-export/collector/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/application-assurance/group/cflowd/direct-export/collector/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/application-assurance/group/cflowd/direct-export/collector/address
                        list address {
                            key "ip-address port";
                            max-elements 2;
                            description "Enter the address list instance";

                            ///nokia-conf/configure/application-assurance/group/cflowd/direct-export/collector/address/ip-address
                            leaf ip-address {
                                type types-sros:ip-unicast-address;
                                description "IP address of the remote cflowd collector host";
                            }

                            ///nokia-conf/configure/application-assurance/group/cflowd/direct-export/collector/address/port
                            leaf port {
                                type types-sros:tcp-udp-port-non-zero;
                                description "Cflowd collector host port number";
                            }

                            ///nokia-conf/configure/application-assurance/group/cflowd/direct-export/collector/address/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "disable";
                                description "Administrative state of the direct export collector";
                            }

                            ///nokia-conf/configure/application-assurance/group/cflowd/direct-export/collector/address/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/application-assurance/group/cflowd/direct-export/collector/address/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list address

                    } // list collector

                } // container direct-export

                ///nokia-conf/configure/application-assurance/group/cflowd/comprehensive
