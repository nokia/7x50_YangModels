submodule nokia-state-aa-group {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-application-assurance { prefix "types-aa"; }
    import nokia-types-esa                { prefix "types-esa"; }
    import nokia-types-isa                { prefix "types-isa"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for application assurance group on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-aa-group YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-06-05";

    grouping state-aa-group {
        ///nokia-state/state/application-assurance/group
        list group {
            key "aa-group-id";
            description "Enter the group list instance";

            ///nokia-state/state/application-assurance/group/aa-group-id
            leaf aa-group-id {
                type uint32 {
                    range "1..255";
                }
                description "AA group ID";
            }

            ///nokia-state/state/application-assurance/group/aa-interface
            container aa-interface {
                description "Enter the aa-interface context";

                ///nokia-state/state/application-assurance/group/aa-interface/sap
                list sap {
                    key "sap-id";
                    description "Enter the sap list instance";

                    ///nokia-state/state/application-assurance/group/aa-interface/sap/sap-id
                    leaf sap-id {
                        type types-sros:sap;
                        description "The VLAN ID on which the ISA-AA is expected to be emitting traffic.";
                    }

                    ///nokia-state/state/application-assurance/group/aa-interface/sap/oper-state
                    leaf oper-state {
                        type types-sros:oper-state;
                        description
                            "The operational state of the AA interface.
                             When the value is 'down' the AA interface is not active.";
                    }

                    ///nokia-state/state/application-assurance/group/aa-interface/sap/isa-address
                    leaf isa-address {
                        type types-sros:ip-prefix-with-host-bits;
                        description "The IP address of the ISA's AA interface.";
                    }

                } // list sap

            } // container aa-interface

            ///nokia-state/state/application-assurance/group/certificate-profile
            list certificate-profile {
                key "cert-prof-name";
                description "Enter the certificate-profile list instance";

                ///nokia-state/state/application-assurance/group/certificate-profile/cert-prof-name
                leaf cert-prof-name {
                    type types-sros:named-item;
                    description "AA group certificate profile name";
                }

                ///nokia-state/state/application-assurance/group/certificate-profile/oper-state
                leaf oper-state {
                    type types-sros:oper-state;
                    description "The operational status of the certificate profile.";
                }

                ///nokia-state/state/application-assurance/group/certificate-profile/oper-flags
                leaf oper-flags {
                    type bits {
                        bit profile-admin-down             { position 0; }
                        bit invalid-cert-file              { position 1; }
                        bit invalid-cert-file-public-key   { position 2; }
                        bit invalid-public-key-modulus     { position 3; }
                        bit invalid-public-key-exponent    { position 4; }
                        bit unsupported-cert-file-algorithm { position 5; }
                    }
                    description "The operational condition of the certificate profile.";
                }

                ///nokia-state/state/application-assurance/group/certificate-profile/reference-count
                leaf reference-count {
                    type uint32;
                    description "The number of clients configured with this profile.";
                }

            } // list certificate-profile

            ///nokia-state/state/application-assurance/group/cflowd
            container cflowd {
                description "Enable the cflowd context";

                ///nokia-state/state/application-assurance/group/cflowd/collector
                list collector {
                    key "ip-address port";
                    description "Enter the collector list instance";

                    ///nokia-state/state/application-assurance/group/cflowd/collector/ip-address
                    leaf ip-address {
                        type types-sros:ip-unicast-address;
                        description "IP address of the remote cflowd collector host";
                    }

                    ///nokia-state/state/application-assurance/group/cflowd/collector/port
                    leaf port {
                        type types-sros:tcp-udp-port-non-zero;
                        description "Cflowd collector host port number";
                    }

                    ///nokia-state/state/application-assurance/group/cflowd/collector/oper-state
                    leaf oper-state {
                        type types-sros:oper-state;
                        description
                            "The operational state for the collector.
                             When 'down' no application assurance Cflowd records
                             are sent to the collector.";
                    }

                    ///nokia-state/state/application-assurance/group/cflowd/collector/statistics
                    container statistics {
                        description "Enter the statistics context";

                        ///nokia-state/state/application-assurance/group/cflowd/collector/statistics/records-sent
                        leaf records-sent {
                            type yang:counter64;
                            description "The total number of flow records sent to the remote Cflowd collector.";
                        }

                    } // container statistics

                } // list collector

                ///nokia-state/state/application-assurance/group/cflowd/direct-export
                container direct-export {
                    description "Enter the direct-export context";

                    ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector
                    list collector {
                        key "collector-id";
                        description "Enter the collector list instance";

                        ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/collector-id
                        leaf collector-id {
                            type uint32 {
                                range "1..65535";
                            }
                            description "Cflowd direct export collector ID";
                        }

                        ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address
                        list address {
                            key "ip-address port";
                            description "Enter the address list instance";

                            ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/ip-address
                            leaf ip-address {
                                type types-sros:ip-unicast-address;
                                description "IP address of the remote cflowd collector host";
                            }

                            ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/port
                            leaf port {
                                type types-sros:tcp-udp-port-non-zero;
                                description "Cflowd collector host port number";
                            }

                            ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/oper-state
                            leaf oper-state {
                                type types-sros:oper-state;
                                description
                                    "The operational state for the Cflowd direct export collector address.
                                     When 'down' no application assurance Cflowd records are sent to the collector.";
                            }

                            ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/esa
                            list esa {
                                key "esa-id vm";
                                description "Enter the esa list instance";

                                ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/esa/esa-id
                                leaf esa-id {
                                    type types-esa:esa-id;
                                    description "ESA ID";
                                }

                                ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/esa/vm
                                leaf vm {
                                    type types-esa:vm-id;
                                    description "VM ID within the ESA";
                                }

                                ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/esa/oper-state
                                leaf oper-state {
                                    type types-sros:oper-state;
                                    description "The operational status of the of the cflowd direct export collector.";
                                }

                                ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/esa/records-sent
                                leaf records-sent {
                                    type yang:counter64;
                                    description "The total number of flow records sent to the remote Cflowd direct export collector.";
                                }

                            } // list esa

                            ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/isa
                            list isa {
                                key "mda-id";
                                description "Enter the isa list instance";

                                ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/isa/mda-id
                                leaf mda-id {
                                    type types-isa:slot-mda;
                                    description "MDA/ISA ID";
                                }

                                ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/isa/oper-state
                                leaf oper-state {
                                    type types-sros:oper-state;
                                    description "The operational status of the of the cflowd direct export collector.";
                                }

                                ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/isa/records-sent
                                leaf records-sent {
                                    type yang:counter64;
                                    description "The total number of flow records sent to the remote Cflowd direct export collector.";
                                }

                            } // list isa

                            ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/summary
                            container summary {
                                description "Enter the summary context";

                                ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/summary/oper-state
                                leaf oper-state {
                                    type types-sros:oper-state;
                                    description "The operational status of the of the cflowd direct export collector.";
                                }

                                ///nokia-state/state/application-assurance/group/cflowd/direct-export/collector/address/summary/records-sent
                                leaf records-sent {
                                    type yang:counter64;
                                    description "The total number of flow records sent to the remote Cflowd direct export collector.";
                                }

                            } // container summary

                        } // list address

                    } // list collector

                } // container direct-export

            } // container cflowd

            ///nokia-state/state/application-assurance/group/dns-ip-cache
            list dns-ip-cache {
                key "dns-ip-cache-name";
                description "Enter the dns-ip-cache list instance";

                ///nokia-state/state/application-assurance/group/dns-ip-cache/dns-ip-cache-name
                leaf dns-ip-cache-name {
                    type types-sros:named-item;
                    description "DNS IP cache name within the AA group";
                }

                ///nokia-state/state/application-assurance/group/dns-ip-cache/esa
                list esa {
                    key "esa-id vm";
                    description "Enter the esa list instance";

                    ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/esa-id
                    leaf esa-id {
                        type types-esa:esa-id;
                        description "ESA ID";
                    }

                    ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/vm
                    leaf vm {
                        type types-esa:vm-id;
                        description "VM ID within the ESA";
                    }

                    ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/dns
                    container dns {
                        description "Enter the dns context";

                        ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/dns/statistics
                        container statistics {
                            description "Enter the statistics context";

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/dns/statistics/total-responses
                            leaf total-responses {
                                type yang:counter64;
                                description
                                    "The number of DNS responses that have been compared to the DNS domain expressions
                                     associated with this cache.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/dns/statistics/domain-name-match
                            leaf domain-name-match {
                                type yang:counter64;
                                description
                                    "The number of DNS responses for domains that matched a DNS domain expression associated
                                     with this cache.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/dns/statistics/domain-name-and-server-match
                            leaf domain-name-and-server-match {
                                type yang:counter64;
                                description
                                    "The number of DNS responses whose domain matched a DNS domain expression and
                                     server IP address matched a trusted DNS server address associated with this cache.
                                     In the absence of a configured trusted DNS server address, all DNS servers are trusted.";
                            }

                        } // container statistics

                    } // container dns

                    ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/cache
                    container cache {
                        description "Enter the cache context";

                        ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/cache/threshold-alarm-clear
                        leaf threshold-alarm-clear {
                            type boolean;
                            description
                                "Indicates the state of the threshold alarm for this cache. A value of 'true' indicates the
                                 alarm is clear, and a value of 'false' indicates the alarm is set.";
                        }

                        ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/cache/statistics
                        container statistics {
                            description "Enter the statistics context";

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/cache/statistics/total-addresses-added
                            leaf total-addresses-added {
                                type yang:counter64;
                                description "The total number of addresses that have been added to this cache.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/cache/statistics/total-addresses-removed
                            leaf total-addresses-removed {
                                type yang:counter64;
                                description "The total number of addresses that have been removed from this cache.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/cache/statistics/cache-full-error
                            leaf cache-full-error {
                                type yang:counter64;
                                description "The number of times an entry could not be added because the cache was full.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/cache/statistics/static-address-match
                            leaf static-address-match {
                                type yang:counter64;
                                description "The number of times an IP address lookup in the cache matched a statically configured address.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/cache/statistics/cached-address-match
                            leaf cached-address-match {
                                type yang:counter64;
                                description "The number of times an IP address lookup in the cache matched a dynamically learned address.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/esa/cache/statistics/address-miss
                            leaf address-miss {
                                type yang:counter64;
                                description "The number of times an IP address lookup in this cache was unsuccessful.";
                            }

                        } // container statistics

                    } // container cache

                } // list esa

                ///nokia-state/state/application-assurance/group/dns-ip-cache/isa
                list isa {
                    key "mda-id";
                    description "Enter the isa list instance";

                    ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/mda-id
                    leaf mda-id {
                        type types-isa:slot-mda;
                        description "MDA/ISA ID";
                    }

                    ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/dns
                    container dns {
                        description "Enter the dns context";

                        ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/dns/statistics
                        container statistics {
                            description "Enter the statistics context";

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/dns/statistics/total-responses
                            leaf total-responses {
                                type yang:counter64;
                                description
                                    "The number of DNS responses that have been compared to the DNS domain expressions
                                     associated with this cache.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/dns/statistics/domain-name-match
                            leaf domain-name-match {
                                type yang:counter64;
                                description
                                    "The number of DNS responses for domains that matched a DNS domain expression associated
                                     with this cache.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/dns/statistics/domain-name-and-server-match
                            leaf domain-name-and-server-match {
                                type yang:counter64;
                                description
                                    "The number of DNS responses whose domain matched a DNS domain expression and
                                     server IP address matched a trusted DNS server address associated with this cache.
                                     In the absence of a configured trusted DNS server address, all DNS servers are trusted.";
                            }

                        } // container statistics

                    } // container dns

                    ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/cache
                    container cache {
                        description "Enter the cache context";

                        ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/cache/threshold-alarm-clear
                        leaf threshold-alarm-clear {
                            type boolean;
                            description
                                "Indicates the state of the threshold alarm for this cache. A value of 'true' indicates the
                                 alarm is clear, and a value of 'false' indicates the alarm is set.";
                        }

                        ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/cache/statistics
                        container statistics {
                            description "Enter the statistics context";

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/cache/statistics/total-addresses-added
                            leaf total-addresses-added {
                                type yang:counter64;
                                description "The total number of addresses that have been added to this cache.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/cache/statistics/total-addresses-removed
                            leaf total-addresses-removed {
                                type yang:counter64;
                                description "The total number of addresses that have been removed from this cache.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/cache/statistics/cache-full-error
                            leaf cache-full-error {
                                type yang:counter64;
                                description "The number of times an entry could not be added because the cache was full.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/cache/statistics/static-address-match
                            leaf static-address-match {
                                type yang:counter64;
                                description "The number of times an IP address lookup in the cache matched a statically configured address.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/cache/statistics/cached-address-match
                            leaf cached-address-match {
                                type yang:counter64;
                                description "The number of times an IP address lookup in the cache matched a dynamically learned address.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/isa/cache/statistics/address-miss
                            leaf address-miss {
                                type yang:counter64;
                                description "The number of times an IP address lookup in this cache was unsuccessful.";
                            }

                        } // container statistics

                    } // container cache

                } // list isa

                ///nokia-state/state/application-assurance/group/dns-ip-cache/summary
                container summary {
                    description "Enter the summary context";

                    ///nokia-state/state/application-assurance/group/dns-ip-cache/summary/dns
                    container dns {
                        description "Enter the dns context";

                        ///nokia-state/state/application-assurance/group/dns-ip-cache/summary/dns/statistics
                        container statistics {
                            description "Enter the statistics context";

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/summary/dns/statistics/total-responses
                            leaf total-responses {
                                type yang:counter64;
                                description
                                    "The number of DNS responses that have been compared to the DNS domain expressions
                                     associated with this cache.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/summary/dns/statistics/domain-name-match
                            leaf domain-name-match {
                                type yang:counter64;
                                description
                                    "The number of DNS responses for domains that matched a DNS domain expression associated
                                     with this cache.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/summary/dns/statistics/domain-name-and-server-match
                            leaf domain-name-and-server-match {
                                type yang:counter64;
                                description
                                    "The number of DNS responses whose domain matched a DNS domain expression and
                                     server IP address matched a trusted DNS server address associated with this cache.
                                     In the absence of a configured trusted DNS server address, all DNS servers are trusted.";
                            }

                        } // container statistics

                    } // container dns

                    ///nokia-state/state/application-assurance/group/dns-ip-cache/summary/cache
                    container cache {
                        description "Enter the cache context";

                        ///nokia-state/state/application-assurance/group/dns-ip-cache/summary/cache/threshold-alarm-clear
                        leaf threshold-alarm-clear {
                            type boolean;
                            description
                                "Indicates the state of the threshold alarm for this cache. A value of 'true' indicates the
                                 alarm is clear, and a value of 'false' indicates the alarm is set.";
                        }

                        ///nokia-state/state/application-assurance/group/dns-ip-cache/summary/cache/statistics
                        container statistics {
                            description "Enter the statistics context";

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/summary/cache/statistics/total-addresses-added
                            leaf total-addresses-added {
                                type yang:counter64;
                                description "The total number of addresses that have been added to this cache.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/summary/cache/statistics/total-addresses-removed
                            leaf total-addresses-removed {
                                type yang:counter64;
                                description "The total number of addresses that have been removed from this cache.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/summary/cache/statistics/cache-full-error
                            leaf cache-full-error {
                                type yang:counter64;
                                description "The number of times an entry could not be added because the cache was full.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/summary/cache/statistics/static-address-match
                            leaf static-address-match {
                                type yang:counter64;
                                description "The number of times an IP address lookup in the cache matched a statically configured address.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/summary/cache/statistics/cached-address-match
                            leaf cached-address-match {
                                type yang:counter64;
                                description "The number of times an IP address lookup in the cache matched a dynamically learned address.";
                            }

                            ///nokia-state/state/application-assurance/group/dns-ip-cache/summary/cache/statistics/address-miss
                            leaf address-miss {
                                type yang:counter64;
                                description "The number of times an IP address lookup in this cache was unsuccessful.";
                            }

                        } // container statistics

                    } // container cache

                } // container summary

            } // list dns-ip-cache


            ///nokia-state/state/application-assurance/group/ip-identification-assist
            container ip-identification-assist {
                description "Enter the ip-identification-assist context";

                ///nokia-state/state/application-assurance/group/ip-identification-assist/esa
                list esa {
                    key "esa-id vm";
                    description "Enter the esa list instance";

                    ///nokia-state/state/application-assurance/group/ip-identification-assist/esa/esa-id
                    leaf esa-id {
                        type types-esa:esa-id;
                        description "ESA ID";
                    }

                    ///nokia-state/state/application-assurance/group/ip-identification-assist/esa/vm
                    leaf vm {
                        type types-esa:vm-id;
                        description "VM ID within the ESA";
                    }

                    ///nokia-state/state/application-assurance/group/ip-identification-assist/esa/statistics
                    container statistics {
                        description "Enter the statistics context";

                        ///nokia-state/state/application-assurance/group/ip-identification-assist/esa/statistics/ip-entries
                        container ip-entries {
                            description "Enter the ip-entries context";

                            ///nokia-state/state/application-assurance/group/ip-identification-assist/esa/statistics/ip-entries/active
                            leaf active {
                                type yang:gauge32;
                                description "The current number of active IP entries on the ISA-AA MDA(s).";
                            }

                            ///nokia-state/state/application-assurance/group/ip-identification-assist/esa/statistics/ip-entries/active-application
                            leaf active-application {
                                type yang:gauge32;
                                description
                                    "The current number of active IP entries that map to an
                                     application on the ISA-AA MDA(s).";
                            }

                            ///nokia-state/state/application-assurance/group/ip-identification-assist/esa/statistics/ip-entries/active-parent
                            leaf active-parent {
                                type yang:gauge32;
                                description
                                    "The current number of active IP entries that map to a
                                     parent application on the ISA-AA MDA(s).  This is due
                                     to the IP address being used by multiple applications
                                     that share the same parent application.";
                            }

                            ///nokia-state/state/application-assurance/group/ip-identification-assist/esa/statistics/ip-entries/active-conflict
                            leaf active-conflict {
                                type yang:gauge32;
                                description
                                    "The current number of active IP entries that do not provide
                                     a positive identification due to an IP conflict between multiple
                                     assisted applications that have different parent applications
                                     on the ISA-AA MDA(s).";
                            }

                            ///nokia-state/state/application-assurance/group/ip-identification-assist/esa/statistics/ip-entries/active-conflict-other
                            leaf active-conflict-other {
                                type yang:gauge32;
