                            leaf address-prefix-len {
                                type uint8;
                                description
                                    "If the next-hop is a tunnel, this has the prefix length for
                                     the tunnel far-end address. Use this in conjunction with the
                                     information from the address leaf to lookup the tunnel in
                                     the tunnel-fib.";
                            }

                        } // container primary-next-hop

                        ///nokia-state/state/router/route-fib/ipv4/route/next-hop-group/backup-next-hop
                        container backup-next-hop {
                            description "Enter the backup-next-hop context";

                            ///nokia-state/state/router/route-fib/ipv4/route/next-hop-group/backup-next-hop/is-indirect
                            leaf is-indirect {
                                type boolean;
                                description
                                    "If true, this indicates the next-hop is an indirect next-hop IP.
