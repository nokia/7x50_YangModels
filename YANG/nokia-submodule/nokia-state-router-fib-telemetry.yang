    grouping state-router-fib-telemetry-route-fib {
        ///nokia-state/state/router/route-fib
        container route-fib {
            description "Enter the route-fib context";

            ///nokia-state/state/router/route-fib/ipv4
            container ipv4 {
                description "Enter the ipv4 context";

                ///nokia-state/state/router/route-fib/ipv4/route
                list route {
                    key "prefix";
                    description "Enter the route list instance";

                    ///nokia-state/state/router/route-fib/ipv4/route/prefix
                    leaf prefix {
                        type types-sros:ipv4-prefix;
                        description "Indicates the IPv4 prefix for this route FIB entry.";
                    }

                    ///nokia-state/state/router/route-fib/ipv4/route/last-updated
                    leaf last-updated {
                        type yang:date-and-time;
                        description "Time when route acknowledged as programmed on the line-cards.";
                    }

                    ///nokia-state/state/router/route-fib/ipv4/route/protocol
                    leaf protocol {
                        type types-router:protocol-type;
                        description "Identifies the routing protocol.";
                    }

                    ///nokia-state/state/router/route-fib/ipv4/route/next-hop-group
                    list next-hop-group {
                        key "index";
                        description "Enter the next-hop-group list instance";

                        ///nokia-state/state/router/route-fib/ipv4/route/next-hop-group/index
                        leaf index {
                            type uint32;
                            description "Index of next-hop group.";
                        }

                        ///nokia-state/state/router/route-fib/ipv4/route/next-hop-group/primary-next-hop
                        container primary-next-hop {
                            description "Enter the primary-next-hop context";

                            ///nokia-state/state/router/route-fib/ipv4/route/next-hop-group/primary-next-hop/is-indirect
                            leaf is-indirect {
                                type boolean;
                                description
                                    "If true, this indicates the next-hop is an indirect next-hop IP.
                                     An additional route lookup will have to be made to map the indirect
                                     IP address to the resolving IP address. The 'if-index' leaf will be
                                     not be provided in this case.";
                            }

                            ///nokia-state/state/router/route-fib/ipv4/route/next-hop-group/primary-next-hop/is-tunnel
                            leaf is-tunnel {
                                type boolean;
                                description
                                    "If true, this indicates indicates the next-hop is a tunnel destination
                                     (the 'address' leaf is a far-end tunnel destination). The 'if-index'
                                     leaf will be not be provided in this case.";
                            }

                            ///nokia-state/state/router/route-fib/ipv4/route/next-hop-group/primary-next-hop/is-backup
                            leaf is-backup {
                                type boolean;
                                description
                                    "If true, this indicates the next-hop is a BGP backup next-hop. The BGP
                                     backup next-hop is used only if all the primary next-hops are down.";
                            }

                            ///nokia-state/state/router/route-fib/ipv4/route/next-hop-group/primary-next-hop/if-index
                            leaf if-index {
                                type uint32;
                                description
                                    "This is the index of the egress interface where the
                                     next-hop resides. If this is not provided, is it not
                                     valid for the type of next-hop.

                                     If the next-hop is not an indirect or tunneled next-hop
                                     and both the 'if-index' and the 'address' leafs are not
                                     present, the next-hop is a blackhole next-hop.";
                            }

                            ///nokia-state/state/router/route-fib/ipv4/route/next-hop-group/primary-next-hop/if-vrtr-id
                            leaf if-vrtr-id {
                                type uint32;
                                description
                                    "Indicates the virtual router instance of the egress interface
                                     defined by 'if-index'.";
                            }

                            ///nokia-state/state/router/route-fib/ipv4/route/next-hop-group/primary-next-hop/address
                            leaf address {
                                type types-sros:ip-address;
                                description
                                    "Indicates the next-hop IP address of the route entry.

                                     The 'address' is a tunnel or SDP far-end IP if the 'is-tunnel'
                                     leaf is true.

                                     The 'address' is an indirect IP if the 'is-indirect' leaf
                                     is true.

                                     If the next-hop is not an indirect or tunneled next-hop
                                     and both the 'if-index' and the 'address' leafs are not
                                     present, the next-hop is a blackhole next-hop.";
                            }

                            ///nokia-state/state/router/route-fib/ipv4/route/next-hop-group/primary-next-hop/tunnel-id
                            leaf tunnel-id {
                                type uint32;
                                description
                                    "This is provided if the 'is-tunnel' leaf is true and the
                                     tunnel is not an LDP/BGP/UDP/GRE/SR tunnel.

                                     This indicates the next-hop tunnel ID or SDP ID
                                     associated with the tunnel.";
                            }

                            ///nokia-state/state/router/route-fib/ipv4/route/next-hop-group/primary-next-hop/tunnel-type
                            leaf tunnel-type {
                                type types-router:tunnel-type;
                                description "Indicates the tunnel owner if the next-hop is a tunnel far-end.";
