    grouping state-router-fib-telemetry-tunnel-fib {
        ///nokia-state/state/router/tunnel-fib
        container tunnel-fib {
            description "Enter the tunnel-fib context";

            ///nokia-state/state/router/tunnel-fib/ipv4
            container ipv4 {
                description "Enter the ipv4 context";

                ///nokia-state/state/router/tunnel-fib/ipv4/tunnel
                list tunnel {
                    key "prefix index";
                    description "Enter the tunnel list instance";

                    ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/prefix
                    leaf prefix {
                        type types-sros:ipv4-prefix;
                        description "The value of this object specifies the IPv4 prefix.";
                    }

                    ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/index
                    leaf index {
                        type uint64;
                        description "This is the index of the next tunnel to the same destination";
                    }

                    ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/preference
                    leaf preference {
                        type uint8;
                        description
                            "Indicates the priority of this tunnel versus the other tunnels
                             from different sources.

                             Lowest value signifies the most preferred entry in the table.";
                    }

                    ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/protocol
                    leaf protocol {
                        type types-router:protocol-type;
                        description "Indicates the owner of this tunnel.";
                    }

                    ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/instance
                    leaf instance {
                        type uint32;
                        description "Indicates an instance of the tunnel of the type indicated by protocol.";
                    }

                    ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/tunnel-id
                    leaf tunnel-id {
                        type types-sros:tunnel-id;
                        description "Indicates the global tunnel ID that represents a particular tunnel.";
                    }

                    ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/last-updated
                    leaf last-updated {
                        type yang:date-and-time;
                        description "Indicates when this entry was last updated";
                    }

                    ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/next-hop-group
                    list next-hop-group {
                        key "index";
                        description "Enter the next-hop-group list instance";

                        ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/next-hop-group/index
                        leaf index {
                            type uint32;
                            description "Index of next-hop group.";
                        }

                        ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/next-hop-group/group-id
                        leaf group-id {
                            type uint32;
                            description
                                "Present only for RIB-API provided next-hop groups and is the ID
                                 provided for the next-hop group in the RIB-API call.";
                        }

                        ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/next-hop-group/primary-active
                        leaf primary-active {
                            type boolean;
                            description
                                "Flag indicating if the primary next-hop is currently active (used for
                                 forwarding traffic). If the primary is not active, then the backup
                                 is active. This flag is useful only if a backup next-hop is present.";
                        }

                        ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/next-hop-group/weight
                        leaf weight {
                            type uint32;
                            description "This is the normalized weight associated with this next-hop.";
                        }

                        ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/next-hop-group/primary-next-hop
                        container primary-next-hop {
                            description "Enter the primary-next-hop context";

                            ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/next-hop-group/primary-next-hop/is-tunnel
                            leaf is-tunnel {
                                type boolean;
                                description
                                    "If true, this indicates indicates the next-hop is a tunnel destination
                                     (the 'address' leaf is a far-end tunnel destination). The 'if-index'
                                     leaf will be not be provided in this case.";
                            }

                            ///nokia-state/state/router/tunnel-fib/ipv4/tunnel/next-hop-group/primary-next-hop/if-index
                            leaf if-index {
                                type uint32;
                                description
                                    "This is the index of the egress interface where the
                                     next-hop resides. If this is not provided, is it not
