submodule nokia-state-filter {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-card               { prefix "types-card"; }
    import nokia-types-filter             { prefix "types-filter"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-state-filter-ip-exception;
    include nokia-state-openflow;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for line card ACL filters on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-filter YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-05-31";

    grouping state-filter {
        ///nokia-state/state/filter
        container filter {
            description "Enter the filter context";

            ///nokia-state/state/filter/redirect-policy
            list redirect-policy {
                key "redirect-policy-name";
                description "Enter the redirect-policy list instance";

                ///nokia-state/state/filter/redirect-policy/redirect-policy-name
                leaf redirect-policy-name {
                    type types-sros:named-item;
                    description "Redirect policy name";
                }

                ///nokia-state/state/filter/redirect-policy/active-destination
                leaf active-destination {
                    type types-sros:ip-address;
                    description
                        "The value indicates the IP address of the active
                         destination. A value of 0 indicates that there is currently
                         no active destination.
                         The value must be consistent with the IP version value of
                         the associated act-dest-ip-address type object.";
                }

                ///nokia-state/state/filter/redirect-policy/best-destination
                leaf best-destination {
                    type types-sros:ip-address;
                    description
                        "The value of the object indicates the IP address of available destination
                         with the highest priority.";
                }

                ///nokia-state/state/filter/redirect-policy/sticky-dest-hold-remain
                leaf sticky-dest-hold-remain {
                    type int32 {
                        range "0..65535";
                    }
                    units "seconds";
                    description "Hold-remain for stickiness to take effect.";
                }

                ///nokia-state/state/filter/redirect-policy/applied
                leaf applied {
                    type boolean;
                    description "Indicates whether the redirect-policy is applied.";
                }

                ///nokia-state/state/filter/redirect-policy/destination
                list destination {
                    key "destination-address";
                    description "Enter the destination list instance";

                    ///nokia-state/state/filter/redirect-policy/destination/destination-address
                    leaf destination-address {
                        type types-sros:ip-unicast-address;
                        description "IP address and type of destination";
                    }

                    ///nokia-state/state/filter/redirect-policy/destination/oper-priority
                    leaf oper-priority {
                        type uint32 {
                            range "0..255";
                        }
                        description
                            "The value of the this object indicates the current dynamic
                             priority of this destination.

                             It is derived from the base priority indicated by the value
                             of the object priority and results of tests assigned
                             to the destination.

                             A destination with the highest operational priority across multiple
                             destinations in a Redirect Policy is used as the preferred
                             destination.

                             If operational state of the destination is 'out-of-service'
                             then the value of this leaf is '0'. ";
                    }

                    ///nokia-state/state/filter/redirect-policy/destination/oper-state
                    leaf oper-state {
                        type types-sros:oper-state;
                        description
                            "The value of the this object indicates the operational
                             state of the destination.";
                    }

                    ///nokia-state/state/filter/redirect-policy/destination/ping-test
                    container ping-test {
                        description "Enable the ping-test context";

                        ///nokia-state/state/filter/redirect-policy/destination/ping-test/hold-down-remain
                        leaf hold-down-remain {
                            type uint32 {
                                range "0..86400";
                            }
                            units "seconds";
                            description
                                "The value of this object indicates the amount of time in seconds that the system
                                 will remain in held down state before being used again.";
                        }

                        ///nokia-state/state/filter/redirect-policy/destination/ping-test/last-action
                        leaf last-action {
                            type types-filter:redirect-policy-test-last-action;
                            description
                                "The value of this object maintains impact that the last test probe made on the
                                 operational status of the destination as maintained in 'redirect-policy destination
                                 shutdown'.

                                 If the last action was 'disable-destination', the destination may not be used for
                                 hold-down seconds.";
                        }

                        ///nokia-state/state/filter/redirect-policy/destination/ping-test/last-action-time
                        leaf last-action-time {
                            type types-sros:date-time;
                            description
                                "The value of this object maintains the time stamp when this test received
                                 a response for a probe sent out.";
                        }

                    } // container ping-test

                    ///nokia-state/state/filter/redirect-policy/destination/unicast-rt-test
                    container unicast-rt-test {
                        description "Enable the unicast-rt-test context";

                        ///nokia-state/state/filter/redirect-policy/destination/unicast-rt-test/last-action
                        leaf last-action {
                            type types-filter:redirect-policy-test-last-action;
                            description
                                "The value of this object indicates an impact that the
                                 last message from RTM module had on the value of the
                                 object 'redirect-policy destination oper-state'.";
                        }

                        ///nokia-state/state/filter/redirect-policy/destination/unicast-rt-test/last-action-time
                        leaf last-action-time {
                            type types-sros:date-time;
                            description
                                "The value of this object indicates the time stamp
                                 when the last update from RTM module for this
                                 destination was received.";
                        }

                    } // container unicast-rt-test

                } // list destination

            } // list redirect-policy

            ///nokia-state/state/filter/redirect-policy-binding
            list redirect-policy-binding {
                key "binding-name";
                description "Enter the redirect-policy-binding list instance";

                ///nokia-state/state/filter/redirect-policy-binding/binding-name
                leaf binding-name {
                    type types-sros:named-item;
                    description "Binding name";
                }

                ///nokia-state/state/filter/redirect-policy-binding/oper-state
                leaf oper-state {
                    type types-sros:oper-state;
                    description
                        "The value of the this object indicates the operational state of the redirect policy
                         binding.";
                }

                ///nokia-state/state/filter/redirect-policy-binding/redirect-policy
                list redirect-policy {
                    key "redirect-policy-name";
                    description "Enter the redirect-policy list instance";

                    ///nokia-state/state/filter/redirect-policy-binding/redirect-policy/redirect-policy-name
                    leaf redirect-policy-name {
                        type leafref {
                            path "../../../redirect-policy/redirect-policy-name";
                        }
                        description "Redirect policy name";
                    }

                    ///nokia-state/state/filter/redirect-policy-binding/redirect-policy/destination
                    list destination {
                        key "destination-address";
                        description "Add a list entry for destination";

                        ///nokia-state/state/filter/redirect-policy-binding/redirect-policy/destination/destination-address
                        leaf destination-address {
                            type leafref {
                                path "../../../../redirect-policy[redirect-policy-name=current()/../../redirect-policy-name]/destination/destination-address";
                            }
                            description "IP address of redirect policy destination to binding";
                        }

                        ///nokia-state/state/filter/redirect-policy-binding/redirect-policy/destination/own-oper-state
                        leaf own-oper-state {
                            type types-sros:oper-state;
                            description
                                "The value of the this object indicates the operational state of the redirect policy
                                 binding destination (based on its own tests only).";
                        }

                    } // list destination

                } // list redirect-policy

            } // list redirect-policy-binding

            ///nokia-state/state/filter/log
            list log {
                key "log-id";
                description "Enter the log list instance";

                ///nokia-state/state/filter/log/log-id
                leaf log-id {
                    type types-filter:filter-log-id;
                    description "Filter log identifier";
                }

                ///nokia-state/state/filter/log/destination
                container destination {
                    description "Enter the destination context";

                    ///nokia-state/state/filter/log/destination/destination
                    choice destination {
                        ///nokia-state/state/filter/log/destination/destination/memory
                        case memory {

                            ///nokia-state/state/filter/log/destination/destination/memory/memory
                            container memory {
                                description "Enter the memory context";

                                ///nokia-state/state/filter/log/destination/destination/memory/memory/num-entries
                                leaf num-entries {
                                    type uint32;
                                    description "Indicates the actual number of logged entries.";
                                }

                                ///nokia-state/state/filter/log/destination/destination/memory/memory/wrapped
                                leaf wrapped {
                                    type boolean;
                                    description "The sequence-number for this log has wrapped.";
                                }

                                ///nokia-state/state/filter/log/destination/destination/memory/memory/entry
                                list entry {
                                    key "id";
                                    description "Enter the entry list instance";

                                    ///nokia-state/state/filter/log/destination/destination/memory/memory/entry/id
                                    leaf id {
                                        type uint32;
                                        description "Message identifier.";
                                    }

                                    ///nokia-state/state/filter/log/destination/destination/memory/memory/entry/message
                                    leaf message {
                                        type string {
                                            length "0..661";
                                        }
                                        description "Message content.";
                                    }

                                } // list entry

                            } // container memory

                        }
                        ///nokia-state/state/filter/log/destination/destination/syslog
                        case syslog {

                            ///nokia-state/state/filter/log/destination/destination/syslog/syslog
                            container syslog {
                                description "Enter the syslog context";

                                ///nokia-state/state/filter/log/destination/destination/syslog/syslog/summary
                                container summary {
                                    description "Enter the summary context";

                                    ///nokia-state/state/filter/log/destination/destination/syslog/syslog/summary/num-entries
                                    leaf num-entries {
                                        type uint32;
                                        description "Indicates the number of log messages received.";
                                    }

                                    ///nokia-state/state/filter/log/destination/destination/syslog/syslog/summary/num-entries-arp
                                    leaf num-entries-arp {
                                        type uint32;
                                        description "Indicates the number of only ARP frames among filter logs.";
                                    }

                                    ///nokia-state/state/filter/log/destination/destination/syslog/syslog/summary/entry
                                    list entry {
                                        key "filter-type id";
                                        description "Enter the entry list instance";

                                        ///nokia-state/state/filter/log/destination/destination/syslog/syslog/summary/entry/filter-type
                                        leaf filter-type {
                                            type enumeration {
                                                enum ip                             { value 0; }
                                                enum ipv6                           { value 1; }
                                                enum mac                            { value 2; }
                                            }
                                            description "Filter type.";
                                        }

                                        ///nokia-state/state/filter/log/destination/destination/syslog/syslog/summary/entry/id
                                        leaf id {
                                            type uint32 {
                                                range "0..4";
                                            }
                                            description "Entry identifier.";
                                        }

                                        ///nokia-state/state/filter/log/destination/destination/syslog/syslog/summary/entry/address
                                        leaf address {
                                            type union {
                                                type types-sros:ip-address;
                                                type types-sros:ipv6-address;
                                                type yang:mac-address;
                                            }
                                            description "Source or destination address.";
                                        }

                                        ///nokia-state/state/filter/log/destination/destination/syslog/syslog/summary/entry/count
                                        leaf count {
                                            type uint32;
                                            description "Number of logs for the source/destination address.";
                                        }

                                    } // list entry

                                } // container summary

                            } // container syslog

                        }
                    }
                } // container destination

            } // list log

            ///nokia-state/state/filter/log-instance
            container log-instance {
                description "Enter the log-instance context";

                ///nokia-state/state/filter/log-instance/max-instances
                leaf max-instances {
                    type uint32;
                    description "Indicates the maximum allowed filter log instances on the system.";
                }

                ///nokia-state/state/filter/log-instance/num-instances
                leaf num-instances {
                    type uint32;
                    description "Indicates the actual number of filter log instances on the system.";
                }

            } // container log-instance

            ///nokia-state/state/filter/match-list
            container match-list {
                description "Enter the match-list context";

                ///nokia-state/state/filter/match-list/ip-prefix-list
                list ip-prefix-list {
                    key "prefix-list-name";
                    description "Enter the ip-prefix-list list instance";

                    ///nokia-state/state/filter/match-list/ip-prefix-list/prefix-list-name
                    leaf prefix-list-name {
                        type types-sros:named-item;
                        description "IP prefix list name";
                    }

                    ///nokia-state/state/filter/match-list/ip-prefix-list/apply-path
                    container apply-path {
                        description "Enter the apply-path context";

                        ///nokia-state/state/filter/match-list/ip-prefix-list/apply-path/bgp-peers
                        list bgp-peers {
                            key "criterion-index";
                            description "Enter the bgp-peers list instance";

                            ///nokia-state/state/filter/match-list/ip-prefix-list/apply-path/bgp-peers/criterion-index
                            leaf criterion-index {
                                type uint32 {
                                    range "1..255";
                                }
                                description "BGP peers auto-generation configuration index";
                            }

                            ///nokia-state/state/filter/match-list/ip-prefix-list/apply-path/bgp-peers/prefix
                            list prefix {
                                key "prefix";
                                description "Enter the prefix list instance";

                                ///nokia-state/state/filter/match-list/ip-prefix-list/apply-path/bgp-peers/prefix/prefix
                                leaf prefix {
                                    type types-sros:ipv4-prefix;
                                    description "prefix.";
                                }

                            } // list prefix

                        } // list bgp-peers



                    } // container apply-path

                } // list ip-prefix-list

                ///nokia-state/state/filter/match-list/ipv6-prefix-list
                list ipv6-prefix-list {
                    key "prefix-list-name";
                    description "Enter the ipv6-prefix-list list instance";

                    ///nokia-state/state/filter/match-list/ipv6-prefix-list/prefix-list-name
                    leaf prefix-list-name {
                        type types-sros:named-item;
                        description "IP prefix list name";
                    }

                    ///nokia-state/state/filter/match-list/ipv6-prefix-list/apply-path
                    container apply-path {
                        description "Enter the apply-path context";

                        ///nokia-state/state/filter/match-list/ipv6-prefix-list/apply-path/bgp-peers
                        list bgp-peers {
                            key "criterion-index";
                            description "Enter the bgp-peers list instance";

                            ///nokia-state/state/filter/match-list/ipv6-prefix-list/apply-path/bgp-peers/criterion-index
                            leaf criterion-index {
                                type uint32 {
                                    range "1..255";
                                }
                                description "BGP peers auto-generation configuration index";
                            }

                            ///nokia-state/state/filter/match-list/ipv6-prefix-list/apply-path/bgp-peers/prefix
                            list prefix {
                                key "prefix";
                                description "Enter the prefix list instance";

                                ///nokia-state/state/filter/match-list/ipv6-prefix-list/apply-path/bgp-peers/prefix/prefix
                                leaf prefix {
                                    type types-sros:ipv6-prefix;
                                    description "prefix.";
                                }

                            } // list prefix

                        } // list bgp-peers


                    } // container apply-path

                } // list ipv6-prefix-list

            } // container match-list

            ///nokia-state/state/filter/ip-filter
            list ip-filter {
                key "filter-name";
                description "Enter the ip-filter list instance";

                ///nokia-state/state/filter/ip-filter/filter-name
                leaf filter-name {
                    type types-filter:filter-name;
                    description "Filter name";
                }

                ///nokia-state/state/filter/ip-filter/origin
                leaf origin {
                    type enumeration {
                        enum config                         { value 0; }
                        enum flowspec                       { value 1; }
                        enum subscriber-mgmt                { value 2; }
                        enum openflow                       { value 4; }
                        enum vsd                            { value 5; }
                        enum tli                            { value 6; }
                        enum dynamic-services               { value 7; }
                    }
                    description "The protocol or mechanism that created this filter.";
                }

                ///nokia-state/state/filter/ip-filter/applied
                leaf applied {
                    type boolean;
                    description "Indicates whether the filter is applied.";
                }

                ///nokia-state/state/filter/ip-filter/num-entries
                leaf num-entries {
                    type uint32;
                    description "Number of entries in this filter.";
                }

                ///nokia-state/state/filter/ip-filter/oper-filter-id
                leaf oper-filter-id {
                    type uint32;
                    description
                        "The value of this object uniquely identifies a filter.
                         User-configured filters have values between (1..65535).
                         Other values are reserved for filters that are created
                         (auto-generated) by the system.";
                }

                ///nokia-state/state/filter/ip-filter/num-sub-entries
                leaf num-sub-entries {
                    type uint32;
                    description "Number of sub-entries.";
                }

                ///nokia-state/state/filter/ip-filter/num-cam-entries-fp2
                leaf num-cam-entries-fp2 {
                    type uint32;
                    description "Number of CAM entries required on FP2 or FP3 based line cards";
                }

                ///nokia-state/state/filter/ip-filter/num-cam-entries-fp4
                leaf num-cam-entries-fp4 {
                    type uint32;
                    description "Number of CAM entries required on FP4 (or higher) based line cards";
                }

                ///nokia-state/state/filter/ip-filter/subscriber-mgmt
                container subscriber-mgmt {
                    description "Enter the subscriber-mgmt context";

                    ///nokia-state/state/filter/ip-filter/subscriber-mgmt/host-specific-entry
                    container host-specific-entry {
                        description "Enter the host-specific-entry context";

                        ///nokia-state/state/filter/ip-filter/subscriber-mgmt/host-specific-entry/filter-rule
                        container filter-rule {
                            description "Enter the filter-rule context";

                            ///nokia-state/state/filter/ip-filter/subscriber-mgmt/host-specific-entry/filter-rule/num-inserted
                            leaf num-inserted {
                                type uint32;
                                description
                                    "Indicates how many host specific filter-rule entries are
                                     currently inserted in the filter on request of RADIUS/DIAMETER.";
                            }

                        } // container filter-rule

                        ///nokia-state/state/filter/ip-filter/subscriber-mgmt/host-specific-entry/credit-control
                        container credit-control {
                            description "Enter the credit-control context";

                            ///nokia-state/state/filter/ip-filter/subscriber-mgmt/host-specific-entry/credit-control/num-inserted
                            leaf num-inserted {
                                type uint32;
                                description
                                    "Indicates how many filter entries are currently inserted
                                     in the filter on request of credit control.";
                            }

                        } // container credit-control

                    } // container host-specific-entry

                    ///nokia-state/state/filter/ip-filter/subscriber-mgmt/shared-entry
                    container shared-entry {
                        description "Enter the shared-entry context";

                        ///nokia-state/state/filter/ip-filter/subscriber-mgmt/shared-entry/num-filters
                        leaf num-filters {
                            type uint32;
                            description
                                "Indicates how many filters are currently created based on
                                 subscriber management shared entries of this filter.";
                        }

                        ///nokia-state/state/filter/ip-filter/subscriber-mgmt/shared-entry/filter-rule
                        container filter-rule {
                            description "Enter the filter-rule context";

                            ///nokia-state/state/filter/ip-filter/subscriber-mgmt/shared-entry/filter-rule/num-inserted
                            leaf num-inserted {
                                type uint32;
                                description
                                    "The value of this object indicates how many host common filter-rule entries are
                                     currently inserted in the filter on request of RADIUS.";
                            }

                        } // container filter-rule

                        ///nokia-state/state/filter/ip-filter/subscriber-mgmt/shared-entry/pcc-rule
                        container pcc-rule {
                            description "Enter the pcc-rule context";

                            ///nokia-state/state/filter/ip-filter/subscriber-mgmt/shared-entry/pcc-rule/num-inserted
                            leaf num-inserted {
                                type uint32;
                                description
                                    "The value of this object indicates how many pcc-rule filter entries received from
                                     DIAMETER have been inserted in this filter.";
                            }

                        } // container pcc-rule

                    } // container shared-entry

                } // container subscriber-mgmt

                ///nokia-state/state/filter/ip-filter/entry
                list entry {
                    key "entry-id";
                    description "Enter the entry list instance";

                    ///nokia-state/state/filter/ip-filter/entry/entry-id
                    leaf entry-id {
                        type types-filter:entry-id;
                        description "ID for a match criteria and the corresponding action";
                    }

                    ///nokia-state/state/filter/ip-filter/entry/inserted-by
                    leaf inserted-by {
                        type enumeration {
                            enum fixed                          { value 0; }
                            enum filter-rule                    { value 1; }
                            enum credit-control                 { value 2; }
                            enum flowspec                       { value 3; }
                            enum li                             { value 4; }
                            enum embedded                       { value 5; }
                            enum shared-filter-rule             { value 6; }
                            enum openflow                       { value 7; }
                            enum pcc                            { value 8; }
                            enum vsd                            { value 9; }
                            enum tli                            { value 10; }
                            enum dynamic-services               { value 11; }
                        }
                        description "The protocol or mechanism that inserted this entry.";
                    }

                    ///nokia-state/state/filter/ip-filter/entry/num-sub-entries
                    leaf num-sub-entries {
                        type uint32;
                        description "Number of sub-entries.";
                    }

                    ///nokia-state/state/filter/ip-filter/entry/num-cam-entries-fp2
                    leaf num-cam-entries-fp2 {
                        type uint32;
                        description "Number of CAM entries required on FP2 or FP3 based line cards";
                    }

                    ///nokia-state/state/filter/ip-filter/entry/num-cam-entries-fp4
                    leaf num-cam-entries-fp4 {
                        type uint32;
                        description "Number of CAM entries required on FP4 (or higher) based line cards";
                    }

                    ///nokia-state/state/filter/ip-filter/entry/log-instantiated
                    leaf log-instantiated {
                        type boolean;
                        description
                            "This object indicates if the filter log for this filter
                             entry has been instantiated or not.";
                    }

                    ///nokia-state/state/filter/ip-filter/entry/downloaded-action
                    leaf downloaded-action {
                        type types-filter:filter-downloaded-action;
                        description
                            "The value of this object indicates the filter entry
                             action downloaded to IOM.";
                    }

                    ///nokia-state/state/filter/ip-filter/entry/pbr-down-action
                    leaf pbr-down-action {
                        type enumeration {
                            enum drop                           { value 1; }
                            enum forward                        { value 2; }
                        }
                        description
                            "Estimation of action performed if PBR target is down. This takes into
                             account configuration of PBR down action override, filter default action, and default
                             behavior of a filter action. But does not take into account where filter is applied
                             therefore performed action may differ at the end.";
                    }

                    ///nokia-state/state/filter/ip-filter/entry/pbr-down-action-reason
                    leaf pbr-down-action-reason {
                        type enumeration {
                            enum entry-default                  { value 1; }
                            enum filter-default                 { value 2; }
                            enum pbr-down-override              { value 3; }
                        }
                        description "The source of the estimated value of PBR down action.";
                    }

                    ///nokia-state/state/filter/ip-filter/entry/sticky-dest-hold-remain
                    leaf sticky-dest-hold-remain {
                        type int32 {
                            range "0..65535";
                        }
                        units "seconds";
                        description
                            "The value of this object indicates the remaining
                             time in seconds, until the best action will become
                             active and will be fixed.

                             Best action is:
                             - primary action if PBR target status of primary
                             action is up.
                             - secondary action if PBR target status of secondary
                             action is up and PBR target status of primary action
                             is down.
                             - none if both PBR targets are down.";
                    }

                    ///nokia-state/state/filter/ip-filter/entry/inserted-by-direction
                    leaf inserted-by-direction {
                        type enumeration {
                            enum ingress                        { value 0; }
                            enum egress                         { value 1; }
                        }
                        description "The direction specified in shared filter rule causing creation of the entry.";
                    }

                    ///nokia-state/state/filter/ip-filter/entry/embedding
                    container embedding {
                        description "Enter the embedding context";

                        ///nokia-state/state/filter/ip-filter/entry/embedding/origin
                        leaf origin {
                            type enumeration {
                                enum fixed                          { value 0; }
                                enum flowspec                       { value 3; }
                                enum openflow                       { value 7; }
                                enum vsd                            { value 9; }
                            }
                            description "Indicates the origin of the embedded filter which is the source of the entry embedded at this position.";
                        }

                        ///nokia-state/state/filter/ip-filter/entry/embedding/filter-name
                        leaf filter-name {
                            type types-filter:filter-name;
                            description "Indicates the name of the embedded filter which is the source of the entry embedded at this position.";
                        }

                        ///nokia-state/state/filter/ip-filter/entry/embedding/entry-id
                        leaf entry-id {
                            type types-filter:entry-id;
                            description "Indicates entry-id of embedded filter which was inserted at this position.";
                        }

                    } // container embedding

                    ///nokia-state/state/filter/ip-filter/entry/statistics
                    container statistics {
                        description "Enter the statistics context";

