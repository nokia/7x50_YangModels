                } // list secondary

                ///nokia-conf/configure/router/mpls/lsp/primary-p2mp-instance
                list primary-p2mp-instance {
                    when "not(../type) or ../type = 'p2mp-rsvp'" {
                        description "not allowed for this LSP type";
                    }
                    key "instance-name";
                    max-elements 1;
                    description "Enter the primary-p2mp-instance list instance";

                    ///nokia-conf/configure/router/mpls/lsp/primary-p2mp-instance/instance-name
                    leaf instance-name {
                        type types-sros:named-item;
                        description "P2MP LSP instance name";
