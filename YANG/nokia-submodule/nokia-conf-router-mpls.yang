                    ///nokia-conf/configure/router/mpls/lsp/primary-p2mp-instance/s2l-path
                    list s2l-path {
                        when "not(../../type) or ../../type = 'p2mp-rsvp'" {
                            description "not allowed for this LSP type";
                        }
                        key "path-name to";
                        max-elements 650;
                        description "Enter the s2l-path list instance";

                        ///nokia-conf/configure/router/mpls/lsp/primary-p2mp-instance/s2l-path/path-name
                        leaf path-name {
                            type leafref {
                                path "../../../../path/path-name";
