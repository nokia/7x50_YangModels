                    description "LSP template name";
                }

                ///nokia-conf/configure/router/mpls/auto-lsp/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/mpls/auto-lsp/apply-groups-exclude
                leaf-list apply-groups-exclude {
