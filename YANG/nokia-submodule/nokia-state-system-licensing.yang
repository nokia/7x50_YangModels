submodule nokia-state-system-licensing {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-sros               { prefix "types-sros"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for system license parameters on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-system-licensing YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2022-03-18";

    grouping state-system-licensing {
        ///nokia-state/state/system/licensing
        container licensing {
            description "Enter the licensing context";

            ///nokia-state/state/system/licensing/uuid
            leaf uuid {
                type types-sros:description;
                description
                    "Indicates the UUID of this system for
                     licensing.";
            }

            ///nokia-state/state/system/licensing/product
            leaf product {
                type types-sros:description;
                description
                    "Indicates the type of license required
                     with this system.";
            }

            ///nokia-state/state/system/licensing/state
            leaf state {
                type enumeration {
                    enum unlicensed                     { value 0; }
                    enum licensed                       { value 1; }
                    enum invalid-license                { value 2; }
                    enum expired-license                { value 3; }
                }
                description "Indicates the state of the system licensing.";
            }

            ///nokia-state/state/system/licensing/reboot-pending
            leaf reboot-pending {
                type boolean;
                description
                    "Indicates if a system reboot is pending due to
                     licensing violation.";
            }

            ///nokia-state/state/system/licensing/active-license
            container active-license {
                description "Enter the active-license context";

                ///nokia-state/state/system/licensing/active-license/uuid
                leaf uuid {
                    type types-sros:description;
                    description "Indicates the target UUID of this system license.";
                }

                ///nokia-state/state/system/licensing/active-license/name
                leaf name {
                    type types-sros:description;
                    description "Indicates the name associated with this license.";
                }

                ///nokia-state/state/system/licensing/active-license/description
                leaf description {
                    type types-sros:description;
                    description "Indicates the description of this license.";
                }

                ///nokia-state/state/system/licensing/active-license/product
                leaf product {
                    type types-sros:description;
                    description "Indicates the product this license is for.";
                }

                ///nokia-state/state/system/licensing/active-license/version
                leaf version {
                    type types-sros:description;
                    description "Indicates the software version this license is for.";
                }

                ///nokia-state/state/system/licensing/active-license/issue-date-time
                leaf issue-date-time {
                    type yang:date-and-time;
                    description "Indicates the date and time license was issued.";
                }

                ///nokia-state/state/system/licensing/active-license/start-date-time
                leaf start-date-time {
                    type yang:date-and-time;
                    description "Indicates the date and time license becomes valid.";
                }

                ///nokia-state/state/system/licensing/active-license/end-date-time
                leaf end-date-time {
                    type yang:date-and-time;
                    description "Indicates the date and time license expires.";
                }

            } // container active-license

            ///nokia-state/state/system/licensing/available-license
            list available-license {
                key "index";
                description "Enter the available-license list instance";

                ///nokia-state/state/system/licensing/available-license/index
                leaf index {
                    type uint32;
                    description "Indicates the index of the matched license.";
                }

                ///nokia-state/state/system/licensing/available-license/uuid
                leaf uuid {
                    type types-sros:description;
                    description "Indicates the target UUID of this system license.";
                }

                ///nokia-state/state/system/licensing/available-license/name
                leaf name {
                    type types-sros:description;
                    description "Indicates the name associated with this license.";
                }

                ///nokia-state/state/system/licensing/available-license/description
                leaf description {
                    type types-sros:description;
                    description "Indicates the description of this license.";
                }

                ///nokia-state/state/system/licensing/available-license/product
                leaf product {
                    type types-sros:description;
                    description "Indicates the product this license is for.";
                }

                ///nokia-state/state/system/licensing/available-license/version
                leaf version {
                    type types-sros:description;
                    description "Indicates the software version this license is for.";
                }

                ///nokia-state/state/system/licensing/available-license/issue-date-time
                leaf issue-date-time {
                    type yang:date-and-time;
                    description "Indicates the date and time license was issued.";
                }

                ///nokia-state/state/system/licensing/available-license/start-date-time
                leaf start-date-time {
                    type yang:date-and-time;
                    description "Indicates the date and time license becomes valid.";
                }

                ///nokia-state/state/system/licensing/available-license/end-date-time
                leaf end-date-time {
                    type yang:date-and-time;
                    description "Indicates the date and time license expires.";
                }

            } // list available-license

            ///nokia-state/state/system/licensing/licensed-feature
            list licensed-feature {
                key "group name";
                description "Enter the licensed-feature list instance";

                ///nokia-state/state/system/licensing/licensed-feature/group
                leaf group {
                    type enumeration {
                        enum system                         { value 0; }
                        enum services                       { value 1; }
                        enum nsp                            { value 2; }
                        enum isa                            { value 3; }
                        enum iom                            { value 4; }
                        enum mda                            { value 5; }
                        enum ports                          { value 6; }
                    }
                    description "Indicates the licensing group.";
                }

                ///nokia-state/state/system/licensing/licensed-feature/name
                leaf name {
                    type types-sros:named-item-or-empty;
                    description "Indicates the name of the licensed feature.";
                }

                ///nokia-state/state/system/licensing/licensed-feature/description
                leaf description {
                    type types-sros:description;
                    description "Indicates the description of this licensed feature.";
                }

                ///nokia-state/state/system/licensing/licensed-feature/type
                leaf type {
                    type enumeration {
                        enum unknown                        { value 0; }
                        enum pool                           { value 1; }
                        enum rtu                            { value 2; }
                        enum monitored                      { value 3; }
                    }
                    description "Indicates the type of feature license.";
                }

                ///nokia-state/state/system/licensing/licensed-feature/pool-size
                leaf pool-size {
                    type int32;
                    description "Indicates the size of the license pool.";
                }

                ///nokia-state/state/system/licensing/licensed-feature/allocated
                leaf allocated {
                    type uint32;
                    description
                        "Indicates the number of these licenses currently allocated
                         in the system.";
                }

                ///nokia-state/state/system/licensing/licensed-feature/present
                leaf present {
                    type boolean;
                    description
                        "Indicates if the current system license grants the right to use
                         the feature on this system.";
                }

                ///nokia-state/state/system/licensing/licensed-feature/state
                leaf state {
                    type enumeration {
                        enum unknown                        { value 0; }
                        enum valid                          { value 1; }
                        enum error                          { value 2; }
                        enum violation                      { value 3; }
                    }
                    description
                        "Indicates the state of the license for this feature.
                         valid     - No issue with application license
                         error     - Insufficient licensing has been detected.
                         violation - License violation has been detected.";
                }

                ///nokia-state/state/system/licensing/licensed-feature/max-24hr
                leaf max-24hr {
                    type uint32;
                    description
                        "Indicates the maximum number of these licenses allocated during
                         the last 24 hour interval.";
                }

                ///nokia-state/state/system/licensing/licensed-feature/date-time-24hr
                leaf date-time-24hr {
                    type yang:date-and-time;
                    description "Indicates the date and time the last 24 hour interval ended.";
                }

                ///nokia-state/state/system/licensing/licensed-feature/max-current
                leaf max-current {
                    type uint32;
                    description
                        "Indicates the maximum number of these licenses allocated during
                         since the last 24 hour interval.";
                }

            } // list licensed-feature

        } // container licensing

    } // grouping state-system-licensing

}
