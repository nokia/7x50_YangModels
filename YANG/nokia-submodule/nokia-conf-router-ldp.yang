                    ///nokia-conf/configure/router/ldp/targeted-session/peer/hello
                    container hello {
                        presence "Enable the hello context";
                        description "Enable the hello context";

                        ///nokia-conf/configure/router/ldp/targeted-session/peer/hello/timeout
                        leaf timeout {
                            type types-sros:time-duration {
                                range "3..65535";
                            }
                            description "Timeout value for Hello";
