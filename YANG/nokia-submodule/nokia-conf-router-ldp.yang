                    ///nokia-conf/configure/router/ldp/targeted-session/peer/local-lsr-id
                    container local-lsr-id {
                        description "Enter the local-lsr-id context";

                        ///nokia-conf/configure/router/ldp/targeted-session/peer/local-lsr-id/interface-name
                        leaf interface-name {
                            type leafref {
                                path "../../../../../interface/interface-name";
