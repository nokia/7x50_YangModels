                    } // container mcast-tunneling

                } // list peer

                ///nokia-conf/configure/router/ldp/targeted-session/peer-template
                list peer-template {
                    key "template-name";
                    max-elements 500;
                    description "Enter the peer-template list instance";

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template/template-name
                    leaf template-name {
                        type types-sros:named-item;
                        description "Name for the targeted peer template";
                    }

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "enable";
                        description "Administrative state of the peer template";
                    }

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template/adv-local-lsr-id
                    leaf adv-local-lsr-id {
                        type boolean;
                        default "false";
                        description "Advertise local LSR ID over a specified LDP session";
                    }

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template/bfd-liveness
                    leaf bfd-liveness {
                        type boolean;
                        default "false";
                        description "Enable BFD tracking of LDP session for sessions created by peer template";
                    }

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template/community
                    leaf community {
                        type types-ldp:policy-string;
                        description "Community string associated with a session to a specified peer";
                    }

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template/tunneling
                    leaf tunneling {
                        type boolean;
                        default "false";
                        description "Allow LDP over tunnels";
                    }

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template/mcast-tunneling
                    leaf mcast-tunneling {
                        type boolean;
                        default "false";
                        description "Enable MLDP over RSVP-TE tunnels for the template";
                    }

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template/hello
                    container hello {
                        presence "Enable the hello context";
                        description "Enable the hello context";

                        ///nokia-conf/configure/router/ldp/targeted-session/peer-template/hello/timeout
                        leaf timeout {
                            type types-sros:time-duration {
                                range "3..65535";
                            }
                            description "Timeout value for Hello";
                        }

                        ///nokia-conf/configure/router/ldp/targeted-session/peer-template/hello/factor
                        leaf factor {
                            type uint32 {
                                range "1..255";
                            }
                            description "Factor value for Hello";
                        }

                    } // container hello

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template/hello-reduction
                    container hello-reduction {
                        presence "Enable the hello-reduction context";
                        description "Enable the hello-reduction context";

                        ///nokia-conf/configure/router/ldp/targeted-session/peer-template/hello-reduction/admin-state
                        leaf admin-state {
                            type types-sros:admin-state-with-inherit;
                            description "Administrative state of suppression of Hello messages";
                        }

                        ///nokia-conf/configure/router/ldp/targeted-session/peer-template/hello-reduction/factor
                        leaf factor {
                            type uint32 {
                                range "3..20";
                            }
                            description "Hello reduction dampening factor";
                        }

                    } // container hello-reduction

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template/keepalive
                    container keepalive {
                        presence "Enable the keepalive context";
                        description "Enable the keepalive context";

                        ///nokia-conf/configure/router/ldp/targeted-session/peer-template/keepalive/timeout
                        leaf timeout {
                            type types-sros:time-duration {
                                range "3..65535";
                            }
                            description "Time that LDP waits before tearing down session";
                        }

                        ///nokia-conf/configure/router/ldp/targeted-session/peer-template/keepalive/factor
                        leaf factor {
                            type uint32 {
                                range "1..255";
                            }
                            description "Factor value for keepalive";
                        }

                    } // container keepalive

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template/local-lsr-id
                    container local-lsr-id {
                        description "Enter the local-lsr-id context";

                        ///nokia-conf/configure/router/ldp/targeted-session/peer-template/local-lsr-id/interface-name
                        leaf interface-name {
                            type leafref {
                                path "../../../../../interface/interface-name";
                            }
                            description "Name of network IP interface from which to obtain IP address to use as LSR-ID of LDP LSP";
                        }

                    } // container local-lsr-id

                } // list peer-template

                ///nokia-conf/configure/router/ldp/targeted-session/peer-template-map
                list peer-template-map {
                    key "template-map-name";
                    max-elements 500;
                    description "Enter the peer-template-map list instance";

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template-map/template-map-name
                    leaf template-map-name {
                        type leafref {
                            path "../../peer-template/template-name";
                        }
                        description "Name to identify the targeted peer template";
                    }

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template-map/policy-map
                    leaf-list policy-map {
                        type leafref {
                            path "../../../../../policy-options/policy-statement/name";
                        }
                        min-elements 1;
                        max-elements 5;
                        ordered-by user;
                        description "Targeted peer template to apply to the given policies";
                    }

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template-map/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/ldp/targeted-session/peer-template-map/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list peer-template-map

                ///nokia-conf/configure/router/ldp/targeted-session/auto-tx
                container auto-tx {
                    description "Enter the auto-tx context";

                    ///nokia-conf/configure/router/ldp/targeted-session/auto-tx/ipv4
                    container ipv4 {
                        description "Enter the ipv4 context";

                        ///nokia-conf/configure/router/ldp/targeted-session/auto-tx/ipv4/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of IPv4 capabilities for peers";
                        }

                        ///nokia-conf/configure/router/ldp/targeted-session/auto-tx/ipv4/tunneling
                        leaf tunneling {
                            type boolean;
                            default "false";
                            description "Enable tunneling for targeted peers";
                        }

                    } // container ipv4

                } // container auto-tx

                ///nokia-conf/configure/router/ldp/targeted-session/auto-rx
                container auto-rx {
                    description "Enter the auto-rx context";

                    ///nokia-conf/configure/router/ldp/targeted-session/auto-rx/ipv4
                    container ipv4 {
                        description "Enter the ipv4 context";

                        ///nokia-conf/configure/router/ldp/targeted-session/auto-rx/ipv4/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of IPv4 capabilities for peers";
                        }

                        ///nokia-conf/configure/router/ldp/targeted-session/auto-rx/ipv4/tunneling
                        leaf tunneling {
                            type boolean;
                            default "false";
                            description "Enable tunneling for targeted peers";
                        }

                    } // container ipv4

                } // container auto-rx

            } // container targeted-session

        } // container ldp

    } // grouping conf-router-ldp

}
