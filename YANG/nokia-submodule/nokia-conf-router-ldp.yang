                    ///nokia-conf/configure/router/ldp/targeted-session/peer/keepalive
                    container keepalive {
                        presence "Enable the keepalive context";
                        description "Enable the keepalive context";

                        ///nokia-conf/configure/router/ldp/targeted-session/peer/keepalive/timeout
                        leaf timeout {
                            type types-sros:time-duration {
                                range "3..65535";
                            }
                            description "Time that LDP waits before tearing down session";
                        }

                        ///nokia-conf/configure/router/ldp/targeted-session/peer/keepalive/factor
                        leaf factor {
                            type uint32 {
                                range "1..255";
                            }
                            description "Factor value for keepalive";
                        }

