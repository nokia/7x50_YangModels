                    ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel/preference
                    leaf preference {
                        type uint32 {
                            range "1..255";
                        }
                        default "5";
                        description "Priority of this static route over the routes from different sources";
                    }

                    ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel/tag
                    leaf tag {
                        type uint32 {
                            range "1..max";
                        }
                        description "Static route tag";
                    }

                    ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel/community
                    leaf community {
                        type types-sros:community;
                        description "Community ID associated with the static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel/destination-class
                    leaf destination-class {
                        type types-sros:class-index;
                        description "Destination class for this static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel/source-class
                    leaf source-class {
                        type types-sros:class-index;
                        description "Source class for the static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        description "Administrative state of the static route operation";
                    }

                    ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel/qos/forwarding-class
                        leaf forwarding-class {
                            type types-sros:fc-name;
                            description "Forwarding class associated with the static route";
                        }

                        ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel/qos/priority
                        leaf priority {
                            type types-sros:priority;
                            description "Static route priority";
                        }

