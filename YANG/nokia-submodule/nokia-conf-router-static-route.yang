submodule nokia-conf-router-static-route {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-router             { prefix "types-router"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-groups;
    include nokia-conf-policy-options;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure Static route attributes on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-router-static-route YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2022-10-07";

    grouping conf-router-static-route-static-routes {
        ///nokia-conf/configure/router/static-routes
        container static-routes {
            description "Enter the static-routes context";

            ///nokia-conf/configure/router/static-routes/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/router/static-routes/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/router/static-routes/hold-down
            container hold-down {
                presence "Enable the hold-down context";
                description "Enable the hold-down context";

                ///nokia-conf/configure/router/static-routes/hold-down/initial
                leaf initial {
                    type uint32 {
                        range "1..65535";
                    }
                    mandatory true;
                    description "Value for the initial hold down time";
                }

                ///nokia-conf/configure/router/static-routes/hold-down/multiplier
                leaf multiplier {
                    type uint32 {
                        range "1..10";
                    }
                    mandatory true;
                    description "Value by which the previous hold-down time is multiplied to calculate the new one";
                }

                ///nokia-conf/configure/router/static-routes/hold-down/max-value
                leaf max-value {
                    type uint32 {
                        range "1..65535";
                    }
                    mandatory true;
                    description "Maximum value of the hold down time";
                }

            } // container hold-down

            ///nokia-conf/configure/router/static-routes/route
            list route {
                key "ip-prefix route-type";
                description "Enter the route list instance";

                ///nokia-conf/configure/router/static-routes/route/ip-prefix
                leaf ip-prefix {
                    type types-sros:ip-prefix;
                    description "IP prefix and prefix length for the static routes";
                }

                ///nokia-conf/configure/router/static-routes/route/route-type
                leaf route-type {
                    type types-router:unicast-multicast;
                    description "Static route type for unicast or multicast RPF";
                }

                ///nokia-conf/configure/router/static-routes/route/community
                leaf-list community {
                    type types-sros:community;
                    max-elements 12;
                    ordered-by user;
                    description "Community ID associated with the static route";
                }

                ///nokia-conf/configure/router/static-routes/route/tag
                leaf tag {
                    type uint32 {
                        range "1..max";
                    }
                    description "Static route tag";
                }

                ///nokia-conf/configure/router/static-routes/route/backup-tag
                leaf backup-tag {
                    type uint32 {
                        range "1..max";
                    }
                    description "Static route backup tag";
                }

                ///nokia-conf/configure/router/static-routes/route/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/static-routes/route/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/router/static-routes/route/blackhole
                container blackhole {
                    presence "Enable the blackhole context";
                    description "Enable the blackhole context";

                    ///nokia-conf/configure/router/static-routes/route/blackhole/description
                    leaf description {
                        type types-sros:description-allow-all-white-spaces;
                        description "Text description";
                    }

                    ///nokia-conf/configure/router/static-routes/route/blackhole/metric
                    leaf metric {
                        type uint32 {
                            range "0..65535";
                        }
                        default "1";
                        description "Static route metric";
                    }

                    ///nokia-conf/configure/router/static-routes/route/blackhole/preference
                    leaf preference {
                        type uint32 {
                            range "1..255";
                        }
                        default "5";
                        description "Priority of this static route over the routes from different sources";
                    }

                    ///nokia-conf/configure/router/static-routes/route/blackhole/tag
                    leaf tag {
                        type uint32 {
                            range "1..max";
                        }
                        description "Static route tag";
                    }

                    ///nokia-conf/configure/router/static-routes/route/blackhole/community
                    leaf community {
                        type types-sros:community;
                        description "Community ID associated with the static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/blackhole/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        description "Administrative state of the static route operation";
                    }

                    ///nokia-conf/configure/router/static-routes/route/blackhole/dynamic-bgp
                    leaf dynamic-bgp {
                        type boolean;
                        default "false";
                        description "Derive static route next hop from BGP next hop value";
                    }

                    ///nokia-conf/configure/router/static-routes/route/blackhole/generate-icmp
                    leaf generate-icmp {
                        type boolean;
                        default "false";
                        description "Send ICMP unreachable messages when received packets match a static route with black-hole next-hop";
                    }

                    ///nokia-conf/configure/router/static-routes/route/blackhole/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/static-routes/route/blackhole/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/static-routes/route/blackhole/prefix-list
                    container prefix-list {
                        description "Enter the prefix-list context";

                        ///nokia-conf/configure/router/static-routes/route/blackhole/prefix-list/name
                        leaf name {
                            type leafref {
                                path "../../../../../../policy-options/prefix-list/name";
                            }
                            description "Prefix list name";
                        }

                        ///nokia-conf/configure/router/static-routes/route/blackhole/prefix-list/flag
                        leaf flag {
                            type enumeration {
                                enum any                            { value 0; }
                                enum all                            { value 1; }
                                enum none                           { value 2; }
                            }
                            default "any";
                            description "Static route match condition from prefix list";
                        }

                    } // container prefix-list

                } // container blackhole

                ///nokia-conf/configure/router/static-routes/route/indirect
                list indirect {
                    key "ip-address";
                    description "Enter the indirect list instance";

                    ///nokia-conf/configure/router/static-routes/route/indirect/ip-address
                    leaf ip-address {
                        type types-sros:ip-address;
                        description "Next-hop IP address used to reach the destination";
                    }

                    ///nokia-conf/configure/router/static-routes/route/indirect/description
                    leaf description {
                        type types-sros:description-allow-all-white-spaces;
                        description "Text description";
                    }

                    ///nokia-conf/configure/router/static-routes/route/indirect/metric
                    leaf metric {
                        type uint32 {
                            range "0..65535";
                        }
                        default "1";
                        description "Static route metric";
                    }

                    ///nokia-conf/configure/router/static-routes/route/indirect/preference
                    leaf preference {
                        type uint32 {
                            range "1..255";
                        }
                        default "5";
                        description "Priority of this static route over the routes from different sources";
                    }

                    ///nokia-conf/configure/router/static-routes/route/indirect/tag
                    leaf tag {
                        type uint32 {
                            range "1..max";
                        }
                        description "Static route tag";
                    }

                    ///nokia-conf/configure/router/static-routes/route/indirect/community
                    leaf community {
                        type types-sros:community;
                        description "Community ID associated with the static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/indirect/destination-class
                    leaf destination-class {
                        type types-sros:class-index;
                        description "Destination class for this static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/indirect/source-class
                    leaf source-class {
                        type types-sros:class-index;
                        description "Source class for the static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/indirect/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        description "Administrative state of the static route operation";
                    }

                    ///nokia-conf/configure/router/static-routes/route/indirect/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/static-routes/route/indirect/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/static-routes/route/indirect/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-conf/configure/router/static-routes/route/indirect/qos/forwarding-class
                        leaf forwarding-class {
                            type types-sros:fc-name;
                            description "Forwarding class associated with the static route";
                        }

                        ///nokia-conf/configure/router/static-routes/route/indirect/qos/priority
                        leaf priority {
                            type types-sros:priority;
                            description "Static route priority";
                        }

                    } // container qos

                    ///nokia-conf/configure/router/static-routes/route/indirect/prefix-list
                    container prefix-list {
                        description "Enter the prefix-list context";

                        ///nokia-conf/configure/router/static-routes/route/indirect/prefix-list/name
                        leaf name {
                            type leafref {
                                path "../../../../../../policy-options/prefix-list/name";
                            }
                            description "Prefix list name";
                        }

                        ///nokia-conf/configure/router/static-routes/route/indirect/prefix-list/flag
                        leaf flag {
                            type enumeration {
                                enum any                            { value 0; }
                                enum all                            { value 1; }
                                enum none                           { value 2; }
                            }
                            default "any";
                            description "Static route match condition from prefix list";
                        }

                    } // container prefix-list

                    ///nokia-conf/configure/router/static-routes/route/indirect/cpe-check
                    list cpe-check {
                        key "address";
                        max-elements 1;
                        description "Enter the cpe-check list instance";

                        ///nokia-conf/configure/router/static-routes/route/indirect/cpe-check/address
                        leaf address {
                            type types-sros:ip-address;
                            description "IP address of the target CPE device";
                        }

                        ///nokia-conf/configure/router/static-routes/route/indirect/cpe-check/drop-count
                        leaf drop-count {
                            type uint32 {
                                range "1..255";
                            }
                            default "3";
                            description "Consecutive ping replies missed before CPE deemed down";
                        }

                        ///nokia-conf/configure/router/static-routes/route/indirect/cpe-check/interval
                        leaf interval {
                            type uint32 {
                                range "1..255";
                            }
                            units "seconds";
                            default "1";
                            description "Interval between ICMP pings to target CPE IP address";
                        }

                        ///nokia-conf/configure/router/static-routes/route/indirect/cpe-check/log
                        leaf log {
                            type boolean;
                            default "false";
                            description "Log CPE connectivity checks transitions";
                        }

                        ///nokia-conf/configure/router/static-routes/route/indirect/cpe-check/padding-size
                        leaf padding-size {
                            type uint32 {
                                range "0..16384";
                            }
                            units "bytes";
                            default "56";
                            description "Padding size for CPE connectivity checks";
                        }

                        ///nokia-conf/configure/router/static-routes/route/indirect/cpe-check/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/static-routes/route/indirect/cpe-check/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list cpe-check

                    ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop
                    container tunnel-next-hop {
                        description "Enter the tunnel-next-hop context";

                        ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/disallow-igp
                        leaf disallow-igp {
                            type boolean;
                            default "false";
                            description "Do not resolve indirect static routes using IGP next-hops in RTM if no tunnel next-hops found in TTM";
                        }

                        ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/resolution
                        leaf resolution {
                            type enumeration {
                                enum none                           { value 0; }
                                enum filter                         { value 1; }
                                enum any                            { value 2; }
                            }
                            default "none";
                            description "Tunnel next hop resolution to resolve indirect static route";
                        }

                        ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/flex-algo
                        leaf flex-algo {
                            type types-router:flex-algo-id;
                            description "Flexible Algorithm ID";
                        }

                        ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/resolution-filter
                        container resolution-filter {
                            description "Enter the resolution-filter context";

                            ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/resolution-filter/ldp
                            leaf ldp {
                                type boolean;
                                default "false";
                                description "Use LDP Route Forwarding Equivalence Class (FEC) tunneling for next-hop resolution";
                            }

                            ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/resolution-filter/sr-isis
                            leaf sr-isis {
                                type boolean;
                                default "false";
                                description "Use the SR ISIS tunneling mechanism to resolve next hop for the static route";
                            }

                            ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/resolution-filter/sr-ospf
                            leaf sr-ospf {
                                type boolean;
                                default "false";
                                description "Use SR OSPF tunneling for next-hop resolution";
                            }

                            ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/resolution-filter/sr-ospf3
                            leaf sr-ospf3 {
                                type boolean;
                                default "false";
                                description "Use SR OSPFv3 tunneling mechanism to resolve next-hop";
                            }

                            ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/resolution-filter/rib-api
                            leaf rib-api {
                                type boolean;
                                default "false";
                                description "Set RIB API type for use with tunnel next-hop";
                            }

                            ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/resolution-filter/mpls-fwd-policy
                            leaf mpls-fwd-policy {
                                type boolean;
                                default "false";
                                description "Use MPLS forwarding policy for tunnel-next-hop";
                            }

                            ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/resolution-filter/rsvp-te
                            container rsvp-te {
                                presence "Enable the rsvp-te context";
                                description "Enable the rsvp-te context";

                                ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/resolution-filter/rsvp-te/lsp
                                list lsp {
                                    key "lsp-name";
                                    description "Add a list entry for lsp";

                                    ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/resolution-filter/rsvp-te/lsp/lsp-name
                                    leaf lsp-name {
                                        type types-sros:named-item-64;
                                        description "Labeled Switch Path name";
                                    }

                                } // list lsp

                            } // container rsvp-te

                            ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/resolution-filter/sr-te
                            container sr-te {
                                presence "Enable the sr-te context";
                                description "Enable the sr-te context";

                                ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/resolution-filter/sr-te/lsp
                                list lsp {
                                    key "lsp-name";
                                    description "Add a list entry for lsp";

                                    ///nokia-conf/configure/router/static-routes/route/indirect/tunnel-next-hop/resolution-filter/sr-te/lsp/lsp-name
                                    leaf lsp-name {
                                        type types-sros:named-item-64;
                                        description "Labeled Switch Path name";
                                    }

                                } // list lsp

                            } // container sr-te

                        } // container resolution-filter

                    } // container tunnel-next-hop

                } // list indirect

                ///nokia-conf/configure/router/static-routes/route/interface
                list interface {
                    key "interface-name";
                    description "Enter the interface list instance";

                    ///nokia-conf/configure/router/static-routes/route/interface/interface-name
                    leaf interface-name {
                        type types-sros:interface-name;
                        description "Router interface name";
                    }

                    ///nokia-conf/configure/router/static-routes/route/interface/description
                    leaf description {
                        type types-sros:description-allow-all-white-spaces;
                        description "Text description";
                    }

                    ///nokia-conf/configure/router/static-routes/route/interface/metric
                    leaf metric {
                        type uint32 {
                            range "0..65535";
                        }
                        default "1";
                        description "Static route metric";
                    }

                    ///nokia-conf/configure/router/static-routes/route/interface/preference
                    leaf preference {
                        type uint32 {
                            range "1..255";
                        }
                        default "5";
                        description "Priority of this static route over the routes from different sources";
                    }

                    ///nokia-conf/configure/router/static-routes/route/interface/tag
                    leaf tag {
                        type uint32 {
                            range "1..max";
                        }
                        description "Static route tag";
                    }

                    ///nokia-conf/configure/router/static-routes/route/interface/community
                    leaf community {
                        type types-sros:community;
                        description "Community ID associated with the static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/interface/destination-class
                    leaf destination-class {
                        type types-sros:class-index;
                        description "Destination class for this static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/interface/source-class
                    leaf source-class {
                        type types-sros:class-index;
                        description "Source class for the static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/interface/load-balancing-weight
                    leaf load-balancing-weight {
                        type uint32 {
                            range "1..max";
                        }
                        description "Load-balancing weight for all of the ECMP next hops";
                    }

                    ///nokia-conf/configure/router/static-routes/route/interface/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        description "Administrative state of the static route operation";
                    }

                    ///nokia-conf/configure/router/static-routes/route/interface/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/static-routes/route/interface/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/static-routes/route/interface/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-conf/configure/router/static-routes/route/interface/qos/forwarding-class
                        leaf forwarding-class {
                            type types-sros:fc-name;
                            description "Forwarding class associated with the static route";
                        }

                        ///nokia-conf/configure/router/static-routes/route/interface/qos/priority
                        leaf priority {
                            type types-sros:priority;
                            description "Static route priority";
                        }

                    } // container qos

                    ///nokia-conf/configure/router/static-routes/route/interface/prefix-list
                    container prefix-list {
                        description "Enter the prefix-list context";

                        ///nokia-conf/configure/router/static-routes/route/interface/prefix-list/name
                        leaf name {
                            type leafref {
                                path "../../../../../../policy-options/prefix-list/name";
                            }
                            description "Prefix list name";
                        }

                        ///nokia-conf/configure/router/static-routes/route/interface/prefix-list/flag
                        leaf flag {
                            type enumeration {
                                enum any                            { value 0; }
                                enum all                            { value 1; }
                                enum none                           { value 2; }
                            }
                            default "any";
                            description "Static route match condition from prefix list";
                        }

                    } // container prefix-list

                    ///nokia-conf/configure/router/static-routes/route/interface/cpe-check
                    list cpe-check {
                        key "address";
                        max-elements 1;
                        description "Enter the cpe-check list instance";

                        ///nokia-conf/configure/router/static-routes/route/interface/cpe-check/address
                        leaf address {
                            type types-sros:ip-address;
                            description "IP address of the target CPE device";
                        }

                        ///nokia-conf/configure/router/static-routes/route/interface/cpe-check/drop-count
                        leaf drop-count {
                            type uint32 {
                                range "1..255";
                            }
                            default "3";
                            description "Consecutive ping replies missed before CPE deemed down";
                        }

                        ///nokia-conf/configure/router/static-routes/route/interface/cpe-check/interval
                        leaf interval {
                            type uint32 {
                                range "1..255";
                            }
                            units "seconds";
                            default "1";
                            description "Interval between ICMP pings to target CPE IP address";
                        }

                        ///nokia-conf/configure/router/static-routes/route/interface/cpe-check/log
                        leaf log {
                            type boolean;
                            default "false";
                            description "Log CPE connectivity checks transitions";
                        }

                        ///nokia-conf/configure/router/static-routes/route/interface/cpe-check/padding-size
                        leaf padding-size {
                            type uint32 {
                                range "0..16384";
                            }
                            units "bytes";
                            default "56";
                            description "Padding size for CPE connectivity checks";
                        }

                        ///nokia-conf/configure/router/static-routes/route/interface/cpe-check/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/static-routes/route/interface/cpe-check/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list cpe-check

                } // list interface

                ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel
                list ipsec-tunnel {
                    key "ipsec-tunnel-name";
                    description "Enter the ipsec-tunnel list instance";

                    ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel/ipsec-tunnel-name
                    leaf ipsec-tunnel-name {
                        type types-sros:named-item;
                        description "IPsec tunnel name.";
                    }

                    ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel/description
                    leaf description {
                        type types-sros:description-allow-all-white-spaces;
                        description "Text description";
                    }

                    ///nokia-conf/configure/router/static-routes/route/ipsec-tunnel/metric
                    leaf metric {
                        type uint32 {
                            range "0..65535";
                        }
                        default "1";
                        description "Static route metric";
