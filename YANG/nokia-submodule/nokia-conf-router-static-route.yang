                } // list ipsec-tunnel

                ///nokia-conf/configure/router/static-routes/route/next-hop
                list next-hop {
                    key "ip-address";
                    description "Enter the next-hop list instance";

                    ///nokia-conf/configure/router/static-routes/route/next-hop/ip-address
                    leaf ip-address {
                        type types-sros:ip-address-with-zone;
                        description "IP address of the directly-connected next hop";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/description
                    leaf description {
                        type types-sros:description-allow-all-white-spaces;
                        description "Text description";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/metric
                    leaf metric {
                        type uint32 {
                            range "0..65535";
                        }
                        default "1";
                        description "Static route metric";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/preference
                    leaf preference {
                        type uint32 {
                            range "1..255";
                        }
                        default "5";
                        description "Priority of this static route over the routes from different sources";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/tag
                    leaf tag {
                        type uint32 {
                            range "1..max";
                        }
                        description "Static route tag";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/community
                    leaf community {
                        type types-sros:community;
                        description "Community ID associated with the static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/destination-class
                    leaf destination-class {
                        type types-sros:class-index;
                        description "Destination class for this static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/source-class
                    leaf source-class {
                        type types-sros:class-index;
                        description "Source class for the static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/bfd-liveness
                    leaf bfd-liveness {
                        type boolean;
                        default "false";
                        description "Use Bidirectional Forwarding Detection on this static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/validate-next-hop
                    leaf validate-next-hop {
                        type boolean;
                        default "false";
                        description "Track the state of the next hop in the IPv4 ARP Cache or the IPv6 Neighbor Cache";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/load-balancing-weight
                    leaf load-balancing-weight {
                        type uint32 {
                            range "1..max";
                        }
                        description "Load-balancing weight for all of the ECMP next hops";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/ldp-sync
                    leaf ldp-sync {
                        type boolean;
                        default "false";
                        description "Use LDP synchronization feature for a static route";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        description "Administrative state of the static route operation";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/static-routes/route/next-hop/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-conf/configure/router/static-routes/route/next-hop/qos/forwarding-class
                        leaf forwarding-class {
                            type types-sros:fc-name;
                            description "Forwarding class associated with the static route";
                        }

                        ///nokia-conf/configure/router/static-routes/route/next-hop/qos/priority
                        leaf priority {
                            type types-sros:priority;
                            description "Static route priority";
                        }

                    } // container qos

                    ///nokia-conf/configure/router/static-routes/route/next-hop/prefix-list
                    container prefix-list {
                        description "Enter the prefix-list context";

                        ///nokia-conf/configure/router/static-routes/route/next-hop/prefix-list/name
                        leaf name {
                            type leafref {
                                path "../../../../../../policy-options/prefix-list/name";
                            }
                            description "Prefix list name";
                        }

                        ///nokia-conf/configure/router/static-routes/route/next-hop/prefix-list/flag
                        leaf flag {
                            type enumeration {
                                enum any                            { value 0; }
                                enum all                            { value 1; }
                                enum none                           { value 2; }
                            }
                            default "any";
                            description "Static route match condition from prefix list";
                        }

                    } // container prefix-list

                    ///nokia-conf/configure/router/static-routes/route/next-hop/backup-next-hop
                    container backup-next-hop {
                        description "Enter the backup-next-hop context";

                        ///nokia-conf/configure/router/static-routes/route/next-hop/backup-next-hop/address
                        leaf address {
                            type types-sros:ip-address;
                            description "Backup next-hop IP address";
                        }

                    } // container backup-next-hop

                    ///nokia-conf/configure/router/static-routes/route/next-hop/cpe-check
                    list cpe-check {
                        key "address";
                        max-elements 1;
                        description "Enter the cpe-check list instance";

                        ///nokia-conf/configure/router/static-routes/route/next-hop/cpe-check/address
                        leaf address {
                            type types-sros:ip-address;
                            description "IP address of the target CPE device";
                        }

                        ///nokia-conf/configure/router/static-routes/route/next-hop/cpe-check/drop-count
                        leaf drop-count {
                            type uint32 {
                                range "1..255";
                            }
                            default "3";
                            description "Consecutive ping replies missed before CPE deemed down";
                        }

                        ///nokia-conf/configure/router/static-routes/route/next-hop/cpe-check/interval
                        leaf interval {
                            type uint32 {
                                range "1..255";
                            }
                            units "seconds";
                            default "1";
                            description "Interval between ICMP pings to target CPE IP address";
                        }

                        ///nokia-conf/configure/router/static-routes/route/next-hop/cpe-check/log
                        leaf log {
                            type boolean;
                            default "false";
                            description "Log CPE connectivity checks transitions";
                        }

                        ///nokia-conf/configure/router/static-routes/route/next-hop/cpe-check/padding-size
                        leaf padding-size {
                            type uint32 {
                                range "0..16384";
                            }
                            units "bytes";
                            default "56";
                            description "Padding size for CPE connectivity checks";
                        }

                        ///nokia-conf/configure/router/static-routes/route/next-hop/cpe-check/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/static-routes/route/next-hop/cpe-check/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list cpe-check

                } // list next-hop

            } // list route

        } // container static-routes

    } // grouping conf-router-static-route-static-routes

}
