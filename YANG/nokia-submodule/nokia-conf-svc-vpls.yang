                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/ccm-ltm-priority
                        leaf ccm-ltm-priority {
                            type types-eth-cfm:frame-priority;
                            default "7";
                            description "Priority of CCM and LTM messages transmitted by the MEP";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/ccm-padding-size
                        leaf ccm-padding-size {
                            type uint32 {
                                range "3..1500";
