                            leaf string {
                                type types-sros:string-not-all-spaces {
                                    length "1..32";
                                }
                                description "User-defined ASCII string for the VSO";
                            }

                        } // container vendor-specific-option

                    } // container option-82

                    ///nokia-conf/configure/service/vpls/sap/dhcp/lease-populate
                    container lease-populate {
                        description "Enter the lease-populate context";

                        ///nokia-conf/configure/service/vpls/sap/dhcp/lease-populate/max-leases
                        leaf max-leases {
                            type uint32 {
                                range "0..131071";
                            }
                            description "Maximum number of DHCPv4 leases";
                        }

                    } // container lease-populate

                } // container dhcp


                ///nokia-conf/configure/service/vpls/sap/static-host
