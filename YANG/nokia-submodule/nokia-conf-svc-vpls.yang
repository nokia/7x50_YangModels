                ///nokia-conf/configure/service/vpls/capture-sap/router-solicit
                container router-solicit {
                    description "Enter the router-solicit context";

                    ///nokia-conf/configure/service/vpls/capture-sap/router-solicit/user-db
                    leaf user-db {
