                } // container router-solicit

            } // list capture-sap

            ///nokia-conf/configure/service/vpls/interface
            list interface {
                when "not(../pbb-type = 'b-vpls') and not(../pbb-type = 'i-vpls') and not(../routed-vpls)" {
                    description "invalid VPLS PBB type or routed VPLS";
                }
                key "interface-name";
                description "Enter the interface list instance";

                ///nokia-conf/configure/service/vpls/interface/interface-name
                leaf interface-name {
                    type types-sros:interface-name;
                    description "IP interface name";
                }

                ///nokia-conf/configure/service/vpls/interface/description
                leaf description {
                    type types-sros:very-long-description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/vpls/interface/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "enable";
                    description "Administrative state of the interface";
                }

                ///nokia-conf/configure/service/vpls/interface/mac
                leaf mac {
                    type types-sros:mac-unicast-address;
                    description "MAC address for the interface";
                }

                ///nokia-conf/configure/service/vpls/interface/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/interface/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/interface/hold-time
                container hold-time {
                    description "Enter the hold-time context";

                    ///nokia-conf/configure/service/vpls/interface/hold-time/ipv4
                    container ipv4 {
                        description "Enter the ipv4 context";

                        ///nokia-conf/configure/service/vpls/interface/hold-time/ipv4/up
                        container up {
                            description "Enter the up context";

                            ///nokia-conf/configure/service/vpls/interface/hold-time/ipv4/up/seconds
                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Up hold time for the IP interface";
                            }

                        } // container up

                        ///nokia-conf/configure/service/vpls/interface/hold-time/ipv4/down
                        container down {
                            description "Enter the down context";

                            ///nokia-conf/configure/service/vpls/interface/hold-time/ipv4/down/seconds
                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Down hold time for the IP interface";
                            }

                            ///nokia-conf/configure/service/vpls/interface/hold-time/ipv4/down/init-only
                            leaf init-only {
                                type boolean;
                                default "false";
                                description "Apply delay only at interface configuration or reboot";
                            }

                        } // container down

                    } // container ipv4

                } // container hold-time

                ///nokia-conf/configure/service/vpls/interface/ipv4
                container ipv4 {
                    description "Enter the ipv4 context";

                    ///nokia-conf/configure/service/vpls/interface/ipv4/primary
                    container primary {
                        presence "Enable the primary context";
                        description "Enable the primary context";

                        ///nokia-conf/configure/service/vpls/interface/ipv4/primary/address
                        leaf address {
                            type types-sros:ipv4-address;
                            mandatory true;
                            description "IP address of the interface";
                        }

                        ///nokia-conf/configure/service/vpls/interface/ipv4/primary/prefix-length
                        leaf prefix-length {
                            type uint32 {
                                range "0..31";
                            }
                            mandatory true;
                            description "IPv4 address prefix length";
                        }

                        ///nokia-conf/configure/service/vpls/interface/ipv4/primary/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/interface/ipv4/primary/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container primary

                    ///nokia-conf/configure/service/vpls/interface/ipv4/neighbor-discovery
                    container neighbor-discovery {
                        description "Enter the neighbor-discovery context";

                        ///nokia-conf/configure/service/vpls/interface/ipv4/neighbor-discovery/timeout
                        leaf timeout {
                            type uint32 {
                                range "0..65535";
                            }
                            units "seconds";
                            default "14400";
                            description "ARP timeout value to determine how long an ARP entry remains in the ARP cache";
                        }

                        ///nokia-conf/configure/service/vpls/interface/ipv4/neighbor-discovery/static-neighbor
                        list static-neighbor {
                            key "ipv4-address";
                            description "Enter the static-neighbor list instance";

                            ///nokia-conf/configure/service/vpls/interface/ipv4/neighbor-discovery/static-neighbor/ipv4-address
                            leaf ipv4-address {
                                type types-sros:ipv4-address;
                                description "IPv4 address that corresponds to the physical address";
                            }

                            ///nokia-conf/configure/service/vpls/interface/ipv4/neighbor-discovery/static-neighbor/mac-address
                            leaf mac-address {
                                type yang:mac-address;
                                mandatory true;
                                description "MAC address for the static neighbor";
                            }

                            ///nokia-conf/configure/service/vpls/interface/ipv4/neighbor-discovery/static-neighbor/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/vpls/interface/ipv4/neighbor-discovery/static-neighbor/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list static-neighbor

                    } // container neighbor-discovery

                } // container ipv4

            } // list interface

            ///nokia-conf/configure/service/vpls/igmp-host-tracking
            container igmp-host-tracking {
                when "not(../pbb-type) and not(../routed-vpls) and not(../m-vpls = 'true')" {
                    description "not supported on m-vpls, when pbb-type is not 'none' and when routed-vpls";
                }
                description "Enable the igmp-host-tracking context";

                ///nokia-conf/configure/service/vpls/igmp-host-tracking/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of IGMP host tracking";
                }

                ///nokia-conf/configure/service/vpls/igmp-host-tracking/expiry-time
                leaf expiry-time {
                    type types-sros:time-duration {
                        range "1..65535";
                    }
                    units "seconds";
                    default "260";
                    description "Time that the system continues to track inactive hosts";
                }

                ///nokia-conf/configure/service/vpls/igmp-host-tracking/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/igmp-host-tracking/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container igmp-host-tracking

            ///nokia-conf/configure/service/vpls/wlan-gw
            container wlan-gw {
                description "Enable the wlan-gw context";

                ///nokia-conf/configure/service/vpls/wlan-gw/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of WLAN-GW";
                }

                ///nokia-conf/configure/service/vpls/wlan-gw/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/vpls/wlan-gw/wlan-gw-group
                leaf wlan-gw-group {
                    type leafref {
                        path "../../../../isa/wlan-gw-group/id";
                    }
                    sros-ext:auto-restart-to-modify;
                    description "WLAN-GW endpoint within the VPLS service";
                }

                ///nokia-conf/configure/service/vpls/wlan-gw/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/wlan-gw/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container wlan-gw

            ///nokia-conf/configure/service/vpls/gsmp
            container gsmp {
                when "not(../pbb-type) and ../etree = 'false'" {
                    description "VPLS PBB type must be 'none' and etree mode must be 'false'";
                }
                description "Enter the gsmp context";

                ///nokia-conf/configure/service/vpls/gsmp/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of GSMP";
                }

                ///nokia-conf/configure/service/vpls/gsmp/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/gsmp/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/gsmp/group
                list group {
                    key "name";
                    max-elements 1024;
                    description "Enter the group list instance";

                    ///nokia-conf/configure/service/vpls/gsmp/group/name
                    leaf name {
                        type types-sros:named-item;
                        description "GSMP group name";
                    }

                    ///nokia-conf/configure/service/vpls/gsmp/group/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the GSMP group";
                    }

                    ///nokia-conf/configure/service/vpls/gsmp/group/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/service/vpls/gsmp/group/keepalive
                    leaf keepalive {
                        type types-sros:time-duration {
                            range "1..25";
                        }
                        units "seconds";
                        default "10";
                        description "Keepalive value for the GSMP connections in this group";
                    }

                    ///nokia-conf/configure/service/vpls/gsmp/group/hold-multiplier
                    leaf hold-multiplier {
                        type uint32 {
                            range "1..100";
                        }
                        default "3";
                        description "Hold-multiplier for the GSMP connections in this group";
                    }

                    ///nokia-conf/configure/service/vpls/gsmp/group/persistency
                    leaf persistency {
                        type boolean;
                        default "false";
                        description "Store DSL line information when the GSMP connection terminates";
                    }

                    ///nokia-conf/configure/service/vpls/gsmp/group/idle-filter
                    leaf idle-filter {
                        type boolean;
                        default "false";
                        description "Filter ANCP messages from IDLE DSL lines";
                    }

                    ///nokia-conf/configure/service/vpls/gsmp/group/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/gsmp/group/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/gsmp/group/ancp
                    container ancp {
                        description "Enter the ancp context";

                        ///nokia-conf/configure/service/vpls/gsmp/group/ancp/dynamic-topology-discovery
                        leaf dynamic-topology-discovery {
                            type boolean;
                            default "true";
                            description "Enable the ANCP dynamic topology discovery capability";
                        }

                        ///nokia-conf/configure/service/vpls/gsmp/group/ancp/oam
                        leaf oam {
                            type boolean;
                            default "false";
                            description "Enable GSMP ANCP OAM capability at startup of GSMP connection";
                        }

                    } // container ancp

                    ///nokia-conf/configure/service/vpls/gsmp/group/neighbor
                    list neighbor {
                        key "remote-address";
                        description "Enter the neighbor list instance";

                        ///nokia-conf/configure/service/vpls/gsmp/group/neighbor/remote-address
                        leaf remote-address {
                            type types-sros:ipv4-unicast-address;
                            description "GSMP neighbor remote IP address";
                        }

                        ///nokia-conf/configure/service/vpls/gsmp/group/neighbor/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the GSMP neighbor";
                        }

                        ///nokia-conf/configure/service/vpls/gsmp/group/neighbor/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/service/vpls/gsmp/group/neighbor/local-address
                        leaf local-address {
                            type types-sros:ipv4-unicast-address;
                            sros-ext:auto-restart-to-modify;
                            description "Restrict connections to this local address only within the service running ANCP";
                        }

                        ///nokia-conf/configure/service/vpls/gsmp/group/neighbor/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            sros-ext:auto-restart-to-modify;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/gsmp/group/neighbor/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            sros-ext:auto-restart-to-modify;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/gsmp/group/neighbor/priority-marking
                        container priority-marking {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the priority-marking context";

                            ///nokia-conf/configure/service/vpls/gsmp/group/neighbor/priority-marking/priority-marking
                            choice priority-marking {
                                ///nokia-conf/configure/service/vpls/gsmp/group/neighbor/priority-marking/priority-marking/dscp
                                case dscp {

                                    ///nokia-conf/configure/service/vpls/gsmp/group/neighbor/priority-marking/priority-marking/dscp/dscp
                                    leaf dscp {
                                        type types-qos:dscp-name;
                                        sros-ext:auto-restart-to-modify;
                                        description "DSCP that is used while remarking the in profile packets";
                                    }

                                }
                                ///nokia-conf/configure/service/vpls/gsmp/group/neighbor/priority-marking/priority-marking/prec
                                case prec {

                                    ///nokia-conf/configure/service/vpls/gsmp/group/neighbor/priority-marking/priority-marking/prec/prec
                                    leaf prec {
                                        type types-qos:precedence-value;
                                        sros-ext:auto-restart-to-modify;
                                        description "Precedence priority marking";
                                    }

                                }
                            }
                        } // container priority-marking

                    } // list neighbor

                } // list group

            } // container gsmp

            ///nokia-conf/configure/service/vpls/provider-tunnel
            container provider-tunnel {
                presence "Enable the provider-tunnel context";
                description "Enable the provider-tunnel context";

                ///nokia-conf/configure/service/vpls/provider-tunnel/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    sros-ext:auto-restart-to-modify;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/provider-tunnel/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    sros-ext:auto-restart-to-modify;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/provider-tunnel/inclusive
                container inclusive {
                    sros-ext:auto-restart-to-modify;
                    description "Enter the inclusive context";

                    ///nokia-conf/configure/service/vpls/provider-tunnel/inclusive/data-delay-interval
                    leaf data-delay-interval {
                        type int32 {
                            range "3..180";
                        }
                        units "seconds";
                        sros-ext:auto-restart-to-modify;
                        default "15";
                        description "I-PMSI data delay timer";
                    }

                    ///nokia-conf/configure/service/vpls/provider-tunnel/inclusive/owner
                    leaf owner {
                        type enumeration {
                            enum bgp-ad                         { value 1; }
                            enum bgp-vpls                       { value 2; }
                            enum bgp-evpn-mpls                  { value 3; }
                        }
                        sros-ext:auto-restart-to-modify;
                        description "Configure provider-tunnel owner";
                    }

                    ///nokia-conf/configure/service/vpls/provider-tunnel/inclusive/root-and-leaf
                    leaf root-and-leaf {
                        type boolean;
                        sros-ext:auto-restart-to-modify;
                        default "false";
                        description "Configure whether the provider tunnel acts as a leaf or both a root and leaf";
                    }

                    ///nokia-conf/configure/service/vpls/provider-tunnel/inclusive/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        sros-ext:auto-restart-to-modify;
                        default "disable";
                        description "Administrative state of P2MP LSP as the I-PMSI";
                    }

                    ///nokia-conf/configure/service/vpls/provider-tunnel/inclusive/ipmsi
                    choice ipmsi {
                        ///nokia-conf/configure/service/vpls/provider-tunnel/inclusive/ipmsi/mldp
                        case mldp {

                            ///nokia-conf/configure/service/vpls/provider-tunnel/inclusive/ipmsi/mldp/mldp
                            leaf mldp {
                                type empty;
                                sros-ext:auto-restart-to-modify;
                                description "Enable/Disable MLDP";
                            }

                        }
                        ///nokia-conf/configure/service/vpls/provider-tunnel/inclusive/ipmsi/rsvp
                        case rsvp {

                            ///nokia-conf/configure/service/vpls/provider-tunnel/inclusive/ipmsi/rsvp/rsvp
                            container rsvp {
                                presence "Enable the rsvp context";
                                sros-ext:auto-restart-to-modify;
                                description "Enable the rsvp context";

                                ///nokia-conf/configure/service/vpls/provider-tunnel/inclusive/ipmsi/rsvp/rsvp/lsp-template
                                leaf lsp-template {
                                    type leafref {
                                        path "../../../../../../router/mpls/lsp-template/template-name";
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    description "Configure LSP template name";
                                }

                            } // container rsvp

                        }
                    }
                } // container inclusive

                ///nokia-conf/configure/service/vpls/provider-tunnel/selective
                container selective {
                    sros-ext:auto-restart-to-modify;
                    description "Enter the selective context";

                    ///nokia-conf/configure/service/vpls/provider-tunnel/selective/data-delay-interval
                    leaf data-delay-interval {
                        type int32 {
                            range "3..180";
                        }
                        units "seconds";
                        sros-ext:auto-restart-to-modify;
                        default "3";
                        description "S-PMSI data delay timer";
                    }

                    ///nokia-conf/configure/service/vpls/provider-tunnel/selective/owner
                    leaf owner {
                        type enumeration {
                            enum bgp-evpn-mpls                  { value 1; }
                        }
                        sros-ext:auto-restart-to-modify;
                        description "Provider-tunnel selective owner";
                    }

                    ///nokia-conf/configure/service/vpls/provider-tunnel/selective/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        sros-ext:auto-restart-to-modify;
                        default "disable";
                        description "Administrative state of the selective provider tunnel";
                    }

                    ///nokia-conf/configure/service/vpls/provider-tunnel/selective/wildcard-spmsi
                    leaf wildcard-spmsi {
                        type boolean;
                        sros-ext:auto-restart-to-modify;
                        default "false";
                        description "Enable wildcard S-PMSI support";
                    }

                    ///nokia-conf/configure/service/vpls/provider-tunnel/selective/mldp
                    leaf mldp {
                        type boolean;
                        sros-ext:auto-restart-to-modify;
                        default "false";
                        description "Enable MLDP";
                    }

                    ///nokia-conf/configure/service/vpls/provider-tunnel/selective/maximum-p2mp-spmsi
                    leaf maximum-p2mp-spmsi {
                        type uint32 {
                            range "1..4000";
                        }
                        sros-ext:auto-restart-to-modify;
                        default "10";
                        description "Maximum number of P2MP S-PMSIi tunnels";
                    }

                    ///nokia-conf/configure/service/vpls/provider-tunnel/selective/data-threshold
                    container data-threshold {
                        sros-ext:auto-restart-to-modify;
                        description "Enter the data-threshold context";

                        ///nokia-conf/configure/service/vpls/provider-tunnel/selective/data-threshold/group-prefix
                        list group-prefix {
                            key "ip-group-prefix";
                            description "Enter the group-prefix list instance";

                            ///nokia-conf/configure/service/vpls/provider-tunnel/selective/data-threshold/group-prefix/ip-group-prefix
                            leaf ip-group-prefix {
                                type types-sros:ip-multicast-prefix;
                                description "Multicast group prefix";
                            }

                            ///nokia-conf/configure/service/vpls/provider-tunnel/selective/data-threshold/group-prefix/threshold
                            leaf threshold {
                                type int64 {
                                    range "0..4294967294";
                                }
                                units "kilobps";
                                mandatory true;
                                description "Threshold rate for a group prefix";
                            }

                            ///nokia-conf/configure/service/vpls/provider-tunnel/selective/data-threshold/group-prefix/pe-threshold-add
                            leaf pe-threshold-add {
                                type uint32 {
                                    range "1..65535";
                                }
                                default "65535";
                                description "Number of receiver PEs for S-PMSI tunnel creation";
                            }

                            ///nokia-conf/configure/service/vpls/provider-tunnel/selective/data-threshold/group-prefix/pe-threshold-delete
                            leaf pe-threshold-delete {
                                type uint32 {
                                    range "2..65535";
                                }
                                default "65535";
                                description "Number of receiver PEs for S-PMSI tunnel deletion";
                            }

                            ///nokia-conf/configure/service/vpls/provider-tunnel/selective/data-threshold/group-prefix/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/vpls/provider-tunnel/selective/data-threshold/group-prefix/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list group-prefix

                    } // container data-threshold

                } // container selective

            } // container provider-tunnel

            ///nokia-conf/configure/service/vpls/isid-policy
            container isid-policy {
                description "Enter the isid-policy context";

                ///nokia-conf/configure/service/vpls/isid-policy/entry
                list entry {
                    when "../../pbb-type = 'b-vpls'" {
                        description "supported on b-vpls service";
                    }
                    key "range-entry-id";
                    description "Enter the entry list instance";

                    ///nokia-conf/configure/service/vpls/isid-policy/entry/range-entry-id
                    leaf range-entry-id {
                        type uint32 {
                            range "1..8191";
                        }
                        description "ISID policy entry ID";
                    }

                    ///nokia-conf/configure/service/vpls/isid-policy/entry/advertise-local
                    leaf advertise-local {
                        type boolean;
                        default "true";
                        description "Advertise locally-defined I-VPLS ISIDs or static ISIDs";
                    }

                    ///nokia-conf/configure/service/vpls/isid-policy/entry/use-def-mcast
                    leaf use-def-mcast {
                        type boolean;
                        default "false";
                        description "Use default multicast tree to propagate ISIS range";
                    }

                    ///nokia-conf/configure/service/vpls/isid-policy/entry/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/isid-policy/entry/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/isid-policy/entry/range
                    container range {
                        description "Enter the range context";

                        ///nokia-conf/configure/service/vpls/isid-policy/entry/range/start
                        leaf start {
                            type types-services:isid-no-zero;
                            description "Lower bound of the ISID range";
                        }

                        ///nokia-conf/configure/service/vpls/isid-policy/entry/range/end
                        leaf end {
                            type types-services:isid-no-zero;
                            description "Upper bound of the ISID range";
                        }

                    } // container range

                } // list entry

            } // container isid-policy

            ///nokia-conf/configure/service/vpls/network
            container network {
                description "Enter the network context";

                ///nokia-conf/configure/service/vpls/network/ingress
                container ingress {
                    description "Enter the ingress context";

                    ///nokia-conf/configure/service/vpls/network/ingress/classification-override
                    leaf classification-override {
                        type leafref {
                            path "../../../../../qos/ingress-classification-policy/ingress-classification-policy-name";
                        }
                        description "Short-pipe mode for CoS propagation";
                    }

                } // container ingress

            } // container network

        } // list vpls

    } // grouping conf-svc-vpls

}
