            ///nokia-conf/configure/service/vpls/bgp-evpn
            container bgp-evpn {
                when "not(../pbb-type = 'i-vpls') and not(../m-vpls = 'true')" {
                    description "not supported on i-vpls or m-vpls";
                }
                presence "Enable the bgp-evpn context";
                description "Enable the bgp-evpn context";

                ///nokia-conf/configure/service/vpls/bgp-evpn/accept-ivpls-evpn-flush
                leaf accept-ivpls-evpn-flush {
                    when "../../pbb-type = 'b-vpls'" {
                        description "VPLS PBB type must be 'b-vpls'";
                    }
                    type boolean;
                    default "false";
                    description "Accept and process non-zero ethernet-tag MAC routes";
                }

                ///nokia-conf/configure/service/vpls/bgp-evpn/evi
                leaf evi {
                    type types-services:evi;
                    description "EVPN ID";
                }

                ///nokia-conf/configure/service/vpls/bgp-evpn/incl-mcast-orig-ip
                leaf incl-mcast-orig-ip {
                    when "not(../../pbb-type = 'b-vpls')" {
                        description "VPLS PBB type must not be 'b-vpls'";
                    }
                    type types-sros:ipv4-unicast-address;
                    description "Originating IP address";
                }

                ///nokia-conf/configure/service/vpls/bgp-evpn/ignore-mtu-mismatch
                leaf ignore-mtu-mismatch {
                    type boolean;
                    default "false";
                    description "Ignore MTU mismatch";
                }

                ///nokia-conf/configure/service/vpls/bgp-evpn/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/bgp-evpn/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/bgp-evpn/routes
                container routes {
                    description "Enter the routes context";

                    ///nokia-conf/configure/service/vpls/bgp-evpn/routes/mac-ip
                    container mac-ip {
                        description "Enter the mac-ip context";

                        ///nokia-conf/configure/service/vpls/bgp-evpn/routes/mac-ip/advertise
                        leaf advertise {
                            type boolean;
                            default "true";
                            description "Status for the BGP-EVPN MAC/IP routes advertisement";
                        }

                        ///nokia-conf/configure/service/vpls/bgp-evpn/routes/mac-ip/cfm-mac
                        leaf cfm-mac {
                            type boolean;
                            default "false";
                            description "Enable advertisement and withdrawal of MAC address";
                        }

                        ///nokia-conf/configure/service/vpls/bgp-evpn/routes/mac-ip/unknown-mac
                        leaf unknown-mac {
                            when "not(../../../../pbb-type = 'b-vpls')" {
                                description "VPLS PBB type must not be 'b-vpls'";
                            }
                            type boolean;
                            default "false";
                            description "Enable advertisement of unknown MAC route in BGP";
                        }

                        ///nokia-conf/configure/service/vpls/bgp-evpn/routes/mac-ip/arp-nd-extended-community
                        leaf arp-nd-extended-community {
                            type boolean;
                            default "false";
                            description "Enable ARP/ND extended community";
                        }

                    } // container mac-ip

                    ///nokia-conf/configure/service/vpls/bgp-evpn/routes/ip-prefix
                    container ip-prefix {
                        when "../../../routed-vpls" {
                            description "routed VPLS must be present";
                        }
                        description "Enter the ip-prefix context";

                        ///nokia-conf/configure/service/vpls/bgp-evpn/routes/ip-prefix/advertise
                        leaf advertise {
                            type boolean;
                            default "false";
                            description "Status for the IP prefix routes advertisement";
                        }

                        ///nokia-conf/configure/service/vpls/bgp-evpn/routes/ip-prefix/include-direct-interface-host
                        leaf include-direct-interface-host {
                            type boolean;
                            default "false";
                            description "Advertise the interface host address in EVPN";
                        }

                        ///nokia-conf/configure/service/vpls/bgp-evpn/routes/ip-prefix/domain-id
                        leaf domain-id {
                            type types-services:domain-id;
                            description "Domain ID of received BGP route before readvertisement";
                        }

                        ///nokia-conf/configure/service/vpls/bgp-evpn/routes/ip-prefix/link-bandwidth
                        container link-bandwidth {
                            description "Enter the link-bandwidth context";

                            ///nokia-conf/configure/service/vpls/bgp-evpn/routes/ip-prefix/link-bandwidth/weighted-ecmp
                            leaf weighted-ecmp {
                                type boolean;
                                default "false";
                                description "Enable weighted ECMP";
                            }

                            ///nokia-conf/configure/service/vpls/bgp-evpn/routes/ip-prefix/link-bandwidth/advertise
                            container advertise {
                                presence "Enable the advertise context";
                                description "Enable the advertise context";

                                ///nokia-conf/configure/service/vpls/bgp-evpn/routes/ip-prefix/link-bandwidth/advertise/max-dynamic-weight
                                leaf max-dynamic-weight {
                                    type uint32 {
                                        range "1..128";
                                    }
                                    default "128";
                                    description "Maximum weight of the advertised evpn-iff route";
                                }

                                ///nokia-conf/configure/service/vpls/bgp-evpn/routes/ip-prefix/link-bandwidth/advertise/weight
                                leaf weight {
                                    type union {
                                        type uint32 {
                                            range "1..128";
                                        }
                                        type enumeration {
                                            enum dynamic                        { value 0; }
                                        }
                                    }
                                    default "dynamic";
                                    description "Weight of the advertised evpn-iff route";
                                }

                            } // container advertise

                        } // container link-bandwidth

                    } // container ip-prefix

                    ///nokia-conf/configure/service/vpls/bgp-evpn/routes/incl-mcast
                    container incl-mcast {
                        description "Enter the incl-mcast context";

                        ///nokia-conf/configure/service/vpls/bgp-evpn/routes/incl-mcast/advertise-ingress-replication
                        leaf advertise-ingress-replication {
                            type boolean;
                            default "true";
                            description "BGP EVPN IMET-IR route advertisement";
                        }

                        ///nokia-conf/configure/service/vpls/bgp-evpn/routes/incl-mcast/advertise-l2-attributes
                        leaf advertise-l2-attributes {
                            type boolean;
                            default "false";
                            description "Advertise L2 attributes";
                        }

                    } // container incl-mcast

                    ///nokia-conf/configure/service/vpls/bgp-evpn/routes/sel-mcast
                    container sel-mcast {
                        description "Enter the sel-mcast context";

                        ///nokia-conf/configure/service/vpls/bgp-evpn/routes/sel-mcast/advertise
                        leaf advertise {
                            type boolean;
                            default "false";
                            description "Advertise Selective Multicast Ethernet Tag routes";
                        }

                    } // container sel-mcast

                } // container routes

                ///nokia-conf/configure/service/vpls/bgp-evpn/mac-duplication
                container mac-duplication {
                    description "Enter the mac-duplication context";

                    ///nokia-conf/configure/service/vpls/bgp-evpn/mac-duplication/retry
                    leaf retry {
                        type union {
                            type uint32 {
                                range "2..60";
                            }
                            type enumeration {
                                enum never                          { value 0; }
                            }
                        }
                        units "minutes";
                        default "9";
                        description "BGP EVPN MAC duplication retry time";
                    }

                    ///nokia-conf/configure/service/vpls/bgp-evpn/mac-duplication/blackhole
                    leaf blackhole {
                        when "not(../../../pbb-type = 'b-vpls')" {
                            description "VPLS PBB type must not be 'b-vpls'";
                        }
                        type boolean;
                        default "false";
                        description "Use duplicated MAC as a black-hole MAC";
                    }

                    ///nokia-conf/configure/service/vpls/bgp-evpn/mac-duplication/trusted-mac-time
                    leaf trusted-mac-time {
                        type uint32 {
                            range "1..15";
                        }
                        units "minutes";
                        default "5";
                        description "Trusted MAC time";
                    }

                    ///nokia-conf/configure/service/vpls/bgp-evpn/mac-duplication/detect
                    container detect {
                        description "Enter the detect context";

                        ///nokia-conf/configure/service/vpls/bgp-evpn/mac-duplication/detect/num-moves
                        leaf num-moves {
                            type uint32 {
                                range "3..10";
                            }
                            default "5";
                            description "BGP EVPN MAC duplication detection number of moves";
                        }

                        ///nokia-conf/configure/service/vpls/bgp-evpn/mac-duplication/detect/window
                        leaf window {
                            type uint32 {
                                range "1..15";
                            }
                            units "minutes";
                            default "3";
                            description "BGP EVPN MAC duplication detection window";
                        }

                        ///nokia-conf/configure/service/vpls/bgp-evpn/mac-duplication/detect/trusted-mac-move-factor
                        leaf trusted-mac-move-factor {
                            type uint32 {
                                range "1..10";
                            }
                            default "1";
                            description "Trusted MAC move factor";
                        }

                    } // container detect

                } // container mac-duplication

                ///nokia-conf/configure/service/vpls/bgp-evpn/isid-route-target
                container isid-route-target {
                    description "Enter the isid-route-target context";

                    ///nokia-conf/configure/service/vpls/bgp-evpn/isid-route-target/range
                    list range {
                        key "start";
                        max-elements 8192;
                        description "Enter the range list instance";

                        ///nokia-conf/configure/service/vpls/bgp-evpn/isid-route-target/range/start
                        leaf start {
                            type types-sros:svc-isid-non-zero;
                            description "Lower bound of the ISID range";
                        }

                        ///nokia-conf/configure/service/vpls/bgp-evpn/isid-route-target/range/end
                        leaf end {
                            type types-sros:svc-isid-non-zero;
                            mandatory true;
                            description "Upper bound of the ISID range";
                        }

                        ///nokia-conf/configure/service/vpls/bgp-evpn/isid-route-target/range/type
                        leaf type {
                            type enumeration {
                                enum auto                           { value 1; }
                                enum configured                     { value 2; }
                            }
                            sros-ext:immutable;
                            default "auto";
                            description "Method used to support the PBB-EVPN ISID-based route target advertisement";
                        }

                        ///nokia-conf/configure/service/vpls/bgp-evpn/isid-route-target/range/route-target
                        leaf route-target {
                            when "../type = 'configured'" {
                                description "type must be 'configured'";
                            }
                            type types-services:route-target;
                            sros-ext:immutable;
                            description "Route for the ISID range";
                        }

                        ///nokia-conf/configure/service/vpls/bgp-evpn/isid-route-target/range/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/bgp-evpn/isid-route-target/range/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list range

                } // container isid-route-target

                ///nokia-conf/configure/service/vpls/bgp-evpn/segment-routing-v6
                list segment-routing-v6 {
                    when "not(../../pbb-type = 'b-vpls') and not(../../pbb-type = 'i-vpls') and ../../m-vpls = 'false' and ../../etree = 'false' and not(../../routed-vpls)" {
                        description "not supported on b-vpls/i-vpls/m-vpls, routed-vpls or etree";
                    }
                    key "bgp-instance";
