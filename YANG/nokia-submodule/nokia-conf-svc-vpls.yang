                container eth-cfm {
                    description "Enable the eth-cfm context";

                    ///nokia-conf/configure/service/vpls/sap/eth-cfm/collect-lmm-stats
                    leaf collect-lmm-stats {
                        type boolean;
                        default "false";
                        description "Collect statistics for loss measurement message tests";
                    }

