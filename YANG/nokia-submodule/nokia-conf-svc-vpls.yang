                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/spb/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/spb/lsp-refresh-interval
                container lsp-refresh-interval {
                    description "Enter the lsp-refresh-interval context";

                    ///nokia-conf/configure/service/vpls/spb/lsp-refresh-interval/interval
                    leaf interval {
                        type uint32 {
                            range "150..65535";
                        }
                        default "600";
                        description "Specify the interval at which LSPs are refreshed";
                    }

                    ///nokia-conf/configure/service/vpls/spb/lsp-refresh-interval/half-lifetime
                    leaf half-lifetime {
                        type boolean;
                        default "true";
                        description "Refresh LSP interval at half the LSP lifetime";
                    }

