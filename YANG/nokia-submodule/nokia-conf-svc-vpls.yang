                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/lbm-svc-act-responder
                        leaf lbm-svc-act-responder {
                            type boolean;
                            description "Process service activation streams in ETH-CFM LBM";
