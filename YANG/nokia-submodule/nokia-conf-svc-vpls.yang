                        } // container app-profile

                        ///nokia-conf/configure/service/vpls/sap/static-host/ipv4/subscriber-id
                        container subscriber-id {
                            description "Enter the subscriber-id context";

                            ///nokia-conf/configure/service/vpls/sap/static-host/ipv4/subscriber-id/subscriber-id
                            choice subscriber-id {
                                ///nokia-conf/configure/service/vpls/sap/static-host/ipv4/subscriber-id/subscriber-id/string
                                case string {

                                    ///nokia-conf/configure/service/vpls/sap/static-host/ipv4/subscriber-id/subscriber-id/string/string
                                    leaf string {
                                        type types-submgt:subscriber-id;
                                        description "Subscriber identification";
                                    }

                                }
                                ///nokia-conf/configure/service/vpls/sap/static-host/ipv4/subscriber-id/subscriber-id/use-sap-id
                                case use-sap-id {

                                    ///nokia-conf/configure/service/vpls/sap/static-host/ipv4/subscriber-id/subscriber-id/use-sap-id/use-sap-id
                                    leaf use-sap-id {
                                        type empty;
                                        description "Use the SAP id as subscriber ID";
                                    }

                                }
                            }
                        } // container subscriber-id

                        ///nokia-conf/configure/service/vpls/sap/static-host/ipv4/shcv
                        container shcv {
                            description "Enter the shcv context";

                        } // container shcv

                    } // list ipv4

                } // container static-host

                ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt
                container sub-sla-mgmt {
                    when "not(../../pbb-type = 'b-vpls') and not(../../pbb-type = 'i-vpls')" {
                        description "invalid VPLS PBB type";
                    }
                    description "Enter the sub-sla-mgmt context";

                    ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of SAP subscriber management";
                    }

                    ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/sub-ident-policy
                    leaf sub-ident-policy {
                        type leafref {
                            path "../../../../../subscriber-mgmt/sub-ident-policy/name";
                        }
                        description "Subscriber identification policy used on this SAP";
                    }

                    ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/subscriber-limit
                    leaf subscriber-limit {
                        type types-submgt:sap-subscriber-limit;
                        default "1";
                        description "Maximum number of subscribers on this SAP";
                    }

                    ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/mac-da-hashing
                    leaf mac-da-hashing {
                        type boolean;
                        default "false";
                        description "Use destination MAC address instead of subscriber ID to select egress LAG link";
                    }

                    ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults
                    container defaults {
                        description "Enter the defaults context";

                        ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/sub-profile
