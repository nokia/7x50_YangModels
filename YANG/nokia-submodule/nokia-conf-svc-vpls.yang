                ///nokia-conf/configure/service/vpls/pim-snooping/ipv6
                container ipv6 {
                    when "not(../../pbb-type = 'i-vpls')" {
                        description "VPLS PBB type must not be 'i-vpls'";
                    }
                    description "Enter the ipv6 context";

                    ///nokia-conf/configure/service/vpls/pim-snooping/ipv6/admin-state
                    leaf admin-state {
