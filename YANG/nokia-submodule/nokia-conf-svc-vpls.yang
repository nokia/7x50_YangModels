                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/one-way-delay-threshold
                        leaf one-way-delay-threshold {
                            type uint32 {
                                range "0..600";
                            }
                            units "seconds";
                            default "3";
                            description "Threshold time limit for the one-way delay test";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/alarm-notification
                        container alarm-notification {
                            description "Enter the alarm-notification context";

                            ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/alarm-notification/fng-alarm-time
                            leaf fng-alarm-time {
                                type int32 {
                                    range "250|500|1000";
                                }
                                units "centiseconds";
                                description "Time that must expire before an FNG alarm is raised";
                            }

                            ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/alarm-notification/fng-reset-time
                            leaf fng-reset-time {
                                type int32 {
                                    range "250|500|1000";
                                }
                                units "centiseconds";
                                description "Time that must expire before an FNG alarm is reset";
                            }

                        } // container alarm-notification

                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/ais
                        container ais {
                            presence "Enable the ais context";
                            description "Enable the ais context";

                            ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/ais/client-meg-level
                            leaf-list client-meg-level {
                                type uint32 {
                                    range "1..7";
                                }
                                max-elements 7;
                                description "Client MEG level for AIS message generation";
                            }

                            ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/ais/interface-support
                            leaf interface-support {
                                type boolean;
                                default "false";
                                description "Enable generation of AIS PDUs based on endpoint state";
                            }

                            ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/ais/interval
                            leaf interval {
                                type uint32 {
                                    range "1|60";
                                }
                                units "seconds";
                                default "1";
                                description "Transmission interval for AIS messages";
                            }

                            ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/ais/low-priority-defect
                            leaf low-priority-defect {
                                type enumeration {
                                    enum all-def                        { value 1; }
                                    enum mac-rem-err-xcon               { value 2; }
                                }
                                default "all-def";
                                description "Lowest priority defect allowed to generate fault alarm";
                            }

                            ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/ais/priority
                            leaf priority {
                                type types-eth-cfm:frame-priority;
                                default "7";
                                description "Priority of the AIS messages generated by the node";
                            }

                        } // container ais

                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/csf
                        container csf {
                            presence "Enable the csf context";
                            description "Enable the csf context";

                            ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/csf/multiplier
                            leaf multiplier {
                                type decimal64 {
                                    range "0.0|2.0..30.0";
                                    fraction-digits 1;
                                }
                                default "3.5";
                                description "Multiplication factor used to clear the CSF condition";
                            }

                        } // container csf

                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/eth-test
                        container eth-test {
                            presence "Enable the eth-test context";
                            description "Enable the eth-test context";

                            ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/eth-test/bit-error-threshold
                            leaf bit-error-threshold {
                                type uint32 {
                                    range "0..11840";
                                }
                                units "bit errors";
                                default "1";
                                description "Lowest priority defect allowed to generate fault alarm";
                            }

                            ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/eth-test/test-pattern
                            container test-pattern {
                                description "Enter the test-pattern context";

                                ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/eth-test/test-pattern/pattern
                                leaf pattern {
                                    type enumeration {
                                        enum all-zeros                      { value 0; }
                                        enum all-ones                       { value 1; }
                                    }
                                    default "all-zeros";
                                    description "Test pattern for Ethernet Test frames";
                                }

                                ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/eth-test/test-pattern/crc-tlv
                                leaf crc-tlv {
                                    type boolean;
                                    default "false";
                                    description "Generate a CRC checksum";
                                }

                            } // container test-pattern

                        } // container eth-test

                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/grace
                        container grace {
                            description "Enter the grace context";

                            ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/grace/eth-ed
                            container eth-ed {
                                description "Enter the eth-ed context";

                                ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/grace/eth-ed/max-rx-defect-window
                                leaf max-rx-defect-window {
                                    type uint32 {
                                        range "1..86400";
                                    }
                                    units "seconds";
                                    description "Maximum received ETH-ED expected defect window duration";
                                }

                                ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/grace/eth-ed/priority
                                leaf priority {
                                    type int32 {
                                        range "0..7";
                                    }
                                    description "Transmission priority for ETH-ED PDUs";
                                }

                                ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/grace/eth-ed/rx-eth-ed
                                leaf rx-eth-ed {
                                    type boolean;
                                    default "true";
                                    description "Receive and process ETH-ED ITU-T Y.1731 PDUs on the MEP";
                                }

                                ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/grace/eth-ed/tx-eth-ed
                                leaf tx-eth-ed {
                                    type boolean;
                                    default "false";
                                    description "Transmit ETH-ED PDUs from the MEP";
                                }

                            } // container eth-ed

                            ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/grace/eth-vsm-grace
                            container eth-vsm-grace {
                                description "Enter the eth-vsm-grace context";

                                ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/grace/eth-vsm-grace/rx-eth-vsm-grace
                                leaf rx-eth-vsm-grace {
                                    type boolean;
                                    default "true";
                                    description "Receive and process Nokia ETH-CFM Grace PDU on the MEP";
                                }

                                ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mep/grace/eth-vsm-grace/tx-eth-vsm-grace
                                leaf tx-eth-vsm-grace {
                                    type boolean;
                                    default "true";
                                    description "Transmit ETH-ED PDUs from the MEP";
                                }

                            } // container eth-vsm-grace

                        } // container grace

                    } // list mep

                    ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mip
                    list mip {
                        key "primary-vlan";
                        description "Enter the mip list instance";

                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mip/primary-vlan
                        leaf primary-vlan {
                            type types-eth-cfm:vlan-id-or-none;
                            description "VLAN ID to which the MIP is attached";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mip/mac-address
                        leaf mac-address {
                            type types-sros:mac-unicast-address-no-zero;
                            description "MAC address of the MIP";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mip/cfm-vlan-tag
                        leaf cfm-vlan-tag {
                            type types-sros:one-or-two-vlan-tags;
                            description "VLAN tags to apply to CFM PDUs for egress processing";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mip/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/eth-cfm/mip/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list mip

                } // container eth-cfm

                ///nokia-conf/configure/service/vpls/spoke-sdp/static-isid
                container static-isid {
                    description "Enter the static-isid context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/static-isid/range
                    list range {
                        when "../../../pbb-type = 'b-vpls'" {
                            description "supported on b-vpls service";
                        }
                        key "range-id";
                        description "Enter the range list instance";

                        ///nokia-conf/configure/service/vpls/spoke-sdp/static-isid/range/range-id
                        leaf range-id {
                            type uint32 {
                                range "1..8191";
                            }
                            description "Range ID for static ISID";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/static-isid/range/start
                        leaf start {
                            type types-services:isid-no-zero;
                            description "Lower bound of the ISID range";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/static-isid/range/end
                        leaf end {
                            type types-services:isid-no-zero;
                            description "Upper bound of the ISID range";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/static-isid/range/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/static-isid/range/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list range

                } // container static-isid

                ///nokia-conf/configure/service/vpls/spoke-sdp/mrp
                container mrp {
                    when "../../pbb-type = 'b-vpls' and not(../../m-vpls = 'true')" {
                        description "supported on non managed b-vpls service";
                    }
                    description "Enter the mrp context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/mrp/join-time
                    leaf join-time {
                        type uint32 {
                            range "1..10";
                        }
                        units "deciseconds";
                        default "2";
                        description "Maximum rate for attribute join messages sent on SDP";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/mrp/leave-time
                    leaf leave-time {
                        type uint32 {
                            range "30..60";
                        }
                        units "deciseconds";
                        default "30";
                        description "Time in LV state before transition to MT state";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/mrp/leave-all-time
                    leaf leave-all-time {
                        type uint32 {
                            range "60..300";
                        }
                        units "deciseconds";
                        default "100";
                        description "Frequency of LeaveAll PDUs by LeaveAll state machine";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/mrp/periodic-time
                    leaf periodic-time {
                        type uint32 {
                            range "10..100";
                        }
                        units "deciseconds";
                        default "10";
                        description "Frequency of periodic events generated by state machine";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/mrp/periodic-timer
                    leaf periodic-timer {
                        type boolean;
                        default "false";
                        description "Enable the Periodic Transmission Timer";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/mrp/policy
                    leaf policy {
                        type leafref {
                            path "../../../../mrp/policy/policy-name";
                        }
                        description "MRP policy to control Group B-MAC attributes";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/mrp/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/mrp/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container mrp

            } // list spoke-sdp

            ///nokia-conf/configure/service/vpls/mesh-sdp
            list mesh-sdp {
                key "sdp-bind-id";
                description "Enter the mesh-sdp list instance";

                ///nokia-conf/configure/service/vpls/mesh-sdp/sdp-bind-id
                leaf sdp-bind-id {
                    type types-services:sdp-bind-id;
                    description "SDP binding ID";
                }

                ///nokia-conf/configure/service/vpls/mesh-sdp/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/vpls/mesh-sdp/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "enable";
                    description "Administrative state of the SDP binding to the service";
                }

                ///nokia-conf/configure/service/vpls/mesh-sdp/vlan-vc-tag
                leaf vlan-vc-tag {
                    type types-services:vlan-vc-tag;
                    description "SDP bind VC tag";
                }

                ///nokia-conf/configure/service/vpls/mesh-sdp/collect-stats
                leaf collect-stats {
                    type boolean;
                    default "false";
                    description "Allow agent to collect accounting statistics";
                }

                ///nokia-conf/configure/service/vpls/mesh-sdp/accounting-policy
                leaf accounting-policy {
                    type leafref {
                        path "../../../../log/accounting-policy/policy-id";
                    }
                    description "Policy to collect accounting statistics";
                }

                ///nokia-conf/configure/service/vpls/mesh-sdp/control-word
                leaf control-word {
                    type boolean;
                    default "false";
                    description "Use the control word as preferred";
                }

                ///nokia-conf/configure/service/vpls/mesh-sdp/force-vc-forwarding
                leaf force-vc-forwarding {
                    type enumeration {
                        enum vlan                           { value 1; }
                        enum qinq-c-tag-c-tag               { value 2; }
                        enum qinq-s-tag-c-tag               { value 3; }
                    }
                    description "VC forwarding action";
                }

                ///nokia-conf/configure/service/vpls/mesh-sdp/etree-root-leaf-tag
                leaf etree-root-leaf-tag {
                    when "../../etree = 'true'" {
                        description "etree must be set";
                    }
                    type boolean;
                    sros-ext:immutable;
                    default "false";
                    description "Status for E-tree root leaf tag";
                }

                ///nokia-conf/configure/service/vpls/mesh-sdp/etree-leaf
                leaf etree-leaf {
                    when "../../etree = 'true'" {
                        description "etree must be set";
                    }
                    type boolean;
                    sros-ext:immutable;
                    default "false";
                    description "Enable etree leaf access-circuit status";
                }

                ///nokia-conf/configure/service/vpls/mesh-sdp/bfd-template
                leaf bfd-template {
                    status obsolete;
                    type leafref {
                        path "../../../../bfd/bfd-template/name";
                    }
                    description "BFD template associated with SDP binding";
                }

                ///nokia-conf/configure/service/vpls/mesh-sdp/vc-type
                leaf vc-type {
                    type types-services:vpls-sdp-bind-vc-type;
                    sros-ext:auto-restart-to-modify;
                    default "ether";
                    description "Type of virtual circuit (VC) associated with the SDP binding; VPLS not supported";
                }

                ///nokia-conf/configure/service/vpls/mesh-sdp/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/mesh-sdp/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/mesh-sdp/label
                choice label {
                    ///nokia-conf/configure/service/vpls/mesh-sdp/label/entropy
                    case entropy {

                        ///nokia-conf/configure/service/vpls/mesh-sdp/label/entropy/entropy-label
                        leaf entropy-label {
                            type empty;
                            description "Enable the use of entropy labels for spoke SDPs";
                        }

                    }
                    ///nokia-conf/configure/service/vpls/mesh-sdp/label/hash
                    case hash {

                        ///nokia-conf/configure/service/vpls/mesh-sdp/label/hash/hash-label
                        container hash-label {
                            presence "Enable the hash-label context";
                            description "Enable the hash-label context";

                            ///nokia-conf/configure/service/vpls/mesh-sdp/label/hash/hash-label/signal-capability
                            leaf signal-capability {
                                type empty;
                                description "Signal hash label capability to the remote PE";
                            }

                        } // container hash-label

                    }
                }
                ///nokia-conf/configure/service/vpls/mesh-sdp/ingress
                container ingress {
                    description "Enter the ingress context";

                    ///nokia-conf/configure/service/vpls/mesh-sdp/ingress/vc-label
                    leaf vc-label {
                        type types-services:ingress-vc-label;
                        sros-ext:auto-restart-to-modify;
                        description "Ingress MPLS VC label to send packets to the far end";
                    }

                    ///nokia-conf/configure/service/vpls/mesh-sdp/ingress/filter
                    container filter {
                        description "Enter the filter context";

                        ///nokia-conf/configure/service/vpls/mesh-sdp/ingress/filter/mac
                        leaf mac {
                            type leafref {
                                path "../../../../../../filter/mac-filter/filter-name";
                            }
                            description "MAC filter policy name";
                        }

                        ///nokia-conf/configure/service/vpls/mesh-sdp/ingress/filter/ip
                        leaf ip {
                            type leafref {
                                path "../../../../../../filter/ip-filter/filter-name";
                            }
                            description "IPv4 filter policy name";
                        }

                        ///nokia-conf/configure/service/vpls/mesh-sdp/ingress/filter/ipv6
                        leaf ipv6 {
                            type leafref {
                                path "../../../../../../filter/ipv6-filter/filter-name";
                            }
                            description "IPv6 filter policy name";
                        }

                    } // container filter

                    ///nokia-conf/configure/service/vpls/mesh-sdp/ingress/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-conf/configure/service/vpls/mesh-sdp/ingress/qos/network
                        container network {
                            description "Enter the network context";

                            ///nokia-conf/configure/service/vpls/mesh-sdp/ingress/qos/network/policy-name
                            leaf policy-name {
                                type leafref {
                                    path "../../../../../../../qos/network/network-policy-name";
                                }
                                description "Network policy ID";
                            }

                            ///nokia-conf/configure/service/vpls/mesh-sdp/ingress/qos/network/fp-redirect-group
                            container fp-redirect-group {
                                description "Enter the fp-redirect-group context";

                                ///nokia-conf/configure/service/vpls/mesh-sdp/ingress/qos/network/fp-redirect-group/group-name
                                leaf group-name {
                                    type leafref {
                                        path "../../../../../../../../qos/queue-group-templates/ingress/queue-group/ingress-queue-group-name";
                                    }
                                    description "Name of the forwarding plane queue group template";
                                }

                                ///nokia-conf/configure/service/vpls/mesh-sdp/ingress/qos/network/fp-redirect-group/instance
                                leaf instance {
                                    type types-services:qos-qgrp-instance-id;
                                    description "Instance of FP ingress queue group for the SDP binding";
                                }

                            } // container fp-redirect-group

                        } // container network

                    } // container qos

                } // container ingress

                ///nokia-conf/configure/service/vpls/mesh-sdp/egress
                container egress {
                    description "Enter the egress context";

                    ///nokia-conf/configure/service/vpls/mesh-sdp/egress/vc-label
                    leaf vc-label {
                        type types-services:egress-vc-label;
                        sros-ext:auto-restart-to-modify;
                        description "Egress MPLS VC label to send packets to the far end";
                    }

                    ///nokia-conf/configure/service/vpls/mesh-sdp/egress/filter
                    container filter {
                        description "Enter the filter context";

                        ///nokia-conf/configure/service/vpls/mesh-sdp/egress/filter/mac
                        leaf mac {
                            type leafref {
                                path "../../../../../../filter/mac-filter/filter-name";
                            }
                            description "MAC filter policy name";
                        }

                        ///nokia-conf/configure/service/vpls/mesh-sdp/egress/filter/ip
                        leaf ip {
                            type leafref {
                                path "../../../../../../filter/ip-filter/filter-name";
                            }
                            description "IPv4 filter policy name";
                        }

                        ///nokia-conf/configure/service/vpls/mesh-sdp/egress/filter/ipv6
                        leaf ipv6 {
                            type leafref {
                                path "../../../../../../filter/ipv6-filter/filter-name";
                            }
                            description "IPv6 filter policy name";
                        }

                    } // container filter

                    ///nokia-conf/configure/service/vpls/mesh-sdp/egress/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-conf/configure/service/vpls/mesh-sdp/egress/qos/network
                        container network {
                            description "Enter the network context";

                            ///nokia-conf/configure/service/vpls/mesh-sdp/egress/qos/network/policy-name
                            leaf policy-name {
                                type leafref {
                                    path "../../../../../../../qos/network/network-policy-name";
                                }
                                description "Network policy ID";
                            }

                            ///nokia-conf/configure/service/vpls/mesh-sdp/egress/qos/network/port-redirect-group
