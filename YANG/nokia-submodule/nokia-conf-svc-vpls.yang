                    ///nokia-conf/configure/service/vpls/capture-sap/dhcp/python-policy
                    leaf python-policy {
                        type leafref {
                            path "../../../../../python/python-policy/name";
                        }
                        description "Python policy";
                    }

                    ///nokia-conf/configure/service/vpls/capture-sap/dhcp/user-db
                    leaf user-db {
                        type leafref {
                            path "../../../../../subscriber-mgmt/local-user-db/name";
