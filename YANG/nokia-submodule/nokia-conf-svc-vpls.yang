                            ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/alarm-notification/fng-reset-time
                            leaf fng-reset-time {
                                type int32 {
                                    range "250|500|1000";
                                }
                                units "centiseconds";
                                description "Time that must expire before an FNG alarm is reset";
                            }

                        } // container alarm-notification

                        ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/ais
                        container ais {
                            presence "Enable the ais context";
