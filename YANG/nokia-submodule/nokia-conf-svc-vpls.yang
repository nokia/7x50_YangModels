                ///nokia-conf/configure/service/vpls/capture-sap/dhcp6
                container dhcp6 {
                    description "Enter the dhcp6 context";

                    ///nokia-conf/configure/service/vpls/capture-sap/dhcp6/python-policy
                    leaf python-policy {
                        type leafref {
                            path "../../../../../python/python-policy/name";
                        }
                        description "Python policy";
