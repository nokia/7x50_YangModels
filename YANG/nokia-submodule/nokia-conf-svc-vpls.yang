                } // container dhcp6

                ///nokia-conf/configure/service/vpls/capture-sap/pppoe
                container pppoe {
                    description "Enter the pppoe context";

                    ///nokia-conf/configure/service/vpls/capture-sap/pppoe/policy
                    leaf policy {
                        type leafref {
                            path "../../../../../subscriber-mgmt/ppp-policy/name";
                        }
                        description "PPP policy";
                    }

                    ///nokia-conf/configure/service/vpls/capture-sap/pppoe/python-policy
                    leaf python-policy {
                        type leafref {
                            path "../../../../../python/python-policy/name";
                        }
                        description "Python policy";
                    }

                    ///nokia-conf/configure/service/vpls/capture-sap/pppoe/user-db
                    leaf user-db {
                        type leafref {
                            path "../../../../../subscriber-mgmt/local-user-db/name";
                        }
