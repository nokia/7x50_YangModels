                    ///nokia-conf/configure/service/vpls/spoke-sdp/stp/link-type
                    leaf link-type {
                        type types-services:stp-link-type;
                        default "pt-pt";
                        description "Configure STP link-type";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/stp/path-cost
                    leaf path-cost {
                        type types-services:stp-path-cost;
                        default "10";
                        description "Configure path-cost";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/stp/priority
                    leaf priority {
                        type int32 {
                            range "0..255";
                        }
                        default "128";
                        description "Configure STP priority";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/stp/root-guard
                    leaf root-guard {
                        type boolean;
                        default "false";
                        description "Enable/disable STP root-guard";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/stp/port-num
                    leaf port-num {
                        type types-services:stp-port-num;
                        description "Virtual port number";
                    }

                } // container stp

