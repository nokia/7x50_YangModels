                presence "Enable the spb context";
                description "Enable the spb context";

                ///nokia-conf/configure/service/vpls/spb/isis-instance
                leaf isis-instance {
                    type int32 {
                        range "1024..2047";
                    }
                    sros-ext:immutable;
                    default "1024";
                    description "ISIS instance";
                }

                ///nokia-conf/configure/service/vpls/spb/fid
                leaf fid {
                    type int32 {
                        range "1..4095";
                    }
                    sros-ext:immutable;
                    default "1";
                    description "FID identifier";
                }

                ///nokia-conf/configure/service/vpls/spb/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of SPB";
                }

                ///nokia-conf/configure/service/vpls/spb/lsp-lifetime
                leaf lsp-lifetime {
                    type uint32 {
                        range "350..65535";
