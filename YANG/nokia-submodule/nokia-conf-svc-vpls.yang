submodule nokia-conf-svc-vpls {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-eth-cfm            { prefix "types-eth-cfm"; }
    import nokia-types-igmp               { prefix "types-igmp"; }
    import nokia-types-isa                { prefix "types-isa"; }
    import nokia-types-mcast-cac          { prefix "types-mcast-cac"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-router             { prefix "types-router"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-subscriber-mgmt    { prefix "types-submgt"; }

    include nokia-conf-aa-group;
    include nokia-conf-application-assurance;
    include nokia-conf-bfd;
    include nokia-conf-eth-cfm;
    include nokia-conf-filter;
    include nokia-conf-groups;
    include nokia-conf-isa;
    include nokia-conf-log;
    include nokia-conf-mcast-cac;
    include nokia-conf-mcast-mgmt;
    include nokia-conf-policy-options;
    include nokia-conf-port;
    include nokia-conf-port-eth-access;
    include nokia-conf-port-ethernet;
    include nokia-conf-pxc;
    include nokia-conf-python;
    include nokia-conf-qos;
    include nokia-conf-redundancy;
    include nokia-conf-router;
    include nokia-conf-router-mpls;
    include nokia-conf-router-segment-routing;
    include nokia-conf-service;
    include nokia-conf-submgt-local-user-db;
    include nokia-conf-subscriber-mgmt;
    include nokia-conf-system;
    include nokia-conf-system-security;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure vpls services on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-svc-vpls YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-05-25";

    grouping conf-svc-vpls {
        ///nokia-conf/configure/service/vpls
        list vpls {
            key "service-name";
            description "Enter the vpls list instance";

            ///nokia-conf/configure/service/vpls/service-name
            leaf service-name {
                type types-services:service-name;
                description "Administrative service name";
            }

            ///nokia-conf/configure/service/vpls/service-id
            leaf service-id {
                type types-services:external-service-id;
                sros-ext:immutable;
                description "Service ID";
            }

            ///nokia-conf/configure/service/vpls/description
            leaf description {
                type types-sros:description;
                description "Text description";
            }

            ///nokia-conf/configure/service/vpls/customer
            leaf customer {
                type leafref {
                    path "../../customer/customer-name";
                }
                sros-ext:immutable;
                mandatory true;
                description "Service customer ID";
            }

            ///nokia-conf/configure/service/vpls/admin-state
            leaf admin-state {
                type types-sros:admin-state;
                default "disable";
                description "Administrative state of the service";
            }

            ///nokia-conf/configure/service/vpls/vpn-id
            leaf vpn-id {
                type types-services:vpn-id;
                sros-ext:immutable;
                description "VPN identifier for the service";
            }

            ///nokia-conf/configure/service/vpls/service-mtu
            leaf service-mtu {
                type int32 {
                    range "1..9782";
                }
                description "MTU size";
            }

            ///nokia-conf/configure/service/vpls/etree
            leaf etree {
                type boolean;
                sros-ext:immutable;
                default "false";
                description "Use VPLS service as an E-Tree VPLS";
            }

            ///nokia-conf/configure/service/vpls/m-vpls
            leaf m-vpls {
                type boolean;
                sros-ext:immutable;
                default "false";
                description "Specify whether this is a management VPLS";
            }

            ///nokia-conf/configure/service/vpls/pbb-type
            leaf pbb-type {
                type types-services:pbb-type;
                sros-ext:immutable;
                description "PBB VPLS type";
            }

            ///nokia-conf/configure/service/vpls/mcast-ipv6-snooping-scope
            leaf mcast-ipv6-snooping-scope {
                type enumeration {
                    enum sg-based                       { value 1; }
                    enum mac-based                      { value 2; }
                }
                default "mac-based";
                description "IPv6 multicast snooping scope";
            }

            ///nokia-conf/configure/service/vpls/multicast-info-policy
            leaf multicast-info-policy {
                type leafref {
                    path "../../../multicast-management/multicast-info-policy/policy-name";
                }
                description "Multicast information policy";
            }

            ///nokia-conf/configure/service/vpls/shcv-policy-ipv4
            leaf shcv-policy-ipv4 {
                type leafref {
                    path "../../../subscriber-mgmt/shcv-policy/name";
                }
                description "Subscriber host connectivity verification policy for IPv4";
            }

            ///nokia-conf/configure/service/vpls/temp-flooding
            leaf temp-flooding {
                type int32 {
                    range "3..600";
                }
                units "seconds";
                description "Temporary flooding";
            }

            ///nokia-conf/configure/service/vpls/ignore-l2vpn-mtu-mismatch
            leaf ignore-l2vpn-mtu-mismatch {
                type boolean;
                default "false";
                description "Ignore the L2 VPN MTU mismatch with local service MTU";
            }

            ///nokia-conf/configure/service/vpls/tunnel-elmi
            leaf tunnel-elmi {
                type boolean;
                default "false";
                description "Allow E-LMI tunneling";
            }

            ///nokia-conf/configure/service/vpls/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/service/vpls/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/service/vpls/fdb
            container fdb {
                description "Enter the fdb context";

                ///nokia-conf/configure/service/vpls/fdb/discard-unknown
                leaf discard-unknown {
                    type boolean;
                    default "false";
                    description "Discard packets with unknown destination MAC addresses";
                }

                ///nokia-conf/configure/service/vpls/fdb/mac-subnet-length
                leaf mac-subnet-length {
                    when "not(../../pbb-type)" {
                        description "VPLS PBB type must be 'none'";
                    }
                    type int32 {
                        range "24..48";
                    }
                    default "48";
                    description "Number of bits performing MAC learning or MAC switching";
                }

                ///nokia-conf/configure/service/vpls/fdb/selective-learning
                leaf selective-learning {
                    when "not(../../pbb-type = 'b-vpls')" {
                        description "VPLS PBB type must not be 'b-vpls'";
                    }
                    type boolean;
                    default "false";
                    description "Allocate FDB entries on selectively learned line cards";
                }

                ///nokia-conf/configure/service/vpls/fdb/table
                container table {
                    description "Enter the table context";

                    ///nokia-conf/configure/service/vpls/fdb/table/high-wmark
                    leaf high-wmark {
                        type int32 {
                            range "0..100";
                        }
                        default "95";
                        description "High watermark for the FDB table";
                    }

                    ///nokia-conf/configure/service/vpls/fdb/table/low-wmark
                    leaf low-wmark {
                        type int32 {
                            range "0..100";
                        }
                        default "90";
                        description "Low watermark for the FDB table";
                    }

                    ///nokia-conf/configure/service/vpls/fdb/table/size
                    leaf size {
                        type int32 {
                            range "1..511999";
                        }
                        default "250";
                        description "Maximum MAC entries in the FDB";
                    }

                } // container table

                ///nokia-conf/configure/service/vpls/fdb/mac-learning
                container mac-learning {
                    description "Enter the mac-learning context";

                    ///nokia-conf/configure/service/vpls/fdb/mac-learning/learning
                    leaf learning {
                        type boolean;
                        default "true";
                        description "Enable learning of new MAC addresses";
                    }

                    ///nokia-conf/configure/service/vpls/fdb/mac-learning/aging
                    leaf aging {
                        type boolean;
                        default "true";
                        description "Enable aging of MAC addresses";
                    }

                    ///nokia-conf/configure/service/vpls/fdb/mac-learning/local-age-time
                    leaf local-age-time {
                        type int32 {
                            range "60..86400";
                        }
                        default "300";
                        description "Aging time for locally learned MAC addresses";
                    }

                    ///nokia-conf/configure/service/vpls/fdb/mac-learning/remote-age-time
                    leaf remote-age-time {
                        type int32 {
                            range "60..86400";
                        }
                        default "900";
                        description "Aging time for remotely learned MAC addresses";
                    }

                } // container mac-learning

                ///nokia-conf/configure/service/vpls/fdb/mac-move
                container mac-move {
                    description "Enter the mac-move context";

                    ///nokia-conf/configure/service/vpls/fdb/mac-move/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of MAC move";
                    }

                    ///nokia-conf/configure/service/vpls/fdb/mac-move/move-frequency
                    leaf move-frequency {
                        type uint32 {
                            range "1..10";
                        }
                        default "2";
                        description "Maximum rate MACs can be re-learned in the VPLS service";
                    }

                    ///nokia-conf/configure/service/vpls/fdb/mac-move/retry-count
                    leaf retry-count {
                        type union {
                            type uint32 {
                                range "1..255";
                            }
                            type enumeration {
                                enum unlimited                      { value 0; }
                            }
                        }
                        default "3";
                        description "Number of retries for re-enabling the SAP or SDP";
                    }

                    ///nokia-conf/configure/service/vpls/fdb/mac-move/hold-down-time
                    leaf hold-down-time {
                        type uint32 {
                            range "0..600";
                        }
                        units "seconds";
                        default "10";
                        description "Wait time before re-enabling disabled SAP";
                    }

                    ///nokia-conf/configure/service/vpls/fdb/mac-move/primary-cumulative-factor
                    leaf primary-cumulative-factor {
                        type uint32 {
                            range "3..10";
                        }
                        default "3";
                        description "Factor for MAC-relearn periods for MAC-relearn rate";
                    }

                    ///nokia-conf/configure/service/vpls/fdb/mac-move/secondary-cumulative-factor
                    leaf secondary-cumulative-factor {
                        type uint32 {
                            range "2..9";
                        }
                        default "2";
                        description "Number of periods to measure mac-relearn rate";
                    }

                    ///nokia-conf/configure/service/vpls/fdb/mac-move/sap
                    list sap {
                        key "sap-id";
                        description "Enter the sap list instance";

                        ///nokia-conf/configure/service/vpls/fdb/mac-move/sap/sap-id
                        leaf sap-id {
                            type leafref {
                                path "../../../../sap/sap-id";
                            }
                            description "SAP identifier";
                        }

                        ///nokia-conf/configure/service/vpls/fdb/mac-move/sap/level
                        leaf level {
                            type enumeration {
                                enum primary                        { value 1; }
                                enum secondary                      { value 2; }
                            }
                            mandatory true;
                            description "Primary or secondary port level";
                        }

                        ///nokia-conf/configure/service/vpls/fdb/mac-move/sap/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/fdb/mac-move/sap/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list sap

                    ///nokia-conf/configure/service/vpls/fdb/mac-move/spoke-sdp
                    list spoke-sdp {
                        key "sdp-bind-id";
                        description "Enter the spoke-sdp list instance";

                        ///nokia-conf/configure/service/vpls/fdb/mac-move/spoke-sdp/sdp-bind-id
                        leaf sdp-bind-id {
                            type leafref {
                                path "../../../../spoke-sdp/sdp-bind-id";
                            }
                            description "SDP binding ID";
                        }

                        ///nokia-conf/configure/service/vpls/fdb/mac-move/spoke-sdp/level
                        leaf level {
                            type enumeration {
                                enum primary                        { value 1; }
                                enum secondary                      { value 2; }
                            }
                            mandatory true;
                            description "Primary or secondary port level";
                        }

                        ///nokia-conf/configure/service/vpls/fdb/mac-move/spoke-sdp/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/fdb/mac-move/spoke-sdp/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list spoke-sdp

                } // container mac-move

                ///nokia-conf/configure/service/vpls/fdb/static-mac
                container static-mac {
                    description "Enter the static-mac context";

                    ///nokia-conf/configure/service/vpls/fdb/static-mac/mac
                    list mac {
                        key "mac-address";
                        description "Enter the mac list instance";

                        ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/mac-address
                        leaf mac-address {
                            type types-sros:mac-unicast-address-no-zero;
                            description "Static MAC address to SAP/SDP-binding or black-hole";
                        }

                        ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/monitor
                        leaf monitor {
                            type enumeration {
                                enum none                           { value 0; }
                                enum forward-status                 { value 1; }
                            }
                            sros-ext:immutable;
                            default "none";
                            description "Entity to be monitored to decide whether this entry can be installed in the FDB";
                        }

                        ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/destination
                        choice destination {
                            mandatory true;
                            ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/destination/spoke-sdp
                            case spoke-sdp {

                                ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/destination/spoke-sdp/spoke-sdp
                                leaf spoke-sdp {
                                    type leafref {
                                        path "../../../../spoke-sdp/sdp-bind-id";
                                    }
                                    sros-ext:immutable;
                                    description "Spoke SDP bind associated with this MAC";
                                }

                            }
                            ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/destination/mesh-sdp
                            case mesh-sdp {

                                ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/destination/mesh-sdp/mesh-sdp
                                leaf mesh-sdp {
                                    type leafref {
                                        path "../../../../mesh-sdp/sdp-bind-id";
                                    }
                                    sros-ext:immutable;
                                    description "Mesh SDP bind associated with this MAC";
                                }

                            }
                            ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/destination/sap
                            case sap {

                                ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/destination/sap/sap
                                leaf sap {
                                    type leafref {
                                        path "../../../../sap/sap-id";
                                    }
                                    sros-ext:immutable;
                                    description "SAP associated with this MAC";
                                }

                            }
                            ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/destination/blackhole
                            case blackhole {

                                ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/destination/blackhole/blackhole
                                leaf blackhole {
                                    type empty;
                                    sros-ext:immutable;
                                    description "Create a static FDB entry for the MAC address to black-hole traffic";
                                }

                            }
                            ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/destination/endpoint
                            case endpoint {

                                ///nokia-conf/configure/service/vpls/fdb/static-mac/mac/destination/endpoint/endpoint
                                leaf endpoint {
                                    type leafref {
                                        path "../../../../endpoint/name";
                                    }
                                    sros-ext:immutable;
                                    description "Endpoint associated with the MAC";
                                }

                            }
                        }
                    } // list mac

                } // container static-mac

            } // container fdb

            ///nokia-conf/configure/service/vpls/mfib
            container mfib {
                description "Enter the mfib context";

                ///nokia-conf/configure/service/vpls/mfib/table
                container table {
                    description "Enter the table context";

                    ///nokia-conf/configure/service/vpls/mfib/table/high-wmark
                    leaf high-wmark {
                        type int32 {
                            range "0..100";
                        }
                        default "95";
                        description "High watermark for the MFIB table";
                    }

                    ///nokia-conf/configure/service/vpls/mfib/table/low-wmark
                    leaf low-wmark {
                        type int32 {
                            range "0..100";
                        }
                        default "90";
                        description "Low watermark for the MFIB table";
                    }

                    ///nokia-conf/configure/service/vpls/mfib/table/size
                    leaf size {
                        type int32 {
                            range "1..40959";
                        }
                        description "Maximum SG entries in the MFIB";
                    }

                } // container table

            } // container mfib

            ///nokia-conf/configure/service/vpls/mac-flush
            container mac-flush {
                description "Enter the mac-flush context";

                ///nokia-conf/configure/service/vpls/mac-flush/tldp
                container tldp {
                    description "Enter the tldp context";

                    ///nokia-conf/configure/service/vpls/mac-flush/tldp/propagate
                    leaf propagate {
                        type boolean;
                        default "false";
                        description "Propagate MAC flush messages received from the T-LDP";
                    }

                    ///nokia-conf/configure/service/vpls/mac-flush/tldp/send-on-failure
                    leaf send-on-failure {
                        type boolean;
                        default "false";
                        description "Send MAC withdraw message on SAP/Spoke-SDP failure";
                    }

                } // container tldp

            } // container mac-flush

            ///nokia-conf/configure/service/vpls/vxlan
            container vxlan {
                when "not(../pbb-type)" {
                    description "VPLS PBB type must be 'none'";
                }
                description "Enter the vxlan context";

                ///nokia-conf/configure/service/vpls/vxlan/source-vtep
                leaf source-vtep {
                    type types-sros:ip-unicast-address;
                    description "Vxlan source virtual tunnel endpoint information";
                }

                ///nokia-conf/configure/service/vpls/vxlan/instance
                list instance {
                    when "../../etree = 'false' and ../../m-vpls = 'false'" {
                        description "etree or m-vpls must be unset";
                    }
                    key "vxlan-instance";
                    description "Enter the instance list instance";

                    ///nokia-conf/configure/service/vpls/vxlan/instance/vxlan-instance
                    leaf vxlan-instance {
                        type uint32 {
                            range "1..2";
                        }
                        description "VXLAN instance";
                    }

                    ///nokia-conf/configure/service/vpls/vxlan/instance/vni
                    leaf vni {
                        type types-services:vxlan-vni;
                        sros-ext:immutable;
                        mandatory true;
                        description "VNI of the VXLAN";
                    }

                    ///nokia-conf/configure/service/vpls/vxlan/instance/source-vtep-security
                    leaf source-vtep-security {
                        type boolean;
                        default "false";
                        description "Enable/disable source vtep security";
                    }

                    ///nokia-conf/configure/service/vpls/vxlan/instance/rx-discard-on-ndf
                    leaf rx-discard-on-ndf {
                        type enumeration {
                            enum bm                             { value 0; }
                            enum bum                            { value 1; }
                            enum none                           { value 2; }
                        }
                        default "bm";
                        description "Received multicast traffic type discarded on NDF";
                    }

                    ///nokia-conf/configure/service/vpls/vxlan/instance/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/vxlan/instance/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/vxlan/instance/assisted-replication
                    container assisted-replication {
                        description "Enter the assisted-replication context";

                        ///nokia-conf/configure/service/vpls/vxlan/instance/assisted-replication/role
                        choice role {
                            ///nokia-conf/configure/service/vpls/vxlan/instance/assisted-replication/role/leaf
                            case leaf {

                                ///nokia-conf/configure/service/vpls/vxlan/instance/assisted-replication/role/leaf/leaf
                                container leaf {
                                    presence "Enable the leaf context";
                                    description "Enable the leaf context";

                                    ///nokia-conf/configure/service/vpls/vxlan/instance/assisted-replication/role/leaf/leaf/acttime
                                    leaf acttime {
                                        type uint32 {
                                            range "1..255";
                                        }
                                        units "seconds";
                                        description "Time for the leaf to wait before sending traffic to a new replicator";
                                    }

                                } // container leaf

                            }
                            ///nokia-conf/configure/service/vpls/vxlan/instance/assisted-replication/role/replicator
                            case replicator {

                                ///nokia-conf/configure/service/vpls/vxlan/instance/assisted-replication/role/replicator/replicator
                                leaf replicator {
                                    type empty;
                                    description "AR role as replicator";
                                }

                            }
                        }
                    } // container assisted-replication

                    ///nokia-conf/configure/service/vpls/vxlan/instance/network
                    container network {
                        description "Enter the network context";

                        ///nokia-conf/configure/service/vpls/vxlan/instance/network/ingress
                        container ingress {
                            description "Enter the ingress context";

                            ///nokia-conf/configure/service/vpls/vxlan/instance/network/ingress/qos
                            container qos {
                                description "Enter the qos context";

                                ///nokia-conf/configure/service/vpls/vxlan/instance/network/ingress/qos/network
                                container network {
                                    description "Enter the network context";

                                    ///nokia-conf/configure/service/vpls/vxlan/instance/network/ingress/qos/network/policy-name
                                    leaf policy-name {
                                        type leafref {
                                            path "../../../../../../../../../qos/network/network-policy-name";
                                        }
                                        description "Ingress network policy name applied to this SDP binding";
                                    }

