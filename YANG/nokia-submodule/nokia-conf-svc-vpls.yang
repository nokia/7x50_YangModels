                        leaf ccm-ltm-priority {
                            type types-eth-cfm:frame-priority;
                            default "7";
                            description "Priority of CCM and LTM messages transmitted by the MEP";
                        }

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/ccm-padding-size
                        leaf ccm-padding-size {
                            type uint32 {
                                range "3..1500";
                            }
                            description "Number of octets of padding to insert in CCM packets";
                        }

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/lbm-svc-act-responder
                        leaf lbm-svc-act-responder {
                            type boolean;
                            description "Process service activation streams in ETH-CFM LBM";
                        }

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/one-way-delay-threshold
                        leaf one-way-delay-threshold {
                            type uint32 {
                                range "0..600";
                            }
                            units "seconds";
                            default "3";
                            description "Threshold time limit for the one-way delay test";
                        }

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/alarm-notification
                        container alarm-notification {
                            description "Enter the alarm-notification context";

                            ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/alarm-notification/fng-alarm-time
                            leaf fng-alarm-time {
                                type int32 {
                                    range "250|500|1000";
                                }
                                units "centiseconds";
                                description "Time that must expire before an FNG alarm is raised";
                            }

                            ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/alarm-notification/fng-reset-time
                            leaf fng-reset-time {
                                type int32 {
                                    range "250|500|1000";
                                }
                                units "centiseconds";
                                description "Time that must expire before an FNG alarm is reset";
                            }

                        } // container alarm-notification

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/ais
                        container ais {
                            presence "Enable the ais context";
                            description "Enable the ais context";

                            ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/ais/client-meg-level
                            leaf-list client-meg-level {
                                type uint32 {
                                    range "1..7";
                                }
                                max-elements 7;
                                description "Client MEG level for AIS message generation";
                            }

                            ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/ais/interface-support
                            leaf interface-support {
                                type boolean;
                                default "false";
                                description "Enable generation of AIS PDUs based on endpoint state";
                            }

                            ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/ais/interval
                            leaf interval {
                                type uint32 {
                                    range "1|60";
                                }
                                units "seconds";
                                default "1";
                                description "Transmission interval for AIS messages";
                            }

                            ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/ais/low-priority-defect
                            leaf low-priority-defect {
                                type enumeration {
                                    enum all-def                        { value 1; }
                                    enum mac-rem-err-xcon               { value 2; }
                                }
                                default "all-def";
                                description "Lowest priority defect allowed to generate fault alarm";
                            }

                            ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/ais/priority
                            leaf priority {
                                type types-eth-cfm:frame-priority;
                                default "7";
                                description "Priority of the AIS messages generated by the node";
                            }

                        } // container ais

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/csf
                        container csf {
                            presence "Enable the csf context";
                            description "Enable the csf context";

                            ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/csf/multiplier
                            leaf multiplier {
                                type decimal64 {
                                    range "0.0|2.0..30.0";
                                    fraction-digits 1;
                                }
                                default "3.5";
                                description "Multiplication factor used to clear the CSF condition";
                            }

                        } // container csf

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/eth-test
                        container eth-test {
                            presence "Enable the eth-test context";
                            description "Enable the eth-test context";

                            ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/eth-test/bit-error-threshold
                            leaf bit-error-threshold {
                                type uint32 {
                                    range "0..11840";
                                }
                                units "bit errors";
                                default "1";
                                description "Lowest priority defect allowed to generate fault alarm";
                            }

                            ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/eth-test/test-pattern
                            container test-pattern {
                                description "Enter the test-pattern context";

                                ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/eth-test/test-pattern/pattern
                                leaf pattern {
                                    type enumeration {
                                        enum all-zeros                      { value 0; }
                                        enum all-ones                       { value 1; }
                                    }
                                    default "all-zeros";
                                    description "Test pattern for Ethernet Test frames";
                                }

                                ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/eth-test/test-pattern/crc-tlv
                                leaf crc-tlv {
                                    type boolean;
                                    default "false";
                                    description "Generate a CRC checksum";
                                }

                            } // container test-pattern

                        } // container eth-test

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/grace
                        container grace {
                            description "Enter the grace context";

                            ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/grace/eth-ed
                            container eth-ed {
                                description "Enter the eth-ed context";

                                ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/grace/eth-ed/max-rx-defect-window
                                leaf max-rx-defect-window {
                                    type uint32 {
                                        range "1..86400";
                                    }
                                    units "seconds";
                                    description "Maximum received ETH-ED expected defect window duration";
                                }

                                ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/grace/eth-ed/priority
                                leaf priority {
                                    type int32 {
                                        range "0..7";
                                    }
                                    description "Transmission priority for ETH-ED PDUs";
                                }

                                ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/grace/eth-ed/rx-eth-ed
                                leaf rx-eth-ed {
                                    type boolean;
                                    default "true";
                                    description "Receive and process ETH-ED ITU-T Y.1731 PDUs on the MEP";
                                }

                                ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/grace/eth-ed/tx-eth-ed
                                leaf tx-eth-ed {
                                    type boolean;
                                    default "false";
                                    description "Transmit ETH-ED PDUs from the MEP";
                                }

                            } // container eth-ed

                            ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/grace/eth-vsm-grace
                            container eth-vsm-grace {
                                description "Enter the eth-vsm-grace context";

                                ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/grace/eth-vsm-grace/rx-eth-vsm-grace
                                leaf rx-eth-vsm-grace {
                                    type boolean;
                                    default "true";
                                    description "Receive and process Nokia ETH-CFM Grace PDU on the MEP";
                                }

                                ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/grace/eth-vsm-grace/tx-eth-vsm-grace
                                leaf tx-eth-vsm-grace {
                                    type boolean;
                                    default "true";
                                    description "Transmit ETH-ED PDUs from the MEP";
                                }

                            } // container eth-vsm-grace

                        } // container grace

                    } // list mep

                    ///nokia-conf/configure/service/vpls/sap/eth-cfm/mip
                    list mip {
                        key "primary-vlan";
                        description "Enter the mip list instance";

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mip/primary-vlan
                        leaf primary-vlan {
                            type types-eth-cfm:vlan-id-or-none;
                            description "VLAN ID to which the MIP is attached";
                        }

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mip/mac-address
                        leaf mac-address {
                            type types-sros:mac-unicast-address-no-zero;
                            description "MAC address of the MIP";
                        }

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mip/cfm-vlan-tag
                        leaf cfm-vlan-tag {
                            type types-sros:one-or-two-vlan-tags;
                            description "VLAN tags to apply to CFM PDUs for egress processing";
                        }

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mip/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mip/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list mip

                } // container eth-cfm

                ///nokia-conf/configure/service/vpls/sap/mc-ring
                container mc-ring {
                    presence "Enable the mc-ring context";
                    description "Enable the mc-ring context";

                    ///nokia-conf/configure/service/vpls/sap/mc-ring/ring-node
                    leaf ring-node {
                        type types-sros:named-item;
                        mandatory true;
                        description "Name for the ring node associated with this SAP";
                    }

                    ///nokia-conf/configure/service/vpls/sap/mc-ring/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/sap/mc-ring/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container mc-ring

                ///nokia-conf/configure/service/vpls/sap/static-isid
                container static-isid {
                    description "Enter the static-isid context";

                    ///nokia-conf/configure/service/vpls/sap/static-isid/range
                    list range {
                        when "../../../pbb-type = 'b-vpls'" {
                            description "supported on b-vpls service";
                        }
                        key "range-id";
                        description "Enter the range list instance";

                        ///nokia-conf/configure/service/vpls/sap/static-isid/range/range-id
                        leaf range-id {
                            type uint32 {
                                range "1..8191";
                            }
                            description "Range ID for static ISID";
                        }

                        ///nokia-conf/configure/service/vpls/sap/static-isid/range/start
                        leaf start {
                            type types-services:isid-no-zero;
                            description "Lower bound of the ISID range";
                        }

                        ///nokia-conf/configure/service/vpls/sap/static-isid/range/end
                        leaf end {
                            type types-services:isid-no-zero;
                            description "Upper bound of the ISID range";
                        }

                        ///nokia-conf/configure/service/vpls/sap/static-isid/range/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/sap/static-isid/range/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list range

                } // container static-isid

                ///nokia-conf/configure/service/vpls/sap/managed-vlan-list
                container managed-vlan-list {
                    when "../../m-vpls = 'true'" {
                        description "supported on managed vpls service";
                    }
                    description "Enter the managed-vlan-list context";

                    ///nokia-conf/configure/service/vpls/sap/managed-vlan-list/range
                    list range {
                        key "vlan-range";
                        description "Add a list entry for range";

                        ///nokia-conf/configure/service/vpls/sap/managed-vlan-list/range/vlan-range
                        leaf vlan-range {
                            type string {
                                pattern "((([0-9]|[1-9][0-9]{1,2}|[1-3][0-9]{3}|40[0-8][0-9]|409[0-4]))(\\.\\.(([0-9]|[1-9][0-9]{1,2}|[1-3][0-9]{3}|40[0-8][0-9]|409[0-4])))?)|(default-sap)";
                            }
                            description "Range of VLANs associated with the M-VPLS SAP";
                        }

                    } // list range

                } // container managed-vlan-list

                ///nokia-conf/configure/service/vpls/sap/pbb
                container pbb {
                    when "../../pbb-type = 'b-vpls'" {
                        description "supported on b-vpls service";
                    }
                    description "Enter the pbb context";

                    ///nokia-conf/configure/service/vpls/sap/pbb/fault-propagation
                    container fault-propagation {
                        description "Enter the fault-propagation context";

                        ///nokia-conf/configure/service/vpls/sap/pbb/fault-propagation/backbone-mac-name
                        list backbone-mac-name {
                            key "name";
                            description "Add a list entry for backbone-mac-name";

                            ///nokia-conf/configure/service/vpls/sap/pbb/fault-propagation/backbone-mac-name/name
                            leaf name {
                                type leafref {
                                    path "../../../../../../pbb/mac/name";
                                }
                                description "Backbone MAC address name";
                            }

                        } // list backbone-mac-name

                        ///nokia-conf/configure/service/vpls/sap/pbb/fault-propagation/backbone-mac-address
                        list backbone-mac-address {
                            key "address";
                            description "Add a list entry for backbone-mac-address";

                            ///nokia-conf/configure/service/vpls/sap/pbb/fault-propagation/backbone-mac-address/address
                            leaf address {
                                type types-sros:mac-unicast-address-no-zero;
                                description "Backbone MAC address";
                            }

                        } // list backbone-mac-address

                    } // container fault-propagation

                } // container pbb

            } // list sap

            ///nokia-conf/configure/service/vpls/capture-sap
            list capture-sap {
                when "not(../pbb-type) and ../etree = 'false' and not(../m-vpls = 'true')" {
                    description "not supported with etree, on m-vpls and when pbb-type is not 'none'";
                }
                key "sap-id";
                description "Enter the capture-sap list instance";

                ///nokia-conf/configure/service/vpls/capture-sap/sap-id
                leaf sap-id {
                    type types-sros:sap;
                    description "SAP identifier";
                }

                ///nokia-conf/configure/service/vpls/capture-sap/description
                leaf description {
                    type types-sros:long-description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/vpls/capture-sap/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "enable";
                    description "Administrative state of the SAP";
                }

                ///nokia-conf/configure/service/vpls/capture-sap/bandwidth
                leaf bandwidth {
                    type types-services:sap-bandwidth;
                    units "kilobps";
                    description "SAP bandwidth";
                }

                ///nokia-conf/configure/service/vpls/capture-sap/dist-cpu-protection
                leaf dist-cpu-protection {
                    type leafref {
                        path "../../../../system/security/dist-cpu-protection/policy/policy-name";
                    }
                    description "Distributed CPU protection policy for SAP";
                }

                ///nokia-conf/configure/service/vpls/capture-sap/host-lockout-policy
                leaf host-lockout-policy {
                    type leafref {
                        path "../../../../subscriber-mgmt/host-lockout-policy/name";
                    }
                    description "Host lockout policy";
                }

                ///nokia-conf/configure/service/vpls/capture-sap/radius-auth-policy
                leaf radius-auth-policy {
                    type leafref {
                        path "../../../../subscriber-mgmt/radius-authentication-policy/name";
                    }
                    description "RADIUS authentication policy";
                }

                ///nokia-conf/configure/service/vpls/capture-sap/nasreq-auth-policy
                leaf nasreq-auth-policy {
                    type leafref {
                        path "../../../../subscriber-mgmt/diameter-nasreq-policy/name";
                    }
                    description "Diameter NASREQ application policy to use for authentication";
                }

                ///nokia-conf/configure/service/vpls/capture-sap/allow-dot1q-msaps
                leaf allow-dot1q-msaps {
                    type boolean;
                    default "false";
                    description "Enable support for triggering managed SAP creation";
                }

                ///nokia-conf/configure/service/vpls/capture-sap/track-srrp
                leaf track-srrp {
                    type uint32 {
                        range "1..4294967295";
                    }
                    description "SRRP instance that this capture SAP tracks";
                }

                ///nokia-conf/configure/service/vpls/capture-sap/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/capture-sap/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/capture-sap/ingress
                container ingress {
                    description "Enter the ingress context";

                    ///nokia-conf/configure/service/vpls/capture-sap/ingress/filter
                    container filter {
                        description "Enter the filter context";

                        ///nokia-conf/configure/service/vpls/capture-sap/ingress/filter/mac
                        leaf mac {
                            type leafref {
                                path "../../../../../../filter/mac-filter/filter-name";
                            }
                            description "MAC filter policy name";
                        }

                    } // container filter

                } // container ingress

                ///nokia-conf/configure/service/vpls/capture-sap/cpu-protection
                container cpu-protection {
                    description "Enter the cpu-protection context";

                    ///nokia-conf/configure/service/vpls/capture-sap/cpu-protection/policy-id
                    leaf policy-id {
                        type leafref {
                            path "../../../../../system/security/cpu-protection/policy/policy-id";
                        }
                        description "CPM protection policy";
                    }

                    ///nokia-conf/configure/service/vpls/capture-sap/cpu-protection/monitoring
                    choice monitoring {
                        ///nokia-conf/configure/service/vpls/capture-sap/cpu-protection/monitoring/mac
                        case mac {

                            ///nokia-conf/configure/service/vpls/capture-sap/cpu-protection/monitoring/mac/mac-monitoring
                            leaf mac-monitoring {
                                type empty;
                                description "Monitor MAC for CPU protection";
                            }

                        }
                    }
                } // container cpu-protection

                ///nokia-conf/configure/service/vpls/capture-sap/pfcp
                container pfcp {
                    description "Enable the pfcp context";

                    ///nokia-conf/configure/service/vpls/capture-sap/pfcp/association
                    leaf association {
                        type leafref {
                            path "../../../../../subscriber-mgmt/pfcp/association/name";
                        }
                        description "Association used for PFCP messages on the capture SAP";
                    }

                    ///nokia-conf/configure/service/vpls/capture-sap/pfcp/l2-access-id-alias
                    leaf l2-access-id-alias {
                        type string {
                            length "1..32";
                        }
                        description "String used as Layer 2 access ID for the capture SAP";
                    }

                    ///nokia-conf/configure/service/vpls/capture-sap/pfcp/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/capture-sap/pfcp/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/capture-sap/pfcp/up-resiliency
                    container up-resiliency {
                        description "Enter the up-resiliency context";

                        ///nokia-conf/configure/service/vpls/capture-sap/pfcp/up-resiliency/monitor-oper-group
                        list monitor-oper-group {
                            key "oper-group";
                            max-elements 2;
                            description "Enter the monitor-oper-group list instance";

                            ///nokia-conf/configure/service/vpls/capture-sap/pfcp/up-resiliency/monitor-oper-group/oper-group
                            leaf oper-group {
                                type leafref {
                                    path "../../../../../../oper-group/name";
                                }
                                description "Operational group name";
                            }

                            ///nokia-conf/configure/service/vpls/capture-sap/pfcp/up-resiliency/monitor-oper-group/health-drop
                            leaf health-drop {
                                type uint32 {
                                    range "1..255";
                                }
                                default "1";
                                description "Number subtracted from the health value per failure";
                            }

                            ///nokia-conf/configure/service/vpls/capture-sap/pfcp/up-resiliency/monitor-oper-group/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/vpls/capture-sap/pfcp/up-resiliency/monitor-oper-group/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list monitor-oper-group

                    } // container up-resiliency

                } // container pfcp

                ///nokia-conf/configure/service/vpls/capture-sap/trigger-packet
                container trigger-packet {
                    description "Enter the trigger-packet context";

                    ///nokia-conf/configure/service/vpls/capture-sap/trigger-packet/arp
                    leaf arp {
                        type boolean;
                        default "false";
                        description "ARP packet";
                    }

                    ///nokia-conf/configure/service/vpls/capture-sap/trigger-packet/data
                    leaf data {
                        type boolean;
                        default "false";
                        description "Data packet";
                    }

                    ///nokia-conf/configure/service/vpls/capture-sap/trigger-packet/dhcp
                    leaf dhcp {
                        type boolean;
                        default "false";
                        description "DHCP packet";
                    }

                    ///nokia-conf/configure/service/vpls/capture-sap/trigger-packet/dhcp6
                    leaf dhcp6 {
                        type boolean;
                        default "false";
                        description "DHCP6 packet";
                    }

                    ///nokia-conf/configure/service/vpls/capture-sap/trigger-packet/pppoe
                    leaf pppoe {
                        type boolean;
                        default "false";
                        description "PPPoE packet";
                    }

                    ///nokia-conf/configure/service/vpls/capture-sap/trigger-packet/rtr-solicit
                    leaf rtr-solicit {
                        type boolean;
                        default "false";
                        description "Router-solicit packet";
                    }

                } // container trigger-packet

                ///nokia-conf/configure/service/vpls/capture-sap/msap-defaults
                container msap-defaults {
                    description "Enter the msap-defaults context";

                    ///nokia-conf/configure/service/vpls/capture-sap/msap-defaults/policy
                    leaf policy {
                        type leafref {
                            path "../../../../../subscriber-mgmt/msap-policy/name";
                        }
