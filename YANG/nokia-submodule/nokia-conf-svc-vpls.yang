                                units "seconds";
                                default "1";
                                description "Transmission interval for AIS messages";
                            }

                            ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/ais/low-priority-defect
                            leaf low-priority-defect {
                                type enumeration {
                                    enum all-def                        { value 1; }
                                    enum mac-rem-err-xcon               { value 2; }
                                }
                                default "all-def";
                                description "Lowest priority defect allowed to generate fault alarm";
                            }

                            ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/ais/priority
                            leaf priority {
                                type types-eth-cfm:frame-priority;
                                default "7";
                                description "Priority of the AIS messages generated by the node";
                            }

                        } // container ais

                        ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/csf
                        container csf {
                            presence "Enable the csf context";
                            description "Enable the csf context";

                            ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/csf/multiplier
                            leaf multiplier {
                                type decimal64 {
                                    range "0.0|2.0..30.0";
                                    fraction-digits 1;
                                }
                                default "3.5";
                                description "Multiplication factor used to clear the CSF condition";
                            }

                        } // container csf

                        ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/eth-test
                        container eth-test {
                            presence "Enable the eth-test context";
                            description "Enable the eth-test context";

                            ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/eth-test/bit-error-threshold
                            leaf bit-error-threshold {
                                type uint32 {
                                    range "0..11840";
                                }
                                units "bit errors";
                                default "1";
                                description "Lowest priority defect allowed to generate fault alarm";
                            }

                            ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/eth-test/test-pattern
                            container test-pattern {
                                description "Enter the test-pattern context";

                                ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/eth-test/test-pattern/pattern
                                leaf pattern {
                                    type enumeration {
                                        enum all-zeros                      { value 0; }
                                        enum all-ones                       { value 1; }
                                    }
                                    default "all-zeros";
                                    description "Test pattern for Ethernet Test frames";
                                }

                                ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/eth-test/test-pattern/crc-tlv
                                leaf crc-tlv {
                                    type boolean;
                                    default "false";
                                    description "Generate a CRC checksum";
                                }

                            } // container test-pattern

                        } // container eth-test

                        ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/grace
                        container grace {
                            description "Enter the grace context";

                            ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/grace/eth-ed
                            container eth-ed {
                                description "Enter the eth-ed context";

                                ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/grace/eth-ed/max-rx-defect-window
                                leaf max-rx-defect-window {
                                    type uint32 {
                                        range "1..86400";
                                    }
                                    units "seconds";
                                    description "Maximum received ETH-ED expected defect window duration";
                                }

                                ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/grace/eth-ed/priority
                                leaf priority {
                                    type int32 {
                                        range "0..7";
                                    }
                                    description "Transmission priority for ETH-ED PDUs";
                                }

                                ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/grace/eth-ed/rx-eth-ed
                                leaf rx-eth-ed {
                                    type boolean;
                                    default "true";
                                    description "Receive and process ETH-ED ITU-T Y.1731 PDUs on the MEP";
                                }

                                ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/grace/eth-ed/tx-eth-ed
                                leaf tx-eth-ed {
                                    type boolean;
                                    default "false";
                                    description "Transmit ETH-ED PDUs from the MEP";
                                }

                            } // container eth-ed

                            ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/grace/eth-vsm-grace
                            container eth-vsm-grace {
                                description "Enter the eth-vsm-grace context";

                                ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/grace/eth-vsm-grace/rx-eth-vsm-grace
                                leaf rx-eth-vsm-grace {
                                    type boolean;
                                    default "true";
                                    description "Receive and process Nokia ETH-CFM Grace PDU on the MEP";
                                }

                                ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mep/grace/eth-vsm-grace/tx-eth-vsm-grace
                                leaf tx-eth-vsm-grace {
                                    type boolean;
                                    default "true";
                                    description "Transmit ETH-ED PDUs from the MEP";
                                }

                            } // container eth-vsm-grace

                        } // container grace

                    } // list mep

                    ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mip
                    list mip {
                        key "primary-vlan";
                        description "Enter the mip list instance";

                        ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mip/primary-vlan
                        leaf primary-vlan {
                            type types-eth-cfm:vlan-id-or-none;
                            description "VLAN ID to which the MIP is attached";
                        }

                        ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mip/mac-address
                        leaf mac-address {
                            type types-sros:mac-unicast-address-no-zero;
                            description "MAC address of the MIP";
                        }

                        ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mip/cfm-vlan-tag
                        leaf cfm-vlan-tag {
                            type types-sros:one-or-two-vlan-tags;
                            description "VLAN tags to apply to CFM PDUs for egress processing";
                        }

                        ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mip/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/mesh-sdp/eth-cfm/mip/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list mip

                } // container eth-cfm

                ///nokia-conf/configure/service/vpls/mesh-sdp/mrp
                container mrp {
                    when "../../pbb-type = 'b-vpls' and not(../../m-vpls = 'true')" {
                        description "supported on non managed b-vpls service";
                    }
                    description "Enter the mrp context";

                    ///nokia-conf/configure/service/vpls/mesh-sdp/mrp/join-time
                    leaf join-time {
                        type uint32 {
                            range "1..10";
                        }
                        units "deciseconds";
                        default "2";
                        description "Maximum rate for attribute join messages sent on SDP";
                    }

                    ///nokia-conf/configure/service/vpls/mesh-sdp/mrp/leave-time
                    leaf leave-time {
                        type uint32 {
                            range "30..60";
                        }
                        units "deciseconds";
                        default "30";
                        description "Time in LV state before transition to MT state";
                    }

                    ///nokia-conf/configure/service/vpls/mesh-sdp/mrp/leave-all-time
                    leaf leave-all-time {
                        type uint32 {
                            range "60..300";
                        }
                        units "deciseconds";
                        default "100";
                        description "Frequency of LeaveAll PDUs by LeaveAll state machine";
                    }

                    ///nokia-conf/configure/service/vpls/mesh-sdp/mrp/periodic-time
                    leaf periodic-time {
                        type uint32 {
                            range "10..100";
                        }
                        units "deciseconds";
                        default "10";
                        description "Frequency of periodic events generated by state machine";
                    }

                    ///nokia-conf/configure/service/vpls/mesh-sdp/mrp/periodic-timer
                    leaf periodic-timer {
                        type boolean;
                        default "false";
                        description "Enable the Periodic Transmission Timer";
                    }

                    ///nokia-conf/configure/service/vpls/mesh-sdp/mrp/policy
                    leaf policy {
                        type leafref {
                            path "../../../../mrp/policy/policy-name";
                        }
                        description "MRP policy to control Group B-MAC attributes";
                    }

                    ///nokia-conf/configure/service/vpls/mesh-sdp/mrp/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/mesh-sdp/mrp/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container mrp

            } // list mesh-sdp

            ///nokia-conf/configure/service/vpls/bgp-mh-site
            list bgp-mh-site {
                key "site-name";
                description "Enter the bgp-mh-site list instance";

                ///nokia-conf/configure/service/vpls/bgp-mh-site/site-name
                leaf site-name {
                    type types-sros:named-item;
                    description "Name for the specific site";
                }

                ///nokia-conf/configure/service/vpls/bgp-mh-site/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of the VPLS BGP multi-homing site";
                }

                ///nokia-conf/configure/service/vpls/bgp-mh-site/id
                leaf id {
                    type types-services:site-id;
                    sros-ext:auto-restart-to-modify;
                    description "Site ID";
                }

                ///nokia-conf/configure/service/vpls/bgp-mh-site/boot-timer
                leaf boot-timer {
                    type types-services:site-boot-timer;
                    units "seconds";
                    description "Wait time after reboot to run the DF election algorithm";
                }

                ///nokia-conf/configure/service/vpls/bgp-mh-site/activation-timer
                leaf activation-timer {
                    type types-services:site-activation-timer;
                    units "seconds";
                    description "Time that the local sites are in standby status, waiting for BGP updates";
                }

                ///nokia-conf/configure/service/vpls/bgp-mh-site/monitor-oper-group
                leaf monitor-oper-group {
                    type leafref {
                        path "../../../oper-group/name";
                    }
                    description "Operational group to monitor";
                }

                ///nokia-conf/configure/service/vpls/bgp-mh-site/failed-threshold
                leaf failed-threshold {
                    type types-services:failed-threshold;
                    default "all";
                    description "Threshold for the site to be declared down";
                }

                ///nokia-conf/configure/service/vpls/bgp-mh-site/min-down-timer
                leaf min-down-timer {
                    type types-services:site-min-down-timer;
                    units "seconds";
                    description "Minimum downtime for BGP multi-homing site after transition from up to down";
                }

                ///nokia-conf/configure/service/vpls/bgp-mh-site/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/bgp-mh-site/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/bgp-mh-site/site-object
                choice site-object {
                    ///nokia-conf/configure/service/vpls/bgp-mh-site/site-object/spoke-sdp
                    case spoke-sdp {

                        ///nokia-conf/configure/service/vpls/bgp-mh-site/site-object/spoke-sdp/spoke-sdp
                        leaf spoke-sdp {
                            type types-services:sdp-bind-id;
                            sros-ext:auto-restart-to-modify;
                            description "SDP associated with the site";
                        }

                    }
                    ///nokia-conf/configure/service/vpls/bgp-mh-site/site-object/sap
                    case sap {

                        ///nokia-conf/configure/service/vpls/bgp-mh-site/site-object/sap/sap
                        leaf sap {
                            type types-sros:sap;
                            sros-ext:auto-restart-to-modify;
                            description "SAP to be associated with this site";
                        }

                    }
                    ///nokia-conf/configure/service/vpls/bgp-mh-site/site-object/mesh-sdp
                    case mesh-sdp {

                        ///nokia-conf/configure/service/vpls/bgp-mh-site/site-object/mesh-sdp/mesh-sdp-binds
                        leaf mesh-sdp-binds {
                            type empty;
                            description "Specify if a mesh-sdp-binding is associated with this site";
                        }

                    }
                    ///nokia-conf/configure/service/vpls/bgp-mh-site/site-object/shg
                    case shg {

                        ///nokia-conf/configure/service/vpls/bgp-mh-site/site-object/shg/shg-name
                        leaf shg-name {
                            type types-sros:named-item;
                            sros-ext:auto-restart-to-modify;
                            description "SHG name to be associated with the site";
                        }

                    }
                }
            } // list bgp-mh-site

            ///nokia-conf/configure/service/vpls/sap
            list sap {
                key "sap-id";
                description "Enter the sap list instance";

                ///nokia-conf/configure/service/vpls/sap/sap-id
                leaf sap-id {
                    type types-sros:sap;
                    description "SAP identifier";
                }

                ///nokia-conf/configure/service/vpls/sap/description
