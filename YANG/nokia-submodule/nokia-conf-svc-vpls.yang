                container l2pt {
                    description "Enter the l2pt context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/l2pt/termination
                    container termination {
                        presence "Enable the termination context";
                        description "Enable the termination context";

                        ///nokia-conf/configure/service/vpls/spoke-sdp/l2pt/termination/protocols
                        container protocols {
                            description "Enter the protocols context";

                            ///nokia-conf/configure/service/vpls/spoke-sdp/l2pt/termination/protocols/stp
                            leaf stp {
                                type boolean;
                                default "true";
                                description "Enable all spanning tree protocols";
                            }

                            ///nokia-conf/configure/service/vpls/spoke-sdp/l2pt/termination/protocols/cdp
                            leaf cdp {
                                type boolean;
