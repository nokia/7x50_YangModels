                ///nokia-conf/configure/service/vpls/pim-snooping/ipv4
                container ipv4 {
                    description "Enter the ipv4 context";

                    ///nokia-conf/configure/service/vpls/pim-snooping/ipv4/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        description "Administrative state of snooping for multicast traffic";
                    }

                    ///nokia-conf/configure/service/vpls/pim-snooping/ipv4/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
