                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/bgp-vpls/ve
                container ve {
                    description "Enter the ve context";

                    ///nokia-conf/configure/service/vpls/bgp-vpls/ve/name
                    leaf name {
                        type types-sros:named-item;
                        description "VPLS Edge instance name";
                    }

                    ///nokia-conf/configure/service/vpls/bgp-vpls/ve/id
                    leaf id {
                        type types-services:ve-id;
                        description "VPLS edge ID";
                    }

                } // container ve

            } // container bgp-vpls

            ///nokia-conf/configure/service/vpls/endpoint
            list endpoint {
                key "name";
                max-elements 10;
                description "Enter the endpoint list instance";

                ///nokia-conf/configure/service/vpls/endpoint/name
                leaf name {
                    type types-sros:named-item;
                    description "Service endpoint name";
                }

                ///nokia-conf/configure/service/vpls/endpoint/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/vpls/endpoint/revert-time
                leaf revert-time {
                    type types-services:revert-time;
                    default "immediate";
                    description "Time to wait before reverting to primary spoke SDP";
                }

                ///nokia-conf/configure/service/vpls/endpoint/ignore-standby-signaling
                leaf ignore-standby-signaling {
                    type boolean;
                    default "false";
                    description "Ignore standby-bit received from TLDP peers when performing internal tasks";
                }

                ///nokia-conf/configure/service/vpls/endpoint/suppress-standby-signaling
                leaf suppress-standby-signaling {
                    type boolean;
                    default "true";
                    description "Do not send pseudowire standby bit to TLDP peer when specified spoke SDP is selected as standby";
                }

                ///nokia-conf/configure/service/vpls/endpoint/block-on-mesh-failure
                leaf block-on-mesh-failure {
                    type boolean;
                    default "false";
                    description "Enable blocking after the endpoints are in a down state";
                }

                ///nokia-conf/configure/service/vpls/endpoint/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/endpoint/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/endpoint/fdb
                container fdb {
                    description "Enter the fdb context";

                    ///nokia-conf/configure/service/vpls/endpoint/fdb/mac-pinning
                    leaf mac-pinning {
                        type boolean;
                        default "false";
                        description "Activate MAC address pinning on this endpoint";
                    }

                    ///nokia-conf/configure/service/vpls/endpoint/fdb/maximum-mac-addresses
                    leaf maximum-mac-addresses {
                        type types-services:max-mac-addr;
                        description "Maximum number of MAC address entries in the FDB";
                    }

                    ///nokia-conf/configure/service/vpls/endpoint/fdb/auto-learn-mac-protect
                    leaf auto-learn-mac-protect {
                        type boolean;
                        default "false";
                        description "Populate automatically MAC protect list with MAC addresses learned on SDP with this endpoint";
                    }

                    ///nokia-conf/configure/service/vpls/endpoint/fdb/protected-src-mac-violation-action
                    leaf protected-src-mac-violation-action {
                        type types-services:sdp-protected-src-mac-violation-action;
                        description "Action when a relearn request for a protected MAC is received on the SDP";
                    }

                } // container fdb

                ///nokia-conf/configure/service/vpls/endpoint/mc-endpoint
                list mc-endpoint {
                    key "mc-ep-id";
                    max-elements 1;
                    description "Enter the mc-endpoint list instance";

                    ///nokia-conf/configure/service/vpls/endpoint/mc-endpoint/mc-ep-id
                    leaf mc-ep-id {
                        type uint32 {
                            range "1..4294967295";
                        }
                        description "MC-EP ID";
                    }

                    ///nokia-conf/configure/service/vpls/endpoint/mc-endpoint/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/endpoint/mc-endpoint/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/endpoint/mc-endpoint/mc-ep-peer
                    container mc-ep-peer {
                        description "Enter the mc-ep-peer context";

                        ///nokia-conf/configure/service/vpls/endpoint/mc-endpoint/mc-ep-peer/peer
                        choice peer {
                            ///nokia-conf/configure/service/vpls/endpoint/mc-endpoint/mc-ep-peer/peer/address
                            case address {

                                ///nokia-conf/configure/service/vpls/endpoint/mc-endpoint/mc-ep-peer/peer/address/peer-address
                                leaf peer-address {
                                    type leafref {
                                        path "../../../../../../redundancy/multi-chassis/peer/ip-address";
                                    }
                                    description "IP address of the MC-EP peer";
                                }

                            }
                            ///nokia-conf/configure/service/vpls/endpoint/mc-endpoint/mc-ep-peer/peer/name
                            case name {

                                ///nokia-conf/configure/service/vpls/endpoint/mc-endpoint/mc-ep-peer/peer/name/name
                                leaf name {
                                    type types-sros:named-item;
                                    description "Name of the MC-EP peer";
                                }

                            }
                        }
                    } // container mc-ep-peer

                } // list mc-endpoint

            } // list endpoint

            ///nokia-conf/configure/service/vpls/split-horizon-group
            list split-horizon-group {
                when "not(../m-vpls = 'true')" {
                    description "not supported on m-vpls";
                }
                key "shg-name";
                description "Enter the split-horizon-group list instance";

                ///nokia-conf/configure/service/vpls/split-horizon-group/shg-name
                leaf shg-name {
                    type types-sros:named-item;
                    description "SHG name to which the SDP belongs";
                }

                ///nokia-conf/configure/service/vpls/split-horizon-group/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/vpls/split-horizon-group/residential
                leaf residential {
                    when "not(../../pbb-type) and not(../../routed-vpls)" {
                        description "VPLS PBB type must be 'none' and routed VPLS must not be set";
                    }
                    type boolean;
                    sros-ext:immutable;
                    default "false";
                    description "Define as a residential split horizon group";
                }

                ///nokia-conf/configure/service/vpls/split-horizon-group/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/split-horizon-group/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/split-horizon-group/fdb
                container fdb {
                    description "Enter the fdb context";

                    ///nokia-conf/configure/service/vpls/split-horizon-group/fdb/saps
                    container saps {
                        description "Enter the saps context";

                        ///nokia-conf/configure/service/vpls/split-horizon-group/fdb/saps/auto-learn-mac-protect
                        leaf auto-learn-mac-protect {
                            type boolean;
                            default "false";
                            description "Populate automatically MAC protect list with MAC addresses learned on SDP with split horizon group";
                        }

                        ///nokia-conf/configure/service/vpls/split-horizon-group/fdb/saps/auto-learn-mac-protect-exclude-list
                        leaf auto-learn-mac-protect-exclude-list {
                            type leafref {
                                path "../../../../../mac-list/name";
                            }
                            description "Referenced MAC protect exclusion list name";
                        }

                        ///nokia-conf/configure/service/vpls/split-horizon-group/fdb/saps/protected-src-mac-violation-action
                        leaf protected-src-mac-violation-action {
                            type types-services:sap-protected-src-mac-violation-action;
                            description "Action to take whenever a relearn request for a protected MAC is received";
                        }

                        ///nokia-conf/configure/service/vpls/split-horizon-group/fdb/saps/discard-unprotected-dest-mac
                        leaf discard-unprotected-dest-mac {
                            type boolean;
                            default "false";
                            description "Discard packet with unprotected destination MAC address";
                        }

                    } // container saps

                } // container fdb

            } // list split-horizon-group

            ///nokia-conf/configure/service/vpls/eth-cfm
            container eth-cfm {
                description "Enable the eth-cfm context";

                ///nokia-conf/configure/service/vpls/eth-cfm/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/eth-cfm/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/eth-cfm/mep
                list mep {
                    key "md-admin-name ma-admin-name mep-id";
                    description "Enter the mep list instance";

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/md-admin-name
                    leaf md-admin-name {
                        type leafref {
                            path "../../../../../eth-cfm/domain/md-admin-name";
                        }
                        description "Maintenance Domain (MD) name";
                    }

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/ma-admin-name
                    leaf ma-admin-name {
                        type leafref {
                            path "../../../../../eth-cfm/domain[md-admin-name=current()/../md-admin-name]/association/ma-admin-name";
                        }
                        description "Maintenance Association (MA) name";
                    }

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/mep-id
                    leaf mep-id {
                        type types-eth-cfm:mep-id-type;
                        description "Maintenance Endpoint (MEP) ID";
                    }

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the MEP";
                    }

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/mac-address
                    leaf mac-address {
                        type types-sros:mac-unicast-address-no-zero;
                        description "MAC address of the MEP";
                    }

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/cfm-vlan-tag
                    leaf cfm-vlan-tag {
                        type types-sros:one-or-two-vlan-tags;
                        description "VLAN tags to apply to CFM PDUs for egress processing";
                    }

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/low-priority-defect
                    leaf low-priority-defect {
                        type types-eth-cfm:lowest-alarm-priority;
                        default "mac-rem-err-xcon";
                        description "Lowest priority defect allowed to generate fault alarm";
                    }

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/ccm
                    leaf ccm {
                        type boolean;
                        default "false";
                        description "Generate CCM messages";
                    }

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/ccm-ltm-priority
                    leaf ccm-ltm-priority {
                        type types-eth-cfm:frame-priority;
                        default "7";
                        description "Priority of CCM and LTM messages transmitted by the MEP";
                    }

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/ccm-padding-size
                    leaf ccm-padding-size {
                        type uint32 {
                            range "3..1500";
                        }
                        description "Number of octets of padding to insert in CCM packets";
                    }

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/one-way-delay-threshold
                    leaf one-way-delay-threshold {
                        type uint32 {
                            range "0..600";
                        }
                        units "seconds";
                        default "3";
                        description "Threshold time limit for the one-way delay test";
                    }

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/alarm-notification
                    container alarm-notification {
                        description "Enter the alarm-notification context";

                        ///nokia-conf/configure/service/vpls/eth-cfm/mep/alarm-notification/fng-alarm-time
                        leaf fng-alarm-time {
                            type int32 {
                                range "250|500|1000";
                            }
                            units "centiseconds";
                            description "Time that must expire before an FNG alarm is raised";
                        }

                        ///nokia-conf/configure/service/vpls/eth-cfm/mep/alarm-notification/fng-reset-time
                        leaf fng-reset-time {
                            type int32 {
                                range "250|500|1000";
                            }
                            units "centiseconds";
                            description "Time that must expire before an FNG alarm is reset";
                        }

                    } // container alarm-notification

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/eth-test
                    container eth-test {
                        presence "Enable the eth-test context";
                        description "Enable the eth-test context";

                        ///nokia-conf/configure/service/vpls/eth-cfm/mep/eth-test/bit-error-threshold
                        leaf bit-error-threshold {
                            type uint32 {
                                range "0..11840";
                            }
                            units "bit errors";
                            default "1";
                            description "Lowest priority defect allowed to generate fault alarm";
                        }

                        ///nokia-conf/configure/service/vpls/eth-cfm/mep/eth-test/test-pattern
                        container test-pattern {
                            description "Enter the test-pattern context";

                            ///nokia-conf/configure/service/vpls/eth-cfm/mep/eth-test/test-pattern/pattern
                            leaf pattern {
                                type enumeration {
                                    enum all-zeros                      { value 0; }
                                    enum all-ones                       { value 1; }
                                }
                                default "all-zeros";
                                description "Test pattern for Ethernet Test frames";
                            }

                            ///nokia-conf/configure/service/vpls/eth-cfm/mep/eth-test/test-pattern/crc-tlv
                            leaf crc-tlv {
                                type boolean;
                                default "false";
                                description "Generate a CRC checksum";
                            }

                        } // container test-pattern

                    } // container eth-test

                    ///nokia-conf/configure/service/vpls/eth-cfm/mep/grace
                    container grace {
                        description "Enter the grace context";

                        ///nokia-conf/configure/service/vpls/eth-cfm/mep/grace/eth-ed
                        container eth-ed {
                            description "Enter the eth-ed context";

                            ///nokia-conf/configure/service/vpls/eth-cfm/mep/grace/eth-ed/max-rx-defect-window
                            leaf max-rx-defect-window {
                                type uint32 {
                                    range "1..86400";
                                }
                                units "seconds";
                                description "Maximum received ETH-ED expected defect window duration";
                            }

                            ///nokia-conf/configure/service/vpls/eth-cfm/mep/grace/eth-ed/priority
                            leaf priority {
                                type int32 {
                                    range "0..7";
                                }
                                description "Transmission priority for ETH-ED PDUs";
                            }

                            ///nokia-conf/configure/service/vpls/eth-cfm/mep/grace/eth-ed/rx-eth-ed
                            leaf rx-eth-ed {
                                type boolean;
                                default "true";
                                description "Receive and process ETH-ED ITU-T Y.1731 PDUs on the MEP";
                            }

                            ///nokia-conf/configure/service/vpls/eth-cfm/mep/grace/eth-ed/tx-eth-ed
                            leaf tx-eth-ed {
                                type boolean;
                                default "false";
                                description "Transmit ETH-ED PDUs from the MEP";
                            }

                        } // container eth-ed

                        ///nokia-conf/configure/service/vpls/eth-cfm/mep/grace/eth-vsm-grace
                        container eth-vsm-grace {
                            description "Enter the eth-vsm-grace context";

                            ///nokia-conf/configure/service/vpls/eth-cfm/mep/grace/eth-vsm-grace/rx-eth-vsm-grace
                            leaf rx-eth-vsm-grace {
                                type boolean;
                                default "true";
                                description "Receive and process Nokia ETH-CFM Grace PDU on the MEP";
                            }

                            ///nokia-conf/configure/service/vpls/eth-cfm/mep/grace/eth-vsm-grace/tx-eth-vsm-grace
                            leaf tx-eth-vsm-grace {
                                type boolean;
                                default "true";
                                description "Transmit ETH-ED PDUs from the MEP";
                            }

                        } // container eth-vsm-grace

                    } // container grace

                } // list mep

            } // container eth-cfm

            ///nokia-conf/configure/service/vpls/mcr-default-gtw
            container mcr-default-gtw {
                when "not(../pbb-type)" {
                    description "VPLS PBB type must be 'none'";
                }
                description "Enable the mcr-default-gtw context";

                ///nokia-conf/configure/service/vpls/mcr-default-gtw/ip
                leaf ip {
                    type types-sros:ipv4-unicast-address;
                    description "Multi-chassis ring default gateway IP address";
                }

                ///nokia-conf/configure/service/vpls/mcr-default-gtw/mac
                leaf mac {
                    type yang:mac-address;
                    default "00:00:00:00:00:00";
                    description "Multi-chassis ring default gateway MAC address";
                }

                ///nokia-conf/configure/service/vpls/mcr-default-gtw/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/mcr-default-gtw/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container mcr-default-gtw

            ///nokia-conf/configure/service/vpls/spoke-sdp
            list spoke-sdp {
                key "sdp-bind-id";
                description "Enter the spoke-sdp list instance";

                ///nokia-conf/configure/service/vpls/spoke-sdp/sdp-bind-id
                leaf sdp-bind-id {
                    type types-services:sdp-bind-id;
                    description "SDP binding ID";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "enable";
                    description "Administrative state of the SDP binding to the service";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/vlan-vc-tag
                leaf vlan-vc-tag {
                    type types-services:vlan-vc-tag;
                    description "SDP bind VC tag";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/split-horizon-group
                leaf split-horizon-group {
                    type leafref {
                        path "../../split-horizon-group/shg-name";
                    }
                    sros-ext:immutable;
                    description "Name of the split horizon group where the spoke SDP bind belongs to";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/multicast-source
                leaf multicast-source {
                    type boolean;
                    sros-ext:immutable;
                    default "false";
                    description "Enable multicast-source on the spoke SDP bind";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/collect-stats
                leaf collect-stats {
                    type boolean;
                    default "false";
                    description "Allow agent to collect accounting statistics";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/accounting-policy
                leaf accounting-policy {
                    type leafref {
                        path "../../../../log/accounting-policy/policy-id";
                    }
                    description "Policy to collect accounting statistics";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/control-word
                leaf control-word {
                    type boolean;
                    default "false";
                    description "Use the control word as preferred";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/force-vc-forwarding
                leaf force-vc-forwarding {
                    type enumeration {
                        enum vlan                           { value 1; }
                        enum qinq-c-tag-c-tag               { value 2; }
                        enum qinq-s-tag-c-tag               { value 3; }
                    }
                    description "VC forwarding action";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/app-profile
                leaf app-profile {
                    type leafref {
                        path "../../../../application-assurance/group/partition/policy/app-profile/app-profile-name";
                    }
                    description "ISA-AA application profile name for this SDP";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/etree-root-leaf-tag
                leaf etree-root-leaf-tag {
                    when "../../etree = 'true'" {
                        description "etree must be set";
                    }
                    type boolean;
                    sros-ext:immutable;
                    default "false";
                    description "E-tree root leaf tag status";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/etree-leaf
                leaf etree-leaf {
                    when "../../etree = 'true'" {
                        description "etree must be set";
                    }
                    type boolean;
                    sros-ext:immutable;
                    default "false";
                    description "Enable etree leaf access-circuit status";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/bfd-template
                leaf bfd-template {
                    status obsolete;
                    type leafref {
                        path "../../../../bfd/bfd-template/name";
                    }
                    description "BFD template associated with SDP binding";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/vc-type
                leaf vc-type {
                    type types-services:vpls-sdp-bind-vc-type;
                    sros-ext:auto-restart-to-modify;
                    default "ether";
                    description "Type of virtual circuit (VC) associated with the SDP binding; VPLS not supported";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/bpdu-translation
                leaf bpdu-translation {
                    type types-services:bpdu-translation;
                    description "BPDU format";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/ignore-standby-signaling
                leaf ignore-standby-signaling {
                    type boolean;
                    default "false";
                    description "Ignore standby-bit received from TLDP peers when performing internal tasks";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/block-on-mesh-failure
                leaf block-on-mesh-failure {
                    type boolean;
                    default "false";
                    description "Enable blocking after all SDPs are operationally down";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/spoke-sdp/label
                choice label {
                    ///nokia-conf/configure/service/vpls/spoke-sdp/label/entropy
                    case entropy {

                        ///nokia-conf/configure/service/vpls/spoke-sdp/label/entropy/entropy-label
                        leaf entropy-label {
                            type empty;
                            description "Enable the use of entropy labels for spoke SDPs";
                        }

                    }
                    ///nokia-conf/configure/service/vpls/spoke-sdp/label/hash
                    case hash {

                        ///nokia-conf/configure/service/vpls/spoke-sdp/label/hash/hash-label
