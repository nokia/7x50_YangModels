                        description "Enter the mep list instance";

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/md-admin-name
                        leaf md-admin-name {
                            type leafref {
                                path "../../../../../../eth-cfm/domain/md-admin-name";
