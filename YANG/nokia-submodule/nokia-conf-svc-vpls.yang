                            default "false";
                            description "MEP provisioned using MA primary VLAN ID";
                        }

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/direction
                        leaf direction {
                            type types-eth-cfm:mp-direction;
                            sros-ext:immutable;
                            description "Direction the MEP faces";
                        }

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the MEP";
