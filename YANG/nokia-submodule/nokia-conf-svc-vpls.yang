                ///nokia-conf/configure/service/vpls/spb/overload
                container overload {
                    presence "Enable the overload context";
                    description "Enable the overload context";

                    ///nokia-conf/configure/service/vpls/spb/overload/timeout
                    leaf timeout {
                        type uint32 {
                            range "0|60..1800";
                        }
                        units "seconds";
                        default "0";
                        description "Time the router operates in overloaded state";
                    }

                } // container overload

                ///nokia-conf/configure/service/vpls/spb/overload-on-boot
                container overload-on-boot {
                    presence "Enable the overload-on-boot context";
                    description "Enable the overload-on-boot context";

                    ///nokia-conf/configure/service/vpls/spb/overload-on-boot/timeout
                    leaf timeout {
                        type uint32 {
                            range "0|60..1800";
                        }
                        units "seconds";
                        default "0";
                        description "Time the router operates in overloaded state";
                    }

                } // container overload-on-boot

                ///nokia-conf/configure/service/vpls/spb/timers
                container timers {
                    description "Enter the timers context";

                    ///nokia-conf/configure/service/vpls/spb/timers/lsp-wait
                    container lsp-wait {
                        presence "Enable the lsp-wait context";
                        description "Enable the lsp-wait context";

                        ///nokia-conf/configure/service/vpls/spb/timers/lsp-wait/max-wait
                        leaf max-wait {
                            type types-services:spb-wait;
                            units "milliseconds";
                            default "5000";
                            description "Max wait time";
                        }

                        ///nokia-conf/configure/service/vpls/spb/timers/lsp-wait/initial-wait
                        leaf initial-wait {
                            type types-services:spb-initial-wait;
                            units "milliseconds";
                            default "10";
                            description "Initial wait";
                        }

                        ///nokia-conf/configure/service/vpls/spb/timers/lsp-wait/second-wait
                        leaf second-wait {
                            type types-services:spb-second-wait;
                            units "milliseconds";
                            default "1000";
                            description "Second wait";
                        }

                    } // container lsp-wait

                    ///nokia-conf/configure/service/vpls/spb/timers/spf-wait
                    container spf-wait {
                        presence "Enable the spf-wait context";
                        description "Enable the spf-wait context";

                        ///nokia-conf/configure/service/vpls/spb/timers/spf-wait/max-wait
                        leaf max-wait {
                            type types-services:spb-wait;
                            units "milliseconds";
                            default "10000";
                            description "Max wait time";
                        }

                        ///nokia-conf/configure/service/vpls/spb/timers/spf-wait/initial-wait
                        leaf initial-wait {
                            type types-services:spb-initial-wait;
                            units "milliseconds";
                            default "1000";
                            description "Initial wait";
                        }

                        ///nokia-conf/configure/service/vpls/spb/timers/spf-wait/second-wait
                        leaf second-wait {
                            type types-services:spb-second-wait;
                            units "milliseconds";
                            default "1000";
                            description "Second wait";
                        }

                    } // container spf-wait

                } // container timers

                ///nokia-conf/configure/service/vpls/spb/level
                list level {
                    key "id";
                    description "Enter the level list instance";

                    ///nokia-conf/configure/service/vpls/spb/level/id
                    leaf id {
                        type int32 {
                            range "1";
                        }
                        description "Level identifier";
                    }

                    ///nokia-conf/configure/service/vpls/spb/level/bridge-priority
                    leaf bridge-priority {
                        type int32 {
                            range "0..15";
                        }
                        default "8";
                        description "SPB Bridge priority";
                    }

                    ///nokia-conf/configure/service/vpls/spb/level/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/spb/level/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/spb/level/forwarding-tree
                    container forwarding-tree {
                        description "Enter the forwarding-tree context";

                        ///nokia-conf/configure/service/vpls/spb/level/forwarding-tree/topology
                        leaf topology {
                            type enumeration {
                                enum spf                            { value 0; }
                                enum st                             { value 1; }
                            }
                            default "spf";
                            description "Topology";
                        }

                    } // container forwarding-tree

                    ///nokia-conf/configure/service/vpls/spb/level/ect-high-path-fid
                    list ect-high-path-fid {
                        key "fid";
                        description "Add a list entry for ect-high-path-fid";

                        ///nokia-conf/configure/service/vpls/spb/level/ect-high-path-fid/fid
                        leaf fid {
                            type types-services:spb-fid;
                            description "SPB control vpls FID";
                        }

                    } // list ect-high-path-fid

                } // list level

            } // container spb

            ///nokia-conf/configure/service/vpls/spbm-control-vpls
            container spbm-control-vpls {
                description "Enter the spbm-control-vpls context";

                ///nokia-conf/configure/service/vpls/spbm-control-vpls/service-name
                leaf service-name {
                    type types-services:service-name;
                    description "SPB control vpls service";
                }

                ///nokia-conf/configure/service/vpls/spbm-control-vpls/fid
                leaf fid {
                    type types-services:spb-fid;
                    description "SPB control vpls FID";
                }

            } // container spbm-control-vpls

            ///nokia-conf/configure/service/vpls/segment-routing-v6
            list segment-routing-v6 {
                when "not(../pbb-type = 'b-vpls') and not(../pbb-type = 'i-vpls') and ../m-vpls = 'false' and ../etree = 'false' and not(../routed-vpls)" {
                    description "not supported on b-vpls/i-vpls/m-vpls, routed-vpls or etree";
                }
                key "instance";
                max-elements 1;
                description "Enter the segment-routing-v6 list instance";

                ///nokia-conf/configure/service/vpls/segment-routing-v6/instance
                leaf instance {
                    type types-services:vpls-srv6-instance;
                    description "Segment routing v6 instance";
                }

                ///nokia-conf/configure/service/vpls/segment-routing-v6/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/segment-routing-v6/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/segment-routing-v6/locator
                list locator {
                    key "locator-name";
                    max-elements 1;
                    description "Enter the locator list instance";

                    ///nokia-conf/configure/service/vpls/segment-routing-v6/locator/locator-name
                    leaf locator-name {
                        type leafref {
                            path "../../../../../router/segment-routing/segment-routing-v6/locator/locator-name";
                        }
                        description "SRv6 locator name";
                    }

                    ///nokia-conf/configure/service/vpls/segment-routing-v6/locator/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/segment-routing-v6/locator/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/segment-routing-v6/locator/function
                    container function {
                        description "Enter the function context";

                        ///nokia-conf/configure/service/vpls/segment-routing-v6/locator/function/end-dt2u
                        container end-dt2u {
                            presence "Enable the end-dt2u context";
                            description "Enable the end-dt2u context";

                            ///nokia-conf/configure/service/vpls/segment-routing-v6/locator/function/end-dt2u/value
                            leaf value {
                                type uint32 {
                                    range "1..1048575";
                                }
                                description "SRv6 function value";
                            }

                        } // container end-dt2u

                        ///nokia-conf/configure/service/vpls/segment-routing-v6/locator/function/end-dt2m
                        container end-dt2m {
                            presence "Enable the end-dt2m context";
                            description "Enable the end-dt2m context";

                            ///nokia-conf/configure/service/vpls/segment-routing-v6/locator/function/end-dt2m/value
                            leaf value {
                                type uint32 {
                                    range "1..1048575";
                                }
                                description "SRv6 function value";
                            }

                        } // container end-dt2m

                    } // container function

                } // list locator

                ///nokia-conf/configure/service/vpls/segment-routing-v6/micro-segment-locator
                list micro-segment-locator {
                    key "locator-name";
                    max-elements 1;
                    description "Enter the micro-segment-locator list instance";

                    ///nokia-conf/configure/service/vpls/segment-routing-v6/micro-segment-locator/locator-name
                    leaf locator-name {
                        type leafref {
                            path "../../../../../router/segment-routing/segment-routing-v6/micro-segment-locator/locator-name";
                        }
                        description "Micro-segment SRv6 locator name";
                    }

                    ///nokia-conf/configure/service/vpls/segment-routing-v6/micro-segment-locator/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/segment-routing-v6/micro-segment-locator/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/segment-routing-v6/micro-segment-locator/function
                    container function {
                        description "Enter the function context";

                        ///nokia-conf/configure/service/vpls/segment-routing-v6/micro-segment-locator/function/udt2m
                        container udt2m {
                            presence "Enable the udt2m context";
                            description "Enable the udt2m context";

                            ///nokia-conf/configure/service/vpls/segment-routing-v6/micro-segment-locator/function/udt2m/value
                            leaf value {
                                type uint32 {
                                    range "1..1048575";
                                }
                                description "SRv6 function value";
                            }

                        } // container udt2m

                        ///nokia-conf/configure/service/vpls/segment-routing-v6/micro-segment-locator/function/udt2u
                        container udt2u {
                            presence "Enable the udt2u context";
                            description "Enable the udt2u context";

                            ///nokia-conf/configure/service/vpls/segment-routing-v6/micro-segment-locator/function/udt2u/value
                            leaf value {
                                type uint32 {
                                    range "1..1048575";
                                }
                                description "SRv6 function value";
                            }

                        } // container udt2u

                    } // container function

                } // list micro-segment-locator

            } // list segment-routing-v6

            ///nokia-conf/configure/service/vpls/stp
            container stp {
                description "Enter the stp context";

                ///nokia-conf/configure/service/vpls/stp/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of STP";
                }

                ///nokia-conf/configure/service/vpls/stp/forward-delay
                leaf forward-delay {
                    type int32 {
                        range "4..30";
                    }
                    default "15";
                    description "Configure forward-delay";
                }

                ///nokia-conf/configure/service/vpls/stp/hello-time
                leaf hello-time {
                    type int32 {
                        range "1..10";
                    }
                    default "2";
                    description "Configure hello-time";
                }

                ///nokia-conf/configure/service/vpls/stp/hold-count
                leaf hold-count {
                    type int32 {
                        range "1..20";
                    }
                    default "6";
                    description "Configure BPDU transmit hold count";
                }

                ///nokia-conf/configure/service/vpls/stp/maximum-age
                leaf maximum-age {
                    type int32 {
                        range "6..40";
                    }
                    default "20";
                    description "Configure maximum STP information age";
                }

                ///nokia-conf/configure/service/vpls/stp/mode
                leaf mode {
                    type enumeration {
                        enum rstp                           { value 2; }
                        enum comp-dot1w                     { value 3; }
                        enum dot1w                          { value 4; }
                        enum mstp                           { value 5; }
                        enum pmstp                          { value 6; }
                    }
                    default "rstp";
                    description "Configure protocol version";
                }

                ///nokia-conf/configure/service/vpls/stp/mst-maximum-hops
                leaf mst-maximum-hops {
                    type int32 {
                        range "1..40";
                    }
                    default "20";
                    description "Maximum number of hops in an MSTP region";
                }

                ///nokia-conf/configure/service/vpls/stp/mst-name
                leaf mst-name {
                    type types-sros:named-item;
                    description "MST region name";
                }

                ///nokia-conf/configure/service/vpls/stp/mst-revision
                leaf mst-revision {
                    type int32 {
                        range "0..65535";
                    }
                    description "MST configuration revision";
                }

                ///nokia-conf/configure/service/vpls/stp/priority
                leaf priority {
                    type int32 {
                        range "0..65535";
                    }
                    default "32768";
                    description "STP bridge priority";
                }

                ///nokia-conf/configure/service/vpls/stp/mst-instance
                list mst-instance {
                    when "../../m-vpls = 'true'" {
                        description "supported on managed vpls service";
                    }
                    key "mst-inst-number";
                    description "Enter the mst-instance list instance";

                    ///nokia-conf/configure/service/vpls/stp/mst-instance/mst-inst-number
                    leaf mst-inst-number {
                        type uint32 {
                            range "1..4094";
                        }
                        description "Multiple Spanning Tree Instance number";
                    }

                    ///nokia-conf/configure/service/vpls/stp/mst-instance/mst-priority
                    leaf mst-priority {
                        type int32 {
                            range "0|4096|8192|12288|16384|20480|24576|28672|32768|36864|40960|45056|49152|53248|57344|61440";
                        }
                        default "32768";
                        description "Priority of multiple spanning tree instance";
                    }

                    ///nokia-conf/configure/service/vpls/stp/mst-instance/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/stp/mst-instance/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/stp/mst-instance/vlan-range
                    list vlan-range {
                        key "range";
                        description "Add a list entry for vlan-range";

                        ///nokia-conf/configure/service/vpls/stp/mst-instance/vlan-range/range
                        leaf range {
                            type string {
                                pattern "((([1-9]|[1-9][0-9]{1,2}|[1-3][0-9]{3}|40[0-8][0-9]|409[0-4]))(\\.\\.(([1-9]|[1-9][0-9]{1,2}|[1-3][0-9]{3}|40[0-8][0-9]|409[0-4])))?)";
                            }
                            description "Range of VLANs associated with the M-VPLS SAP";
                        }

                    } // list vlan-range

                } // list mst-instance

            } // container stp

            ///nokia-conf/configure/service/vpls/bgp
            list bgp {
                key "bgp-instance";
                description "Enter the bgp list instance";

                ///nokia-conf/configure/service/vpls/bgp/bgp-instance
                leaf bgp-instance {
                    type int32 {
                        range "1..2";
                    }
                    description "BGP instance";
                }

                ///nokia-conf/configure/service/vpls/bgp/route-distinguisher
                leaf route-distinguisher {
                    type types-services:vpn-route-distinguisher-or-auto-rd;
                    description "High-order 6 bytes that are used as string to compose VSI-ID for use in NLRI";
                }

                ///nokia-conf/configure/service/vpls/bgp/vsi-import
                leaf-list vsi-import {
                    type leafref {
                        path "../../../../policy-options/policy-statement/name";
                    }
                    max-elements 5;
                    ordered-by user;
                    description "VSI import policies";
                }

                ///nokia-conf/configure/service/vpls/bgp/vsi-export
                leaf-list vsi-export {
                    type leafref {
                        path "../../../../policy-options/policy-statement/name";
                    }
                    max-elements 5;
                    ordered-by user;
                    description "VSI export policies";
                }

                ///nokia-conf/configure/service/vpls/bgp/adv-service-mtu
                leaf adv-service-mtu {
                    type int32 {
                        range "0..9782";
                    }
                    description "Advertised service MTU value";
                }

                ///nokia-conf/configure/service/vpls/bgp/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/bgp/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vpls/bgp/route-target
                container route-target {
                    description "Enter the route-target context";

                    ///nokia-conf/configure/service/vpls/bgp/route-target/export
                    leaf export {
                        type types-services:route-target;
                        description "Extended community name for default import policy";
                    }

                    ///nokia-conf/configure/service/vpls/bgp/route-target/import
                    leaf import {
                        type types-services:route-target;
                        description "Extended community name for default import policy";
                    }

                } // container route-target

                ///nokia-conf/configure/service/vpls/bgp/pw-template-binding
                list pw-template-binding {
                    key "pw-template-name";
                    max-elements 100;
                    description "Enter the pw-template-binding list instance";

                    ///nokia-conf/configure/service/vpls/bgp/pw-template-binding/pw-template-name
                    leaf pw-template-name {
                        type leafref {
                            path "../../../../pw-template/pw-template-name";
                        }
                        description "Policy name";
                    }

                    ///nokia-conf/configure/service/vpls/bgp/pw-template-binding/split-horizon-group
                    leaf split-horizon-group {
                        type types-sros:named-item;
                        description "Split horizon group";
                    }

                    ///nokia-conf/configure/service/vpls/bgp/pw-template-binding/bfd-template
                    leaf bfd-template {
                        type leafref {
                            path "../../../../../bfd/bfd-template/name";
                        }
                        description "BFD template name for PW-Template binding";
                    }

                    ///nokia-conf/configure/service/vpls/bgp/pw-template-binding/bfd-liveness
                    leaf bfd-liveness {
                        type boolean;
                        default "false";
                        description "Enable BFD";
                    }

                    ///nokia-conf/configure/service/vpls/bgp/pw-template-binding/import-rt
                    leaf-list import-rt {
                        type types-services:route-target;
                        max-elements 5;
                        description "Import route-target communities";
                    }

                    ///nokia-conf/configure/service/vpls/bgp/pw-template-binding/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/bgp/pw-template-binding/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/bgp/pw-template-binding/oper-group-association
                    choice oper-group-association {
                        ///nokia-conf/configure/service/vpls/bgp/pw-template-binding/oper-group-association/oper-group
                        case oper-group {

                            ///nokia-conf/configure/service/vpls/bgp/pw-template-binding/oper-group-association/oper-group/oper-group
                            leaf oper-group {
                                type leafref {
                                    path "../../../../oper-group/name";
                                }
                                description "Operational group";
                            }

                        }
                        ///nokia-conf/configure/service/vpls/bgp/pw-template-binding/oper-group-association/monitor-oper-group
                        case monitor-oper-group {

                            ///nokia-conf/configure/service/vpls/bgp/pw-template-binding/oper-group-association/monitor-oper-group/monitor-oper-group
                            leaf monitor-oper-group {
                                type leafref {
                                    path "../../../../oper-group/name";
                                }
                                description "Operational group to monitor";
                            }

                        }
