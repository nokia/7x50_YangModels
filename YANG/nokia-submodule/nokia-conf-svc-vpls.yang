                    }
                }
                ///nokia-conf/configure/service/vpls/spoke-sdp/oper-group-association
                choice oper-group-association {
                    ///nokia-conf/configure/service/vpls/spoke-sdp/oper-group-association/oper-group
                    case oper-group {

                        ///nokia-conf/configure/service/vpls/spoke-sdp/oper-group-association/oper-group/oper-group
                        leaf oper-group {
                            type leafref {
                                path "../../../oper-group/name";
                            }
                            description "Operational group identifier";
                        }

                    }
                    ///nokia-conf/configure/service/vpls/spoke-sdp/oper-group-association/monitor-oper-group
                    case monitor-oper-group {

                        ///nokia-conf/configure/service/vpls/spoke-sdp/oper-group-association/monitor-oper-group/monitor-oper-group
                        leaf monitor-oper-group {
                            type leafref {
                                path "../../../oper-group/name";
                            }
                            description "Operational group that affects state of the SDP bind";
                        }

                    }
                }
                ///nokia-conf/configure/service/vpls/spoke-sdp/ingress
                container ingress {
                    description "Enter the ingress context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/ingress/vc-label
                    leaf vc-label {
                        type types-services:ingress-vc-label;
                        sros-ext:auto-restart-to-modify;
                        description "Ingress MPLS VC label to send packets to the far end";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/ingress/filter
                    container filter {
                        description "Enter the filter context";

                        ///nokia-conf/configure/service/vpls/spoke-sdp/ingress/filter/mac
                        leaf mac {
                            type leafref {
                                path "../../../../../../filter/mac-filter/filter-name";
                            }
                            description "MAC filter policy name";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/ingress/filter/ip
                        leaf ip {
                            type leafref {
                                path "../../../../../../filter/ip-filter/filter-name";
                            }
                            description "IPv4 filter policy name";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/ingress/filter/ipv6
                        leaf ipv6 {
                            type leafref {
                                path "../../../../../../filter/ipv6-filter/filter-name";
                            }
                            description "IPv6 filter policy name";
                        }

                    } // container filter

                    ///nokia-conf/configure/service/vpls/spoke-sdp/ingress/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-conf/configure/service/vpls/spoke-sdp/ingress/qos/network
                        container network {
                            description "Enter the network context";

                            ///nokia-conf/configure/service/vpls/spoke-sdp/ingress/qos/network/policy-name
                            leaf policy-name {
                                type leafref {
                                    path "../../../../../../../qos/network/network-policy-name";
                                }
                                description "Network policy ID";
                            }

                            ///nokia-conf/configure/service/vpls/spoke-sdp/ingress/qos/network/fp-redirect-group
                            container fp-redirect-group {
                                description "Enter the fp-redirect-group context";

                                ///nokia-conf/configure/service/vpls/spoke-sdp/ingress/qos/network/fp-redirect-group/group-name
                                leaf group-name {
                                    type leafref {
                                        path "../../../../../../../../qos/queue-group-templates/ingress/queue-group/ingress-queue-group-name";
                                    }
                                    description "Name of the forwarding plane queue group template";
                                }

                                ///nokia-conf/configure/service/vpls/spoke-sdp/ingress/qos/network/fp-redirect-group/instance
                                leaf instance {
                                    type types-services:qos-qgrp-instance-id;
                                    description "Instance of FP ingress queue group for the SDP binding";
                                }

                            } // container fp-redirect-group

                        } // container network

                    } // container qos

                } // container ingress

                ///nokia-conf/configure/service/vpls/spoke-sdp/egress
                container egress {
                    description "Enter the egress context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/egress/vc-label
                    leaf vc-label {
                        type types-services:egress-vc-label;
                        sros-ext:auto-restart-to-modify;
                        description "Egress MPLS VC label to send packets to the far end";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/egress/filter
                    container filter {
                        description "Enter the filter context";

                        ///nokia-conf/configure/service/vpls/spoke-sdp/egress/filter/mac
                        leaf mac {
                            type leafref {
                                path "../../../../../../filter/mac-filter/filter-name";
                            }
                            description "MAC filter policy name";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/egress/filter/ip
                        leaf ip {
                            type leafref {
                                path "../../../../../../filter/ip-filter/filter-name";
                            }
                            description "IPv4 filter policy name";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/egress/filter/ipv6
                        leaf ipv6 {
                            type leafref {
                                path "../../../../../../filter/ipv6-filter/filter-name";
                            }
                            description "IPv6 filter policy name";
                        }

                    } // container filter

                    ///nokia-conf/configure/service/vpls/spoke-sdp/egress/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-conf/configure/service/vpls/spoke-sdp/egress/qos/network
                        container network {
                            description "Enter the network context";

                            ///nokia-conf/configure/service/vpls/spoke-sdp/egress/qos/network/policy-name
                            leaf policy-name {
                                type leafref {
                                    path "../../../../../../../qos/network/network-policy-name";
                                }
                                description "Network policy ID";
                            }

                            ///nokia-conf/configure/service/vpls/spoke-sdp/egress/qos/network/port-redirect-group
                            container port-redirect-group {
                                description "Enter the port-redirect-group context";

                                ///nokia-conf/configure/service/vpls/spoke-sdp/egress/qos/network/port-redirect-group/group-name
                                leaf group-name {
                                    type leafref {
                                        path "../../../../../../../../qos/queue-group-templates/egress/queue-group/egress-queue-group-name";
                                    }
                                    description "Name of the egress port queue group";
                                }

                                ///nokia-conf/configure/service/vpls/spoke-sdp/egress/qos/network/port-redirect-group/instance
                                leaf instance {
                                    type types-services:qos-qgrp-instance-id;
                                    description "Queue-group instance ID";
                                }

                            } // container port-redirect-group

                        } // container network

                    } // container qos

                    ///nokia-conf/configure/service/vpls/spoke-sdp/egress/mfib-allowed-mda-destinations
                    container mfib-allowed-mda-destinations {
                        description "Enter the mfib-allowed-mda-destinations context";

                        ///nokia-conf/configure/service/vpls/spoke-sdp/egress/mfib-allowed-mda-destinations/mda
                        list mda {
                            key "mda-id";
                            description "Add a list entry for mda";

                            ///nokia-conf/configure/service/vpls/spoke-sdp/egress/mfib-allowed-mda-destinations/mda/mda-id
                            leaf mda-id {
                                type types-isa:slot-mda;
                                description "MFIB allowed MDA destination";
                            }

                        } // list mda

                    } // container mfib-allowed-mda-destinations

                } // container egress

                ///nokia-conf/configure/service/vpls/spoke-sdp/endpoint
                container endpoint {
                    description "Enter the endpoint context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/endpoint/name
                    leaf name {
                        type leafref {
                            path "../../../../vpls[service-name=current()/../../../service-name]/endpoint/name";
                        }
                        sros-ext:auto-restart-to-modify;
                        description "Name of service endpoint to which SDP bind is attached";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/endpoint/precedence
                    leaf precedence {
                        type types-services:sdp-precedence;
                        default "4";
                        description "Precedence of this SDP bind when there are multiple SDP binds attached to one service endpoint";
                    }

                } // container endpoint

                ///nokia-conf/configure/service/vpls/spoke-sdp/cpu-protection
                container cpu-protection {
                    description "Enter the cpu-protection context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/cpu-protection/policy-id
                    leaf policy-id {
                        type leafref {
                            path "../../../../../system/security/cpu-protection/policy/policy-id";
                        }
                        description "CPM protection policy";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/cpu-protection/monitoring
                    choice monitoring {
                        ///nokia-conf/configure/service/vpls/spoke-sdp/cpu-protection/monitoring/mac
                        case mac {

                            ///nokia-conf/configure/service/vpls/spoke-sdp/cpu-protection/monitoring/mac/mac-monitoring
                            leaf mac-monitoring {
                                type empty;
                                description "Monitor MAC for CPU protection";
                            }

                        }
                        ///nokia-conf/configure/service/vpls/spoke-sdp/cpu-protection/monitoring/cfm
                        case cfm {

                            ///nokia-conf/configure/service/vpls/spoke-sdp/cpu-protection/monitoring/cfm/eth-cfm-monitoring
                            container eth-cfm-monitoring {
                                presence "Enable the eth-cfm-monitoring context";
                                description "Enable the eth-cfm-monitoring context";

                                ///nokia-conf/configure/service/vpls/spoke-sdp/cpu-protection/monitoring/cfm/eth-cfm-monitoring/aggregate
                                leaf aggregate {
                                    type empty;
                                    description "Apply rate limit to the sum of the per peer packet rates";
                                }

                                ///nokia-conf/configure/service/vpls/spoke-sdp/cpu-protection/monitoring/cfm/eth-cfm-monitoring/car
                                leaf car {
                                    type empty;
                                    description "Ignore Ethernet CFM packets when enforcing overall rate";
                                }

                            } // container eth-cfm-monitoring

                        }
                    }
                } // container cpu-protection

                ///nokia-conf/configure/service/vpls/spoke-sdp/pw-status
                container pw-status {
                    description "Enter the pw-status context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/pw-status/signaling
                    leaf signaling {
                        type boolean;
                        default "true";
                        description "Enable the use of pseudowire status signaling";
                    }

                } // container pw-status

                ///nokia-conf/configure/service/vpls/spoke-sdp/bfd-liveness
                container bfd-liveness {
                    status obsolete;
                    presence "Enable the bfd-liveness context";
                    description "Enable the bfd-liveness context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/bfd-liveness/encap
                    leaf encap {
                        status obsolete;
                        type types-services:bfd-encap;
                        default "ipv4";
                        description "BFD encapsulation used on the SDP binding";
                    }

                } // container bfd-liveness

                ///nokia-conf/configure/service/vpls/spoke-sdp/bfd
                container bfd {
                    description "Enter the bfd context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/bfd/bfd-template
                    leaf bfd-template {
                        type leafref {
                            path "../../../../../bfd/bfd-template/name";
                        }
                        description "BFD template associated with the SDP binding";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/bfd/failure-action
                    leaf failure-action {
                        type types-services:sbind-bfd-fail-action;
                        default "none";
                        description "VCCV BFD action taken on the SDP binding";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/bfd/wait-for-up-timer
                    leaf wait-for-up-timer {
                        type uint32 {
                            range "1..60";
                        }
                        units "seconds";
                        description "Time waited for BFD up status";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/bfd/bfd-liveness
                    container bfd-liveness {
                        presence "Enable the bfd-liveness context";
                        description "Enable the bfd-liveness context";

                        ///nokia-conf/configure/service/vpls/spoke-sdp/bfd/bfd-liveness/encap
                        leaf encap {
                            type types-services:bfd-encap;
                            default "ipv4";
                            description "BFD encapsulation used on the SDP binding";
                        }

                    } // container bfd-liveness

                } // container bfd

                ///nokia-conf/configure/service/vpls/spoke-sdp/transit-policy
                container transit-policy {
                    presence "Enable the transit-policy context";
                    description "Enable the transit-policy context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/transit-policy/prefix
                    leaf prefix {
                        type leafref {
                            path "../../../../../application-assurance/group/partition/transit-prefix-policy/prefix-policy-id";
                        }
                        sros-ext:immutable;
                        mandatory true;
                        description "IP prefix policy ID";
                    }

                } // container transit-policy

                ///nokia-conf/configure/service/vpls/spoke-sdp/pbb
                container pbb {
                    when "../../pbb-type = 'b-vpls'" {
                        description "supported on b-vpls service";
                    }
                    description "Enter the pbb context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/pbb/fault-propagation
                    container fault-propagation {
                        description "Enter the fault-propagation context";

                        ///nokia-conf/configure/service/vpls/spoke-sdp/pbb/fault-propagation/backbone-mac-name
                        list backbone-mac-name {
                            key "name";
                            description "Add a list entry for backbone-mac-name";

                            ///nokia-conf/configure/service/vpls/spoke-sdp/pbb/fault-propagation/backbone-mac-name/name
                            leaf name {
                                type leafref {
                                    path "../../../../../../pbb/mac/name";
                                }
                                description "Backbone MAC address name";
                            }

                        } // list backbone-mac-name

                        ///nokia-conf/configure/service/vpls/spoke-sdp/pbb/fault-propagation/backbone-mac-address
                        list backbone-mac-address {
                            key "address";
                            description "Add a list entry for backbone-mac-address";

                            ///nokia-conf/configure/service/vpls/spoke-sdp/pbb/fault-propagation/backbone-mac-address/address
                            leaf address {
                                type types-sros:mac-unicast-address-no-zero;
                                description "Backbone MAC address";
                            }

                        } // list backbone-mac-address

                    } // container fault-propagation

                } // container pbb

                ///nokia-conf/configure/service/vpls/spoke-sdp/spb
                container spb {
                    presence "Enable the spb context";
                    description "Enable the spb context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/spb/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Admin state";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/spb/lsp-pacing-interval
                    leaf lsp-pacing-interval {
                        type uint32 {
                            range "0..65535";
                        }
                        units "milliseconds";
                        default "100";
                        description "Lsp pacing interval";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/spb/retransmit-interval
                    leaf retransmit-interval {
                        type uint32 {
                            range "1..65535";
                        }
                        units "seconds";
                        default "5";
                        description "Retransmit interval";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/spb/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/spb/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/spb/level
                    list level {
                        key "id";
                        description "Enter the level list instance";

                        ///nokia-conf/configure/service/vpls/spoke-sdp/spb/level/id
                        leaf id {
                            type int32 {
                                range "1";
                            }
                            description "Level identifier";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/spb/level/hello-interval
                        leaf hello-interval {
                            type uint32 {
                                range "1..20000";
                            }
                            units "seconds";
                            default "9";
                            description "Hello interval";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/spb/level/hello-multiplier
                        leaf hello-multiplier {
                            type int32 {
                                range "2..100";
                            }
                            default "3";
                            description "Hello multiplier";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/spb/level/metric
                        leaf metric {
                            type uint32 {
                                range "0..16777215";
                            }
                            default "0";
                            description "Metric";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/spb/level/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/spb/level/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list level

                } // container spb

                ///nokia-conf/configure/service/vpls/spoke-sdp/i-vpls-mac-flush
                container i-vpls-mac-flush {
                    when "../../pbb-type = 'i-vpls'" {
                        description "VPLS PBB type must be 'i-vpls'";
                    }
                    description "Enter the i-vpls-mac-flush context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/i-vpls-mac-flush/bgp-evpn
                    container bgp-evpn {
                        description "Enter the bgp-evpn context";

                        ///nokia-conf/configure/service/vpls/spoke-sdp/i-vpls-mac-flush/bgp-evpn/send-to-bvpls
                        leaf send-to-bvpls {
                            type boolean;
                            default "true";
                            description "Send B-VPLS EVPN flush";
                        }

                    } // container bgp-evpn

                } // container i-vpls-mac-flush

                ///nokia-conf/configure/service/vpls/spoke-sdp/fdb
                container fdb {
                    description "Enter the fdb context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/fdb/limit-mac-move
                    leaf limit-mac-move {
                        type types-services:limit-mac-move;
                        default "blockable";
                        description "MAC move";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/fdb/discard-unknown-source
                    leaf discard-unknown-source {
                        type boolean;
                        default "false";
                        description "Discard packets with unknown destination MAC addresses";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/fdb/mac-pinning
                    leaf mac-pinning {
                        type boolean;
                        default "false";
                        description "MAC address pinning in active status";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/fdb/maximum-mac-addresses
                    leaf maximum-mac-addresses {
                        type types-services:max-mac-addr;
                        description "Maximum number of MAC address entries in the FDB";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/fdb/auto-learn-mac-protect
                    leaf auto-learn-mac-protect {
                        type boolean;
                        default "false";
                        description "Populate automatically MAC protect list with source MAC addresses learned on SDP";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/fdb/auto-learn-mac-protect-exclude-list
                    leaf auto-learn-mac-protect-exclude-list {
                        type leafref {
                            path "../../../../mac-list/name";
                        }
                        description "Referenced MAC protect exclusion list name";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/fdb/protected-src-mac-violation-action
                    leaf protected-src-mac-violation-action {
                        type types-services:sdp-protected-src-mac-violation-action;
                        description "Action when a relearn request for a protected MAC is received on the SDP";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/fdb/mac-learning
                    container mac-learning {
                        description "Enter the mac-learning context";

                        ///nokia-conf/configure/service/vpls/spoke-sdp/fdb/mac-learning/learning
                        leaf learning {
                            type boolean;
                            default "true";
                            description "Enable learning of new MAC addresses";
                        }

                        ///nokia-conf/configure/service/vpls/spoke-sdp/fdb/mac-learning/aging
                        leaf aging {
                            type boolean;
                            default "true";
                            description "Enable aging of MAC addresses";
                        }

                    } // container mac-learning

                } // container fdb

                ///nokia-conf/configure/service/vpls/spoke-sdp/stp
                container stp {
                    description "Enter the stp context";

                    ///nokia-conf/configure/service/vpls/spoke-sdp/stp/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        description "Administrative state of STP";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/stp/auto-edge
                    leaf auto-edge {
                        type boolean;
                        default "true";
                        description "Enable automatic detection of edge port characteristics";
                    }

                    ///nokia-conf/configure/service/vpls/spoke-sdp/stp/edge-port
                    leaf edge-port {
                        type boolean;
                        default "false";
                        description "Designate SAP or SDP as an edge port";
