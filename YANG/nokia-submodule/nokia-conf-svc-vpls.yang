                                }
                                ///nokia-conf/configure/service/vpls/sap/dhcp/option-82/remote-id/remote-id/hex-string
                                case hex-string {

                                    ///nokia-conf/configure/service/vpls/sap/dhcp/option-82/remote-id/remote-id/hex-string/hex-string
                                    leaf hex-string {
                                        type types-sros:hex-string {
                                            length "1..66";
                                        }
                                        description "Option as a hexadecimal string";
                                    }

                                }
                            }
                        } // container remote-id

                        ///nokia-conf/configure/service/vpls/sap/dhcp/option-82/vendor-specific-option
                        container vendor-specific-option {
                            description "Enter the vendor-specific-option context";

                            ///nokia-conf/configure/service/vpls/sap/dhcp/option-82/vendor-specific-option/system-id
                            leaf system-id {
                                type boolean;
                                default "false";
                                description "Send the system ID in the VSO";
                            }

                            ///nokia-conf/configure/service/vpls/sap/dhcp/option-82/vendor-specific-option/client-mac-address
                            leaf client-mac-address {
                                type boolean;
                                default "false";
                                description "Send the MAC address in the VSO";
                            }

