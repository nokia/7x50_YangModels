                            type leafref {
                                path "../../../../../../subscriber-mgmt/sub-profile/name";
                            }
                            description "Default subscriber profile for the SAP";
                        }

                        ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/sla-profile
                        leaf sla-profile {
                            type leafref {
                                path "../../../../../../subscriber-mgmt/sla-profile/name";
                            }
                            description "Default SLA profile for hosts on this SAP";
                        }

                        ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/app-profile
                        leaf app-profile {
                            type leafref {
                                path "../../../../../../application-assurance/group/partition/policy/app-profile/app-profile-name";
                            }
                            description "Default application profile name for this SAP";
                        }

                        ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/subscriber-id
                        container subscriber-id {
                            description "Enter the subscriber-id context";

                            ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/subscriber-id/subscriber-id
                            choice subscriber-id {
                                ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/subscriber-id/subscriber-id/auto-id
                                case auto-id {

                                    ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/subscriber-id/subscriber-id/auto-id/auto-id
                                    leaf auto-id {
                                        type empty;
                                        description "Use auto-generated subscriber identification string";
                                    }

                                }
                                ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/subscriber-id/subscriber-id/sap-id
                                case sap-id {

                                    ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/subscriber-id/subscriber-id/sap-id/sap-id
                                    leaf sap-id {
                                        type empty;
                                        description "Use SAP ID as default subscriber identification string";
                                    }

                                }
                                ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/subscriber-id/subscriber-id/string
                                case string {

                                    ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/subscriber-id/subscriber-id/string/string
                                    leaf string {
                                        type types-submgt:subscriber-id;
                                        description "Default subscriber identification string for the SAP";
                                    }

                                }
                            }
                        } // container subscriber-id

                        ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/int-dest-id
                        container int-dest-id {
                            description "Enter the int-dest-id context";

                            ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/int-dest-id/int-dest-id
                            choice int-dest-id {
                                ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/int-dest-id/int-dest-id/top-q-tag
                                case top-q-tag {

                                    ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/int-dest-id/int-dest-id/top-q-tag/top-q-tag
                                    leaf top-q-tag {
                                        type empty;
                                        description "Use the top Q-tag of this SAP";
                                    }

                                }
                                ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/int-dest-id/int-dest-id/string
                                case string {

                                    ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/defaults/int-dest-id/int-dest-id/string/string
                                    leaf string {
                                        type types-submgt:int-dest-id;
                                        description "Use the configured string";
                                    }

                                }
                            }
                        } // container int-dest-id

                    } // container defaults

                    ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/single-sub-parameters
                    container single-sub-parameters {
                        description "Enter the single-sub-parameters context";

                        ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/single-sub-parameters/profiled-traffic-only
                        leaf profiled-traffic-only {
                            type boolean;
                            default "false";
                            description "Include all traffic in subscriber profile";
                        }

                        ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/single-sub-parameters/non-sub-traffic
                        container non-sub-traffic {
                            presence "Enable the non-sub-traffic context";
                            description "Enable the non-sub-traffic context";

                            ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/single-sub-parameters/non-sub-traffic/sub-profile
                            leaf sub-profile {
                                type leafref {
                                    path "../../../../../../../subscriber-mgmt/sub-profile/name";
                                }
                                mandatory true;
                                description "Subscriber profile all non-subscriber traffic";
                            }

                            ///nokia-conf/configure/service/vpls/sap/sub-sla-mgmt/single-sub-parameters/non-sub-traffic/sla-profile
                            leaf sla-profile {
                                type leafref {
                                    path "../../../../../../../subscriber-mgmt/sla-profile/name";
                                }
