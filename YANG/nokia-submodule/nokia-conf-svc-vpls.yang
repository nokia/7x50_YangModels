                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/service/vpls/sap/eth-cfm/mep/mac-address
                        leaf mac-address {
                            type types-sros:mac-unicast-address-no-zero;
                            description "MAC address of the MEP";
                        }

