submodule nokia-conf-router-dhcp-server {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-dhcp               { prefix "types-dhcp"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-groups;
    include nokia-conf-redundancy;
    include nokia-conf-submgt-local-user-db;
    include nokia-conf-subscriber-mgmt;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure DHCP server on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-router-dhcp-server YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2021-10-04";

    grouping conf-router-dhcp-server {
        ///nokia-conf/configure/router/dhcp-server
        container dhcp-server {
            when "../router-name = 'Base'" {
                description "only allowed in the Base router";
            }
            description "Enter the dhcp-server context";

            ///nokia-conf/configure/router/dhcp-server/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/router/dhcp-server/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/router/dhcp-server/dhcpv4
            list dhcpv4 {
                key "name";
                description "Enter the dhcpv4 list instance";

                ///nokia-conf/configure/router/dhcp-server/dhcpv4/name
                leaf name {
                    type types-sros:named-item;
                    description "DHCP server name";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv4/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of the DHCP server";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv4/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv4/user-db
                leaf user-db {
                    type leafref {
                        path "../../../../subscriber-mgmt/local-user-db/name";
                    }
                    description "Local user database for authentication";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv4/user-identification
                leaf user-identification {
                    type enumeration {
                        enum mac-circuit-id                 { value 1; }
                        enum duid                           { value 2; }
                        enum interface-id                   { value 3; }
                        enum interface-id-link-local        { value 4; }
                        enum client-id                      { value 5; }
                        enum mac                            { value 6; }
                        enum circuit-id                     { value 7; }
                        enum remote-id                      { value 8; }
                    }
                    sros-ext:auto-restart-to-modify;
                    description "User identification method for the DHCP server";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv4/force-renews
                leaf force-renews {
                    type boolean;
                    default "false";
                    description "Send FORCERENEW messages to force renewals of leases";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv4/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv4/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool-selection
                container pool-selection {
                    description "Enter the pool-selection context";

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool-selection/use-gi-address
                    container use-gi-address {
                        presence "Enable the use-gi-address context";
                        description "Enable the use-gi-address context";

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool-selection/use-gi-address/scope
                        leaf scope {
                            type enumeration {
                                enum subnet                         { value 0; }
                                enum pool                           { value 1; }
                            }
                            default "subnet";
                            description "GI address-matching scope";
                        }

                    } // container use-gi-address

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool-selection/use-pool-from-client
                    container use-pool-from-client {
                        presence "Enable the use-pool-from-client context";
                        description "Enable the use-pool-from-client context";

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool-selection/use-pool-from-client/delimiter
                        leaf delimiter {
                            type types-sros:string-not-all-spaces {
                                length "1";
                            }
                            description "Delimiter to combine primary and secondary pool names";
                        }

                    } // container use-pool-from-client

                } // container pool-selection

                ///nokia-conf/configure/router/dhcp-server/dhcpv4/lease-hold
                container lease-hold {
                    description "Enter the lease-hold context";

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/lease-hold/time
                    leaf time {
                        type types-sros:time-duration {
                            range "1..631152000";
                        }
                        units "seconds";
                        description "Lease hold time";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/lease-hold/additional-scenarios
                    container additional-scenarios {
                        description "Enter the additional-scenarios context";

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/lease-hold/additional-scenarios/internal-lease-ipsec
                        leaf internal-lease-ipsec {
                            type boolean;
                            default "false";
                            description "Apply the lease hold timer to local IPsec clients";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/lease-hold/additional-scenarios/solicited-release
                        leaf solicited-release {
                            type boolean;
                            default "false";
                            description "Apply lease hold timer for solicited releases";
                        }

                    } // container additional-scenarios

                } // container lease-hold

                ///nokia-conf/configure/router/dhcp-server/dhcpv4/failover
                container failover {
                    description "Enter the failover context";

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/failover/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the failover mechanism";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/failover/ignore-mclt-on-takeover
                    leaf ignore-mclt-on-takeover {
                        type boolean;
                        default "false";
                        description "Ignore maximum client lead during takeover from partner";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/failover/maximum-client-lead-time
                    leaf maximum-client-lead-time {
                        type types-sros:time-duration {
                            range "600..86399";
                        }
                        units "seconds";
                        sros-ext:auto-restart-to-modify;
                        default "600";
                        description "Maximum time that DHCP server can extend client's lease";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/failover/partner-down-delay
                    leaf partner-down-delay {
                        type types-sros:time-duration {
                            range "0..86399";
                        }
                        units "seconds";
                        sros-ext:auto-restart-to-modify;
                        default "86399";
                        description "Delay to prevent lease duplication during link failure";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/failover/startup-wait-time
                    leaf startup-wait-time {
                        type types-sros:time-duration {
                            range "60..3600";
                        }
                        units "seconds";
                        sros-ext:auto-restart-to-modify;
                        default "120";
                        description "Time between initialization and assuming active role";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/failover/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/failover/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/failover/peer
                    list peer {
                        key "address";
                        max-elements 1;
                        description "Enter the peer list instance";

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/failover/peer/address
                        leaf address {
                            type leafref {
                                path "../../../../../../redundancy/multi-chassis/peer/ip-address";
                            }
                            description "IP address of the failover peer";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/failover/peer/sync-tag
                        leaf sync-tag {
                            type types-sros:named-item;
                            sros-ext:immutable;
                            mandatory true;
                            description "Tag that identifies synchronizing server or pool pairs";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/failover/peer/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/failover/peer/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list peer

                } // container failover

                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool
                list pool {
                    key "pool-name";
                    description "Enter the pool list instance";

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/pool-name
                    leaf pool-name {
                        type types-sros:named-item;
                        description "DHCP server pool name";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/min-lease-time
                    leaf min-lease-time {
                        type types-sros:time-duration {
                            range "10..315446399";
                        }
                        units "seconds";
                        default "600";
                        description "Minimum lease time";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/max-lease-time
                    leaf max-lease-time {
                        type types-sros:time-duration {
                            range "10..315446399";
                        }
                        units "seconds";
                        default "864000";
                        description "Maximum lease time";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/offer-time
                    leaf offer-time {
                        type types-sros:time-duration {
                            range "10..600";
                        }
                        units "seconds";
                        default "60";
                        description "Time interval during which a DHCP offer remains valid";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/nak-non-matching-subnet
                    leaf nak-non-matching-subnet {
                        type boolean;
                        default "false";
                        description "Send NAK if no match for request address pool range";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/minimum-free
                    container minimum-free {
                        description "Enter the minimum-free context";

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/minimum-free/event-when-depleted
                        leaf event-when-depleted {
                            type boolean;
                            default "false";
                            description "Generate notification when addresses are depleted";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/minimum-free/value
                        choice value {
                            default "absolute";
                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/minimum-free/value/absolute
                            case absolute {

                                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/minimum-free/value/absolute/absolute
                                leaf absolute {
                                    type uint32 {
                                        range "0..255";
                                    }
                                    default "1";
                                    description "Minimum number of free addresses in this pool or subnet";
                                }

                            }
                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/minimum-free/value/percent
                            case percent {

                                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/minimum-free/value/percent/percent
                                leaf percent {
                                    type uint32 {
                                        range "0..100";
                                    }
                                    default "1";
                                    description "Minimum free addresses as a percentage";
                                }

                            }
                        }
                    } // container minimum-free

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options
                    container options {
                        description "Enter the options context";

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option
                        list option {
                            key "number";
                            description "Enter the option list instance";

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/number
                            leaf number {
                                type union {
                                    type uint32 {
                                        range "1..254";
                                    }
                                    type enumeration {
                                        enum subnet-mask                    { value 1; }
                                        enum default-router                 { value 3; }
                                        enum dns-server                     { value 6; }
                                        enum domain-name                    { value 15; }
                                        enum netbios-name-server            { value 44; }
                                        enum netbios-node-type              { value 46; }
                                        enum lease-time                     { value 51; }
                                        enum lease-renew-time               { value 58; }
                                        enum lease-rebind-time              { value 59; }
                                    }
                                }
                                description "DHCP option to send identification strings to client";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/value
                            choice value {
                                mandatory true;
                                description "The value of the DHCP option.";
                                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/value/ascii-string
                                case ascii-string {

                                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/value/ascii-string/ascii-string
                                    leaf ascii-string {
                                        when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                            description "ascii-string is supported only when the option is neither netbios-node-type(46), lease-time(51), lease-renew-time(58) nor lease-rebind-time(59)";
                                        }
                                        type types-sros:string-not-all-spaces {
                                            length "1..127";
                                        }
                                        description "DHCP option specified as an ASCII string";
                                    }

                                }
                                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/value/hex-string
                                case hex-string {

                                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/value/hex-string/hex-string
                                    leaf hex-string {
                                        when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                            description "hex-string is supported only when the option is neither netbios-node-type(46), lease-time(51), lease-renew-time(58) nor lease-rebind-time(59)";
                                        }
                                        type types-sros:hex-string {
                                            length "1..256";
                                        }
                                        description "DHCP option specified as hexadecimal string";
                                    }

                                }
                                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/value/netbios-node-type
                                case netbios-node-type {

                                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/value/netbios-node-type/netbios-node-type
                                    leaf netbios-node-type {
                                        when "../number = 46 or ../number = 'netbios-node-type'" {
                                            description "netbios-node-type is supported only when the option is netbios-node-type(46)";
                                        }
                                        type enumeration {
                                            enum b-node                         { value 1; }
                                            enum p-node                         { value 2; }
                                            enum m-node                         { value 4; }
                                            enum h-node                         { value 8; }
                                        }
                                        description "DHCP option specified as a NetBIOS node type";
                                    }

                                }
                                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/value/duration
                                case duration {

                                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/value/duration/duration
                                    leaf duration {
                                        when "../number = 51 or ../number = 'lease-time' or ../number = 58 or ../number = 'lease-renew-time' or ../number = 59 or ../number = 'lease-rebind-time'" {
                                            description "duration is supported only when the option is either lease-time(51), lease-renew-time(58) or lease-rebind-time(59)";
                                        }
                                        type types-sros:time-duration {
                                            range "10..315446399";
                                        }
                                        units "seconds";
                                        description "DHCP option specified as time duration";
                                    }

                                }
                                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/value/ipv4-address
                                case ipv4-address {

                                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/value/ipv4-address/ipv4-address
                                    leaf-list ipv4-address {
                                        when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                            description "ipv4-address is supported only when the option is neither netbios-node-type(46), lease-time(51), lease-renew-time(58) nor lease-rebind-time(59)";
                                        }
                                        type types-sros:ipv4-address;
                                        max-elements 4;
                                        ordered-by user;
                                        description "DHCP option specified as a list of IPv4 addresses";
                                    }

                                }
                                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/value/empty
                                case empty {

                                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/options/option/value/empty/empty
                                    leaf empty {
                                        when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                            description "empty is supported only when the option is neither netbios-node-type(46), lease-time(51), lease-renew-time(58) nor lease-rebind-time(59)";
                                        }
                                        type empty;
                                        description "Remove DHCP option from configuration";
                                    }

                                }
                            }
                        } // list option

                    } // container options

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/failover
                    container failover {
                        description "Enable the failover context";

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/failover/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the failover mechanism";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/failover/ignore-mclt-on-takeover
                        leaf ignore-mclt-on-takeover {
                            type boolean;
                            default "false";
                            description "Ignore maximum client lead during takeover from partner";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/failover/maximum-client-lead-time
                        leaf maximum-client-lead-time {
                            type types-sros:time-duration {
                                range "600..86399";
                            }
                            units "seconds";
                            sros-ext:auto-restart-to-modify;
                            default "600";
                            description "Maximum time that DHCP server can extend client's lease";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/failover/partner-down-delay
                        leaf partner-down-delay {
                            type types-sros:time-duration {
                                range "0..86399";
                            }
                            units "seconds";
                            sros-ext:auto-restart-to-modify;
                            default "86399";
                            description "Delay to prevent lease duplication during link failure";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/failover/startup-wait-time
                        leaf startup-wait-time {
                            type types-sros:time-duration {
                                range "60..3600";
                            }
                            units "seconds";
                            sros-ext:auto-restart-to-modify;
                            default "120";
                            description "Time between initialization and assuming active role";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/failover/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/failover/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/failover/peer
                        list peer {
                            key "address";
                            max-elements 1;
                            description "Enter the peer list instance";

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/failover/peer/address
                            leaf address {
                                type leafref {
                                    path "../../../../../../../redundancy/multi-chassis/peer/ip-address";
                                }
                                description "IP address of the failover peer";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/failover/peer/sync-tag
                            leaf sync-tag {
                                type types-sros:named-item;
                                sros-ext:immutable;
                                mandatory true;
                                description "Tag that identifies synchronizing server or pool pairs";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/failover/peer/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/failover/peer/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list peer

                    } // container failover

                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet
                    list subnet {
                        key "ipv4-prefix";
                        description "Enter the subnet list instance";

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/ipv4-prefix
                        leaf ipv4-prefix {
                            type types-sros:ipv4-unicast-prefix;
                            description "IPv4 prefix for the subnet";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/drain
                        leaf drain {
                            type boolean;
                            default "false";
                            description "Prevent new lease assignment from this subnet";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/maximum-declined
                        leaf maximum-declined {
                            type uint32;
                            default "64";
                            description "Maximum number of declined addresses allowed";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/minimum-free
                        container minimum-free {
                            description "Enter the minimum-free context";

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/minimum-free/event-when-depleted
                            leaf event-when-depleted {
                                type boolean;
                                default "false";
                                description "Generate notification when addresses are depleted";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/minimum-free/value
                            choice value {
                                default "absolute";
                                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/minimum-free/value/absolute
                                case absolute {

                                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/minimum-free/value/absolute/absolute
                                    leaf absolute {
                                        type uint32 {
                                            range "0..255";
                                        }
                                        default "1";
                                        description "Minimum number of free addresses in this pool or subnet";
                                    }

                                }
                                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/minimum-free/value/percent
                                case percent {

                                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/minimum-free/value/percent/percent
                                    leaf percent {
                                        type uint32 {
                                            range "0..100";
                                        }
                                        default "1";
                                        description "Minimum free addresses as a percentage";
                                    }

                                }
                            }
                        } // container minimum-free

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options
                        container options {
                            description "Enter the options context";

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option
                            list option {
                                key "number";
                                description "Enter the option list instance";

                                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/number
                                leaf number {
                                    type union {
                                        type uint32 {
                                            range "1..254";
                                        }
                                        type enumeration {
                                            enum subnet-mask                    { value 1; }
                                            enum default-router                 { value 3; }
                                            enum dns-server                     { value 6; }
                                            enum domain-name                    { value 15; }
                                            enum netbios-name-server            { value 44; }
                                            enum netbios-node-type              { value 46; }
                                            enum lease-time                     { value 51; }
                                            enum lease-renew-time               { value 58; }
                                            enum lease-rebind-time              { value 59; }
                                        }
                                    }
                                    description "DHCP option to send identification strings to client";
                                }

                                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/value
                                choice value {
                                    mandatory true;
                                    description "The value of the DHCP option.";
                                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/value/ascii-string
                                    case ascii-string {

                                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/value/ascii-string/ascii-string
                                        leaf ascii-string {
                                            when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                                description "ascii-string is supported only when the option is neither netbios-node-type(46), lease-time(51), lease-renew-time(58) nor lease-rebind-time(59)";
                                            }
                                            type types-sros:string-not-all-spaces {
                                                length "1..127";
                                            }
                                            description "DHCP option specified as an ASCII string";
                                        }

                                    }
                                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/value/hex-string
                                    case hex-string {

                                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/value/hex-string/hex-string
                                        leaf hex-string {
                                            when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                                description "hex-string is supported only when the option is neither netbios-node-type(46), lease-time(51), lease-renew-time(58) nor lease-rebind-time(59)";
                                            }
                                            type types-sros:hex-string {
                                                length "1..256";
                                            }
                                            description "DHCP option specified as hexadecimal string";
                                        }

                                    }
                                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/value/netbios-node-type
                                    case netbios-node-type {

                                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/value/netbios-node-type/netbios-node-type
                                        leaf netbios-node-type {
                                            when "../number = 46 or ../number = 'netbios-node-type'" {
                                                description "netbios-node-type is supported only when the option is netbios-node-type(46)";
                                            }
                                            type enumeration {
                                                enum b-node                         { value 1; }
                                                enum p-node                         { value 2; }
                                                enum m-node                         { value 4; }
                                                enum h-node                         { value 8; }
                                            }
                                            description "DHCP option specified as a NetBIOS node type";
                                        }

                                    }
                                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/value/duration
                                    case duration {

                                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/value/duration/duration
                                        leaf duration {
                                            when "../number = 51 or ../number = 'lease-time' or ../number = 58 or ../number = 'lease-renew-time' or ../number = 59 or ../number = 'lease-rebind-time'" {
                                                description "duration is supported only when the option is either lease-time(51), lease-renew-time(58) or lease-rebind-time(59)";
                                            }
                                            type types-sros:time-duration {
                                                range "10..315446399";
                                            }
                                            units "seconds";
                                            description "DHCP option specified as time";
                                        }

                                    }
                                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/value/ipv4-address
                                    case ipv4-address {

                                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/value/ipv4-address/ipv4-address
                                        leaf-list ipv4-address {
                                            when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                                description "ipv4-address is supported only when the option is neither netbios-node-type(46), lease-time(51), lease-renew-time(58) nor lease-rebind-time(59)";
                                            }
                                            type types-sros:ipv4-address;
                                            max-elements 4;
                                            ordered-by user;
                                            description "DHCP option specified as a list of IPv4 addresses";
                                        }

                                    }
                                    ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/value/empty
                                    case empty {

                                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/options/option/value/empty/empty
                                        leaf empty {
                                            when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                                description "empty is supported only when the option is neither netbios-node-type(46), lease-time(51), lease-renew-time(58) nor lease-rebind-time(59)";
                                            }
                                            type empty;
                                            description "Empty DHCP option";
                                        }

                                    }
                                }
                            } // list option

                        } // container options

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/address-range
                        list address-range {
                            key "start end";
                            description "Enter the address-range list instance";

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/address-range/start
                            leaf start {
                                type types-sros:ipv4-unicast-address;
                                description "Lower bound of the IP address range";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/address-range/end
                            leaf end {
                                type types-sros:ipv4-unicast-address;
                                description "Upper bound of the IP address range";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/address-range/failover-control-type
                            leaf failover-control-type {
                                type types-dhcp:failover-control-type;
                                sros-ext:immutable;
                                default "local";
                                description "Failover control type for this range";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/address-range/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/address-range/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list address-range

                        ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/exclude-addresses
                        list exclude-addresses {
                            key "start end";
                            description "Add a list entry for exclude-addresses";

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/exclude-addresses/start
                            leaf start {
                                type types-sros:ipv4-unicast-address;
                                description "Lower bound of the IP address range";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv4/pool/subnet/exclude-addresses/end
                            leaf end {
                                type types-sros:ipv4-unicast-address;
                                description "Upper bound of the IP address range";
                            }

                        } // list exclude-addresses

                    } // list subnet

                } // list pool

            } // list dhcpv4

            ///nokia-conf/configure/router/dhcp-server/dhcpv6
            list dhcpv6 {
                key "name";
                description "Enter the dhcpv6 list instance";

                ///nokia-conf/configure/router/dhcp-server/dhcpv6/name
                leaf name {
                    type types-sros:named-item;
                    description "DHCP server name";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv6/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of the DHCP server";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv6/auto-provisioned
                leaf auto-provisioned {
                    type boolean;
                    sros-ext:immutable;
                    default "false";
                    description "Auto-provision the pools of this server";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv6/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv6/user-identification
                leaf user-identification {
                    when "../auto-provisioned = 'false'" {
                        description "only allowed when auto-provisioned is false";
                    }
                    type enumeration {
                        enum mac-circuit-id                 { value 1; }
                        enum duid                           { value 2; }
                        enum interface-id                   { value 3; }
                        enum interface-id-link-local        { value 4; }
                        enum client-id                      { value 5; }
                        enum mac                            { value 6; }
                        enum circuit-id                     { value 7; }
                        enum remote-id                      { value 8; }
                    }
                    sros-ext:auto-restart-to-modify;
                    description "User identification method for the DHCP server";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv6/ignore-rapid-commit
                leaf ignore-rapid-commit {
                    type boolean;
                    default "false";
                    description "Ignore Rapid Commit option";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv6/interface-id-mapping
                leaf interface-id-mapping {
                    type boolean;
                    sros-ext:auto-restart-to-modify;
                    default "false";
                    description "Map hosts within interface-to-prefix combinations";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv6/lease-query
                leaf lease-query {
                    when "../auto-provisioned = 'false'" {
                        description "only allowed when auto-provisioned is false";
                    }
                    type boolean;
                    default "false";
                    description "Handle and reply to lease query messages";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv6/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv6/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool-selection
                container pool-selection {
                    when "../auto-provisioned = 'false'" {
                        description "only allowed when auto-provisioned is false";
                    }
                    description "Enter the pool-selection context";

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool-selection/use-link-address
                    container use-link-address {
                        presence "Enable the use-link-address context";
                        description "Enable the use-link-address context";

                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool-selection/use-link-address/scope
                        leaf scope {
                            type enumeration {
                                enum subnet                         { value 0; }
                                enum pool                           { value 1; }
                            }
                            default "subnet";
                            description "Scope of the IP address selection";
                        }

                    } // container use-link-address

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool-selection/use-pool-from-client
                    container use-pool-from-client {
                        presence "Enable the use-pool-from-client context";
                        description "Enable the use-pool-from-client context";

                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool-selection/use-pool-from-client/delimiter
                        leaf delimiter {
                            type types-sros:string-not-all-spaces {
                                length "1";
                            }
                            description "Delimiter to combine primary and secondary pool names";
                        }

                    } // container use-pool-from-client

                } // container pool-selection

                ///nokia-conf/configure/router/dhcp-server/dhcpv6/lease-hold
                container lease-hold {
                    when "../auto-provisioned = 'false'" {
                        description "only allowed when auto-provisioned is false";
                    }
                    description "Enter the lease-hold context";

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/lease-hold/time
                    leaf time {
                        type types-sros:time-duration {
                            range "1..631152000";
                        }
                        units "seconds";
                        description "Lease hold time";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/lease-hold/additional-scenarios
                    container additional-scenarios {
                        description "Enter the additional-scenarios context";

                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/lease-hold/additional-scenarios/internal-lease-ipsec
                        leaf internal-lease-ipsec {
                            type boolean;
                            default "false";
                            description "Apply the lease hold timer to local IPsec clients";
                        }

                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/lease-hold/additional-scenarios/solicited-release
                        leaf solicited-release {
                            type boolean;
                            default "false";
                            description "Apply lease hold timer for solicited releases";
                        }

                    } // container additional-scenarios

                } // container lease-hold

                ///nokia-conf/configure/router/dhcp-server/dhcpv6/server-id
                container server-id {
                    when "../auto-provisioned = 'false'" {
                        description "only allowed when auto-provisioned is false";
                    }
                    description "Enable the server-id context";

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/server-id/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/server-id/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/server-id/value
                    choice value {
                        default "duid-link-local";
                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/server-id/value/duid-link-local
                        case duid-link-local {

                            ///nokia-conf/configure/router/dhcp-server/dhcpv6/server-id/value/duid-link-local/duid-link-local
                            leaf duid-link-local {
                                type empty;
                                description "Derive DUID server ID from a system link-layer address";
                            }

                        }
                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/server-id/value/duid-enterprise
                        case duid-enterprise {

                            ///nokia-conf/configure/router/dhcp-server/dhcpv6/server-id/value/duid-enterprise/duid-enterprise
                            container duid-enterprise {
                                description "Enter the duid-enterprise context";

                                ///nokia-conf/configure/router/dhcp-server/dhcpv6/server-id/value/duid-enterprise/duid-enterprise/duid-enterprise-value
                                choice duid-enterprise-value {
                                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/server-id/value/duid-enterprise/duid-enterprise/duid-enterprise-value/ascii-string
                                    case ascii-string {

                                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/server-id/value/duid-enterprise/duid-enterprise/duid-enterprise-value/ascii-string/ascii-string
                                        leaf ascii-string {
                                            type types-sros:string-not-all-spaces {
                                                length "1..58";
                                            }
                                            description "DUID enterprise server ID specified as an ASCII string";
                                        }

                                    }
                                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/server-id/value/duid-enterprise/duid-enterprise/duid-enterprise-value/hex-string
                                    case hex-string {

                                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/server-id/value/duid-enterprise/duid-enterprise/duid-enterprise-value/hex-string/hex-string
                                        leaf hex-string {
                                            type types-sros:hex-string {
                                                length "1..118";
                                            }
                                            description "DUID enterprise server ID specified as a hex string";
                                        }

                                    }
                                }
                            } // container duid-enterprise

                        }
                    }
                } // container server-id

                ///nokia-conf/configure/router/dhcp-server/dhcpv6/failover
                container failover {
                    when "../auto-provisioned = 'false'" {
                        description "only allowed when auto-provisioned is false";
                    }
                    description "Enter the failover context";

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/failover/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the failover mechanism";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/failover/ignore-mclt-on-takeover
                    leaf ignore-mclt-on-takeover {
                        type boolean;
                        default "false";
                        description "Ignore maximum client lead during takeover from partner";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/failover/maximum-client-lead-time
                    leaf maximum-client-lead-time {
                        type types-sros:time-duration {
                            range "600..86399";
                        }
                        units "seconds";
                        sros-ext:auto-restart-to-modify;
                        default "600";
                        description "Maximum time that DHCP server can extend client's lease";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/failover/partner-down-delay
                    leaf partner-down-delay {
                        type types-sros:time-duration {
                            range "0..86399";
                        }
                        units "seconds";
                        sros-ext:auto-restart-to-modify;
                        default "86399";
                        description "Delay to prevent lease duplication during link failure";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/failover/startup-wait-time
                    leaf startup-wait-time {
                        type types-sros:time-duration {
                            range "60..3600";
                        }
                        units "seconds";
                        sros-ext:auto-restart-to-modify;
                        default "120";
                        description "Time between initialization and assuming active role";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/failover/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/failover/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/failover/peer
                    list peer {
                        key "address";
