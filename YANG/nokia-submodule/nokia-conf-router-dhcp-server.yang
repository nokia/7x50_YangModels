                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/options/option/value
                                choice value {
                                    mandatory true;
                                    description "The value of the DHCP option.";
                                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/options/option/value/ascii-string
                                    case ascii-string {

                                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/options/option/value/ascii-string/ascii-string
                                        leaf ascii-string {
                                            type types-sros:string-not-all-spaces {
                                                length "1..127";
                                            }
                                            description "DHCP option specified as an ASCII string";
                                        }

                                    }
                                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/options/option/value/hex-string
                                    case hex-string {

                                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/options/option/value/hex-string/hex-string
                                        leaf hex-string {
                                            type types-sros:hex-string {
                                                length "1..256";
                                            }
                                            description "DHCP option specified as hexadecimal string";
                                        }

                                    }
                                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/options/option/value/domain-string
                                    case domain-string {

                                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/options/option/value/domain-string/domain-string
                                        leaf domain-string {
                                            type string {
                                                length "1..127";
                                                pattern "[a-z0-9\\-]+(\\.[a-z0-9\\-]+)*";
                                            }
                                            description "DHCP option specified as a domain name";
                                        }

                                    }
                                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/options/option/value/duration
                                    case duration {

                                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/options/option/value/duration/duration
                                        leaf duration {
                                            type types-sros:time-duration {
                                                range "10..315446399";
                                            }
                                            units "seconds";
                                            description "DHCP option specified as time";
                                        }

                                    }
                                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/options/option/value/ipv6-address
                                    case ipv6-address {

                                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/options/option/value/ipv6-address/ipv6-address
                                        leaf-list ipv6-address {
                                            type types-sros:ipv6-address;
                                            max-elements 4;
                                            ordered-by user;
                                            description "DHCP option specified as a list of IPv6 addresses";
                                        }

                                    }
                                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/options/option/value/empty
                                    case empty {

                                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/options/option/value/empty/empty
                                        leaf empty {
                                            type empty;
                                            description "Empty DHCP option";
                                        }

                                    }
                                }
                            } // list option

                        } // container options

                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/prefix-length-threshold
                        list prefix-length-threshold {
                            key "prefix-length";
                            max-elements 8;
                            description "Enter the prefix-length-threshold list instance";

                            ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/prefix-length-threshold/prefix-length
                            leaf prefix-length {
                                type uint32 {
                                    range "1..128";
                                }
                                description "Delegated prefix length for pool thresholds";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/prefix-length-threshold/event-when-depleted
                            leaf event-when-depleted {
                                type boolean;
                                default "false";
                                description "Generate a notification when this pool is depleted";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/prefix-length-threshold/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/prefix-length-threshold/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/prefix-length-threshold/value
                            choice value {
                                ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/prefix-length-threshold/value/absolute
                                case absolute {

                                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/prefix-length-threshold/value/absolute/absolute
                                    leaf absolute {
                                        type uint32 {
                                            range "1..4294967295";
                                        }
                                        description "Minimum number of free prefixes for this prefix length";
                                    }

                                }
                                ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/prefix-length-threshold/value/percent
                                case percent {

                                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/prefix/prefix-length-threshold/value/percent/percent
                                    leaf percent {
                                        type uint32 {
                                            range "1..100";
                                        }
                                        description "Minimum percentage of free prefixes for prefix length";
                                    }

                                }
                            }
                        } // list prefix-length-threshold

                    } // list prefix

                    ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/exclude-prefix
                    list exclude-prefix {
                        key "ipv6-prefix";
                        description "Add a list entry for exclude-prefix";

                        ///nokia-conf/configure/router/dhcp-server/dhcpv6/pool/exclude-prefix/ipv6-prefix
                        leaf ipv6-prefix {
                            type types-sros:ipv6-prefix;
                            description "IPv6 prefix to be excluded from available pool prefixes";
                        }

                    } // list exclude-prefix

                } // list pool

            } // list dhcpv6

        } // container dhcp-server

    } // grouping conf-router-dhcp-server

}
