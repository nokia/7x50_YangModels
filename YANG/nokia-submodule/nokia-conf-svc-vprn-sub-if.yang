                        description "Administrative state of router advertisements";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/max-advertisement-interval
                    leaf max-advertisement-interval {
                        type types-sros:time-duration {
                            range "900..1800";
                        }
                        units "seconds";
                        default "1800";
                        description "Maximum advertisement interval";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/min-advertisement-interval
                    leaf min-advertisement-interval {
                        type types-sros:time-duration {
                            range "900..1350";
                        }
                        units "seconds";
                        default "900";
                        description "Minimum advertisement interval";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/force-mcast
                    leaf force-mcast {
                        type enumeration {
                            enum ip                             { value 1; }
                            enum ip-mac                         { value 2; }
                        }
                        description "Protocol with forced multicast";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/options
                    container options {
                        description "Enter the options context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/options/current-hop-limit
                        leaf current-hop-limit {
                            type uint32 {
                                range "0..255";
                            }
                            default "64";
                            description "Hop limit to be advertised";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/options/managed-configuration
                        leaf managed-configuration {
                            type boolean;
                            default "false";
                            description "Managed address configuration flag";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/options/mtu
                        leaf mtu {
                            type union {
                                type uint32 {
                                    range "1280..9212";
                                }
                                type enumeration {
                                    enum not-included                   { value 0; }
                                }
                            }
                            units "bytes";
                            default "not-included";
                            description "Advertised MTU value";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/options/other-stateful-configuration
                        leaf other-stateful-configuration {
                            type boolean;
                            default "false";
                            description "Other stateful configuration flag";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/options/reachable-time
                        leaf reachable-time {
                            type types-sros:time-duration {
                                range "0..3600000";
                            }
                            units "milliseconds";
                            default "0";
                            description "Reachable time for advertisements";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/options/retransmit-timer
                        leaf retransmit-timer {
                            type uint32 {
                                range "0..1800000";
                            }
                            units "seconds";
                            default "0";
                            description "Retransmit time in router advertisements from interface";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/options/router-lifetime
                        leaf router-lifetime {
                            type union {
                                type types-sros:time-duration {
                                    range "2700..9000";
                                }
                                type enumeration {
                                    enum no-default-router              { value 0; }
                                }
                            }
                            units "seconds";
                            default "4500";
                            description "Router lifetime";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/options/dns
                        container dns {
                            description "Enter the dns context";

                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/options/dns/include-rdnss
                            leaf include-rdnss {
                                type boolean;
                                default "false";
                                description "Include the RDNSS server option 25";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/options/dns/rdnss-lifetime
                            leaf rdnss-lifetime {
                                type union {
                                    type types-sros:time-duration {
                                        range "900..3600";
                                    }
                                    type enumeration {
                                        enum infinite                       { value -1; }
                                    }
                                }
                                units "seconds";
                                default "3600";
                                description "Maximum time for the RDNSS address to remain valid";
                            }

                        } // container dns

                    } // container options

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/prefix-options
                    container prefix-options {
                        description "Enter the prefix-options context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/prefix-options/autonomous
                        leaf autonomous {
                            type boolean;
                            default "false";
                            description "Value of the autonomous flag";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/prefix-options/on-link
                        leaf on-link {
                            type boolean;
                            default "true";
                            description "Assign the prefix to an interface on the specified link";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/prefix-options/preferred-lifetime
                        leaf preferred-lifetime {
                            type types-submgt:router-advertisement-prefix-options-lifetime;
                            units "seconds";
                            default "3600";
                            description "Time for a prefix to remain preferred";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-advertisements/prefix-options/valid-lifetime
                        leaf valid-lifetime {
                            type types-submgt:router-advertisement-prefix-options-lifetime;
                            units "seconds";
                            default "86400";
                            description "Time for a prefix to remain valid";
                        }

                    } // container prefix-options

                } // container router-advertisements

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-solicit
                container router-solicit {
                    when "../../fwd-service" {
                        description "fwd-service must be set";
                    }
                    description "Enable the router-solicit context";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-solicit/inactivity-timer
                    leaf inactivity-timer {
                        type union {
                            type types-sros:time-duration {
                                range "1..31536000";
                            }
                            type enumeration {
                                enum infinite                       { value -1; }
                            }
                        }
                        units "seconds";
                        default "300";
                        description "Time before an inactive host is removed";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-solicit/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/router-solicit/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container router-solicit

            } // container ipv6

            ///nokia-conf/configure/service/vprn/subscriber-interface/hold-time
            container hold-time {
                description "Enter the hold-time context";

                ///nokia-conf/configure/service/vprn/subscriber-interface/hold-time/ipv4
                container ipv4 {
                    description "Enter the ipv4 context";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/hold-time/ipv4/up
                    container up {
                        description "Enter the up context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/hold-time/ipv4/up/seconds
                        leaf seconds {
                            type types-router:hold-time-seconds;
                            units "seconds";
                            description "Up hold time for the IP interface";
                        }

                    } // container up

                    ///nokia-conf/configure/service/vprn/subscriber-interface/hold-time/ipv4/down
                    container down {
                        description "Enter the down context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/hold-time/ipv4/down/seconds
                        leaf seconds {
                            type types-router:hold-time-seconds;
                            units "seconds";
                            description "Down hold time for the IP interface";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/hold-time/ipv4/down/init-only
                        leaf init-only {
                            type boolean;
                            default "false";
                            description "Apply delay only at interface configuration or reboot";
                        }

                    } // container down

                } // container ipv4

                ///nokia-conf/configure/service/vprn/subscriber-interface/hold-time/ipv6
                container ipv6 {
                    description "Enter the ipv6 context";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/hold-time/ipv6/up
                    container up {
                        description "Enter the up context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/hold-time/ipv6/up/seconds
                        leaf seconds {
                            type types-router:hold-time-seconds;
                            units "seconds";
                            description "Up hold time for the IP interface";
                        }

                    } // container up

                    ///nokia-conf/configure/service/vprn/subscriber-interface/hold-time/ipv6/down
                    container down {
                        description "Enter the down context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/hold-time/ipv6/down/seconds
                        leaf seconds {
                            type types-router:hold-time-seconds;
                            units "seconds";
                            description "Down hold time for the IP interface";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/hold-time/ipv6/down/init-only
                        leaf init-only {
                            type boolean;
                            default "false";
                            description "Apply delay only at interface configuration or reboot";
                        }

                    } // container down

                } // container ipv6

            } // container hold-time

            ///nokia-conf/configure/service/vprn/subscriber-interface/ipoe-linking
            container ipoe-linking {
                when "../fwd-service" {
                    description "fwd-service must be set";
                }
                description "Enter the ipoe-linking context";

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipoe-linking/gratuitous-router-advertisement
                leaf gratuitous-router-advertisement {
                    type boolean;
                    default "false";
                    description "Send unsolicited router advertisement after DHCP setup";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipoe-linking/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipoe-linking/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container ipoe-linking

            ///nokia-conf/configure/service/vprn/subscriber-interface/ipoe-session
            container ipoe-session {
                when "../fwd-service" {
                    description "fwd-service must be set";
                }
                description "Enable the ipoe-session context";

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipoe-session/session-limit
                leaf session-limit {
                    type uint32 {
                        range "1..500000";
                    }
                    description "Maximum number of sessions on this group interface";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipoe-session/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipoe-session/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container ipoe-session

            ///nokia-conf/configure/service/vprn/subscriber-interface/pppoe
            container pppoe {
                when "../fwd-service" {
                    description "fwd-service must be set";
                }
                description "Enable the pppoe context";

                ///nokia-conf/configure/service/vprn/subscriber-interface/pppoe/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/pppoe/session-limit
                leaf session-limit {
                    type uint32 {
                        range "1..333823";
                    }
                    default "1";
                    description "Maximum PPPoE sessions";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/pppoe/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/pppoe/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container pppoe

            ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment
            container local-address-assignment {
                when "../fwd-service" {
                    description "fwd-service must be set";
                }
                description "Enter the local-address-assignment context";

                ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of Local Address Assignment";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment/ipv4
                container ipv4 {
                    description "Enter the ipv4 context";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment/ipv4/server
                    leaf server {
                        type leafref {
                            path "../../../../dhcp-server/dhcpv4/name";
                        }
                        description "Local DHCPv4 server for local pools management";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment/ipv4/default-pool
                    leaf-list default-pool {
                        type types-sros:named-item;
                        max-elements 2;
                        ordered-by user;
                        description "Default pools";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment/ipv4/client-applications
                    container client-applications {
                        description "Enter the client-applications context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment/ipv4/client-applications/ppp
                        leaf ppp {
                            type boolean;
                            default "false";
                            description "Request local addresses for PPP IPCP hosts";
                        }

                    } // container client-applications

                } // container ipv4

                ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment/ipv6
                container ipv6 {
                    description "Enter the ipv6 context";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment/ipv6/server
                    leaf server {
                        type leafref {
                            path "../../../../dhcp-server/dhcpv6/name";
                        }
                        description "Local DHCPv6 server for local pools management";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment/ipv6/client-applications
                    container client-applications {
                        description "Enter the client-applications context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment/ipv6/client-applications/ppp-slaac
                        leaf ppp-slaac {
                            type boolean;
                            default "false";
                            description "Request local addresses for PPP SLAAC hosts";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment/ipv6/client-applications/ipoe-wan
                        leaf ipoe-wan {
                            type boolean;
                            default "false";
                            description "Request local addresses for IPoE IA NA hosts";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/local-address-assignment/ipv6/client-applications/ipoe-slaac
                        leaf ipoe-slaac {
                            type boolean;
                            default "false";
                            description "Request local addresses for IPoE SLAAC hosts";
                        }

                    } // container client-applications

                } // container ipv6

            } // container local-address-assignment

            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface
            list group-interface {
                when "not(../fwd-service)" {
                    description "fwd-service must be unset";
                }
                key "group-interface-name";
                description "Enter the group-interface list instance";

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/group-interface-name
                leaf group-interface-name {
                    type types-sros:interface-name;
                    description "Group interface name";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "enable";
                    description "Administrative state of the interface";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/description
                leaf description {
                    type types-sros:very-long-description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/type
                leaf type {
                    type types-services:group-interface-type;
                    sros-ext:immutable;
                    default "plain";
                    description "Group interface type";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/radius-auth-policy
                leaf radius-auth-policy {
                    type leafref {
                        path "../../../../../subscriber-mgmt/radius-authentication-policy/name";
                    }
                    description "RADIUS authentication policy";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/gx-policy
                leaf gx-policy {
                    when "../type = 'plain' or ../type = 'lns' or ../type = 'wlan-gw' or ../type = 'gtp'" {
                        description "invalid group interface type";
                    }
                    type leafref {
                        path "../../../../../subscriber-mgmt/diameter-gx-policy/name";
                    }
                    description "Diameter application policy";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/nasreq-auth-policy
                leaf nasreq-auth-policy {
                    when "../type = 'plain'" {
                        description "group interface type must be 'plain'";
                    }
                    type leafref {
                        path "../../../../../subscriber-mgmt/diameter-nasreq-policy/name";
                    }
                    description "Diameter NASREQ application policy to use for authentication";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ingress-stats
                leaf ingress-stats {
                    type boolean;
                    default "false";
                    description "Collect ingress interface statistics";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ip-mtu
                leaf ip-mtu {
                    when "not(../type = 'bonding')" {
                        description "group interface type must not be 'bonding'";
                    }
                    type types-router:ip-mtu-ext;
                    units "bytes";
                    description "Interface IP MTU";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/mac
                leaf mac {
                    when "not(../type = 'wlan-gw') and not(../type = 'gtp') and not(../type = 'bonding')" {
                        description "invalid group interface type";
                    }
                    type types-sros:mac-unicast-address;
                    description "MAC address for the interface";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/oper-up-while-empty
                leaf oper-up-while-empty {
                    when "../type = 'plain' or ../type = 'lns'" {
                        description "invalid group interface type";
                    }
                    type boolean;
                    default "false";
                    description "Enable this group interface without any active SAPs";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/redundant-interface
                leaf redundant-interface {
                    when "../type = 'plain' or ../type = 'lns'" {
                        description "invalid group interface type";
                    }
                    type leafref {
                        path "../../../redundant-interface/interface-name";
                    }
                    description "Redundant interface";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/shcv-policy
                leaf shcv-policy {
                    when "../type = 'plain' or ../type = 'lns' or ../type = 'wlan-gw'" {
                        description "invalid group interface type";
                    }
                    type leafref {
                        path "../../../../../subscriber-mgmt/shcv-policy/name";
                    }
                    description "SHCV policy for IPv4 and IPv6 subscriber hosts";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/shcv-policy-ipv4
                leaf shcv-policy-ipv4 {
                    when "../type = 'plain' or ../type = 'lns' or ../type = 'wlan-gw'" {
                        description "invalid group interface type";
                    }
                    type leafref {
                        path "../../../../../subscriber-mgmt/shcv-policy/name";
                    }
                    description "SHCV for IPv4 subscriber hosts";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/shcv-policy-ipv6
                leaf shcv-policy-ipv6 {
                    when "../type = 'plain' or ../type = 'lns' or ../type = 'wlan-gw'" {
                        description "invalid group interface type";
                    }
                    type leafref {
                        path "../../../../../subscriber-mgmt/shcv-policy/name";
                    }
                    description "SHCV for IPv6 subscriber hosts";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/suppress-aa-sub
                leaf suppress-aa-sub {
                    type boolean;
                    default "false";
                    description "Enable application assurance suppression for ESM subscribers";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/tos-marking-state
                leaf tos-marking-state {
                    type types-router:tos-marking-state;
                    default "trusted";
                    description "TOS marking state";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ingress
                container ingress {
                    description "Enter the ingress context";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ingress/policy-accounting
                    leaf policy-accounting {
                        type leafref {
                            path "../../../../../../routing-options/policy-accounting/policy-acct-template/name";
                        }
                        description "Ingress policy accounting template name";
                    }

                } // container ingress

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/dynamic-routes-track-srrp
                container dynamic-routes-track-srrp {
                    when "../type = 'plain' or ../type = 'lns'" {
                        description "invalid group interface type";
                    }
                    presence "Enable the dynamic-routes-track-srrp context";
                    description "Enable the dynamic-routes-track-srrp context";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/dynamic-routes-track-srrp/hold-time
                    leaf hold-time {
                        type uint32 {
                            range "1..50";
                        }
                        units "deciseconds";
                        description "Time before route state updated after SRRP transition";
                    }

                } // container dynamic-routes-track-srrp

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4
                container ipv4 {
                    description "Enter the ipv4 context";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/qos-route-lookup
                    leaf qos-route-lookup {
                        type types-router:qos-route-lookup-grp-if;
                        description "QoS route lookup";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/ignore-df-bit
                    leaf ignore-df-bit {
                        when "../../type = 'wlan-gw'" {
                            description "group interface type must be 'wlan-gw'";
                        }
                        type boolean;
                        default "false";
                        description "Ignore DF bit in the IPv4 header when fragmenting";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp
                    container icmp {
                        description "Enter the icmp context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/mask-reply
                        leaf mask-reply {
                            type boolean;
                            default "true";
                            description "Allow responses to ICMP mask requests on the interface";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/redirects
                        container redirects {
                            description "Enter the redirects context";

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/redirects/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sending ICMP redirect messages";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/redirects/number
                            leaf number {
                                type uint16 {
                                    range "10..1000";
                                }
                                default "100";
                                description "Maximum number of ICMP redirect messages to send";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/redirects/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time used to limit the number of ICMP redirect messages";
                            }

                        } // container redirects

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/ttl-expired
                        container ttl-expired {
                            description "Enter the ttl-expired context";

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/ttl-expired/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sending TTL expired messages";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/ttl-expired/number
                            leaf number {
                                type uint16 {
                                    range "10..2000";
                                }
                                default "100";
                                description "Maximum number of TTL expired messages to send";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/ttl-expired/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time used to limit the number of TTL expired messages";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/ttl-expired/use-matching-address
                            leaf use-matching-address {
                                type boolean;
                                default "false";
                                description "Use the subscriber interface address as source address";
                            }

                        } // container ttl-expired

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/unreachables
                        container unreachables {
                            description "Enter the unreachables context";

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/unreachables/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sending unreachable messages";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/unreachables/number
                            leaf number {
                                type uint16 {
                                    range "10..2000";
                                }
                                default "100";
                                description "Maximum number of unreachable messages to send";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/unreachables/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time to limit the number of ICMP unreachable messages";
                            }

                        } // container unreachables

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/param-problem
                        container param-problem {
                            description "Enter the param-problem context";

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/param-problem/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sent Parameter Problem messages";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/param-problem/number
                            leaf number {
                                type uint32 {
                                    range "10..1000";
                                }
                                default "100";
                                description "Maximum number of Parameter Problem messages to send";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/icmp/param-problem/seconds
                            leaf seconds {
                                type uint32 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time used to limit number of Parameter Problem messages";
                            }

                        } // container param-problem

                    } // container icmp

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/urpf-check
                    container urpf-check {
                        when "not(../../type = 'bonding')" {
                            description "group interface type must not be 'bonding'";
                        }
                        presence "Enable the urpf-check context";
                        description "Enable the urpf-check context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/urpf-check/mode
                        leaf mode {
                            type types-router:urpf-check-mode;
                            default "strict";
                            description "Unicast RPF check mode";
                        }

                    } // container urpf-check

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/arp-host
                    container arp-host {
                        when "../../type = 'plain' or ../../type = 'lns'" {
                            description "invalid group interface type";
                        }
                        description "Enable the arp-host context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/arp-host/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            description "Administrative state of ARP hosts";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/arp-host/host-limit
                        leaf host-limit {
                            type uint32 {
                                range "1..524287";
                            }
                            description "Maximum number of ARP hosts";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/arp-host/sap-host-limit
                        leaf sap-host-limit {
                            type uint32 {
                                range "1..131071";
                            }
                            default "1";
                            description "Maximum number of ARP hosts per SAP";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/arp-host/min-auth-interval
                        leaf min-auth-interval {
                            type types-sros:time-duration {
                                range "1..6000";
                            }
                            units "minutes";
                            default "15";
                            description "Minimum authentication interval";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/arp-host/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/arp-host/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container arp-host

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/neighbor-discovery
                    container neighbor-discovery {
                        when "../../type = 'plain' or ../../type = 'lns' or ../../type = 'wlan-gw'" {
                            description "invalid group interface type";
                        }
                        description "Enter the neighbor-discovery context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/neighbor-discovery/local-proxy-arp
                        leaf local-proxy-arp {
                            type boolean;
                            description "Enable local proxy ARP on interface";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/neighbor-discovery/remote-proxy-arp
                        leaf remote-proxy-arp {
                            type boolean;
                            default "false";
                            description "Enable remote proxy ARP on the interface";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/neighbor-discovery/proxy-arp-policy
                        leaf-list proxy-arp-policy {
                            type leafref {
                                path "../../../../../../../policy-options/policy-statement/name";
                            }
                            max-elements 5;
                            ordered-by user;
                            description "Proxy ARP policy name";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/neighbor-discovery/populate
                        leaf populate {
                            when "../../../type = 'plain' or ../../../type = 'lns'" {
                                description "invalid group interface type";
                            }
                            type boolean;
                            description "Allow population of static and dynamic hosts";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/neighbor-discovery/timeout
                        leaf timeout {
                            when "../../../type = 'plain' or ../../../type = 'lns'" {
                                description "invalid group interface type";
                            }
                            type uint32 {
                                range "0..65535";
                            }
                            units "seconds";
                            default "14400";
                            description "Timeout for an ARP entry learned on the interface";
                        }

                    } // container neighbor-discovery

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/bfd
                    container bfd {
                        description "Enter the bfd context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/bfd/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of BFD sessions";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/bfd/transmit-interval
                        leaf transmit-interval {
                            type uint32 {
                                range "10..100000";
                            }
                            units "milliseconds";
                            default "100";
                            description "BFD transmit interval over this interface";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/bfd/receive
                        leaf receive {
                            type uint32 {
                                range "10..100000";
                            }
                            units "milliseconds";
                            default "100";
                            description "BFD receive interval over this interface";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/bfd/multiplier
                        leaf multiplier {
                            type uint32 {
                                range "1..20";
                            }
                            default "3";
                            description "Number of consecutive BFD messages missed from the peer";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/bfd/echo-receive
                        leaf echo-receive {
                            type uint32 {
                                range "100..100000";
                            }
                            units "milliseconds";
                            description "Minimum echo interval over this interface";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/bfd/type
                        leaf type {
                            type enumeration {
                                enum cpm-np                         { value 1; }
                                enum auto                           { value 2; }
                                enum fp                             { value 3; }
                            }
                            default "auto";
                            description "Local termination point for the BFD session";
                        }

                    } // container bfd

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp
                    container dhcp {
                        when "../../type = 'plain' or ../../type = 'lns' or ../../type = 'wlan-gw' or ../../type = 'gtp'" {
                            description "invalid group interface type";
                        }
                        description "Enable the dhcp context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            description "Administrative state of DHCP";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/filter
                        leaf filter {
                            type leafref {
                                path "../../../../../../../filter/dhcp-filter/filter-id";
                            }
                            description "DHCP filter ID for the group interface";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/python-policy
                        leaf python-policy {
                            type leafref {
                                path "../../../../../../../python/python-policy/name";
                            }
                            description "Python policy name";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/server
                        leaf-list server {
                            type types-sros:ipv4-unicast-address;
                            max-elements 8;
                            ordered-by user;
                            description "IP addresses for DHCP server requests";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/trusted
                        leaf trusted {
                            type boolean;
                            default "false";
                            description "Relay untrusted packets";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/gi-address
                        leaf gi-address {
                            type types-sros:ipv4-unicast-address;
                            description "GI address for the DHCP relay";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/release-include-gi-address
                        leaf release-include-gi-address {
                            type boolean;
                            default "false";
                            description "Include gateway IP address in DHCP Release messages";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/src-ip-addr
                        leaf src-ip-addr {
                            type enumeration {
                                enum auto                           { value 0; }
                                enum gi-address                     { value 1; }
                            }
                            default "auto";
                            description "Type of source address to use for DHCP relay";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/match-circuit-id
                        leaf match-circuit-id {
                            when "not(../../../type = 'gtp')" {
                                description "group interface type must not be 'gtp'";
                            }
                            type boolean;
                            default "false";
                            description "Enable Option 82 circuit ID on relayed DHCP packets";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/user-db
                        leaf user-db {
                            when "not(../../../type = 'gtp')" {
                                description "group interface type must not be 'gtp'";
                            }
                            type leafref {
                                path "../../../../../../../subscriber-mgmt/local-user-db/name";
                            }
                            sros-ext:auto-restart-to-modify;
                            description "Local user database for authentication";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/proxy-server
                        container proxy-server {
                            description "Enter the proxy-server context";

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/proxy-server/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "disable";
                                description "Administrative state of the DHCP proxy server";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/proxy-server/emulated-server
                            leaf emulated-server {
                                type types-sros:ipv4-unicast-address;
                                description "IP address used as DHCP server address in SAP context";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/proxy-server/lease-time
                            container lease-time {
                                description "Enter the lease-time context";

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/proxy-server/lease-time/value
                                leaf value {
                                    type types-sros:time-duration {
                                        range "300..315446399";
                                    }
                                    units "seconds";
                                    description "DHCP lease time";
                                }

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/proxy-server/lease-time/radius-override
                                leaf radius-override {
                                    type boolean;
                                    default "false";
                                    description "Use lease time information provided by RADIUS server";
                                }

                            } // container lease-time

                        } // container proxy-server

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82
                        container option-82 {
                            description "Enter the option-82 context";

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/action
                            leaf action {
                                type enumeration {
                                    enum replace                        { value 1; }
                                    enum drop                           { value 2; }
                                    enum keep                           { value 3; }
                                }
                                default "keep";
                                description "Action to take with received DHCP Option 82";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/circuit-id
                            container circuit-id {
                                description "Enter the circuit-id context";

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/circuit-id/circuit-id
                                choice circuit-id {
                                    default "ascii-tuple";
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/circuit-id/circuit-id/none
                                    case none {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/circuit-id/circuit-id/none/none
                                        leaf none {
                                            type empty;
                                            description "Do not include the circuit ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ifindex
                                    case ifindex {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ifindex/ifindex
                                        leaf ifindex {
                                            type empty;
                                            description "Use the interface index for the circuit ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ascii-tuple
                                    case ascii-tuple {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ascii-tuple/ascii-tuple
                                        leaf ascii-tuple {
                                            type empty;
                                            description "Use the ASCII-encoded tuple for the circuit ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/circuit-id/circuit-id/sap-id
                                    case sap-id {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/circuit-id/circuit-id/sap-id/sap-id
                                        leaf sap-id {
                                            type empty;
                                            description "Use the SAP ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/circuit-id/circuit-id/vlan-ascii-tuple
                                    case vlan-ascii-tuple {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/circuit-id/circuit-id/vlan-ascii-tuple/vlan-ascii-tuple
                                        leaf vlan-ascii-tuple {
                                            type empty;
                                            description "Include the VLAN ID and dot1p bits in the ASCII tuple";
                                        }

                                    }
                                }
                            } // container circuit-id

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/remote-id
                            container remote-id {
                                description "Enter the remote-id context";

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/remote-id/remote-id
                                choice remote-id {
                                    default "none";
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/remote-id/remote-id/none
                                    case none {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/remote-id/remote-id/none/none
                                        leaf none {
                                            type empty;
                                            description "Do not include the remote ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/remote-id/remote-id/mac
                                    case mac {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/remote-id/remote-id/mac/mac
                                        leaf mac {
                                            type empty;
                                            description "Use the MAC address for the remote ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/remote-id/remote-id/ascii-string
                                    case ascii-string {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/remote-id/remote-id/ascii-string/ascii-string
                                        leaf ascii-string {
                                            type types-sros:string-not-all-spaces {
                                                length "1..32";
                                            }
                                            description "User-defined ASCII string for the remote ID";
                                        }

                                    }
                                }
                            } // container remote-id

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/vendor-specific-option
                            container vendor-specific-option {
                                description "Enter the vendor-specific-option context";

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/vendor-specific-option/system-id
                                leaf system-id {
                                    type boolean;
                                    default "false";
                                    description "Send the system ID in the VSO";
                                }

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/vendor-specific-option/client-mac-address
                                leaf client-mac-address {
                                    type boolean;
                                    default "false";
                                    description "Send the MAC address in the VSO";
                                }

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv4/dhcp/option-82/vendor-specific-option/pool-name
                                leaf pool-name {
                                    type boolean;
                                    default "false";
                                    description "Send the pool name in the VSO";
                                }

