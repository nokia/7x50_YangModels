                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/address
                list address {
                    key "ipv6-address";
                    description "Enter the address list instance";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/address/ipv6-address
                    leaf ipv6-address {
                        type types-sros:ipv6-address;
                        description "IPv6 address for the subscriber interface";
