                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/relay/advertise-selection/mode
                            choice mode {
                                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/relay/advertise-selection/mode/mode-client-mac
                                case mode-client-mac {

                                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/relay/advertise-selection/mode/mode-client-mac/client-mac
                                    container client-mac {
                                        description "Enter the client-mac context";

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/relay/advertise-selection/mode/mode-client-mac/client-mac/mac-address
                                        leaf mac-address {
                                            type enumeration {
                                                enum odd                            { value 1; }
                                                enum even                           { value 2; }
                                            }
                                            description "Designated client MAC addresses for Advertise selection";
                                        }

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/relay/advertise-selection/mode/mode-client-mac/client-mac/solicit-delay
                                        leaf solicit-delay {
                                            type int32 {
                                                range "1..100";
                                            }
                                            units "deciseconds";
                                            description "Delay before sending DHCPv6 Solicit messages";
                                        }

