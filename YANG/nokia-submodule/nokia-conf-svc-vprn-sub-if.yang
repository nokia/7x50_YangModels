                                        container preference-option {
                                            description "Enter the preference-option context";

                                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/relay/advertise-selection/mode/mode-client-mac/client-mac/preference-option/value
                                            leaf value {
                                                type int32 {
                                                    range "0..255";
                                                }
                                                description "Preference option value for DHCPv6 Advertise messages";
                                            }

                                        } // container preference-option

                                    } // container client-mac

                                }
                                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/relay/advertise-selection/mode/mode-server
                                case mode-server {

