                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/proxy-server
                    container proxy-server {
                        description "Enter the proxy-server context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/proxy-server/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the DHCPv6 proxy server";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/proxy-server/preferred-lifetime
