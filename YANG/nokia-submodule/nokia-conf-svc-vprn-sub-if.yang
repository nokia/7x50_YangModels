                                            }
                                            max-elements 8;
                                            ordered-by user;
                                            description "Exclude a configuration group at this level";
                                        }

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/relay/advertise-selection/mode/mode-server/server/preference-option
                                        container preference-option {
                                            description "Enter the preference-option context";

                                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/relay/advertise-selection/mode/mode-server/server/preference-option/value
                                            leaf value {
                                                type int32 {
                                                    range "0..255";
                                                }
                                                description "Preference option value for DHCPv6 Advertise messages";
                                            }

                                        } // container preference-option

                                    } // list server

                                }
                            }
                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/relay/advertise-selection/preference-option
                            container preference-option {
                                description "Enter the preference-option context";

                                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/relay/advertise-selection/preference-option/value
                                leaf value {
                                    type int32 {
                                        range "0..255";
                                    }
                                    description "Preference option value for DHCPv6 Advertise messages";
                                }

                            } // container preference-option

