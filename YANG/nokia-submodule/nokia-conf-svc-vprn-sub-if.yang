                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/address/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list address

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/prefix
