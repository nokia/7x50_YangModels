                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/relay/lease-split/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    default "disable";
                                    description "Administrative state of DHCPv6 lease split";
                                }

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/relay/lease-split/valid-lifetime
                                leaf valid-lifetime {
                                    type types-submgt:dhcp6-prefix-lifetime-new;
                                    default "3600";
                                    description "DHCPv6 lease split valid lifetime (short lease time)";
                                }

                            } // container lease-split

                        } // container relay

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server
                        container proxy-server {
                            description "Enter the proxy-server context";

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "disable";
                                description "Administrative state of the DHCPv6 proxy server";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/preferred-lifetime
                            leaf preferred-lifetime {
                                type types-submgt:dhcp6-prefix-lifetime;
                                default "3600";
                                description "Time for prefix to remain preferred on this interface";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/valid-lifetime
                            leaf valid-lifetime {
                                type types-submgt:dhcp6-prefix-lifetime;
                                default "86400";
                                description "Time for prefix to remain valid on this interface";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/renew-timer
                            leaf renew-timer {
                                type types-submgt:dhcp6-renew-timer;
                                description "Renew timer (T1)";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/rebind-timer
                            leaf rebind-timer {
                                type types-submgt:dhcp6-rebind-timer;
                                description "Rebind timer (T2) for this interface";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/client-applications
                            container client-applications {
                                when "not(../../../../type = 'gtp')" {
                                    description "group interface type must not be 'gtp'";
                                }
                                description "Enter the client-applications context";

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/client-applications/dhcp
                                leaf dhcp {
                                    type boolean;
                                    default "true";
                                    description "Enable IPoE clients to use DHCP relay or proxy server";
                                }

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/client-applications/ppp
                                leaf ppp {
                                    when "not(../../../../../type = 'wlan-gw')" {
                                        description "group interface type must not be 'wlan-gw'";
                                    }
                                    type boolean;
                                    default "false";
                                    description "Allow PPPoE clients to use DHCP relay functionality";
                                }

                            } // container client-applications

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/server-id
                            container server-id {
                                description "Enable the server-id context";

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/server-id/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/server-id/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/server-id/server-id
                                choice server-id {
                                    default "duid-ll";
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/server-id/server-id/duid-ll
                                    case duid-ll {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/server-id/server-id/duid-ll/duid-ll
                                        leaf duid-ll {
                                            type empty;
                                            description "Use link-layer address (DUID-LL) as DUID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/server-id/server-id/duid-en-hex
                                    case duid-en-hex {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/server-id/server-id/duid-en-hex/duid-en-hex
                                        leaf duid-en-hex {
                                            type string {
                                                length "1..118";
                                            }
                                            description "DUID system ID in hexadecimal format";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/server-id/server-id/duid-en-ascii
                                    case duid-en-ascii {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/proxy-server/server-id/server-id/duid-en-ascii/duid-en-ascii
                                        leaf duid-en-ascii {
                                            type types-sros:string-not-all-spaces {
                                                length "1..58";
                                            }
                                            description "Vendor-assigned ID based on Enterprise Number (DUID-EN)";
                                        }

                                    }
                                }
                            } // container server-id

                        } // container proxy-server

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/option
                        container option {
                            description "Enable the option context";

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/option/remote-id
                            leaf remote-id {
                                type boolean;
                                default "false";
                                description "Send remote ID option in the DHCPv6 relay packet";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/option/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/option/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/option/interface-id
                            container interface-id {
                                description "Enter the interface-id context";

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/option/interface-id/interface-id
                                choice interface-id {
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/option/interface-id/interface-id/ascii-tuple
                                    case ascii-tuple {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/option/interface-id/interface-id/ascii-tuple/ascii-tuple
                                        leaf ascii-tuple {
                                            type empty;
                                            description "Use ASCII-encoded concatenated tuple";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/option/interface-id/interface-id/sap-id
                                    case sap-id {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/option/interface-id/interface-id/sap-id/sap-id
                                        leaf sap-id {
                                            type empty;
                                            description "Use SAP ID in interface ID option in relay packet";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/option/interface-id/interface-id/if-index
                                    case if-index {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/option/interface-id/interface-id/if-index/if-index
                                        leaf if-index {
                                            type empty;
                                            description "Use interface index in the DHCPv6 relay packet";
                                        }

                                    }
                                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/option/interface-id/interface-id/string
                                    case string {

                                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/dhcp6/option/interface-id/interface-id/string/string
                                        leaf string {
                                            type types-sros:string-not-all-spaces {
                                                length "1..80";
                                            }
                                            description "String for interface ID option in DHCPv6 relay packet";
                                        }

                                    }
                                }
                            } // container interface-id

                        } // container option

                    } // container dhcp6

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/neighbor-discovery
                    container neighbor-discovery {
                        when "../../type = 'plain' or ../../type = 'lns' or ../../type = 'wlan-gw'" {
                            description "invalid group interface type";
                        }
                        description "Enter the neighbor-discovery context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/neighbor-discovery/dad-snooping
                        leaf dad-snooping {
                            type boolean;
                            default "false";
                            description "Populate table via duplicate address detection packets";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/neighbor-discovery/neighbor-limit
                        leaf neighbor-limit {
                            type int32 {
                                range "1..8";
                            }
                            default "1";
                            description "Maximum neighbor entries learned per SLAAC host";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/neighbor-discovery/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/neighbor-discovery/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container neighbor-discovery

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements
                    container router-advertisements {
                        when "../../type = 'plain' or ../../type = 'lns' or ../../type = 'wlan-gw' or ../../type = 'gtp'" {
                            description "invalid group interface type";
                        }
                        description "Enter the router-advertisements context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of router advertisements";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/max-advertisement-interval
                        leaf max-advertisement-interval {
                            type types-sros:time-duration {
                                range "900..1800";
                            }
                            units "seconds";
                            default "1800";
                            description "Maximum advertisement interval";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/min-advertisement-interval
                        leaf min-advertisement-interval {
                            type types-sros:time-duration {
                                range "900..1350";
                            }
                            units "seconds";
                            default "900";
                            description "Minimum advertisement interval";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/force-mcast
                        leaf force-mcast {
                            type enumeration {
                                enum ip                             { value 1; }
                                enum ip-mac                         { value 2; }
                            }
                            description "Protocol with forced multicast";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/options
                        container options {
                            description "Enter the options context";

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/options/current-hop-limit
                            leaf current-hop-limit {
                                type uint32 {
                                    range "0..255";
                                }
                                default "64";
                                description "Hop limit to be advertised";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/options/managed-configuration
                            leaf managed-configuration {
                                type boolean;
                                default "false";
                                description "Managed address configuration flag";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/options/mtu
                            leaf mtu {
                                type union {
                                    type uint32 {
                                        range "1280..9212";
                                    }
                                    type enumeration {
                                        enum not-included                   { value 0; }
                                    }
                                }
                                units "bytes";
                                default "not-included";
                                description "Advertised MTU value";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/options/other-stateful-configuration
                            leaf other-stateful-configuration {
                                type boolean;
                                default "false";
                                description "Other stateful configuration flag";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/options/reachable-time
                            leaf reachable-time {
                                type types-sros:time-duration {
                                    range "0..3600000";
                                }
                                units "milliseconds";
                                default "0";
                                description "Reachable time for advertisements";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/options/retransmit-timer
                            leaf retransmit-timer {
                                type uint32 {
                                    range "0..1800000";
                                }
                                units "seconds";
                                default "0";
                                description "Retransmit time in router advertisements from interface";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/options/router-lifetime
                            leaf router-lifetime {
                                type union {
                                    type types-sros:time-duration {
                                        range "2700..9000";
                                    }
                                    type enumeration {
                                        enum no-default-router              { value 0; }
                                    }
                                }
                                units "seconds";
                                default "4500";
                                description "Router lifetime";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/options/dns
                            container dns {
                                description "Enter the dns context";

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/options/dns/include-rdnss
                                leaf include-rdnss {
                                    type boolean;
                                    default "false";
                                    description "Include the RDNSS server option 25";
                                }

                                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/options/dns/rdnss-lifetime
                                leaf rdnss-lifetime {
                                    type union {
                                        type types-sros:time-duration {
                                            range "900..3600";
                                        }
                                        type enumeration {
                                            enum infinite                       { value -1; }
                                        }
                                    }
                                    units "seconds";
                                    default "3600";
                                    description "Maximum time for the RDNSS address to remain valid";
                                }

                            } // container dns

                        } // container options

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/prefix-options
                        container prefix-options {
                            description "Enter the prefix-options context";

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/prefix-options/autonomous
                            leaf autonomous {
                                type boolean;
                                default "false";
                                description "Value of the autonomous flag";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/prefix-options/on-link
                            leaf on-link {
                                type boolean;
                                default "true";
                                description "Assign the prefix to an interface on the specified link";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/prefix-options/preferred-lifetime
                            leaf preferred-lifetime {
                                type types-submgt:router-advertisement-prefix-options-lifetime;
                                units "seconds";
                                default "3600";
                                description "Time for a prefix to remain preferred";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-advertisements/prefix-options/valid-lifetime
                            leaf valid-lifetime {
                                type types-submgt:router-advertisement-prefix-options-lifetime;
                                units "seconds";
                                default "86400";
                                description "Time for a prefix to remain valid";
                            }

                        } // container prefix-options

                    } // container router-advertisements

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-solicit
                    container router-solicit {
                        when "../../type = 'plain' or ../../type = 'lns' or ../../type = 'wlan-gw'" {
                            description "invalid group interface type";
                        }
                        description "Enable the router-solicit context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-solicit/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of router-solicit authentication";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-solicit/inactivity-timer
                        leaf inactivity-timer {
                            type union {
                                type types-sros:time-duration {
                                    range "1..31536000";
                                }
                                type enumeration {
                                    enum infinite                       { value -1; }
                                }
                            }
                            units "seconds";
                            default "300";
                            description "Time before an inactive host is removed";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-solicit/min-auth-interval
                        leaf min-auth-interval {
                            type types-sros:time-duration {
                                range "1..360000";
                            }
                            units "seconds";
                            default "300";
                            description "Minimum time between successive authentication attempts";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-solicit/user-db
                        leaf user-db {
                            type leafref {
                                path "../../../../../../../subscriber-mgmt/local-user-db/name";
                            }
                            description "Local user database used for authentication";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-solicit/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipv6/router-solicit/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container router-solicit

                } // container ipv6

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/cflowd-parameters
                container cflowd-parameters {
                    description "Enter the cflowd-parameters context";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/cflowd-parameters/sampling
                    list sampling {
                        key "sampling-type";
                        description "Enter the sampling list instance";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/cflowd-parameters/sampling/sampling-type
                        leaf sampling-type {
                            type types-router:cflowd-sampling;
                            description "Traffic sampling type";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/cflowd-parameters/sampling/type
                        leaf type {
                            when "not(../sampling-type = 'both')";
                            type enumeration {
                                enum acl                            { value 1; }
                                enum interface                      { value 2; }
                            }
                            mandatory true;
                            description "Type of cflowd analysis";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/cflowd-parameters/sampling/direction
                        leaf direction {
                            type enumeration {
                                enum ingress-only                   { value 1; }
                                enum egress-only                    { value 2; }
                                enum both                           { value 3; }
                            }
                            default "ingress-only";
                            description "Direction of traffic for cflowd sampling";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/cflowd-parameters/sampling/sample-profile
                        leaf sample-profile {
                            type union {
                                type enumeration {
                                    enum 1                              { value 1; }
                                }
                                type uint32;
                            }
                            description "Sample profile ID";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/cflowd-parameters/sampling/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/cflowd-parameters/sampling/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list sampling

                } // container cflowd-parameters

                ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipoe-linking
                container ipoe-linking {
                    when "../type = 'plain' or ../type = 'lns' or ../type = 'wlan-gw'" {
                        description "invalid group interface type";
                    }
                    description "Enable the ipoe-linking context";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipoe-linking/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of IPoE host linking";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipoe-linking/gratuitous-router-advertisement
                    leaf gratuitous-router-advertisement {
                        type boolean;
                        sros-ext:auto-restart-to-modify;
                        default "false";
                        description "Send unsolicited router advertisement after DHCP setup";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/group-interface/ipoe-linking/shared-circuit-id
