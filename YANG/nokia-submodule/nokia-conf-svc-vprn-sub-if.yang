submodule nokia-conf-svc-vprn-sub-if {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-eth-cfm            { prefix "types-eth-cfm"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-router             { prefix "types-router"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-subscriber-mgmt    { prefix "types-submgt"; }

    include nokia-conf-aa-group;
    include nokia-conf-aaa;
    include nokia-conf-application-assurance;
    include nokia-conf-eth-cfm;
    include nokia-conf-filter;
    include nokia-conf-filter-dhcp;
    include nokia-conf-groups;
    include nokia-conf-log;
    include nokia-conf-policy-options;
    include nokia-conf-port;
    include nokia-conf-port-eth-access;
    include nokia-conf-port-ethernet;
    include nokia-conf-pxc;
    include nokia-conf-python;
    include nokia-conf-qos;
    include nokia-conf-routing-options;
    include nokia-conf-service;
    include nokia-conf-submgt-local-user-db;
    include nokia-conf-submgt-vrgw;
    include nokia-conf-subscriber-mgmt;
    include nokia-conf-svc-epipe;
    include nokia-conf-svc-vprn;
    include nokia-conf-svc-vprn-dhcp-server;
    include nokia-conf-svc-vprn-grp-if-wlangw;
    include nokia-conf-svc-vprn-sub-if-wlangw;
    include nokia-conf-system;
    include nokia-conf-system-security;
    include nokia-conf-vrrp;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure vprn service subscriber interfaces on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-svc-vprn-sub-if YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2022-12-07";

    grouping conf-svc-vprn-sub-if-subscriber-interface {
        ///nokia-conf/configure/service/vprn/subscriber-interface
        list subscriber-interface {
            key "interface-name";
            description "Enter the subscriber-interface list instance";

            ///nokia-conf/configure/service/vprn/subscriber-interface/interface-name
            leaf interface-name {
                type types-sros:interface-name;
                description "Subscriber interface name";
            }

            ///nokia-conf/configure/service/vprn/subscriber-interface/admin-state
            leaf admin-state {
                type types-sros:admin-state;
                default "enable";
                description "Administrative state of the subscriber interface";
            }

            ///nokia-conf/configure/service/vprn/subscriber-interface/description
            leaf description {
                type types-sros:very-long-description;
                description "Text description";
            }

            ///nokia-conf/configure/service/vprn/subscriber-interface/fwd-service
            leaf fwd-service {
                type leafref {
                    path "../../../vprn/service-name";
                }
                sros-ext:immutable;
                description "Configure the forwarding service.";
            }

            ///nokia-conf/configure/service/vprn/subscriber-interface/fwd-subscriber-interface
            leaf fwd-subscriber-interface {
                type leafref {
                    path "../../../vprn[service-name=current()/../fwd-service]/subscriber-interface/interface-name";
                }
                sros-ext:immutable;
                description "Forwarding subscriber interface name";
            }

            ///nokia-conf/configure/service/vprn/subscriber-interface/wan-mode
            leaf wan-mode {
                type enumeration {
                    enum mode64                         { value 1; }
                    enum mode128                        { value 2; }
                }
                sros-ext:immutable;
                default "mode64";
                description "Mode of operation for hosts with /128 WAN IPv6 address";
            }

            ///nokia-conf/configure/service/vprn/subscriber-interface/private-retail-subnets
            leaf private-retail-subnets {
                when "../fwd-service" {
                    description "fwd-service must be set";
                }
                type boolean;
                sros-ext:immutable;
                default "false";
                description "Export subnets to the forwarding service";
            }

            ///nokia-conf/configure/service/vprn/subscriber-interface/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/service/vprn/subscriber-interface/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4
            container ipv4 {
                description "Enter the ipv4 context";

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/allow-unmatching-subnets
                leaf allow-unmatching-subnets {
                    type boolean;
                    default "false";
                    description "Allow subscriber hosts without a matching subnet";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/export-host-routes
                leaf export-host-routes {
                    when "../../fwd-service" {
                        description "fwd-service must be set";
                    }
                    type boolean;
                    default "false";
                    description "Allow export of subscriber management host routes";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/default-dns
                leaf-list default-dns {
                    type types-sros:ipv4-unicast-address;
                    max-elements 2;
                    ordered-by user;
                    description "Default DNS server addresses";
                }

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/address
                list address {
                    key "ipv4-address";
                    description "Enter the address list instance";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/address/ipv4-address
                    leaf ipv4-address {
                        type types-sros:ipv4-unicast-address;
                        description "IP address associated with the subscriber subnet";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/address/prefix-length
                    leaf prefix-length {
                        type uint32 {
                            range "0..32";
                        }
                        mandatory true;
                        description "IPv4 address prefix length";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/address/gateway
                    leaf gateway {
                        type types-sros:ipv4-unicast-address;
                        description "Gateway IP address within the subnet for SRRP routing";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/address/track-srrp
                    leaf track-srrp {
                        type uint32 {
                            range "1..4294967295";
                        }
                        description "SRRP instance whose state is tracked on this IP address";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/address/populate-host-routes
                    leaf populate-host-routes {
                        type boolean;
                        default "false";
                        description "Populate subscriber host routes in local FIB";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/address/holdup-time
                    leaf holdup-time {
                        type uint32 {
                            range "100..5000";
                        }
                        units "milliseconds";
                        description "Wait time before accepting new state attribute";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/address/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/address/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list address

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/arp-host
                container arp-host {
                    when "../../fwd-service" {
                        description "fwd-service must be set";
                    }
                    description "Enter the arp-host context";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/arp-host/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        description "Administrative state of ARP hosts";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/arp-host/host-limit
                    leaf host-limit {
                        type uint32 {
                            range "1..524287";
                        }
                        description "Maximum number of ARP hosts";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/arp-host/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/arp-host/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container arp-host

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/unnumbered
                container unnumbered {
                    description "Enter the unnumbered context";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/unnumbered/source
                    choice source {
                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/unnumbered/source/ip-address
                        case ip-address {

                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/unnumbered/source/ip-address/ip-address
                            leaf ip-address {
                                type types-sros:ipv4-unicast-address;
                                description "IP address for the subscriber interface";
                            }

                        }
                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/unnumbered/source/ip-int-name
                        case ip-int-name {

                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/unnumbered/source/ip-int-name/ip-int-name
                            leaf ip-int-name {
                                type types-sros:interface-name;
                                description "Interface name from which an IPv4 addressed is borrowed";
                            }

                        }
                    }
                } // container unnumbered

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/bfd
                container bfd {
                    description "Enter the bfd context";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/bfd/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of BFD sessions";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/bfd/transmit-interval
                    leaf transmit-interval {
                        type uint32 {
                            range "10..100000";
                        }
                        units "milliseconds";
                        default "100";
                        description "BFD transmit interval over this interface";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/bfd/receive
                    leaf receive {
                        type uint32 {
                            range "10..100000";
                        }
                        units "milliseconds";
                        default "100";
                        description "BFD receive interval over this interface";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/bfd/multiplier
                    leaf multiplier {
                        type uint32 {
                            range "1..20";
                        }
                        default "3";
                        description "Number of consecutive BFD messages missed from the peer";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/bfd/echo-receive
                    leaf echo-receive {
                        type uint32 {
                            range "100..100000";
                        }
                        units "milliseconds";
                        description "Minimum echo interval over this interface";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/bfd/type
                    leaf type {
                        type enumeration {
                            enum cpm-np                         { value 1; }
                            enum auto                           { value 2; }
                            enum fp                             { value 3; }
                        }
                        default "auto";
                        description "Local termination point for the BFD session";
                    }

                } // container bfd

                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp
                container dhcp {
                    description "Enable the dhcp context";

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/admin-state
                    leaf admin-state {
                        when "../../../fwd-service" {
                            description "fwd-service must be set";
                        }
                        type types-sros:admin-state;
                        description "Administrative state of DHCP";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/description
                    leaf description {
                        when "../../../fwd-service" {
                            description "fwd-service must be set";
                        }
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/python-policy
                    leaf python-policy {
                        when "../../../fwd-service" {
                            description "fwd-service must be set";
                        }
                        type leafref {
                            path "../../../../../../python/python-policy/name";
                        }
                        description "Python policy";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/server
                    leaf-list server {
                        when "../../../fwd-service" {
                            description "fwd-service must be set";
                        }
                        type types-sros:ipv4-unicast-address;
                        max-elements 8;
                        ordered-by user;
                        description "DHCP server IP addresses";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/gi-address
                    leaf gi-address {
                        type types-sros:ipv4-unicast-address;
                        description "GI address for the DHCP relay";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/release-include-gi-address
                    leaf release-include-gi-address {
                        when "../../../fwd-service" {
                            description "fwd-service must be set";
                        }
                        type boolean;
                        default "false";
                        description "Include the GI address in DHCP relay messages";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/src-ip-addr
                    leaf src-ip-addr {
                        type enumeration {
                            enum auto                           { value 0; }
                            enum gi-address                     { value 1; }
                        }
                        default "auto";
                        description "Type of source address to use for DHCP relay";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/virtual-subnet
                    leaf virtual-subnet {
                        when "not(../../../fwd-service)" {
                            description "fwd-service must be unset";
                        }
                        type boolean;
                        default "false";
                        description "Enable a virtual subnet for DHCPv4 hosts";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/proxy-server
                    container proxy-server {
                        when "../../../fwd-service" {
                            description "fwd-service must be set";
                        }
                        description "Enter the proxy-server context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/proxy-server/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the DHCP proxy server";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/proxy-server/emulated-server
                        leaf emulated-server {
                            type types-sros:ipv4-unicast-address;
                            description "IP address used as DHCP server address in SAP context";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/proxy-server/lease-time
                        container lease-time {
                            description "Enter the lease-time context";

                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/proxy-server/lease-time/value
                            leaf value {
                                type types-sros:time-duration {
                                    range "300..315446399";
                                }
                                units "seconds";
                                description "DHCP lease time";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/proxy-server/lease-time/radius-override
                            leaf radius-override {
                                type boolean;
                                default "false";
                                description "Use lease time information provided by RADIUS server";
                            }

                        } // container lease-time

                    } // container proxy-server

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/option-82
                    container option-82 {
                        when "../../../fwd-service" {
                            description "fwd-service must be set";
                        }
                        description "Enter the option-82 context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/option-82/vendor-specific-option
                        container vendor-specific-option {
                            description "Enter the vendor-specific-option context";

                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/option-82/vendor-specific-option/system-id
                            leaf system-id {
                                type boolean;
                                default "false";
                                description "Send the system ID in the VSO";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/option-82/vendor-specific-option/client-mac-address
                            leaf client-mac-address {
                                type boolean;
                                default "false";
                                description "Send the MAC address in the VSO";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/option-82/vendor-specific-option/service-id
                            leaf service-id {
                                type boolean;
                                default "false";
                                description "Send the service ID in the Vendor Specific Option";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/option-82/vendor-specific-option/sap-id
                            leaf sap-id {
                                type boolean;
                                default "false";
                                description "Send SAP ID in the sub-option of the DHCP relay packet";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/option-82/vendor-specific-option/string
                            leaf string {
                                type types-sros:string-not-all-spaces {
                                    length "1..32";
                                }
                                description "User-defined ASCII string for the VSO";
                            }

                        } // container vendor-specific-option

                    } // container option-82

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/lease-populate
                    container lease-populate {
                        when "../../../fwd-service" {
                            description "fwd-service must be set";
                        }
                        description "Enter the lease-populate context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/lease-populate/max-leases
                        leaf max-leases {
                            type uint32 {
                                range "0..511999";
                            }
                            description "Maximum number of DHCPv4 leases";
                        }

                    } // container lease-populate

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/client-applications
                    container client-applications {
                        when "../../../fwd-service" {
                            description "fwd-service must be set";
                        }
                        description "Enter the client-applications context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/client-applications/dhcp
                        leaf dhcp {
                            type boolean;
                            default "true";
                            description "Enable IPoE clients to use DHCP relay or proxy server";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/client-applications/ppp
                        leaf ppp {
                            type boolean;
                            default "false";
                            description "Enable PPPoE clients to use DHCP relay or proxy server";
                        }

                    } // container client-applications

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/relay-proxy
                    container relay-proxy {
                        when "../../../fwd-service" {
                            description "fwd-service must be set";
                        }
                        presence "Enable the relay-proxy context";
                        description "Enable the relay-proxy context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/relay-proxy/release-update-src-ip
                        leaf release-update-src-ip {
                            type boolean;
                            default "false";
                            description "Update the source IP address of a DHCP RELEASE message";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/relay-proxy/siaddr-override
                        leaf siaddr-override {
                            type types-sros:ipv4-unicast-address;
                            description "DHCP server IP address for address hiding function";
                        }

                    } // container relay-proxy

                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/offer-selection
                    container offer-selection {
                        description "Enter the offer-selection context";

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/offer-selection/discover-delay
                        leaf discover-delay {
                            type int32 {
                                range "1..100";
                            }
                            units "deciseconds";
                            description "Delay before sending DHCP Discover messages";
                        }

                        ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/offer-selection/mode
                        choice mode {
                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/offer-selection/mode/mode-client-mac
                            case mode-client-mac {

                                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/offer-selection/mode/mode-client-mac/client-mac
                                container client-mac {
                                    description "Enter the client-mac context";

                                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/offer-selection/mode/mode-client-mac/client-mac/mac-address
                                    leaf mac-address {
                                        type enumeration {
                                            enum odd                            { value 1; }
                                            enum even                           { value 2; }
                                        }
                                        description "Designated client MAC addresses for Offer selection";
                                    }

                                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/offer-selection/mode/mode-client-mac/client-mac/discover-delay
                                    leaf discover-delay {
                                        type int32 {
                                            range "1..100";
                                        }
                                        units "deciseconds";
                                        description "Delay before sending DHCP Discover messages";
                                    }

                                } // container client-mac

                            }
                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/offer-selection/mode/mode-server
                            case mode-server {

                                ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/offer-selection/mode/mode-server/server
                                list server {
                                    key "ipv4-address";
                                    max-elements 8;
                                    description "Enter the server list instance";

                                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/offer-selection/mode/mode-server/server/ipv4-address
                                    leaf ipv4-address {
                                        type types-sros:ipv4-unicast-address;
                                        description "IP address of the DHCP server";
                                    }

                                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/offer-selection/mode/mode-server/server/discover-delay
                                    leaf discover-delay {
                                        type int32 {
                                            range "1..100";
                                        }
                                        units "deciseconds";
                                        description "Delay before sending DHCP Discover messages";
                                    }

                                    ///nokia-conf/configure/service/vprn/subscriber-interface/ipv4/dhcp/offer-selection/mode/mode-server/server/apply-groups
                                    leaf-list apply-groups {
                                        type leafref {
                                            path "../../../../../../../../groups/group/name";
                                        }
                                        max-elements 8;
                                        ordered-by user;
                                        description "Apply a configuration group at this level";
