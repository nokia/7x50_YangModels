                            leaf dhcp {
                                type boolean;
                                default "true";
                                description "Enable IPoE clients to use DHCP relay or proxy server";
                            }

                            ///nokia-conf/configure/service/vprn/subscriber-interface/ipv6/dhcp6/proxy-server/client-applications/ppp
                            leaf ppp {
                                type boolean;
                                default "false";
                                description "Allow PPPoE clients to use DHCP relay functionality";
