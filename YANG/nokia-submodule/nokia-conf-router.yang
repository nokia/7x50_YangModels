                ///nokia-conf/configure/router/subscriber-mgmt/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/router/subscriber-mgmt/dhcpv4
                container dhcpv4 {
                    description "Enter the dhcpv4 context";

                    ///nokia-conf/configure/router/subscriber-mgmt/dhcpv4/routed-subnet-transparent-forward
                    leaf routed-subnet-transparent-forward {
                        type boolean;
                        default "false";
                        description "Transparently forward DHCPv4 from CPE LAN over IPoE";
                    }

                    ///nokia-conf/configure/router/subscriber-mgmt/dhcpv4/apply-groups
                    leaf-list apply-groups {
