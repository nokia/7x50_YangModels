                        description "Multi-chassis shunt interface name";
                    }

                    ///nokia-conf/configure/router/ipsec/multi-chassis-shunt-interface/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/ipsec/multi-chassis-shunt-interface/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/ipsec/multi-chassis-shunt-interface/next-hop
                    container next-hop {
                        description "Enter the next-hop context";

                        ///nokia-conf/configure/router/ipsec/multi-chassis-shunt-interface/next-hop/address
                        leaf address {
                            type types-sros:ip-unicast-address;
                            description "Next hop address for the shunting interface";
                        }

                    } // container next-hop

                } // list multi-chassis-shunt-interface

                ///nokia-conf/configure/router/ipsec/multi-chassis-shunting-profile
                list multi-chassis-shunting-profile {
                    key "name";
                    max-elements 64;
                    description "Enter the multi-chassis-shunting-profile list instance";

                    ///nokia-conf/configure/router/ipsec/multi-chassis-shunting-profile/name
                    leaf name {
                        type types-sros:named-item;
                        description "Multi-chassis shunting profile name";
                    }

                    ///nokia-conf/configure/router/ipsec/multi-chassis-shunting-profile/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/ipsec/multi-chassis-shunting-profile/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/ipsec/multi-chassis-shunting-profile/peer
                    list peer {
                        key "ip-address";
                        max-elements 3;
                        description "Enter the peer list instance";

                        ///nokia-conf/configure/router/ipsec/multi-chassis-shunting-profile/peer/ip-address
                        leaf ip-address {
                            type leafref {
                                path "../../../../../redundancy/multi-chassis/peer/ip-address";
                            }
                            description "Peer address";
                        }

                        ///nokia-conf/configure/router/ipsec/multi-chassis-shunting-profile/peer/multi-chassis-shunt-interface
                        leaf multi-chassis-shunt-interface {
                            type leafref {
                                path "../../../../../router/ipsec/multi-chassis-shunt-interface/name";
                            }
                            description "Multi-chassis shunt interface";
                        }

                        ///nokia-conf/configure/router/ipsec/multi-chassis-shunting-profile/peer/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/ipsec/multi-chassis-shunting-profile/peer/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list peer

                } // list multi-chassis-shunting-profile

            } // container ipsec

            ///nokia-conf/configure/router/subscriber-mgmt
            container subscriber-mgmt {
                when "../router-name = 'Base'" {
                    description "only allowed in the Base router";
                }
                description "Enter the subscriber-mgmt context";

                ///nokia-conf/configure/router/subscriber-mgmt/apply-groups
                leaf-list apply-groups {
                    type leafref {
