submodule nokia-conf-router {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import ietf-inet-types                { prefix "inet"; }
    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-eth-cfm            { prefix "types-eth-cfm"; }
    import nokia-types-ipsec              { prefix "types-ipsec"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-redundancy         { prefix "types-redundancy"; }
    import nokia-types-router             { prefix "types-router"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-eth-cfm;
    include nokia-conf-filter;
    include nokia-conf-filter-ip-exception;
    include nokia-conf-groups;
    include nokia-conf-ipsec;
    include nokia-conf-isa;
    include nokia-conf-log;
    include nokia-conf-mcast-mgmt;
    include nokia-conf-policy-options;
    include nokia-conf-pxc;
    include nokia-conf-python;
    include nokia-conf-qos;
    include nokia-conf-redundancy;
    include nokia-conf-router-bgp;
    include nokia-conf-router-bier;
    include nokia-conf-router-dhcp-server;
    include nokia-conf-router-firewall;
    include nokia-conf-router-gtm;
    include nokia-conf-router-igmp;
    include nokia-conf-router-isis;
    include nokia-conf-router-l2tp;
    include nokia-conf-router-ldp;
    include nokia-conf-router-mld;
    include nokia-conf-router-mpls;
    include nokia-conf-router-msdp;
    include nokia-conf-router-nat;
    include nokia-conf-router-origin-validation;
    include nokia-conf-router-ospf;
    include nokia-conf-router-ospf3;
    include nokia-conf-router-p2mp-sr-tree;
    include nokia-conf-router-pcep;
    include nokia-conf-router-pim;
    include nokia-conf-router-radius;
    include nokia-conf-router-rib-api;
    include nokia-conf-router-rip;
    include nokia-conf-router-ripng;
    include nokia-conf-router-rsvp;
    include nokia-conf-router-segment-routing;
    include nokia-conf-router-static-route;
    include nokia-conf-router-twamp-light;
    include nokia-conf-router-wpp;
    include nokia-conf-routing-options;
    include nokia-conf-service;
    include nokia-conf-submgt-gtp;
    include nokia-conf-subscriber-mgmt;
    include nokia-conf-svc-nat;
    include nokia-conf-svc-vprn;
    include nokia-conf-system;
    include nokia-conf-system-security;
    include nokia-conf-test-oam;
    include nokia-conf-vrrp;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure routing protocols on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-router YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-06-21";

    grouping conf-router {
        ///nokia-conf/configure/router
        list router {
            key "router-name";
            max-elements 4;
            description "Enter the router list instance";

            ///nokia-conf/configure/router/router-name
            leaf router-name {
                type types-sros:named-item-64 {
                    pattern ".{1,32}" {
                        error-message "vRtrName needs to be extended to 64 to support this name";
                    }
                }
                description "Administrative router name";
            }

            ///nokia-conf/configure/router/description
            leaf description {
                when "not(../router-name = 'management')" {
                    description "not allowed in the management router";
                }
                type types-sros:description;
                description "Text description";
            }

            ///nokia-conf/configure/router/triggered-policy
            leaf triggered-policy {
                when "../router-name = 'Base'" {
                    description "only allowed in the Base router";
                }
                type boolean;
                default "false";
                description "Trigger route policy re-evaluation";
            }

            ///nokia-conf/configure/router/allow-bgp-to-igp-export
            leaf allow-bgp-to-igp-export {
                when "../router-name = 'Base'" {
                    description "only allowed in the base router";
                }
                type boolean;
                default "true";
                description "Enable the distribution of BGP routes into the IGP";
            }

            ///nokia-conf/configure/router/allow-icmp-redirect
            leaf allow-icmp-redirect {
                when "../router-name = 'management'" {
                    description "only allowed in the management router";
                }
                type boolean;
                default "false";
                description "Allow ICMP redirects on the management interface";
            }

            ///nokia-conf/configure/router/allow-icmp6-redirect
            leaf allow-icmp6-redirect {
                when "../router-name = 'management'" {
                    description "only allowed in the management router";
                }
                type boolean;
                default "false";
                description "Allow IPv6 ICMP redirects on the management interface";
            }

            ///nokia-conf/configure/router/class-forwarding
            leaf class-forwarding {
                when "../router-name = 'Base'" {
                    description "only allowed in the Base router";
                }
                type boolean;
                default "false";
                description "Allow class-based forwarding over IGP shortcuts";
            }

            ///nokia-conf/configure/router/icmp-tunneling
            leaf icmp-tunneling {
                when "../router-name = 'Base'" {
                    description "only allowed in the Base router";
                }
                type boolean;
                default "false";
                description "Allow tunneling of ICMP reply packets over MPLS LSPs";
            }

            ///nokia-conf/configure/router/fib-telemetry
            leaf fib-telemetry {
                when "../router-name = 'Base'" {
                    description "only allowed in the Base router";
                }
                type boolean;
                default "false";
                description "Collect extra state information";
            }

            ///nokia-conf/configure/router/selective-fib
            leaf selective-fib {
                when "../router-name = 'Base'" {
                    description "only allowed in the Base router";
                }
                type boolean;
                default "true";
                description "Enable selective FIB";
            }

            ///nokia-conf/configure/router/autonomous-system
            leaf autonomous-system {
                type uint32 {
                    range "1..max";
                }
                description "AS number advertised to peers for this router";
            }

            ///nokia-conf/configure/router/ecmp
            leaf ecmp {
                type types-router:ip-ecmp;
                default "1";
                description "Maximum equal-cost routes for routing table instance";
            }

            ///nokia-conf/configure/router/entropy-label
            leaf entropy-label {
                type boolean;
                default "false";
                description "Use entropy label";
            }

            ///nokia-conf/configure/router/fib-priority
            leaf fib-priority {
                type enumeration {
                    enum standard                       { value 1; }
                    enum high                           { value 2; }
                }
                default "standard";
                description "FIB priority for VPRN BGP routes";
            }

            ///nokia-conf/configure/router/multicast-info-policy
            leaf multicast-info-policy {
                type leafref {
                    path "../../multicast-management/multicast-info-policy/policy-name";
                }
                description "Multicast policy name for virtual router";
            }

            ///nokia-conf/configure/router/router-id
            leaf router-id {
                type types-router:router-id;
                description "Unique router ID for the router in the AS";
            }

            ///nokia-conf/configure/router/weighted-ecmp
            leaf weighted-ecmp {
                type enumeration {
                    enum false                          { value 0; }
                    enum true                           { value 1; }
                    enum strict                         { value 2; }
                }
                default "false";
                description "Weighted load-balancing capability for ECMP routes";
            }

            ///nokia-conf/configure/router/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/router/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/router/leak-export
            container leak-export {
                when "../router-name = 'Base'" {
                    description "only allowed in the Base router";
                }
                description "Enter the leak-export context";

                ///nokia-conf/configure/router/leak-export/policy-name
                leaf-list policy-name {
                    type union {
                        type types-sros:policy-expr-string;
                        type string;
                    }
                    max-elements 5;
                    ordered-by user;
                    description "Route policy name";
                }

                ///nokia-conf/configure/router/leak-export/leak-export-limit
                leaf leak-export-limit {
                    type uint32 {
                        range "1..10000";
                    }
                    default "5";
                    description "Maximum limit on number of GRT routes leaked into VPRNs";
                }

            } // container leak-export

            ///nokia-conf/configure/router/gtp
            container gtp {
                when "../router-name = 'Base'" {
                    description "only allowed in the Base router";
                }
                description "Enter the gtp context";

                ///nokia-conf/configure/router/gtp/s11
                container s11 {
                    description "Enter the s11 context";

                    ///nokia-conf/configure/router/gtp/s11/interface
                    list interface {
                        key "interface-name";
                        description "Enter the interface list instance";

                        ///nokia-conf/configure/router/gtp/s11/interface/interface-name
                        leaf interface-name {
                            type leafref {
                                path "../../../../interface/interface-name";
                            }
                            description "Name for the S11 interface";
                        }

                        ///nokia-conf/configure/router/gtp/s11/interface/apn-policy
                        leaf apn-policy {
                            type leafref {
                                path "../../../../../subscriber-mgmt/gtp/apn-policy/name";
                            }
                            description "Access point name policy for the S11 interface";
                        }

                        ///nokia-conf/configure/router/gtp/s11/interface/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/gtp/s11/interface/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list interface

                    ///nokia-conf/configure/router/gtp/s11/peer-profile-map
                    container peer-profile-map {
                        description "Enter the peer-profile-map context";

                        ///nokia-conf/configure/router/gtp/s11/peer-profile-map/prefix
                        list prefix {
                            key "peer-prefix";
                            description "Enter the prefix list instance";

                            ///nokia-conf/configure/router/gtp/s11/peer-profile-map/prefix/peer-prefix
                            leaf peer-prefix {
                                type types-sros:ip-unicast-prefix;
                                description "IP prefix and prefix length for the subnet";
                            }

                            ///nokia-conf/configure/router/gtp/s11/peer-profile-map/prefix/peer-profile
                            leaf peer-profile {
                                type leafref {
                                    path "../../../../../../subscriber-mgmt/gtp/peer-profile/name";
                                }
                                mandatory true;
                                description "GTP peer profile associated with the address prefix";
                            }

                            ///nokia-conf/configure/router/gtp/s11/peer-profile-map/prefix/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/gtp/s11/peer-profile-map/prefix/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list prefix

                    } // container peer-profile-map

                } // container s11

                ///nokia-conf/configure/router/gtp/upf-data-endpoint
                container upf-data-endpoint {
                    presence "Enable the upf-data-endpoint context";
                    description "Enable the upf-data-endpoint context";

                    ///nokia-conf/configure/router/gtp/upf-data-endpoint/interface
                    leaf interface {
                        type leafref {
                            path "../../../interface/interface-name";
                        }
                        mandatory true;
                        description "Interface on which the GTP-U packets terminate";
                    }

                    ///nokia-conf/configure/router/gtp/upf-data-endpoint/fpe
                    leaf fpe {
                        type leafref {
                            path "../../../../fwd-path-ext/fpe/fpe-id";
                        }
                        mandatory true;
                        description "FPE used to encapsulate and decapsulate GTP-U traffic";
                    }

                    ///nokia-conf/configure/router/gtp/upf-data-endpoint/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/gtp/upf-data-endpoint/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container upf-data-endpoint

                ///nokia-conf/configure/router/gtp/uplink
                container uplink {
                    presence "Enable the uplink context";
                    description "Enable the uplink context";

                    ///nokia-conf/configure/router/gtp/uplink/apn
                    leaf apn {
                        type string {
                            length "1..80";
                            pattern "([a-zA-Z0-9]+-+)*([a-zA-Z0-9]+\\.([a-zA-Z0-9]+-+)*)*[a-zA-Z0-9]+" {
                                error-message "wrong format of apn name";
                            }
                        }
                        description "Network identifier part of the APN for the uplink";
                    }

                    ///nokia-conf/configure/router/gtp/uplink/pdn-type
                    leaf pdn-type {
                        type enumeration {
                            enum ipv4                           { value 0; }
                            enum ipv6                           { value 1; }
                            enum ipv4v6                         { value 2; }
                        }
                        default "ipv4";
                        description "Default PDN to be signaled in GTP";
                    }

                    ///nokia-conf/configure/router/gtp/uplink/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/gtp/uplink/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/gtp/uplink/peer-profile-map
                    container peer-profile-map {
                        description "Enter the peer-profile-map context";

                        ///nokia-conf/configure/router/gtp/uplink/peer-profile-map/prefix
                        list prefix {
                            key "peer-prefix";
                            description "Enter the prefix list instance";

                            ///nokia-conf/configure/router/gtp/uplink/peer-profile-map/prefix/peer-prefix
                            leaf peer-prefix {
                                type types-sros:ip-unicast-prefix;
                                description "IP prefix and prefix length for the subnet";
                            }

                            ///nokia-conf/configure/router/gtp/uplink/peer-profile-map/prefix/peer-profile
                            leaf peer-profile {
                                type leafref {
                                    path "../../../../../../subscriber-mgmt/gtp/peer-profile/name";
                                }
                                mandatory true;
                                description "GTP peer profile associated with the address prefix";
                            }

                            ///nokia-conf/configure/router/gtp/uplink/peer-profile-map/prefix/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/gtp/uplink/peer-profile-map/prefix/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list prefix

                    } // container peer-profile-map

                } // container uplink

            } // container gtp

            ///nokia-conf/configure/router/bfd
            container bfd {
                when "../router-name = 'Base'" {
                    description "only allowed in the Base router";
                }
                description "Enter the bfd context";

                ///nokia-conf/configure/router/bfd/seamless-bfd
                container seamless-bfd {
                    description "Enter the seamless-bfd context";

                    ///nokia-conf/configure/router/bfd/seamless-bfd/peer
                    list peer {
                        key "address";
                        max-elements 8000;
                        description "Enter the peer list instance";

                        ///nokia-conf/configure/router/bfd/seamless-bfd/peer/address
                        leaf address {
                            type types-sros:ip-unicast-address;
                            description "Address of the remote node";
                        }

                        ///nokia-conf/configure/router/bfd/seamless-bfd/peer/discriminator
                        leaf discriminator {
                            type uint32 {
                                range "1..4294967295";
                            }
                            description "Discriminator of the remote node";
                        }

                        ///nokia-conf/configure/router/bfd/seamless-bfd/peer/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/bfd/seamless-bfd/peer/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list peer

                } // container seamless-bfd

            } // container bfd

            ///nokia-conf/configure/router/dns
            container dns {
                when "../router-name = 'Base'" {
                    description "Only allowed in the Base router";
                }
                description "Enter the dns context";

                ///nokia-conf/configure/router/dns/redirect-vprn
                container redirect-vprn {
                    description "Enter the redirect-vprn context";

                    ///nokia-conf/configure/router/dns/redirect-vprn/service
                    list service {
                        key "service-name";
                        max-elements 1;
                        description "Enter the service list instance";

                        ///nokia-conf/configure/router/dns/redirect-vprn/service/service-name
                        leaf service-name {
                            type leafref {
                                path "../../../../../service/vprn/service-name";
                            }
                            description "Administrative service name";
                        }

                        ///nokia-conf/configure/router/dns/redirect-vprn/service/preference
                        leaf preference {
                            type uint32 {
                                range "0..255";
                            }
                            description "Service preference";
                        }

                        ///nokia-conf/configure/router/dns/redirect-vprn/service/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/dns/redirect-vprn/service/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list service

                } // container redirect-vprn

            } // container dns

            ///nokia-conf/configure/router/aggregates
            container aggregates {
                when "../router-name = 'Base'" {
                    description "only allowed in the Base router";
                }
                description "Enable the aggregates context";

                ///nokia-conf/configure/router/aggregates/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/aggregates/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/router/aggregates/aggregate
                list aggregate {
                    key "ip-prefix";
                    description "Enter the aggregate list instance";

                    ///nokia-conf/configure/router/aggregates/aggregate/ip-prefix
                    leaf ip-prefix {
                        type types-sros:ip-unicast-prefix;
                        description "Destination IP address prefix of the aggregate route";
                    }

                    ///nokia-conf/configure/router/aggregates/aggregate/as-set
                    leaf as-set {
                        type boolean;
                        default "false";
                        description "Use AS_SET path segment type for the aggregate route";
                    }

                    ///nokia-conf/configure/router/aggregates/aggregate/community
                    leaf-list community {
                        type types-sros:community;
                        max-elements 12;
                        ordered-by user;
                        description "Community name that is added to the aggregate route";
                    }

                    ///nokia-conf/configure/router/aggregates/aggregate/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/router/aggregates/aggregate/summary-only
                    leaf summary-only {
                        type boolean;
                        default "false";
                        description "Advertise the aggregate route only";
                    }

                    ///nokia-conf/configure/router/aggregates/aggregate/local-preference
                    leaf local-preference {
                        type int64 {
                            range "0..4294967295";
                        }
                        description "Local preference used when aggregate route is exported";
                    }

                    ///nokia-conf/configure/router/aggregates/aggregate/discard-component-communities
                    leaf discard-component-communities {
                        type boolean;
                        default "false";
                        description "Advertise aggregate with aggregate route community set";
                    }

                    ///nokia-conf/configure/router/aggregates/aggregate/policy
                    leaf policy {
                        type leafref {
                            path "../../../../policy-options/policy-statement/name";
                        }
                        description "Policy name for the aggregated route";
                    }

                    ///nokia-conf/configure/router/aggregates/aggregate/tunnel-group
                    leaf tunnel-group {
                        type types-redundancy:tunnel-group-id;
                        description "Tunnel group from which to associate the MC IPSec state";
                    }

                    ///nokia-conf/configure/router/aggregates/aggregate/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/aggregates/aggregate/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/aggregates/aggregate/next-hop
                    choice next-hop {
                        ///nokia-conf/configure/router/aggregates/aggregate/next-hop/blackhole
                        case blackhole {

                            ///nokia-conf/configure/router/aggregates/aggregate/next-hop/blackhole/blackhole
                            container blackhole {
                                presence "Enable the blackhole context";
                                description "Enable the blackhole context";

                                ///nokia-conf/configure/router/aggregates/aggregate/next-hop/blackhole/blackhole/generate-icmp
                                leaf generate-icmp {
                                    type boolean;
                                    default "false";
                                    description "Send ICMP unreachable messages for aggregate routes";
                                }

                            } // container blackhole

                        }
                        ///nokia-conf/configure/router/aggregates/aggregate/next-hop/indirect
                        case indirect {

                            ///nokia-conf/configure/router/aggregates/aggregate/next-hop/indirect/indirect
                            leaf indirect {
                                type types-sros:ip-unicast-address;
                                sros-ext:immutable;
                                description "Address of the indirect next hop";
                            }

                        }
                    }
                    ///nokia-conf/configure/router/aggregates/aggregate/aggregator
                    container aggregator {
                        description "Enter the aggregator context";

                        ///nokia-conf/configure/router/aggregates/aggregate/aggregator/as-number
                        leaf as-number {
                            type uint32 {
                                range "1..max";
                            }
                            description "Aggregator AS number";
                        }

                        ///nokia-conf/configure/router/aggregates/aggregate/aggregator/address
                        leaf address {
                            type types-sros:ipv4-unicast-address;
                            description "Aggregator IP address";
                        }

                    } // container aggregator

                } // list aggregate

            } // container aggregates

            ///nokia-conf/configure/router/ipv6-te-router-id
            container ipv6-te-router-id {
                when "../router-name = 'Base'" {
                    description "only allowed in the Base router";
                }
                description "Enter the ipv6-te-router-id context";

                ///nokia-conf/configure/router/ipv6-te-router-id/interface
                leaf interface {
                    type leafref {
                        path "../../interface/interface-name";
                    }
                    description "Network IP interface name to obtain IP address";
                }

            } // container ipv6-te-router-id

            ///nokia-conf/configure/router/mc-maximum-routes
            container mc-maximum-routes {
                description "Enter the mc-maximum-routes context";

                ///nokia-conf/configure/router/mc-maximum-routes/value
                leaf value {
                    type int32 {
                        range "1..2147483647";
                    }
                    description "Maximum multicast routes configured on virtual router";
                }

                ///nokia-conf/configure/router/mc-maximum-routes/log-only
                leaf log-only {
                    type boolean;
                    default "false";
                    description "Log and allow learning of new multicast routes";
                }

                ///nokia-conf/configure/router/mc-maximum-routes/threshold
                leaf threshold {
                    type uint32 {
                        range "1..100";
                    }
                    units "percent";
                    description "Maximum multicast routes which the VRF holds";
                }

            } // container mc-maximum-routes

            ///nokia-conf/configure/router/vrgw
            container vrgw {
                when "../router-name = 'Base'" {
                    description "only allowed in the Base router";
                }
                description "Enter the vrgw context";

                ///nokia-conf/configure/router/vrgw/lanext
                container lanext {
                    presence "Enable the lanext context";
                    description "Enable the lanext context";

                    ///nokia-conf/configure/router/vrgw/lanext/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of Home LAN Extension";
                    }

                    ///nokia-conf/configure/router/vrgw/lanext/vxlan-port
                    leaf vxlan-port {
                        type uint32 {
                            range "4789|8472";
                        }
                        sros-ext:auto-restart-to-modify;
                        default "4789";
                        description "UDP port to be used by local or remote VXLAN VTEP";
                    }

                    ///nokia-conf/configure/router/vrgw/lanext/wlan-gw-group
                    leaf wlan-gw-group {
                        type leafref {
                            path "../../../../isa/wlan-gw-group/id";
                        }
                        sros-ext:immutable;
                        sros-ext:auto-restart-to-modify;
                        description "WLAN-GW group used for Home LAN Extension";
                    }

                    ///nokia-conf/configure/router/vrgw/lanext/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/vrgw/lanext/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/vrgw/lanext/vxlan-vtep-range
                    container vxlan-vtep-range {
                        description "Enter the vxlan-vtep-range context";

                        ///nokia-conf/configure/router/vrgw/lanext/vxlan-vtep-range/start
                        leaf start {
                            type types-sros:ip-unicast-without-local-address;
                            sros-ext:auto-restart-to-modify;
                            description "Lower bound of the VXLAN VTEP range";
                        }

                        ///nokia-conf/configure/router/vrgw/lanext/vxlan-vtep-range/end
                        leaf end {
                            type types-sros:ip-unicast-without-local-address;
                            sros-ext:auto-restart-to-modify;
                            description "Upper bound of the VXLAN VTEP range";
                        }

                    } // container vxlan-vtep-range


                } // container lanext

            } // container vrgw

            ///nokia-conf/configure/router/wlan-gw
            container wlan-gw {
                presence "Enable the wlan-gw context";
                description "Enable the wlan-gw context";

                ///nokia-conf/configure/router/wlan-gw/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/wlan-gw/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/router/wlan-gw/mobility-triggered-accounting
                container mobility-triggered-accounting {
                    description "Enter the mobility-triggered-accounting context";

                    ///nokia-conf/configure/router/wlan-gw/mobility-triggered-accounting/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of mobility triggered accounting";
                    }

                    ///nokia-conf/configure/router/wlan-gw/mobility-triggered-accounting/include-counters
                    leaf include-counters {
                        type boolean;
                        default "false";
                        description "Include counters in interim updates";
                    }

                    ///nokia-conf/configure/router/wlan-gw/mobility-triggered-accounting/hold-down
                    leaf hold-down {
                        type types-sros:time-duration {
                            range "60..86400";
                        }
                        units "seconds";
                        description "Hold additional mobility-triggered update until this timer expires";
                    }

                } // container mobility-triggered-accounting

                ///nokia-conf/configure/router/wlan-gw/distributed-subscriber-mgmt
                container distributed-subscriber-mgmt {
                    description "Enable the distributed-subscriber-mgmt context";

                    ///nokia-conf/configure/router/wlan-gw/distributed-subscriber-mgmt/ipv6-tcp-mss-adjust
                    leaf ipv6-tcp-mss-adjust {
                        type uint32 {
                            range "160..10240";
                        }
                        description "TCP-MSS adjustment value in upstream direction for DSM";
                    }

                    ///nokia-conf/configure/router/wlan-gw/distributed-subscriber-mgmt/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/wlan-gw/distributed-subscriber-mgmt/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }


                } // container distributed-subscriber-mgmt

                ///nokia-conf/configure/router/wlan-gw/xconnect
                container xconnect {
                    description "Enter the xconnect context";

                    ///nokia-conf/configure/router/wlan-gw/xconnect/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the WLAN-GW cross-connect";
                    }

                    ///nokia-conf/configure/router/wlan-gw/xconnect/tunnel-source-ip
                    leaf tunnel-source-ip {
                        type types-sros:ipv6-prefix;
                        sros-ext:auto-restart-to-modify;
                        description "IPv6 address and prefix for the tunnel source";
                    }

                    ///nokia-conf/configure/router/wlan-gw/xconnect/wlan-gw-group
                    leaf wlan-gw-group {
                        type leafref {
                            path "../../../../isa/wlan-gw-group/id";
                        }
                        sros-ext:auto-restart-to-modify;
                        description "ISA WLAN-GW Group";
                    }

                    ///nokia-conf/configure/router/wlan-gw/xconnect/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/wlan-gw/xconnect/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container xconnect

            } // container wlan-gw

            ///nokia-conf/configure/router/mss-adjust
            container mss-adjust {
                presence "Enable the mss-adjust context";
                description "Enable the mss-adjust context";

                ///nokia-conf/configure/router/mss-adjust/nat-group
                leaf nat-group {
                    type uint32;
                    sros-ext:immutable;
                    mandatory true;
                    description "NAT group used for TCP-MSS adjustment";
                }

                ///nokia-conf/configure/router/mss-adjust/segment-size
                leaf segment-size {
                    type uint32 {
                        range "160..10240";
                    }
                    mandatory true;
                    description "TCP-MSS option value in transmitted TCP SYN requests";
                }

                ///nokia-conf/configure/router/mss-adjust/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/mss-adjust/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container mss-adjust

            ///nokia-conf/configure/router/reassembly
            container reassembly {
                presence "Enable the reassembly context";
                description "Enable the reassembly context";

                ///nokia-conf/configure/router/reassembly/nat-group
                leaf nat-group {
                    type uint32;
                    sros-ext:immutable;
                    description "NAT group (including WLAN Gateway group) that executes the reassembly";
                }

                ///nokia-conf/configure/router/reassembly/to-base-network
                leaf to-base-network {
                    type boolean;
                    default "false";
                    description "Allow reassembled traffic sent to network interface";
                }

                ///nokia-conf/configure/router/reassembly/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/reassembly/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container reassembly

            ///nokia-conf/configure/router/sfm-overload
            container sfm-overload {
                presence "Enable the sfm-overload context";
                description "Enable the sfm-overload context";

                ///nokia-conf/configure/router/sfm-overload/holdoff-time
                leaf holdoff-time {
                    type uint32 {
                        range "1..600";
                    }
                    units "seconds";
                    description "Delay in detecting SFM failures and setting overload";
                }

            } // container sfm-overload

            ///nokia-conf/configure/router/confederation
            container confederation {
                when "../router-name = 'Base'" {
                    description "only allowed in the Base router";
                }
                description "Enter the confederation context";

                ///nokia-conf/configure/router/confederation/confed-as-num
                leaf confed-as-num {
                    type inet:as-number {
                        range "1..4294967295";
                    }
                    description "Confederation AS number";
                }

                ///nokia-conf/configure/router/confederation/members
                list members {
                    key "as-number";
                    max-elements 256;
                    description "Add a list entry for members";

                    ///nokia-conf/configure/router/confederation/members/as-number
                    leaf as-number {
                        type inet:as-number {
                            range "1..4294967295";
                        }
                        description "Confederation member AS number";
                    }

                } // list members

            } // container confederation

            ///nokia-conf/configure/router/interface
            list interface {
                key "interface-name";
                description "Enter the interface list instance";

                ///nokia-conf/configure/router/interface/interface-name
                leaf interface-name {
                    type types-sros:interface-name;
                    description "Router interface name";
                }

                ///nokia-conf/configure/router/interface/flavor
                leaf flavor {
                    when "not(../../router-name = 'management')" {
                        description "not allowed in the management router";
                    }
                    type enumeration {
                        enum regular {
                            value 0;
                            description "Regular network interface";
                        }
                        enum pdn {
                            value 2;
                            description "PDN interface";
                        }
                        enum unnumbered-mpls-tp {
                            value 3;
                            description "Unnumbered MPLS-TP interface";
                        }
                        enum control-tunnel {
                            status obsolete;
                            description "Creates a network interface of control-tunnel type.";
                        }
                        enum gmpls-loopback {
                            status obsolete;
                            description "Creates a network interface of gmpls-loopback type.";
                        }
                    }
                    sros-ext:immutable;
                    default "regular";
                    description "Network interface flavor";
                }

                ///nokia-conf/configure/router/interface/cpu-protection
                leaf cpu-protection {
                    when "../../router-name = 'Base' and not(../flavor = 'control-tunnel') and not(../flavor = 'pdn')" {
                        description "only allowed in router Base, and not in an interface with flavor control-tunnel|pdn";
                    }
                    type leafref {
                        path "../../../system/security/cpu-protection/policy/policy-id";
                    }
                    description "CPU protection policy ID for the interface";
                }

                ///nokia-conf/configure/router/interface/description
                leaf description {
                    when "not(../../router-name = 'management')" {
                        description "not allowed in the management router";
                    }
                    type types-sros:very-long-description;
                    description "Text description";
                }

                ///nokia-conf/configure/router/interface/ip-mtu
                leaf ip-mtu {
                    when "not(../../router-name = 'management')" {
                        description "not allowed in the management router";
                    }
                    type types-router:ip-mtu-ext;
                    units "bytes";
                    description "Interface IP MTU";
                }

                ///nokia-conf/configure/router/interface/admin-state
                leaf admin-state {
                    when "not(../../router-name = 'management')" {
                        description "not allowed in the management router";
                    }
                    type types-sros:admin-state;
                    description "Administrative state of the interface";
                }

                ///nokia-conf/configure/router/interface/strip-label
                leaf strip-label {
                    when "../../router-name = 'Base' and not(../flavor = 'pdn') and not(../flavor = 'control-tunnel') and not(../flavor = 'gmpls-loopback') and not(../interface-name = 'system')" {
                        description "only allowed in router Base, and not in the system interface nor in an interface with flavor control-tunnel|gmpls-loopback|pdn";
                    }
                    type boolean;
                    default "false";
                    description "Strip all MPLS labels before PBR processing";
                }

                ///nokia-conf/configure/router/interface/dist-cpu-protection
                leaf dist-cpu-protection {
                    when "../../router-name = 'Base' and not(../flavor = 'control-tunnel') and not(../flavor = 'gmpls-loopback') and not(../flavor = 'pdn') and not(../interface-name = 'system')" {
                        description "only allowed in router Base, and not in the system interface nor in an interface with flavor control-tunnel|gmpls-loopback|pdn";
                    }
                    type leafref {
                        path "../../../system/security/dist-cpu-protection/policy/policy-name";
                    }
                    description "DCP policy name for interface";
                }

                ///nokia-conf/configure/router/interface/mac
                leaf mac {
                    when "../../router-name = 'Base' and not(../flavor = 'control-tunnel') and not(../flavor = 'pdn')" {
                        description "only allowed in router Base, and not in an interface with flavor control-tunnel|pdn";
                    }
                    type types-sros:mac-unicast-address;
                    description "MAC address of the interface";
                }

                ///nokia-conf/configure/router/interface/tos-marking-state
                leaf tos-marking-state {
                    when "../../router-name = 'Base' and not(../flavor = 'control-tunnel')" {
                        description "only allowed in router Base, and not in an interface with flavor control-tunnel";
                    }
                    type types-router:tos-marking-state;
                    default "trusted";
                    description "TOS marking state";
                }

                ///nokia-conf/configure/router/interface/ingress-stats
                leaf ingress-stats {
                    when "../../router-name = 'Base' and not(../flavor = 'control-tunnel')" {
                        description "only allowed in router Base, and not in an interface with flavor control-tunnel";
                    }
                    type boolean;
                    default "false";
                    description "Collect ingress statistics";
                }

                ///nokia-conf/configure/router/interface/mac-accounting
                leaf mac-accounting {
                    when "../../router-name = 'Base' and not(../flavor = 'control-tunnel') and not(../flavor = 'pdn')" {
                        description "only allowed in router Base, and not in an interface with flavor control-tunnel|pdn";
                    }
                    type boolean;
                    default "false";
                    description "Enable MAC accounting functionality";
                }

                ///nokia-conf/configure/router/interface/vas-if-type
                leaf vas-if-type {
                    when "../../router-name = 'Base' and ../flavor = 'regular' and not(../interface-name = 'system')" {
                        description "only allowed in regular interfaces in router Base, and not in the system interface";
                    }
                    type types-router:vas-if-type;
                    description "VAS interface type";
                }

                ///nokia-conf/configure/router/interface/gre-termination
                leaf gre-termination {
                    when "../../router-name = 'Base' and ../flavor = 'regular' and not(../interface-name = 'system')" {
                        description "only allowed in regular interfaces in router Base, and not in the system interface";
                    }
                    type boolean;
                    default "false";
                    description "Terminate GRE packets on primary subnet of interface";
                }

                ///nokia-conf/configure/router/interface/urpf-selected-vprns
                leaf urpf-selected-vprns {
                    when "../../router-name = 'Base' and ../flavor = 'regular' and not(../interface-name = 'system')" {
                        description "only allowed in regular interfaces in router Base, and not in the system interface";
                    }
                    type boolean;
                    default "false";
                    description "Enable uRPF checking of incoming traffic on interface";
                }

                ///nokia-conf/configure/router/interface/accounting-policy
                leaf accounting-policy {
                    type leafref {
                        path "../../../log/accounting-policy/policy-id";
                    }
                    description "Accounting policy to apply to the router interface";
                }

                ///nokia-conf/configure/router/interface/collect-stats
                leaf collect-stats {
                    type boolean;
                    default "false";
                    description "Enable collection of statistics on the router interface";
                }

                ///nokia-conf/configure/router/interface/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/interface/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/router/interface/port-binding
                choice port-binding {
                    ///nokia-conf/configure/router/interface/port-binding/port-encap
                    case port-encap {

                        ///nokia-conf/configure/router/interface/port-binding/port-encap/port
                        leaf port {
                            when "not(../../router-name = 'management') and not(../flavor = 'control-tunnel') and not(../flavor = 'gmpls-loopback') and not(../interface-name = 'system')" {
                                description "not allowed in the management router, and not in the system interface nor in an interface with flavor control-tunnel|gmpls-loopback";
                            }
                            type types-router:router-port;
                            description "Port to bind to this interface";
                        }

                    }
                    ///nokia-conf/configure/router/interface/port-binding/loopback
                    case loopback {

                        ///nokia-conf/configure/router/interface/port-binding/loopback/loopback
                        leaf loopback {
                            when "not(../../router-name = 'management') and not(../flavor = 'unnumbered-mpls-tp') and not(../interface-name = 'system')" {
                                description "not allowed in the management router, and not in the system interface nor in an interface with flavor unnumbered-mpls-tp";
                            }
                            type empty;
                            description "Use interface as a loopback interface";
                        }

                    }
                }
                ///nokia-conf/configure/router/interface/egress
                container egress {
                    when "../../router-name = 'Base' and not(../flavor = 'control-tunnel') and not(../interface-name = 'system')" {
                        description "only allowed in the Base router, not in the system interface or an interface with flavor control-tunnel";
                    }
                    description "Enter the egress context";

                    ///nokia-conf/configure/router/interface/egress/filter
                    container filter {
                        description "Enter the filter context";

                        ///nokia-conf/configure/router/interface/egress/filter/ip
                        leaf ip {
                            type leafref {
                                path "../../../../../filter/ip-filter/filter-name";
                            }
                            description "IPv4 filter policy name";
                        }

                        ///nokia-conf/configure/router/interface/egress/filter/ipv6
                        leaf ipv6 {
                            type leafref {
                                path "../../../../../filter/ipv6-filter/filter-name";
                            }
                            description "IPv6 filter policy name";
                        }

                    } // container filter

                    ///nokia-conf/configure/router/interface/egress/qos
                    container qos {
                        description "Enable the qos context";

                        ///nokia-conf/configure/router/interface/egress/qos/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/egress/qos/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/egress/qos/egress-agg-rate
                        choice egress-agg-rate {
                            default "agg-rate";
                            ///nokia-conf/configure/router/interface/egress/qos/egress-agg-rate/agg-rate
                            case agg-rate {

                                ///nokia-conf/configure/router/interface/egress/qos/egress-agg-rate/agg-rate/agg-rate
                                container agg-rate {
                                    description "Enter the agg-rate context";

                                    ///nokia-conf/configure/router/interface/egress/qos/egress-agg-rate/agg-rate/agg-rate/rate
                                    container rate {
                                        description "Enter the rate context";

                                        ///nokia-conf/configure/router/interface/egress/qos/egress-agg-rate/agg-rate/agg-rate/rate/pir
                                        leaf pir {
                                            type union {
                                                type int32 {
                                                    range "1..400000000";
                                                }
                                                type enumeration {
                                                    enum max                            { value -1; }
                                                }
                                            }
                                            units "kilobps";
                                            default "max";
                                            description "Administrative PIR";
                                        }

                                        ///nokia-conf/configure/router/interface/egress/qos/egress-agg-rate/agg-rate/agg-rate/rate/cir
                                        leaf cir {
                                            type union {
                                                type int32 {
                                                    range "0..400000000";
                                                }
                                                type enumeration {
                                                    enum max                            { value -1; }
                                                }
                                            }
                                            units "kilobps";
                                            default "0";
                                            description "Administrative CIR";
                                        }

                                    } // container rate

                                } // container agg-rate

                            }
                            ///nokia-conf/configure/router/interface/egress/qos/egress-agg-rate/percent-agg-rate
                            case percent-agg-rate {

                                ///nokia-conf/configure/router/interface/egress/qos/egress-agg-rate/percent-agg-rate/percent-agg-rate
                                container percent-agg-rate {
                                    description "Enter the percent-agg-rate context";

                                    ///nokia-conf/configure/router/interface/egress/qos/egress-agg-rate/percent-agg-rate/percent-agg-rate/pir
                                    leaf pir {
                                        type decimal64 {
                                            range "0.01..100.00";
                                            fraction-digits 2;
                                        }
                                        description "PIR percent rate";
                                    }

                                    ///nokia-conf/configure/router/interface/egress/qos/egress-agg-rate/percent-agg-rate/percent-agg-rate/cir
                                    leaf cir {
                                        type decimal64 {
                                            range "0.00..100.00";
                                            fraction-digits 2;
                                        }
                                        description "CIR percent rate";
                                    }

                                } // container percent-agg-rate

                            }
                        }
                        ///nokia-conf/configure/router/interface/egress/qos/vlan-qos-policy
                        container vlan-qos-policy {
                            description "Enter the vlan-qos-policy context";

                            ///nokia-conf/configure/router/interface/egress/qos/vlan-qos-policy/policy-name
                            leaf policy-name {
                                type leafref {
                                    path "../../../../../../qos/vlan-qos-policy/vlan-qos-policy-name";
                                }
                                description "VLAN QoS policy name";
                            }

                            ///nokia-conf/configure/router/interface/egress/qos/vlan-qos-policy/port-redirect
                            leaf port-redirect {
                                type boolean;
                                default "false";
                                description "Enable port-redirect for the egress VLAN QoS policy";
                            }

                        } // container vlan-qos-policy

                        ///nokia-conf/configure/router/interface/egress/qos/egress-remark-policy
                        container egress-remark-policy {
                            description "Enter the egress-remark-policy context";

                            ///nokia-conf/configure/router/interface/egress/qos/egress-remark-policy/policy-name
                            leaf policy-name {
                                type leafref {
                                    path "../../../../../../qos/egress-remark-policy/egress-remark-policy-name";
                                }
                                description "Egress remark policy name";
                            }

                        } // container egress-remark-policy

                    } // container qos

                } // container egress

                ///nokia-conf/configure/router/interface/ingress
                container ingress {
                    when "../../router-name = 'Base' and not(../flavor = 'control-tunnel') and not(../interface-name = 'system')" {
                        description "only allowed in router Base, and not in the system interface nor in an interface with flavor control-tunnel";
                    }
                    description "Enter the ingress context";

                    ///nokia-conf/configure/router/interface/ingress/policy-accounting
                    leaf policy-accounting {
                        when "not(../../flavor = 'pdn')" {
                            description "not allowed in an interface with flavor pdn";
                        }
                        type leafref {
                            path "../../../../routing-options/policy-accounting/policy-acct-template/name";
                        }
                        description "Policy accounting template applied to the interface";
                    }

                    ///nokia-conf/configure/router/interface/ingress/destination-class-lookup
                    leaf destination-class-lookup {
                        when "../../flavor = 'regular'" {
                            description "only allowed in regular interfaces in router Base, and not in the system interface";
                        }
                        type boolean;
                        default "false";
                        description "Enable BGP destination class lookup";
                    }

                    ///nokia-conf/configure/router/interface/ingress/ip-filter-pair
                    leaf ip-filter-pair {
                        type boolean;
                        default "false";
                        description "Enable pairing IPv4 and IPv6 filters";
                    }

                    ///nokia-conf/configure/router/interface/ingress/filter
                    container filter {
                        description "Enter the filter context";

                        ///nokia-conf/configure/router/interface/ingress/filter/ip
                        leaf ip {
                            type leafref {
                                path "../../../../../filter/ip-filter/filter-name";
                            }
                            description "IPv4 filter policy name";
                        }

                        ///nokia-conf/configure/router/interface/ingress/filter/ipv6
                        leaf ipv6 {
                            type leafref {
                                path "../../../../../filter/ipv6-filter/filter-name";
                            }
                            description "IPv6 filter policy name";
                        }

                    } // container filter

                    ///nokia-conf/configure/router/interface/ingress/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-conf/configure/router/interface/ingress/qos/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/ingress/qos/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/ingress/qos/network-ingress
                        container network-ingress {
                            description "Enter the network-ingress context";

                            ///nokia-conf/configure/router/interface/ingress/qos/network-ingress/policy-name
                            leaf policy-name {
                                type leafref {
                                    path "../../../../../../qos/network-ingress/network-ingress-policy-name";
                                }
                                description "Network ingress QoS policy name";
                            }

                        } // container network-ingress

                    } // container qos

                } // container ingress

                ///nokia-conf/configure/router/interface/ldp-sync-timer
                container ldp-sync-timer {
                    when "../../router-name = 'Base' and not(../flavor = 'control-tunnel') and not(../flavor = 'pdn') and not(../interface-name = 'system')" {
                        description "only allowed in router Base, and not in the system interface nor in an interface with flavor control-tunnel|pdn";
                    }
                    description "Enter the ldp-sync-timer context";

                    ///nokia-conf/configure/router/interface/ldp-sync-timer/seconds
                    leaf seconds {
                        type types-router:ldp-sync-timer;
                        description "Time interval for the LDP synchronization timer";
                    }

                    ///nokia-conf/configure/router/interface/ldp-sync-timer/end-of-lib
                    leaf end-of-lib {
                        type boolean;
                        default "false";
                        description "Terminate LDP synchronization timer";
                    }

                } // container ldp-sync-timer

                ///nokia-conf/configure/router/interface/load-balancing
                container load-balancing {
                    when "../../router-name = 'Base' and not(../flavor = 'pdn') and not(../flavor = 'control-tunnel') and not(../interface-name = 'system')" {
                        description "only allowed in router Base, and not in the system interface nor in an interface with flavor control-tunnel|pdn";
                    }
                    description "Enter the load-balancing context";

                    ///nokia-conf/configure/router/interface/load-balancing/lsr-load-balancing
                    leaf lsr-load-balancing {
                        type types-router:lsr-load-balancing;
                        description "LSR load-balancing algorithm";
                    }

                    ///nokia-conf/configure/router/interface/load-balancing/ip-load-balancing
                    leaf ip-load-balancing {
                        type types-router:ip-load-balancing;
                        default "both";
                        description "IP load-balancing algorithm";
                    }

                    ///nokia-conf/configure/router/interface/load-balancing/spi-load-balancing
                    leaf spi-load-balancing {
                        type boolean;
                        default "false";
                        description "Enable SPI use in hashing";
                    }

                    ///nokia-conf/configure/router/interface/load-balancing/teid-load-balancing
                    leaf teid-load-balancing {
                        type boolean;
                        default "false";
                        description "Enable use of TEID in hashing";
                    }

                    ///nokia-conf/configure/router/interface/load-balancing/flow-label-load-balancing
                    leaf flow-label-load-balancing {
                        type boolean;
                        default "false";
                        description "Enable flow label load balancing";
                    }

                } // container load-balancing

                ///nokia-conf/configure/router/interface/lag
                container lag {
                    when "../../router-name = 'Base' and not(../flavor = 'control-tunnel') and not(../flavor = 'gmpls-loopback') and not(../flavor = 'pdn') and not(../interface-name = 'system')" {
                        description "only allowed in router Base, and not in the system interface nor in an interface with flavor control-tunnel|gmpls-loopback|pdn";
                    }
                    description "Enter the lag context";

                    ///nokia-conf/configure/router/interface/lag/link-map-profile
                    leaf link-map-profile {
                        type types-services:link-map-profile-id;
                        description "LAG link map profile for a SAP or network interface";
                    }

                    ///nokia-conf/configure/router/interface/lag/per-link-hash
                    container per-link-hash {
                        when "not(../../flavor = 'unnumbered-mpls-tp')" {
                            description "not allowed on interfaces with flavor unnumbered-mpls-tp";
                        }
                        description "Enter the per-link-hash context";

                        ///nokia-conf/configure/router/interface/lag/per-link-hash/class
                        leaf class {
                            type types-services:sap-lag-per-link-hash-class;
                            default "1";
                            description "Class used on LAG egress using weighted per-link-hash";
                        }

                        ///nokia-conf/configure/router/interface/lag/per-link-hash/weight
                        leaf weight {
                            type types-services:sap-lag-per-link-hash-weight;
                            default "1";
                            description "Weight used on LAG egress using weighted per-link-hash";
                        }

                    } // container per-link-hash

                } // container lag

                ///nokia-conf/configure/router/interface/ptp-hw-assist
                container ptp-hw-assist {
                    when "../../router-name = 'Base'" {
                        description "only allowed in the Base router";
                    }
                    description "Enter the ptp-hw-assist context";

                    ///nokia-conf/configure/router/interface/ptp-hw-assist/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the PTP time stamping assist";
                    }

                    ///nokia-conf/configure/router/interface/ptp-hw-assist/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/interface/ptp-hw-assist/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container ptp-hw-assist

                ///nokia-conf/configure/router/interface/autoconfigure
                container autoconfigure {
                    description "Enter the autoconfigure context";

                    ///nokia-conf/configure/router/interface/autoconfigure/ipv4
                    container ipv4 {
                        when "not(../../../router-name = 'management')" {
                            description "not allowed in the management router";
                        }
                        description "Enter the ipv4 context";

                        ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client
                        container dhcp-client {
                            when "../../../../router-name = 'Base' and not(../../../interface-name = 'system') and not(../../../flavor = 'pdn')" {
                                description "only allowed in router Base, and not in the system interface nor in an interface with flavor pdn";
                            }
                            presence "Enable the dhcp-client context";
                            description "Enable the dhcp-client context";

                            ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "disable";
                                description "Administrative state of the DHCP client";
                            }

                            ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/lease-time
                            leaf lease-time {
                                type union {
                                    type types-sros:time-duration {
                                        range "10..315446399";
                                    }
                                    type enumeration {
                                        enum infinite                       { value -1; }
                                    }
                                }
                                sros-ext:auto-restart-to-modify;
                                default "86400";
                                description "Lease time the DHCP client requests";
                            }

                            ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                sros-ext:auto-restart-to-modify;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                sros-ext:auto-restart-to-modify;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/class-id
                            container class-id {
                                sros-ext:auto-restart-to-modify;
                                description "Enter the class-id context";

                                ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/class-id/format
                                choice format {
                                    ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/class-id/format/ascii-string
                                    case ascii-string {

                                        ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/class-id/format/ascii-string/ascii-string
                                        leaf ascii-string {
                                            type string {
                                                length "1..127";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            description "Vendor class identifier as an ASCII string";
                                        }

                                    }
                                    ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/class-id/format/hex-string
                                    case hex-string {

                                        ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/class-id/format/hex-string/hex-string
                                        leaf hex-string {
                                            type types-sros:hex-string {
                                                length "1..256";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            description "Vendor class identifier as a hexadecimal string";
                                        }

                                    }
                                }
                            } // container class-id

                            ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/client-id
                            container client-id {
                                sros-ext:auto-restart-to-modify;
                                description "Enter the client-id context";

                                ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/client-id/format
                                choice format {
                                    ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/client-id/format/mac
                                    case mac {

                                        ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/client-id/format/mac/mac
                                        leaf mac {
                                            type empty;
                                            sros-ext:auto-restart-to-modify;
                                            description "MAC as client identifier";
                                        }

                                    }
                                    ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/client-id/format/interface
                                    case interface {

                                        ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/client-id/format/interface/interface
                                        leaf interface {
                                            type empty;
                                            sros-ext:auto-restart-to-modify;
                                            description "Interface name as client identifier";
                                        }

                                    }
                                    ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/client-id/format/ascii-string
                                    case ascii-string {

                                        ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/client-id/format/ascii-string/ascii-string
                                        leaf ascii-string {
                                            type string {
                                                length "1..127";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            description "Client identifier as an ASCII string";
                                        }

                                    }
                                    ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/client-id/format/hex-string
                                    case hex-string {

                                        ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/client-id/format/hex-string/hex-string
                                        leaf hex-string {
                                            type types-sros:hex-string {
                                                length "1..256";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            description "Client identifier as a hexadecimal string";
                                        }

                                    }
                                }
                            } // container client-id

                            ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/request-options
                            container request-options {
                                sros-ext:auto-restart-to-modify;
                                description "Enter the request-options context";

                                ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/request-options/dns-server
                                leaf dns-server {
                                    type boolean;
                                    sros-ext:auto-restart-to-modify;
                                    default "false";
                                    description "DNS server option";
                                }

                                ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/request-options/router
                                leaf router {
                                    type boolean;
                                    sros-ext:auto-restart-to-modify;
                                    default "false";
                                    description "Router option";
                                }

                                ///nokia-conf/configure/router/interface/autoconfigure/ipv4/dhcp-client/request-options/static-route
                                leaf static-route {
                                    type boolean;
                                    sros-ext:auto-restart-to-modify;
                                    default "false";
                                    description "Static route option";
                                }

                            } // container request-options

                        } // container dhcp-client

                    } // container ipv4

                } // container autoconfigure

                ///nokia-conf/configure/router/interface/ipsec
                container ipsec {
                    presence "Enable the ipsec context";
                    description "Enable the ipsec context";

                    ///nokia-conf/configure/router/interface/ipsec/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of IPsec secured interface";
                    }

                    ///nokia-conf/configure/router/interface/ipsec/tunnel-group
                    leaf tunnel-group {
                        type leafref {
                            path "../../../../isa/tunnel-group/id";
                        }
                        sros-ext:immutable;
                        mandatory true;
                        description "Tunnel group ID";
                    }

                    ///nokia-conf/configure/router/interface/ipsec/public-sap
                    leaf public-sap {
                        type uint32 {
                            range "0..4094";
                        }
                        sros-ext:immutable;
                        mandatory true;
                        description "Public SAP ID";
                    }

                    ///nokia-conf/configure/router/interface/ipsec/ip-exception
                    leaf ip-exception {
                        type leafref {
                            path "../../../../filter/ip-exception/filter-name";
                        }
                        description "IP exception filter";
                    }

                    ///nokia-conf/configure/router/interface/ipsec/ipv6-exception
                    leaf ipv6-exception {
                        type leafref {
                            path "../../../../filter/ipv6-exception/filter-name";
                        }
                        description "IPv6 filter exception used to bypass encryption";
                    }

                    ///nokia-conf/configure/router/interface/ipsec/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/interface/ipsec/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel
                    list ipsec-tunnel {
                        key "name";
                        description "Enter the ipsec-tunnel list instance";

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/name
                        leaf name {
                            type types-sros:named-item {
                                pattern "[a-zA-Z]+.*" {
                                    error-message "IPSec tunnel name cannot start with a number";
                                }
                            }
                            description "IPsec tunnel name";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/private-sap
                        leaf private-sap {
                            type uint32 {
                                range "0..4094";
                            }
                            sros-ext:immutable;
                            sros-ext:auto-restart-to-modify;
                            mandatory true;
                            description "Private SAP ID";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the IPsec tunnel";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/clear-df-bit
                        leaf clear-df-bit {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "false";
                            description "Reset the DF bit to 0 in all payload IP packets";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/copy-traffic-class-upon-decapsulation
                        leaf copy-traffic-class-upon-decapsulation {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "false";
                            description "Enable traffic class copy upon decapsulation";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/encapsulated-ip-mtu
                        leaf encapsulated-ip-mtu {
                            type uint32 {
                                range "512..9000";
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "Maximum size of the encapsulated tunnel packet";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/ip-mtu
                        leaf ip-mtu {
                            type uint32 {
                                range "512..9000";
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "Private MTU of the IPsec tunnel";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/replay-window
                        leaf replay-window {
                            type uint32 {
                                range "32|64|128|256|512";
                            }
                            units "packets";
                            sros-ext:auto-restart-to-modify;
                            description "Anti-replay window size";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/public-tcp-mss-adjust
                        leaf public-tcp-mss-adjust {
                            type union {
                                type uint32 {
                                    range "512..9000";
                                }
                                type enumeration {
                                    enum auto                           { value 0; }
                                }
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "TCP maximum segment size (MSS) on the public network";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/private-tcp-mss-adjust
                        leaf private-tcp-mss-adjust {
                            type int32 {
                                range "512..9000";
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "TCP maximum segment size (MSS) adjustment";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/local-gateway-address-override
                        leaf local-gateway-address-override {
                            type types-sros:ip-address;
                            sros-ext:auto-restart-to-modify;
                            description "Local IPsec tunnel endpoint address";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/remote-gateway-address
                        leaf remote-gateway-address {
                            type types-sros:ip-address;
                            sros-ext:auto-restart-to-modify;
                            description "Remote IPsec tunnel endpoint address";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/private-service
                        leaf private-service {
                            type types-services:service-name;
                            sros-ext:immutable;
                            sros-ext:auto-restart-to-modify;
                            description "Private service name";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/propagate-pmtu-v4
                        leaf propagate-pmtu-v4 {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "true";
                            description "Enable propagation of the path MTU to IPv4 hosts";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/propagate-pmtu-v6
                        leaf propagate-pmtu-v6 {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "true";
                            description "Enable propagation of the path MTU to IPv6 hosts";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/pmtu-discovery-aging
                        leaf pmtu-discovery-aging {
                            type uint32 {
                                range "900..3600";
                            }
                            units "seconds";
                            sros-ext:auto-restart-to-modify;
                            default "900";
                            description "Aging out time of the learned path MTU";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            sros-ext:auto-restart-to-modify;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            sros-ext:auto-restart-to-modify;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/bfd
                        container bfd {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the bfd context";

                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/bfd/bfd-designate
                            leaf bfd-designate {
                                type boolean;
                                sros-ext:auto-restart-to-modify;
                                default "false";
                                description "Designate IPsec tunnel to carry BFD traffic";
                            }

                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/bfd/bfd-liveness
                            container bfd-liveness {
                                presence "Enable the bfd-liveness context";
                                sros-ext:auto-restart-to-modify;
                                description "Enable the bfd-liveness context";

                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/bfd/bfd-liveness/dest-ip
                                leaf dest-ip {
                                    type types-sros:ipv4-unicast-address;
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Destination address used for the BFD session";
                                }

                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/bfd/bfd-liveness/interface
                                leaf interface {
                                    type types-sros:interface-name;
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Name of the interface used by the BFD session";
                                }

                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/bfd/bfd-liveness/service-name
                                leaf service-name {
                                    type types-services:service-name;
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Administrative service name";
                                }

                            } // container bfd-liveness

                        } // container bfd

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/icmp6-generation
                        container icmp6-generation {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the icmp6-generation context";

                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/icmp6-generation/packet-too-big
                            container packet-too-big {
                                sros-ext:auto-restart-to-modify;
                                description "Enter the packet-too-big context";

                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/icmp6-generation/packet-too-big/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    sros-ext:auto-restart-to-modify;
                                    default "enable";
                                    description "Administrative state of Packet Too Big message sends";
                                }

                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/icmp6-generation/packet-too-big/message-count
                                leaf message-count {
                                    type uint32 {
                                        range "10..1000";
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    default "100";
                                    description "Maximum number of ICMPv6 PTB messages that can be sent";
                                }

                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/icmp6-generation/packet-too-big/interval
                                leaf interval {
                                    type uint32 {
                                        range "1..60";
                                    }
                                    units "seconds";
                                    sros-ext:auto-restart-to-modify;
                                    default "10";
                                    description "Interval for sending Packet Too Big messages";
                                }

                            } // container packet-too-big

                        } // container icmp6-generation

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange
                        container key-exchange {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the key-exchange context";

                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type
                            choice type {
                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic
                                case dynamic {

                                    ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic
                                    container dynamic {
                                        presence "Enable the dynamic context";
                                        sros-ext:auto-restart-to-modify;
                                        description "Enable the dynamic context";

                                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/auto-establish
                                        leaf auto-establish {
                                            type boolean;
                                            sros-ext:auto-restart-to-modify;
                                            default "false";
                                            description "Attempt to establish a phase 1 exchange automatically";
                                        }

                                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-policy
                                        leaf ike-policy {
                                            type leafref {
                                                path "../../../../../../../ipsec/ike-policy/id";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            description "IKE policy ID";
                                        }

                                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ipsec-transform
                                        leaf-list ipsec-transform {
                                            type leafref {
                                                path "../../../../../../../ipsec/ipsec-transform/id";
                                            }
                                            max-elements 4;
                                            sros-ext:auto-restart-to-modify;
                                            description "IPsec transform IDs used by the dynamic key";
                                        }

                                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/pre-shared-key
                                        leaf pre-shared-key {
                                            type types-sros:encrypted-leaf {
                                                length "1..115";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            description "Pre-shared key for authentication";
                                        }

                                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert
                                        container cert {
                                            sros-ext:auto-restart-to-modify;
                                            description "Enter the cert context";

                                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/cert-profile
                                            leaf cert-profile {
                                                type leafref {
                                                    path "../../../../../../../../ipsec/cert-profile/name";
                                                }
                                                description "Certificate profile name";
                                            }

                                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/trust-anchor-profile
                                            leaf trust-anchor-profile {
                                                type leafref {
                                                    path "../../../../../../../../ipsec/trust-anchor-profile/name";
                                                }
                                                description "Trust anchor profile name";
                                            }

                                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify
                                            container status-verify {
                                                sros-ext:auto-restart-to-modify;
                                                description "Enter the status-verify context";

                                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify/default-result
                                                leaf default-result {
                                                    type enumeration {
                                                        enum revoked                        { value 0; }
                                                        enum good                           { value 1; }
                                                    }
                                                    default "revoked";
                                                    description "Default result for Certificate Status Verification";
                                                }

                                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify/primary
                                                leaf primary {
                                                    type types-ipsec:cert-revocation-status;
                                                    default "crl";
                                                    description "Primary method of CSV to verify the revocation status";
                                                }

                                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify/secondary
                                                leaf secondary {
                                                    type types-ipsec:cert-revocation-status-with-none;
                                                    default "none";
                                                    description "Secondary method used to verify certificate revocation";
                                                }

                                            } // container status-verify

                                        } // container cert

                                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id
                                        container id {
                                            sros-ext:auto-restart-to-modify;
                                            description "Enter the id context";

                                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id
                                            choice id {
                                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv4
                                                case ipv4 {

                                                    ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv4/ipv4
                                                    leaf ipv4 {
                                                        type types-sros:ipv4-unicast-address;
                                                        sros-ext:auto-restart-to-modify;
                                                        description "IPv4 as the local ID type";
                                                    }

                                                }
                                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv6
                                                case ipv6 {

                                                    ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv6/ipv6
                                                    leaf ipv6 {
                                                        type types-sros:ip-address;
                                                        sros-ext:auto-restart-to-modify;
                                                        description "IPv6 used as the local IKE ID type";
                                                    }

                                                }
                                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/fqdn
                                                case fqdn {

                                                    ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/fqdn/fqdn
                                                    leaf fqdn {
                                                        type types-sros:fully-qualified-domain-name;
                                                        sros-ext:auto-restart-to-modify;
                                                        description "FQDN used as the local ID IKE type";
                                                    }

                                                }
                                            }
                                        } // container id

                                    } // container dynamic

                                }
                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/manual
                                case manual {

                                    ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual
                                    container manual {
                                        presence "Enable the manual context";
                                        sros-ext:auto-restart-to-modify;
                                        description "Enable the manual context";

                                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys
                                        list keys {
                                            key "security-association direction";
                                            description "Enter the keys list instance";

                                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/security-association
                                            leaf security-association {
                                                type uint32 {
                                                    range "1..16";
                                                }
                                                description "SA entry ID";
                                            }

                                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/direction
                                            leaf direction {
                                                type enumeration {
                                                    enum inbound                        { value 1; }
                                                    enum outbound                       { value 2; }
                                                }
                                                description "Direction of the IPsec tunnel";
                                            }

                                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/ipsec-transform
                                            leaf ipsec-transform {
                                                type leafref {
                                                    path "../../../../../../../../ipsec/ipsec-transform/id";
                                                }
                                                sros-ext:immutable;
                                                mandatory true;
                                                description "Transform entry used by manual SAs";
                                            }

                                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/spi
                                            leaf spi {
                                                type uint32 {
                                                    range "256..16383";
                                                }
                                                sros-ext:immutable;
                                                mandatory true;
                                                description "SPI of inbound and outbound packets";
                                            }

                                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/authentication-key
                                            leaf authentication-key {
                                                type types-sros:hex-string {
                                                    length "1..130";
                                                }
                                                sros-ext:immutable;
                                                description "Key used for the authentication algorithm";
                                            }

                                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/encryption-key
                                            leaf encryption-key {
                                                type types-sros:hex-string {
                                                    length "1..66";
                                                }
                                                sros-ext:immutable;
                                                description "Key used for the encryption algorithm";
                                            }

                                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/apply-groups
                                            leaf-list apply-groups {
                                                type leafref {
                                                    path "../../../../../../../../groups/group/name";
                                                }
                                                max-elements 8;
                                                ordered-by user;
                                                description "Apply a configuration group at this level";
                                            }

                                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/apply-groups-exclude
                                            leaf-list apply-groups-exclude {
                                                type leafref {
                                                    path "../../../../../../../../groups/group/name";
                                                }
                                                max-elements 8;
                                                ordered-by user;
                                                description "Exclude a configuration group at this level";
                                            }

                                        } // list keys

                                    } // container manual

                                }
                            }
                        } // container key-exchange

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/max-history-key-records
                        container max-history-key-records {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the max-history-key-records context";

                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/max-history-key-records/ike
                            leaf ike {
                                type uint32 {
                                    range "1..3";
                                }
                                sros-ext:auto-restart-to-modify;
                                description "Maximum number of historical IKE key records";
                            }

                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/max-history-key-records/esp
                            leaf esp {
                                type uint32 {
                                    range "1..48";
                                }
                                sros-ext:auto-restart-to-modify;
                                description "Maximum number of recent records";
                            }

                        } // container max-history-key-records

                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/icmp-generation
                        container icmp-generation {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the icmp-generation context";

                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/icmp-generation/frag-required
                            container frag-required {
                                sros-ext:auto-restart-to-modify;
                                description "Enter the frag-required context";

                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/icmp-generation/frag-required/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    sros-ext:auto-restart-to-modify;
                                    default "enable";
                                    description "Administrative state of sending ICMP messages";
                                }

                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/icmp-generation/frag-required/message-count
                                leaf message-count {
                                    type uint32 {
                                        range "10..1000";
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    default "100";
                                    description "Maximum number of ICMP messages that can be sent";
                                }

                                ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/icmp-generation/frag-required/interval
                                leaf interval {
                                    type uint32 {
                                        range "1..60";
                                    }
                                    units "seconds";
                                    sros-ext:auto-restart-to-modify;
                                    default "10";
                                    description "Interval for sending ICMP messages";
                                }

                            } // container frag-required

                        } // container icmp-generation


                        ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/security-policy
                        container security-policy {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the security-policy context";

                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/security-policy/id
                            leaf id {
                                type uint32;
                                sros-ext:auto-restart-to-modify;
                                description "Security policy ID for use by the tunnel";
                            }

                            ///nokia-conf/configure/router/interface/ipsec/ipsec-tunnel/security-policy/strict-match
                            leaf strict-match {
                                type boolean;
                                sros-ext:auto-restart-to-modify;
                                default "false";
                                description "Enable strict match of the security policy entry";
                            }

                        } // container security-policy

                    } // list ipsec-tunnel

                } // container ipsec

                ///nokia-conf/configure/router/interface/cflowd-parameters
                container cflowd-parameters {
                    when "../../router-name = 'Base'" {
                        description "only allowed in the Base router";
                    }
                    description "Enter the cflowd-parameters context";

                    ///nokia-conf/configure/router/interface/cflowd-parameters/sampling
                    list sampling {
                        key "sampling-type";
                        description "Enter the sampling list instance";

                        ///nokia-conf/configure/router/interface/cflowd-parameters/sampling/sampling-type
                        leaf sampling-type {
                            type types-router:cflowd-sampling;
                            description "Traffic sampling type";
                        }

                        ///nokia-conf/configure/router/interface/cflowd-parameters/sampling/type
                        leaf type {
                            when "not(../sampling-type = 'both')";
                            type enumeration {
                                enum acl                            { value 1; }
                                enum interface                      { value 2; }
                            }
                            mandatory true;
                            description "Type of cflowd analysis";
                        }

                        ///nokia-conf/configure/router/interface/cflowd-parameters/sampling/direction
                        leaf direction {
                            type enumeration {
                                enum ingress-only                   { value 1; }
                                enum egress-only                    { value 2; }
                                enum both                           { value 3; }
                            }
                            default "ingress-only";
                            description "Direction of traffic for cflowd sampling";
                        }

                        ///nokia-conf/configure/router/interface/cflowd-parameters/sampling/sample-profile
                        leaf sample-profile {
                            type union {
                                type enumeration {
                                    enum 1                              { value 1; }
                                }
                                type uint32;
                            }
                            description "Sample profile ID";
                        }

                        ///nokia-conf/configure/router/interface/cflowd-parameters/sampling/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/cflowd-parameters/sampling/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list sampling

                } // container cflowd-parameters

                ///nokia-conf/configure/router/interface/hold-time
                container hold-time {
                    when "../../router-name = 'Base' and not(../flavor = 'control-tunnel')" {
                        description "only allowed in router Base, and not in an interface with flavor control-tunnel";
                    }
                    description "Enter the hold-time context";

                    ///nokia-conf/configure/router/interface/hold-time/ipv4
                    container ipv4 {
                        description "Enter the ipv4 context";

                        ///nokia-conf/configure/router/interface/hold-time/ipv4/up
                        container up {
                            description "Enter the up context";

                            ///nokia-conf/configure/router/interface/hold-time/ipv4/up/seconds
                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Up hold time for the IP interface";
                            }

                        } // container up

                        ///nokia-conf/configure/router/interface/hold-time/ipv4/down
                        container down {
                            description "Enter the down context";

                            ///nokia-conf/configure/router/interface/hold-time/ipv4/down/seconds
                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Down hold time for the IP interface";
                            }

                            ///nokia-conf/configure/router/interface/hold-time/ipv4/down/init-only
                            leaf init-only {
                                type boolean;
                                default "false";
                                description "Apply delay only at interface configuration or reboot";
                            }

                        } // container down

                    } // container ipv4

                    ///nokia-conf/configure/router/interface/hold-time/ipv6
                    container ipv6 {
                        when "not(../../flavor = 'pdn')" {
                            description "not allowed in an interface with flavor pdn";
                        }
                        description "Enter the ipv6 context";

                        ///nokia-conf/configure/router/interface/hold-time/ipv6/up
                        container up {
                            description "Enter the up context";

                            ///nokia-conf/configure/router/interface/hold-time/ipv6/up/seconds
                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Up hold time for the IP interface";
                            }

                        } // container up

                        ///nokia-conf/configure/router/interface/hold-time/ipv6/down
                        container down {
                            description "Enter the down context";

                            ///nokia-conf/configure/router/interface/hold-time/ipv6/down/seconds
                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Down hold time for the IP interface";
                            }

                            ///nokia-conf/configure/router/interface/hold-time/ipv6/down/init-only
                            leaf init-only {
                                type boolean;
                                default "false";
                                description "Apply delay only at interface configuration or reboot";
                            }

                        } // container down

                    } // container ipv6

                } // container hold-time

                ///nokia-conf/configure/router/interface/eth-cfm
                container eth-cfm {
                    when "../../router-name = 'Base'" {
                        description "only allowed in the Base router";
                    }
                    description "Enter the eth-cfm context";

                    ///nokia-conf/configure/router/interface/eth-cfm/mep
                    list mep {
                        key "md-admin-name ma-admin-name mep-id";
                        max-elements 1;
                        description "Enter the mep list instance";

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/md-admin-name
                        leaf md-admin-name {
                            type leafref {
                                path "../../../../../eth-cfm/domain/md-admin-name";
                            }
                            description "Maintenance Domain (MD) name";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/ma-admin-name
                        leaf ma-admin-name {
                            type leafref {
                                path "../../../../../eth-cfm/domain[md-admin-name=current()/../md-admin-name]/association/ma-admin-name";
                            }
                            description "Maintenance Association (MA) name";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/mep-id
                        leaf mep-id {
                            type types-eth-cfm:mep-id-type;
                            description "Maintenance Endpoint (MEP) ID";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the MEP";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/mac-address
                        leaf mac-address {
                            type types-sros:mac-unicast-address-no-zero;
                            description "MAC address of the MEP";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/install-mep
                        leaf install-mep {
                            type boolean;
                            default "false";
                            description "Install MEP in the forwarding plane";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/facility-fault
                        leaf facility-fault {
                            type boolean;
                            default "false";
                            description "Allow the facility MEP to generate a network action";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/ccm
                        leaf ccm {
                            type boolean;
                            default "false";
                            description "Generate CCM messages";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/ccm-ltm-priority
                        leaf ccm-ltm-priority {
                            type types-eth-cfm:frame-priority;
                            default "7";
                            description "Priority of CCM and LTM messages transmitted by the MEP";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/ccm-padding-size
                        leaf ccm-padding-size {
                            type uint32 {
                                range "3..1500";
                            }
                            description "Number of octets of padding to insert in CCM packets";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/ccm-tlv-ignore
                        leaf-list ccm-tlv-ignore {
                            type types-eth-cfm:ccm-tlv-ignore-type;
                            max-elements 2;
                            description "TLV to ignore on reception";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/low-priority-defect
                        leaf low-priority-defect {
                            type types-eth-cfm:lowest-alarm-priority;
                            default "mac-rem-err-xcon";
                            description "Lowest priority defect for fault alarm generation";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/collect-lmm-stats
                        leaf collect-lmm-stats {
                            type boolean;
                            default "false";
                            description "Collect statistics for loss measurement message tests";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/lbm-svc-act-responder
                        leaf lbm-svc-act-responder {
                            type boolean;
                            description "Process service activation streams in ETH-CFM LBM";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/one-way-delay-threshold
                        leaf one-way-delay-threshold {
                            type uint32 {
                                range "0..600";
                            }
                            units "seconds";
                            default "3";
                            description "Threshold time limit for the one-way delay test";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/alarm-notification
                        container alarm-notification {
                            description "Enter the alarm-notification context";

                            ///nokia-conf/configure/router/interface/eth-cfm/mep/alarm-notification/fng-alarm-time
                            leaf fng-alarm-time {
                                type int32 {
                                    range "250|500|1000";
                                }
                                units "centiseconds";
                                description "Time that must expire before an FNG alarm is raised";
                            }

                            ///nokia-conf/configure/router/interface/eth-cfm/mep/alarm-notification/fng-reset-time
                            leaf fng-reset-time {
                                type int32 {
                                    range "250|500|1000";
                                }
                                units "centiseconds";
                                description "Time that must expire before an FNG alarm is reset";
                            }

                        } // container alarm-notification

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/eth-test
                        container eth-test {
                            presence "Enable the eth-test context";
                            description "Enable the eth-test context";

                            ///nokia-conf/configure/router/interface/eth-cfm/mep/eth-test/bit-error-threshold
                            leaf bit-error-threshold {
                                type uint32 {
                                    range "0..11840";
                                }
                                units "bit errors";
                                default "1";
                                description "Lowest priority defect allowed to generate fault alarm";
                            }

                            ///nokia-conf/configure/router/interface/eth-cfm/mep/eth-test/test-pattern
                            container test-pattern {
                                description "Enter the test-pattern context";

                                ///nokia-conf/configure/router/interface/eth-cfm/mep/eth-test/test-pattern/pattern
                                leaf pattern {
                                    type enumeration {
                                        enum all-zeros                      { value 0; }
                                        enum all-ones                       { value 1; }
                                    }
                                    default "all-zeros";
                                    description "Test pattern for Ethernet Test frames";
                                }

                                ///nokia-conf/configure/router/interface/eth-cfm/mep/eth-test/test-pattern/crc-tlv
                                leaf crc-tlv {
                                    type boolean;
                                    default "false";
                                    description "Generate a CRC checksum";
                                }

                            } // container test-pattern

                        } // container eth-test

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/collect-lmm-fc-stats
                        container collect-lmm-fc-stats {
                            description "Enter the collect-lmm-fc-stats context";

                            ///nokia-conf/configure/router/interface/eth-cfm/mep/collect-lmm-fc-stats/fc
                            leaf-list fc {
                                type types-eth-cfm:fc-type;
                                max-elements 8;
                                description "Forwarding class name for profile-unaware counter";
                            }

                            ///nokia-conf/configure/router/interface/eth-cfm/mep/collect-lmm-fc-stats/fc-in-profile
                            leaf-list fc-in-profile {
                                type types-eth-cfm:fc-type;
                                max-elements 8;
                                description "Forwarding class name for profile-aware counter";
                            }

                        } // container collect-lmm-fc-stats

                        ///nokia-conf/configure/router/interface/eth-cfm/mep/grace
                        container grace {
                            description "Enter the grace context";

                            ///nokia-conf/configure/router/interface/eth-cfm/mep/grace/eth-ed
                            container eth-ed {
                                description "Enter the eth-ed context";

                                ///nokia-conf/configure/router/interface/eth-cfm/mep/grace/eth-ed/max-rx-defect-window
                                leaf max-rx-defect-window {
                                    type uint32 {
                                        range "1..86400";
                                    }
                                    units "seconds";
                                    description "Maximum received ETH-ED expected defect window duration";
                                }

                                ///nokia-conf/configure/router/interface/eth-cfm/mep/grace/eth-ed/priority
                                leaf priority {
                                    type int32 {
                                        range "0..7";
                                    }
                                    description "Transmission priority for ETH-ED PDUs";
                                }

                                ///nokia-conf/configure/router/interface/eth-cfm/mep/grace/eth-ed/rx-eth-ed
                                leaf rx-eth-ed {
                                    type boolean;
                                    default "true";
                                    description "Receive and process ETH-ED ITU-T Y.1731 PDUs on the MEP";
                                }

                                ///nokia-conf/configure/router/interface/eth-cfm/mep/grace/eth-ed/tx-eth-ed
                                leaf tx-eth-ed {
                                    type boolean;
                                    default "false";
                                    description "Transmit ETH-ED PDUs from the MEP";
                                }

                            } // container eth-ed

                            ///nokia-conf/configure/router/interface/eth-cfm/mep/grace/eth-vsm-grace
                            container eth-vsm-grace {
                                description "Enter the eth-vsm-grace context";

                                ///nokia-conf/configure/router/interface/eth-cfm/mep/grace/eth-vsm-grace/rx-eth-vsm-grace
                                leaf rx-eth-vsm-grace {
                                    type boolean;
                                    default "true";
                                    description "Receive and process Nokia ETH-CFM Grace PDU on the MEP";
                                }

                                ///nokia-conf/configure/router/interface/eth-cfm/mep/grace/eth-vsm-grace/tx-eth-vsm-grace
                                leaf tx-eth-vsm-grace {
                                    type boolean;
                                    default "true";
                                    description "Transmit ETH-ED PDUs from the MEP";
                                }

                            } // container eth-vsm-grace

                        } // container grace

                    } // list mep

                } // container eth-cfm

                ///nokia-conf/configure/router/interface/ipv4
                container ipv4 {
                    when "not(../../router-name = 'management')" {
                        description "not allowed in the management router";
                    }
                    description "Enter the ipv4 context";

                    ///nokia-conf/configure/router/interface/ipv4/tcp-mss
                    leaf tcp-mss {
                        when "../../../router-name = 'Base' and not(../../flavor = 'control-tunnel') and not(../../flavor = 'pdn')" {
                            description "only allowed in router Base, and not in an interface with flavor control-tunnel|pdn";
                        }
                        type types-router:ipv4-tcp-mss-ext;
                        description "TCP maximum segment size for the interface";
                    }

                    ///nokia-conf/configure/router/interface/ipv4/allow-directed-broadcasts
                    leaf allow-directed-broadcasts {
                        when "../../../router-name = 'Base' and not(../../flavor = 'control-tunnel') and not(../../flavor = 'pdn')" {
                            description "only allowed in router Base, and not in an interface with flavor control-tunnel|pdn";
                        }
                        type boolean;
                        default "false";
                        description "Forward directed broadcasts";
                    }

                    ///nokia-conf/configure/router/interface/ipv4/local-dhcp-server
                    leaf local-dhcp-server {
                        when "../../../router-name = 'Base' and not(../../flavor = 'control-tunnel') and not(../../flavor = 'pdn')" {
                            description "only allowed in router Base, and not in an interface with flavor control-tunnel|pdn";
                        }
                        type leafref {
                            path "../../../dhcp-server/dhcpv4/name";
                        }
                        description "DHCP server for the interface";
                    }

                    ///nokia-conf/configure/router/interface/ipv4/qos-route-lookup
                    leaf qos-route-lookup {
                        when "../../../router-name = 'Base' and not(../../flavor = 'control-tunnel') and not(../../flavor = 'pdn')" {
                            description "only allowed in router Base, and not in an interface with flavor control-tunnel|pdn";
                        }
                        type types-router:qos-route-lookup;
                        description "QoS route lookup of ingress IP packets";
                    }

                    ///nokia-conf/configure/router/interface/ipv4/ip-helper-address
                    leaf ip-helper-address {
                        when "../../../router-name = 'Base' and ../../flavor = 'regular' and not(../../interface-name = 'system')" {
                            description "only allowed in regular interfaces in router Base, and not in the system interface";
                        }
                        type types-sros:ipv4-unicast-address;
                        description "IP address of the target UDP broadcast gateway";
                    }

                    ///nokia-conf/configure/router/interface/ipv4/icmp
                    container icmp {
                        when "../../../router-name = 'Base' and not(../../flavor = 'control-tunnel')" {
                            description "only allowed in router Base, and not in an interface with flavor control-tunnel";
                        }
                        description "Enter the icmp context";

                        ///nokia-conf/configure/router/interface/ipv4/icmp/mask-reply
                        leaf mask-reply {
                            type boolean;
                            default "true";
                            description "Allow responses to ICMP mask requests on the interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/icmp/redirects
                        container redirects {
                            description "Enter the redirects context";

                            ///nokia-conf/configure/router/interface/ipv4/icmp/redirects/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sending ICMP redirect messages";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/icmp/redirects/number
                            leaf number {
                                type uint16 {
                                    range "10..1000";
                                }
                                default "100";
                                description "Maximum number of ICMP redirect messages to send";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/icmp/redirects/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time used to limit the number of ICMP redirect messages";
                            }

                        } // container redirects

                        ///nokia-conf/configure/router/interface/ipv4/icmp/ttl-expired
                        container ttl-expired {
                            description "Enter the ttl-expired context";

                            ///nokia-conf/configure/router/interface/ipv4/icmp/ttl-expired/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sending TTL expired messages";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/icmp/ttl-expired/number
                            leaf number {
                                type uint16 {
                                    range "10..2000";
                                }
                                default "100";
                                description "Maximum number of TTL expired messages to send";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/icmp/ttl-expired/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time used to limit the number of TTL expired messages";
                            }

                        } // container ttl-expired

                        ///nokia-conf/configure/router/interface/ipv4/icmp/unreachables
                        container unreachables {
                            description "Enter the unreachables context";

                            ///nokia-conf/configure/router/interface/ipv4/icmp/unreachables/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sending unreachable messages";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/icmp/unreachables/number
                            leaf number {
                                type uint16 {
                                    range "10..2000";
                                }
                                default "100";
                                description "Maximum number of unreachable messages to send";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/icmp/unreachables/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time to limit the number of ICMP unreachable messages";
                            }

                        } // container unreachables

                        ///nokia-conf/configure/router/interface/ipv4/icmp/param-problem
                        container param-problem {
                            description "Enter the param-problem context";

                            ///nokia-conf/configure/router/interface/ipv4/icmp/param-problem/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sent Parameter Problem messages";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/icmp/param-problem/number
                            leaf number {
                                type uint32 {
                                    range "10..1000";
                                }
                                default "100";
                                description "Maximum number of Parameter Problem messages to send";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/icmp/param-problem/seconds
                            leaf seconds {
                                type uint32 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time used to limit number of Parameter Problem messages";
                            }

                        } // container param-problem

                    } // container icmp

                    ///nokia-conf/configure/router/interface/ipv4/unnumbered
                    container unnumbered {
                        when "../../../router-name = 'Base' and not(../../flavor = 'control-tunnel') and not(../../interface-name = 'system')" {
                            description "only allowed in router Base, and not in the system interface nor in an interface with flavor control-tunnel";
                        }
                        description "Enter the unnumbered context";

                        ///nokia-conf/configure/router/interface/ipv4/unnumbered/source
                        choice source {
                            ///nokia-conf/configure/router/interface/ipv4/unnumbered/source/ip-address
                            case ip-address {

                                ///nokia-conf/configure/router/interface/ipv4/unnumbered/source/ip-address/ip-address
                                leaf ip-address {
                                    type types-sros:ipv4-unicast-address;
                                    description "IP address to associate with unnumbered IP interface";
                                }

                            }
                            ///nokia-conf/configure/router/interface/ipv4/unnumbered/source/ip-int-name
                            case ip-int-name {

                                ///nokia-conf/configure/router/interface/ipv4/unnumbered/source/ip-int-name/ip-int-name
                                leaf ip-int-name {
                                    type types-sros:interface-name;
                                    description "Unnumbered IP interface name";
                                }

                            }
                            ///nokia-conf/configure/router/interface/ipv4/unnumbered/source/ip-int-system
                            case ip-int-system {

                                ///nokia-conf/configure/router/interface/ipv4/unnumbered/source/ip-int-system/system
                                leaf system {
                                    type empty;
                                    description "Use system IP address for the unnumbered IP interface";
                                }

                            }
                        }
                    } // container unnumbered

                    ///nokia-conf/configure/router/interface/ipv4/urpf-check
                    container urpf-check {
                        when "../../../router-name = 'Base' and not(../../flavor = 'control-tunnel') and not(../../flavor = 'pdn') and not(../../interface-name = 'system')" {
                            description "only allowed in router Base, and not in the system interface nor in an interface with flavor control-tunnel|pdn";
                        }
                        presence "Enable the urpf-check context";
                        description "Enable the urpf-check context";

                        ///nokia-conf/configure/router/interface/ipv4/urpf-check/ignore-default
                        leaf ignore-default {
                            type boolean;
                            default "false";
                            description "Ignore default routes when performing a uRPF check";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/urpf-check/mode
                        leaf mode {
                            type types-router:urpf-check-mode;
                            default "strict";
                            description "Unicast RPF check mode";
                        }

                    } // container urpf-check

                    ///nokia-conf/configure/router/interface/ipv4/dhcp
                    container dhcp {
                        when "../../../router-name = 'Base' and not(../../interface-name = 'system') and not(../../flavor = 'pdn')" {
                            description "only allowed in router Base, and not in the system interface nor in an interface with flavor pdn";
                        }
                        description "Enable the dhcp context";

                        ///nokia-conf/configure/router/interface/ipv4/dhcp/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            description "Administrative state of DHCP";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/dhcp/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/dhcp/python-policy
                        leaf python-policy {
                            type leafref {
                                path "../../../../../python/python-policy/name";
                            }
                            description "Python policy name";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/dhcp/server
                        leaf-list server {
                            type types-sros:ipv4-unicast-address;
                            max-elements 8;
                            ordered-by user;
                            description "IP addresses for DHCP server requests";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/dhcp/trusted
                        leaf trusted {
                            type boolean;
                            default "false";
                            description "Relay untrusted packets";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/dhcp/gi-address
                        leaf gi-address {
                            type types-sros:ipv4-unicast-address;
                            description "GI address for the DHCP relay";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/dhcp/release-include-gi-address
                        leaf release-include-gi-address {
                            type boolean;
                            default "false";
                            description "Include gateway IP address in DHCP Release messages";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/dhcp/src-ip-addr
                        leaf src-ip-addr {
                            type enumeration {
                                enum auto                           { value 0; }
                                enum gi-address                     { value 1; }
                            }
                            default "auto";
                            description "Type of source address to use for DHCP relay";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/dhcp/relay-plain-bootp
                        leaf relay-plain-bootp {
                            type boolean;
                            default "false";
                            description "Relay plain BOOTP messages";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/dhcp/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/dhcp/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82
                        container option-82 {
                            description "Enter the option-82 context";

                            ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/action
                            leaf action {
                                type enumeration {
                                    enum replace                        { value 1; }
                                    enum drop                           { value 2; }
                                    enum keep                           { value 3; }
                                }
                                default "keep";
                                description "Action to take with received DHCP Option 82";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/circuit-id
                            container circuit-id {
                                description "Enter the circuit-id context";

                                ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/circuit-id/circuit-id
                                choice circuit-id {
                                    default "ascii-tuple";
                                    ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/none
                                    case none {

                                        ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/none/none
                                        leaf none {
                                            type empty;
                                            description "Do not include the circuit ID";
                                        }

                                    }
                                    ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ifindex
                                    case ifindex {

                                        ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ifindex/ifindex
                                        leaf ifindex {
                                            type empty;
                                            description "Use the interface index for the circuit ID";
                                        }

                                    }
                                    ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ascii-tuple
                                    case ascii-tuple {

                                        ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ascii-tuple/ascii-tuple
                                        leaf ascii-tuple {
                                            type empty;
                                            description "Use the ASCII-encoded tuple for the circuit ID";
                                        }

                                    }
                                    ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/vlan-ascii-tuple
                                    case vlan-ascii-tuple {

                                        ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/vlan-ascii-tuple/vlan-ascii-tuple
                                        leaf vlan-ascii-tuple {
                                            type empty;
                                            description "Include the VLAN ID and dot1p bits in the ASCII tuple";
                                        }

                                    }
                                    ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/port-id
                                    case port-id {

                                        ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/port-id/port-id
                                        leaf port-id {
                                            type empty;
                                            description "Use the port ID for the circuit ID";
                                        }

                                    }
                                    ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/if-name
                                    case if-name {

                                        ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/if-name/if-name
                                        leaf if-name {
                                            type empty;
                                            description "Use the interface name for the circuit ID";
                                        }

                                    }
                                }
                            } // container circuit-id

                            ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/remote-id
                            container remote-id {
                                description "Enter the remote-id context";

                                ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/remote-id/remote-id
                                choice remote-id {
                                    default "none";
                                    ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/remote-id/remote-id/none
                                    case none {

                                        ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/remote-id/remote-id/none/none
                                        leaf none {
                                            type empty;
                                            description "Do not include the remote ID";
                                        }

                                    }
                                    ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/remote-id/remote-id/mac
                                    case mac {

                                        ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/remote-id/remote-id/mac/mac
                                        leaf mac {
                                            type empty;
                                            description "Use the MAC address for the remote ID";
                                        }

                                    }
                                    ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/remote-id/remote-id/ascii-string
                                    case ascii-string {

                                        ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/remote-id/remote-id/ascii-string/ascii-string
                                        leaf ascii-string {
                                            type types-sros:string-not-all-spaces {
                                                length "1..32";
                                            }
                                            description "User-defined ASCII string for the remote ID";
                                        }

                                    }
                                }
                            } // container remote-id

                            ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/vendor-specific-option
                            container vendor-specific-option {
                                description "Enter the vendor-specific-option context";

                                ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/vendor-specific-option/system-id
                                leaf system-id {
                                    type boolean;
                                    default "false";
                                    description "Send the system ID in the VSO";
                                }

                                ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/vendor-specific-option/client-mac-address
                                leaf client-mac-address {
                                    type boolean;
                                    default "false";
                                    description "Send the MAC address in the VSO";
                                }

                                ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/vendor-specific-option/pool-name
                                leaf pool-name {
                                    type boolean;
                                    default "false";
                                    description "Send the pool name in the VSO";
                                }

                                ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/vendor-specific-option/port-id
                                leaf port-id {
                                    type boolean;
                                    default "false";
                                    description "Send the port ID in the VSO";
                                }

                                ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/vendor-specific-option/service-id
                                leaf service-id {
                                    type boolean;
                                    default "false";
                                    description "Send the service ID in the Vendor Specific Option";
                                }

                                ///nokia-conf/configure/router/interface/ipv4/dhcp/option-82/vendor-specific-option/string
                                leaf string {
                                    type types-sros:string-not-all-spaces {
                                        length "1..32";
                                    }
                                    description "User-defined ASCII string for the VSO";
                                }

                            } // container vendor-specific-option

                        } // container option-82

                    } // container dhcp

                    ///nokia-conf/configure/router/interface/ipv4/bfd
                    container bfd {
                        when "../../../router-name = 'Base'" {
                            description "only allowed in the Base router";
                        }
                        description "Enter the bfd context";

                        ///nokia-conf/configure/router/interface/ipv4/bfd/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of BFD sessions";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/bfd/transmit-interval
                        leaf transmit-interval {
                            type uint32 {
                                range "10..100000";
                            }
                            units "milliseconds";
                            default "100";
                            description "BFD transmit interval over this interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/bfd/receive
                        leaf receive {
                            type uint32 {
                                range "10..100000";
                            }
                            units "milliseconds";
                            default "100";
                            description "BFD receive interval over this interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/bfd/multiplier
                        leaf multiplier {
                            type uint32 {
                                range "1..20";
                            }
                            default "3";
                            description "Number of consecutive BFD messages missed from the peer";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/bfd/echo-receive
                        leaf echo-receive {
                            type uint32 {
                                range "100..100000";
                            }
                            units "milliseconds";
                            description "Minimum echo interval over this interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/bfd/type
                        leaf type {
                            type enumeration {
                                enum cpm-np                         { value 1; }
                                enum auto                           { value 2; }
                                enum fp                             { value 3; }
                            }
                            default "auto";
                            description "Local termination point for the BFD session";
                        }

                    } // container bfd

                    ///nokia-conf/configure/router/interface/ipv4/primary
                    container primary {
                        when "not(../../../router-name = 'management') and not(../../flavor = 'pdn') and not(../../flavor = 'unnumbered-mpls-tp')" {
                            description "not allowed in an interface with flavor pdn|unnumbered-mpls-tp";
                        }
                        presence "Enable the primary context";
                        description "Enable the primary context";

                        ///nokia-conf/configure/router/interface/ipv4/primary/address
                        leaf address {
                            type types-sros:ipv4-unicast-address;
                            mandatory true;
                            description "Primary IPv4 address assigned to the interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/primary/prefix-length
                        leaf prefix-length {
                            type uint32 {
                                range "0..32";
                            }
                            mandatory true;
                            description "IPv4 address prefix length";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/primary/broadcast
                        leaf broadcast {
                            when "../../../../router-name = 'Base'" {
                                description "only allowed in the Base router";
                            }
                            type types-services:broadcast-address-format;
                            default "host-ones";
                            description "Broadcast address format";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/primary/track-srrp
                        leaf track-srrp {
                            when "../../../../router-name = 'Base' and ../../../flavor = 'regular'" {
                                description "only allowed in regular interfaces in router Base";
                            }
                            type uint32 {
                                range "1..4294967295";
                            }
                            description "SRRP instance whose state is tracked on this IP address";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/primary/gre-termination
                        leaf gre-termination {
                            when "../../../../router-name = 'Base' and ../../../flavor = 'regular' and not(../../../interface-name = 'system')" {
                                description "only allowed in regular interfaces in router Base, and not in the system interface";
                            }
                            type boolean;
                            default "false";
                            description "Enable GRE termination";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/primary/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/primary/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container primary

                    ///nokia-conf/configure/router/interface/ipv4/secondary
                    list secondary {
                        when "../../../router-name = 'Base' and ../../flavor = 'regular' and not(../../interface-name = 'system')" {
                            description "only allowed in regular interfaces in router Base, and not in the system interface";
                        }
                        key "address";
                        description "Enter the secondary list instance";

                        ///nokia-conf/configure/router/interface/ipv4/secondary/address
                        leaf address {
                            type types-sros:ipv4-unicast-address;
                            description "Secondary IPv4 address assigned to the interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/secondary/prefix-length
                        leaf prefix-length {
                            type uint32 {
                                range "0..32";
                            }
                            mandatory true;
                            description "IPv4 address prefix length";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/secondary/igp-inhibit
                        leaf igp-inhibit {
                            type boolean;
                            default "false";
                            description "Disable the running IGP from recognizing secondary IP";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/secondary/broadcast
                        leaf broadcast {
                            when "../../../../router-name = 'Base'" {
                                description "only allowed in the Base router";
                            }
                            type types-services:broadcast-address-format;
                            default "host-ones";
                            description "Broadcast address format";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/secondary/track-srrp
                        leaf track-srrp {
                            when "../../../../router-name = 'Base' and ../../../flavor = 'regular'" {
                                description "only allowed in regular interfaces in router Base";
                            }
                            type uint32 {
                                range "1..4294967295";
                            }
                            description "SRRP instance whose state is tracked on this IP address";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/secondary/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/secondary/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list secondary

                    ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery
                    container neighbor-discovery {
                        when "not(../../interface-name = 'system') and not(../../flavor = 'pdn') and not(../../flavor = 'control-tunnel') and not(../../flavor = 'gmpls-loopback')" {
                            description "not allowed in the system interface nor in an interface with flavor pdn|control-tunnel|gmpls-loopback";
                        }
                        description "Enter the neighbor-discovery context";

                        ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/timeout
                        leaf timeout {
                            when "../../../../router-name = 'Base'" {
                                description "only allowed in the Base router";
                            }
                            type uint32 {
                                range "0..65535";
                            }
                            units "seconds";
                            default "14400";
                            description "Timeout for an ARP entry learned on the interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/retry-timer
                        leaf retry-timer {
                            when "../../../../router-name = 'Base'" {
                                description "only allowed in the Base router";
                            }
                            type uint32 {
                                range "1..300";
                            }
                            units "deciseconds";
                            default "50";
                            description "ARP retry interval";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/learn-unsolicited
                        leaf learn-unsolicited {
                            when "../../../../router-name = 'Base'" {
                                description "only allowed in the Base router";
                            }
                            type boolean;
                            default "false";
                            description "Learn new entries from any received NA message";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/proactive-refresh
                        leaf proactive-refresh {
                            when "../../../../router-name = 'Base'" {
                                description "only allowed in the Base router";
                            }
                            type boolean;
                            default "false";
                            description "Send a single refresh message before entry timeout";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/local-proxy-arp
                        leaf local-proxy-arp {
                            when "../../../../router-name = 'Base'" {
                                description "only allowed in the Base router";
                            }
                            type boolean;
                            description "Enable local proxy ARP on interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/remote-proxy-arp
                        leaf remote-proxy-arp {
                            when "../../../../router-name = 'Base'" {
                                description "only allowed in the Base router";
                            }
                            type boolean;
                            default "false";
                            description "Enable remote proxy ARP on the interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/proxy-arp-policy
                        leaf-list proxy-arp-policy {
                            when "../../../../router-name = 'Base'" {
                                description "only allowed in the Base router";
                            }
                            type leafref {
                                path "../../../../../policy-options/policy-statement/name";
                            }
                            max-elements 5;
                            ordered-by user;
                            description "Proxy ARP policy name";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/limit
                        container limit {
                            when "../../../../router-name = 'Base'" {
                                description "only allowed in the Base router";
                            }
                            description "Enter the limit context";

                            ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/limit/max-entries
                            leaf max-entries {
                                type types-router:ipv4-neighbor-limit;
                                description "Maximum number of entries learned on an IP interface";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/limit/log-only
                            leaf log-only {
                                type boolean;
                                default "false";
                                description "Generate log entries only if limit is reached";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/limit/threshold
                            leaf threshold {
                                type uint32 {
                                    range "1..100";
                                }
                                units "percent";
                                default "90";
                                description "Threshold value that triggers a warning message";
                            }

                        } // container limit

                        ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/static-neighbor-unnumbered
                        container static-neighbor-unnumbered {
                            when "../../../../router-name = 'Base'" {
                                description "only allowed in router Base";
                            }
                            presence "Enable the static-neighbor-unnumbered context";
                            description "Enable the static-neighbor-unnumbered context";

                            ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/static-neighbor-unnumbered/mac-address
                            leaf mac-address {
                                type yang:mac-address;
                                sros-ext:auto-restart-to-modify;
                                mandatory true;
                                description "MAC address for the static neighbor";
                            }

                        } // container static-neighbor-unnumbered

                        ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/static-neighbor
                        list static-neighbor {
                            when "../../../../router-name = 'Base'" {
                                description "only allowed in router Base";
                            }
                            key "ipv4-address";
                            description "Enter the static-neighbor list instance";

                            ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/static-neighbor/ipv4-address
                            leaf ipv4-address {
                                type types-sros:ipv4-address;
                                description "IPv4 address that corresponds to the physical address";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/static-neighbor/mac-address
                            leaf mac-address {
                                type yang:mac-address;
                                mandatory true;
                                description "MAC address for the static neighbor";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/static-neighbor/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/neighbor-discovery/static-neighbor/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list static-neighbor

                    } // container neighbor-discovery

                    ///nokia-conf/configure/router/interface/ipv4/vrrp
                    list vrrp {
                        when "../../../router-name = 'Base' and not(../../interface-name = 'system')" {
                            description "only allowed in the Base router, and not in the system interface";
                        }
                        key "virtual-router-id";
                        description "Enter the vrrp list instance";

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/virtual-router-id
                        leaf virtual-router-id {
                            type int32 {
                                range "1..255";
                            }
                            description "Virtual Router Identifier (VRID) for the IP interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/authentication-key
                        leaf authentication-key {
                            type types-sros:encrypted-leaf {
                                length "1..38";
                            }
                            description "Password for simple text authentication";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/backup
                        leaf-list backup {
                            type types-sros:ipv4-unicast-address;
                            max-elements 16;
                            description "Virtual router IP addresses for the interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/message-interval
                        leaf message-interval {
                            type uint32 {
                                range "1..2559";
                            }
                            units "deciseconds";
                            default "10";
                            description "Interval for sending VRRP advertisement messages";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/owner
                        leaf owner {
                            type boolean;
                            sros-ext:immutable;
                            default "false";
                            description "Designate the virtual router instance as owner";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/passive
                        leaf passive {
                            type boolean;
                            sros-ext:immutable;
                            default "false";
                            description "Suppress the processing of VRRP advertisement messages";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/init-delay
                        leaf init-delay {
                            type uint32 {
                                range "1..65535";
                            }
                            units "seconds";
                            description "VRRP initialization delay timer";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/mac
                        leaf mac {
                            type types-sros:mac-unicast-address;
                            description "Virtual MAC address to use in ARP responses";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/priority
                        leaf priority {
                            type uint32 {
                                range "1..255";
                            }
                            description "Base priority for the VRRP";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "enable";
                            description "Administrative state of VRRP";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/master-int-inherit
                        leaf master-int-inherit {
                            type boolean;
                            description "Allow master instance to dictate the master down timer";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/ping-reply
                        leaf ping-reply {
                            type boolean;
                            default "false";
                            description "Allow non-owner master to reply to ICMP echo requests";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/policy
                        leaf policy {
                            type leafref {
                                path "../../../../../vrrp/policy/policy-id";
                            }
                            description "VRRP priority control policy";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/preempt
                        leaf preempt {
                            type boolean;
                            default "true";
                            description "Allow the VRRP to override an existing non-owner master";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/telnet-reply
                        leaf telnet-reply {
                            type boolean;
                            default "false";
                            description "Allow non-owner master to reply to Telnet requests";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/traceroute-reply
                        leaf traceroute-reply {
                            type boolean;
                            default "false";
                            description "Allow non-owner master to reply to traceroute requests";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/standby-forwarding
                        leaf standby-forwarding {
                            type boolean;
                            default "false";
                            description "Allow standby router to forward traffic";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/ssh-reply
                        leaf ssh-reply {
                            type boolean;
                            default "false";
                            description "Allow the non-owner master to reply to SSH requests";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/ntp-reply
                        leaf ntp-reply {
                            type boolean;
                            default "false";
                            description "Allow processing of NTP requests";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/oper-group
                        leaf oper-group {
                            type leafref {
                                path "../../../../../service/oper-group/name";
                            }
                            description "Operational group name associated with the VRRP";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/monitor-oper-group
                        leaf monitor-oper-group {
                            type leafref {
                                path "../../../../../service/oper-group/name";
                            }
                            sros-ext:immutable;
                            description "VRRP instance to follow a specified operational group";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/ipv4/vrrp/bfd-liveness
                        container bfd-liveness {
                            presence "Enable the bfd-liveness context";
                            description "Enable the bfd-liveness context";

                            ///nokia-conf/configure/router/interface/ipv4/vrrp/bfd-liveness/dest-ip
                            leaf dest-ip {
                                type types-sros:ipv4-address;
                                sros-ext:immutable;
                                mandatory true;
                                description "Destination IP address to use for BFD session";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/vrrp/bfd-liveness/service-name
                            leaf service-name {
                                type types-services:service-name;
                                sros-ext:immutable;
                                description "Administrative service name";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/vrrp/bfd-liveness/interface-name
                            leaf interface-name {
                                type types-sros:interface-name;
                                sros-ext:immutable;
                                mandatory true;
                                description "Name of the interface running BFD";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/vrrp/bfd-liveness/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/interface/ipv4/vrrp/bfd-liveness/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // container bfd-liveness

                    } // list vrrp

                } // container ipv4

                ///nokia-conf/configure/router/interface/ipv6
                container ipv6 {
                    when "../../router-name = 'Base' and not(../flavor = 'control-tunnel') and not(../flavor = 'gmpls-loopback')" {
                        description "only allowed in router Base, and not in an interface with flavor control-tunnel|gmpls-loopback";
                    }
                    presence "Enable the ipv6 context";
                    description "Enable the ipv6 context";

                    ///nokia-conf/configure/router/interface/ipv6/qos-route-lookup
                    leaf qos-route-lookup {
                        when "../../../router-name = 'Base' and not(../../flavor = 'control-tunnel') and not(../../flavor = 'pdn')" {
                            description "only allowed in router Base, and not in an interface with flavor control-tunnel|pdn";
                        }
                        type types-router:qos-route-lookup;
                        description "QoS route lookup of ingress IP packets";
                    }

                    ///nokia-conf/configure/router/interface/ipv6/forward-ipv4-packets
                    leaf forward-ipv4-packets {
                        when "../../../router-name = 'Base' and ../../flavor = 'regular' and not(../../interface-name = 'system')" {
                            description "only allowed in regular interfaces in router Base, and not in the system interface";
                        }
                        type boolean;
                        default "false";
                        description "Forward unencapsulated IPv4 packets";
                    }

                    ///nokia-conf/configure/router/interface/ipv6/tcp-mss
                    leaf tcp-mss {
                        when "../../../router-name = 'Base' and not(../../flavor = 'control-tunnel') and not(../../flavor = 'pdn')" {
                            description "only allowed in router Base, and not in an interface with flavor control-tunnel|pdn";
                        }
                        type types-router:ipv6-tcp-mss-ext;
                        description "TCP maximum segment size for the IPv6 interface";
                    }

                    ///nokia-conf/configure/router/interface/ipv6/duplicate-address-detection
                    leaf duplicate-address-detection {
                        when "../../../router-name = 'Base'" {
                            description "only allowed in the Base router";
                        }
                        type boolean;
                        default "true";
                        description "Enable Duplicate Address Detection";
                    }

                    ///nokia-conf/configure/router/interface/ipv6/local-dhcp-server
                    leaf local-dhcp-server {
                        type leafref {
                            path "../../../dhcp-server/dhcpv6/name";
                        }
                        description "DHCP server for the interface";
                    }

                    ///nokia-conf/configure/router/interface/ipv6/icmp6
                    container icmp6 {
                        description "Enter the icmp6 context";

                        ///nokia-conf/configure/router/interface/ipv6/icmp6/packet-too-big
                        container packet-too-big {
                            description "Enter the packet-too-big context";

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/packet-too-big/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of Packet Too Big message sends";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/packet-too-big/number
                            leaf number {
                                type uint16 {
                                    range "10..1000";
                                }
                                default "100";
                                description "Number of Packet Too big Messages issued per time frame";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/packet-too-big/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                default "10";
                                description "Time used to limit Packet Too Big messages";
                            }

                        } // container packet-too-big

                        ///nokia-conf/configure/router/interface/ipv6/icmp6/param-problem
                        container param-problem {
                            description "Enter the param-problem context";

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/param-problem/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of Parameter Problem message sends";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/param-problem/number
                            leaf number {
                                type uint16 {
                                    range "10..1000";
                                }
                                default "100";
                                description "Number used to limit ICMPv6 Parameter Problem messages";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/param-problem/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                default "10";
                                description "Time used to limit ICMPv6 Parameter Problem messages";
                            }

                        } // container param-problem

                        ///nokia-conf/configure/router/interface/ipv6/icmp6/redirects
                        container redirects {
                            description "Enter the redirects context";

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/redirects/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of Redirect message sends";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/redirects/number
                            leaf number {
                                type uint16 {
                                    range "10..1000";
                                }
                                default "100";
                                description "Number to limit ICMPv6 Redirect messages per time frame";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/redirects/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                default "10";
                                description "Time used to limit ICMPv6 Redirect messages";
                            }

                        } // container redirects

                        ///nokia-conf/configure/router/interface/ipv6/icmp6/time-exceeded
                        container time-exceeded {
                            description "Enter the time-exceeded context";

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/time-exceeded/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of Time Exceeded message sends";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/time-exceeded/number
                            leaf number {
                                type uint16 {
                                    range "10..2000";
                                }
                                default "100";
                                description "Number to limit Time Exceeded messages per time frame";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/time-exceeded/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                default "10";
                                description "Time used to limit ICMPv6 Time Exceeded messages";
                            }

                        } // container time-exceeded

                        ///nokia-conf/configure/router/interface/ipv6/icmp6/unreachables
                        container unreachables {
                            description "Enter the unreachables context";

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/unreachables/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of Unreachable message sends";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/unreachables/number
                            leaf number {
                                type uint16 {
                                    range "10..2000";
                                }
                                default "100";
                                description "Number to limit Unreachable messages per time frame";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/icmp6/unreachables/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                default "10";
                                description "Time used to limit ICMPv6 Unreachable messages";
                            }

                        } // container unreachables

                    } // container icmp6

                    ///nokia-conf/configure/router/interface/ipv6/link-local-address
                    container link-local-address {
                        when "not(../../interface-name = 'system')" {
                            description "not allowed on the system interface";
                        }
                        description "Enter the link-local-address context";

                        ///nokia-conf/configure/router/interface/ipv6/link-local-address/address
                        leaf address {
                            type types-sros:ipv6-address;
                            description "IPv6 link-local address";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/link-local-address/duplicate-address-detection
                        leaf duplicate-address-detection {
                            type boolean;
                            default "true";
                            description "Enable Duplicate Address Detection";
                        }

                    } // container link-local-address

                    ///nokia-conf/configure/router/interface/ipv6/urpf-check
                    container urpf-check {
                        when "../../../router-name = 'Base' and not(../../flavor = 'control-tunnel') and not(../../flavor = 'pdn') and not(../../interface-name = 'system')" {
                            description "only allowed in router Base, and not in the system interface nor in an interface with flavor control-tunnel|pdn";
                        }
                        presence "Enable the urpf-check context";
                        description "Enable the urpf-check context";

                        ///nokia-conf/configure/router/interface/ipv6/urpf-check/ignore-default
                        leaf ignore-default {
                            type boolean;
                            default "false";
                            description "Ignore default route when performing a uRPF check";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/urpf-check/mode
                        leaf mode {
                            type types-router:urpf-check-mode;
                            default "strict";
                            description "Unicast RPF check mode";
                        }

                    } // container urpf-check

                    ///nokia-conf/configure/router/interface/ipv6/bfd
                    container bfd {
                        description "Enter the bfd context";

                        ///nokia-conf/configure/router/interface/ipv6/bfd/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of BFD sessions";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/bfd/transmit-interval
                        leaf transmit-interval {
                            type uint32 {
                                range "10..100000";
                            }
                            units "milliseconds";
                            default "100";
                            description "BFD transmit interval over this interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/bfd/receive
                        leaf receive {
                            type uint32 {
                                range "10..100000";
                            }
                            units "milliseconds";
                            default "100";
                            description "BFD receive interval over this interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/bfd/multiplier
                        leaf multiplier {
                            type uint32 {
                                range "1..20";
                            }
                            default "3";
                            description "Number of consecutive BFD messages missed from the peer";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/bfd/echo-receive
                        leaf echo-receive {
                            type uint32 {
                                range "100..100000";
                            }
                            units "milliseconds";
                            description "Minimum echo interval over this interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/bfd/type
                        leaf type {
                            type enumeration {
                                enum cpm-np                         { value 1; }
                                enum auto                           { value 2; }
                                enum fp                             { value 3; }
                            }
                            default "auto";
                            description "Local termination point for the BFD session";
                        }

                    } // container bfd

                    ///nokia-conf/configure/router/interface/ipv6/address
                    list address {
                        key "ipv6-address";
                        description "Enter the address list instance";

                        ///nokia-conf/configure/router/interface/ipv6/address/ipv6-address
                        leaf ipv6-address {
                            type types-sros:ipv6-address;
                            description "IPv6 address assigned to the interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/address/prefix-length
                        leaf prefix-length {
                            type uint32 {
                                range "4..128";
                            }
                            mandatory true;
                            description "IPv6 address prefix length";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/address/eui-64
                        leaf eui-64 {
                            type boolean;
                            sros-ext:immutable;
                            default "false";
                            description "Form IPv6 address from prefix and 64-bit interface ID";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/address/duplicate-address-detection
                        leaf duplicate-address-detection {
                            type boolean;
                            sros-ext:immutable;
                            default "true";
                            description "Enable Duplicate Address Detection";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/address/primary-preference
                        leaf primary-preference {
                            type uint32 {
                                range "1..4294967295";
                            }
                            description "Index assigned to the IPv6 address of the interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/address/track-srrp
                        leaf track-srrp {
                            type uint32 {
                                range "1..4294967295";
                            }
                            description "SRRP ID whose state is tracked on this IP address";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/address/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/address/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list address

                    ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery
                    container neighbor-discovery {
                        when "not(../../interface-name = 'system') and not(../../flavor = 'pdn') and not(../../flavor = 'control-tunnel') and not(../../flavor = 'gmpls-loopback')" {
                            description "not allowed in the system interface";
                        }
                        description "Enter the neighbor-discovery context";

                        ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/reachable-time
                        leaf reachable-time {
                            type types-router:neighbor-discovery-reachable-time;
                            description "Neighbor reachability detection timer";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/stale-time
                        leaf stale-time {
                            type types-router:neighbor-discovery-stale-time;
                            description "Time a Neighbor Discovery cache entry remains stale";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/learn-unsolicited
                        leaf learn-unsolicited {
                            type types-router:ipv6-address-type-selection;
                            description "Type of entries learned from unsolicited NA messages";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/proactive-refresh
                        leaf proactive-refresh {
                            type types-router:ipv6-address-type-selection;
                            description "Proactive refresh of neighbor entries";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/local-proxy-nd
                        leaf local-proxy-nd {
                            type boolean;
                            default "false";
                            description "Enable local proxy neighbor discovery on the interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/proxy-nd-policy
                        leaf-list proxy-nd-policy {
                            type leafref {
                                path "../../../../../policy-options/policy-statement/name";
                            }
                            max-elements 5;
                            ordered-by user;
                            description "Proxy Neighbor Discovery policy name for the interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/secure-nd
                        container secure-nd {
                            description "Enter the secure-nd context";

                            ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/secure-nd/allow-unsecured-msgs
                            leaf allow-unsecured-msgs {
                                type boolean;
                                default "true";
                                description "Accept unsecured messages";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/secure-nd/public-key-min-bits
                            leaf public-key-min-bits {
                                type uint32 {
                                    range "512..1024";
                                }
                                default "1024";
                                description "Minimum acceptable key length for public keys in CGA";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/secure-nd/security-parameter
                            leaf security-parameter {
                                type uint32 {
                                    range "0..1";
                                }
                                default "1";
                                description "Security parameter used in the generation of a CGA";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/secure-nd/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "disable";
                                description "Administrative state of Secure Neighbor Discovery";
                            }

                        } // container secure-nd

                        ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/limit
                        container limit {
                            description "Enter the limit context";

                            ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/limit/max-entries
                            leaf max-entries {
                                type types-router:ipv6-neighbor-limit;
                                description "Maximum number of entries learned on an IP interface";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/limit/log-only
                            leaf log-only {
                                type boolean;
                                default "false";
                                description "Generate log entries when limit is reached";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/limit/threshold
                            leaf threshold {
                                type uint32 {
                                    range "1..100";
                                }
                                units "percent";
                                default "90";
                                description "Threshold percentage that triggers a warning message";
                            }

                        } // container limit

                        ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/static-neighbor
                        list static-neighbor {
                            key "ipv6-address";
                            description "Enter the static-neighbor list instance";

                            ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/static-neighbor/ipv6-address
                            leaf ipv6-address {
                                type types-sros:ipv6-address;
                                description "IPv6 address corresponding to the physical address";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/static-neighbor/mac-address
                            leaf mac-address {
                                type yang:mac-address;
                                mandatory true;
                                description "MAC address for the static neighbor";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/static-neighbor/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/neighbor-discovery/static-neighbor/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list static-neighbor

                    } // container neighbor-discovery

                    ///nokia-conf/configure/router/interface/ipv6/vrrp
                    list vrrp {
                        when "../../../router-name = 'Base' and not(../../interface-name = 'system')" {
                            description "only allowed in the Base router, and not in the system interface";
                        }
                        key "virtual-router-id";
                        max-elements 4;
                        description "Enter the vrrp list instance";

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/virtual-router-id
                        leaf virtual-router-id {
                            type int32 {
                                range "1..255";
                            }
                            description "Virtual Router Identifier (VRID) for the IP interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/backup
                        leaf-list backup {
                            type types-sros:ipv6-address;
                            max-elements 4;
                            description "Virtual router IP addresses for the interface";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/message-interval
                        leaf message-interval {
                            type uint32 {
                                range "10..4095";
                            }
                            units "centiseconds";
                            default "100";
                            description "Interval for sending VRRP advertisement messages";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/owner
                        leaf owner {
                            type boolean;
                            sros-ext:immutable;
                            default "false";
                            description "Designate the virtual router instance as owner";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/passive
                        leaf passive {
                            type boolean;
                            sros-ext:immutable;
                            default "false";
                            description "Suppress the processing of VRRP advertisement messages";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/init-delay
                        leaf init-delay {
                            type uint32 {
                                range "1..65535";
                            }
                            units "seconds";
                            description "VRRP initialization delay timer";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/mac
                        leaf mac {
                            type types-sros:mac-unicast-address;
                            description "Virtual MAC address to use in ARP responses";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/priority
                        leaf priority {
                            type uint32 {
                                range "1..255";
                            }
                            description "Base priority for the VRRP";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "enable";
                            description "Administrative state of VRRP";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/master-int-inherit
                        leaf master-int-inherit {
                            type boolean;
                            description "Allow master instance to dictate the master down timer";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/ping-reply
                        leaf ping-reply {
                            type boolean;
                            default "false";
                            description "Allow non-owner master to reply to ICMP echo requests";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/policy
                        leaf policy {
                            type leafref {
                                path "../../../../../vrrp/policy/policy-id";
                            }
                            description "VRRP priority control policy";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/preempt
                        leaf preempt {
                            type boolean;
                            default "true";
                            description "Allow the VRRP to override an existing non-owner master";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/telnet-reply
                        leaf telnet-reply {
                            type boolean;
                            default "false";
                            description "Allow non-owner master to reply to Telnet requests";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/traceroute-reply
                        leaf traceroute-reply {
                            type boolean;
                            default "false";
                            description "Allow non-owner master to reply to traceroute requests";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/standby-forwarding
                        leaf standby-forwarding {
                            type boolean;
                            default "false";
                            description "Allow standby router to forward traffic";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/ntp-reply
                        leaf ntp-reply {
                            type boolean;
                            default "false";
                            description "Allow processing of NTP requests";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/oper-group
                        leaf oper-group {
                            type leafref {
                                path "../../../../../service/oper-group/name";
                            }
                            description "Operational group name associated with the VRRP";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/monitor-oper-group
                        leaf monitor-oper-group {
                            type leafref {
                                path "../../../../../service/oper-group/name";
                            }
                            sros-ext:immutable;
                            description "VRRP instance to follow a specified operational group";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/interface/ipv6/vrrp/bfd-liveness
                        container bfd-liveness {
                            presence "Enable the bfd-liveness context";
                            description "Enable the bfd-liveness context";

                            ///nokia-conf/configure/router/interface/ipv6/vrrp/bfd-liveness/dest-ip
                            leaf dest-ip {
                                type types-sros:ip-address;
                                sros-ext:immutable;
                                mandatory true;
                                description "Destination address for the BFD session";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/vrrp/bfd-liveness/service-name
                            leaf service-name {
                                type types-services:service-name;
                                sros-ext:immutable;
                                description "Administrative service name";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/vrrp/bfd-liveness/interface-name
                            leaf interface-name {
                                type types-sros:interface-name;
                                sros-ext:immutable;
                                mandatory true;
                                description "Name of the interface running BFD";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/vrrp/bfd-liveness/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/interface/ipv6/vrrp/bfd-liveness/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // container bfd-liveness

                    } // list vrrp

                } // container ipv6

                ///nokia-conf/configure/router/interface/ip-tunnel
                container ip-tunnel {
                    status obsolete;
                    when "../flavor = 'control-tunnel'" {
                        description "only allowed in an interface with flavor control-tunnel";
                    }
                    description "Enter the ip-tunnel context";

                    ///nokia-conf/configure/router/interface/ip-tunnel/remote-ip
                    leaf remote-ip {
                        status obsolete;
                        type types-sros:ipv4-unicast-address;
                        description "IP address of the remote end of the IP tunnel";
                    }

                } // container ip-tunnel

                ///nokia-conf/configure/router/interface/qos
                container qos {
                    description "Enter the qos context";

                    ///nokia-conf/configure/router/interface/qos/network-policy
                    leaf network-policy {
                        type leafref {
                            path "../../../../qos/network/network-policy-name";
                        }
                        description "Network policy name associated with a network interface";
                    }

                    ///nokia-conf/configure/router/interface/qos/egress-port-redirect-group
                    leaf egress-port-redirect-group {
                        type leafref {
                            path "../../../../qos/queue-group-templates/egress/queue-group/egress-queue-group-name";
                        }
                        description "QoS queue group name";
                    }

                    ///nokia-conf/configure/router/interface/qos/egress-instance
                    leaf egress-instance {
                        type types-services:qos-qgrp-instance-id;
                        description "Port egress queue group instance for this interface";
                    }

                    ///nokia-conf/configure/router/interface/qos/ingress-fp-redirect-group
                    leaf ingress-fp-redirect-group {
                        type leafref {
                            path "../../../../qos/queue-group-templates/ingress/queue-group/ingress-queue-group-name";
                        }
                        description "Forwarding plane queue group policy for the interface";
                    }

                    ///nokia-conf/configure/router/interface/qos/ingress-instance
                    leaf ingress-instance {
                        type types-services:qos-qgrp-instance-id;
                        description "Forwarding plane ingress queue group for this interface";
                    }

                    ///nokia-conf/configure/router/interface/qos/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/interface/qos/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container qos

                ///nokia-conf/configure/router/interface/if-attribute
                container if-attribute {
                    when "../../router-name = 'Base' and not(../flavor = 'gmpls-loopback') and not(../flavor = 'control-tunnel') and not(../flavor = 'unnumbered-mpls-tp')" {
                        description "only allowed in router Base, and not in an interface with flavor control-tunnel|gmpls-loopback|unnumbered-mpls-tp";
                    }
                    description "Enter the if-attribute context";

                    ///nokia-conf/configure/router/interface/if-attribute/admin-group
                    leaf-list admin-group {
                        type leafref {
                            path "../../../../routing-options/if-attribute/admin-group/group-name";
                        }
                        max-elements 32;
                        description "Administrative group name for the interface";
                    }

                    ///nokia-conf/configure/router/interface/if-attribute/srlg-group
                    list srlg-group {
                        key "name";
                        description "Add a list entry for srlg-group";

                        ///nokia-conf/configure/router/interface/if-attribute/srlg-group/name
                        leaf name {
                            type leafref {
                                path "../../../../../routing-options/if-attribute/srlg-group/name";
                            }
                            description "SRLG name";
                        }

                    } // list srlg-group

                    ///nokia-conf/configure/router/interface/if-attribute/delay
                    container delay {
                        description "Enter the delay context";

                        ///nokia-conf/configure/router/interface/if-attribute/delay/delay-selection
                        leaf delay-selection {
                            when "not(../../../interface-name = 'system')" {
                                description "Not modifiable on this interface.";
                            }
                            type enumeration {
                                enum static                         { value 1; }
                                enum dynamic                        { value 2; }
                                enum static-preferred               { value 3; }
                                enum dynamic-preferred              { value 4; }
                            }
                            default "static-preferred";
                            description "Delay source advertised by IGP for the interface";
                        }

                        ///nokia-conf/configure/router/interface/if-attribute/delay/static
                        leaf static {
                            type int32 {
                                range "1..16777214";
                            }
                            units "microseconds";
                            description "Unidirectional link delay static information";
                        }

                        ///nokia-conf/configure/router/interface/if-attribute/delay/dynamic
                        container dynamic {
                            when "not(../../../interface-name = 'system')" {
                                description "Not modifiable on this interface.";
                            }
                            description "Enter the dynamic context";

                            ///nokia-conf/configure/router/interface/if-attribute/delay/dynamic/measurement-template
                            leaf measurement-template {
                                type leafref {
                                    path "../../../../../../test-oam/link-measurement/measurement-template/template-name";
                                }
                                description "Link delay measurement template for the interface";
                            }

                            ///nokia-conf/configure/router/interface/if-attribute/delay/dynamic/twamp-light
                            container twamp-light {
                                description "Enter the twamp-light context";

                                ///nokia-conf/configure/router/interface/if-attribute/delay/dynamic/twamp-light/ipv4
                                container ipv4 {
                                    description "Enter the ipv4 context";

                                    ///nokia-conf/configure/router/interface/if-attribute/delay/dynamic/twamp-light/ipv4/admin-state
                                    leaf admin-state {
                                        type types-sros:admin-state;
                                        default "disable";
                                        description "Administrative state of IPv4 TWAMP Light packets";
                                    }

                                    ///nokia-conf/configure/router/interface/if-attribute/delay/dynamic/twamp-light/ipv4/destination
                                    leaf destination {
                                        type types-sros:ipv4-unicast-address;
                                        description "TWAMP Light measurement packet destination IPv4 address";
                                    }

                                    ///nokia-conf/configure/router/interface/if-attribute/delay/dynamic/twamp-light/ipv4/source
                                    leaf source {
                                        type types-sros:ipv4-unicast-address;
                                        description "TWAMP Light measurement packets source IPv4 address";
                                    }

                                } // container ipv4

                                ///nokia-conf/configure/router/interface/if-attribute/delay/dynamic/twamp-light/ipv6
                                container ipv6 {
                                    description "Enter the ipv6 context";

                                    ///nokia-conf/configure/router/interface/if-attribute/delay/dynamic/twamp-light/ipv6/admin-state
                                    leaf admin-state {
                                        type types-sros:admin-state;
                                        default "disable";
                                        description "Administrative state of IPv6 TWAMP Light packets";
                                    }

                                    ///nokia-conf/configure/router/interface/if-attribute/delay/dynamic/twamp-light/ipv6/destination
                                    leaf destination {
                                        type types-sros:ipv6-unicast-or-linklocal-address;
                                        description "TWAMP Light measurement packet destination IPv6 address";
                                    }

                                    ///nokia-conf/configure/router/interface/if-attribute/delay/dynamic/twamp-light/ipv6/source
                                    leaf source {
                                        type types-sros:ipv6-unicast-or-linklocal-address;
                                        description "TWAMP Light measurement packets source IPv6 address";
                                    }

                                } // container ipv6

                            } // container twamp-light

                        } // container dynamic

                    } // container delay

                } // container if-attribute

                ///nokia-conf/configure/router/interface/untrusted
                container untrusted {
                    when "../../router-name = 'Base' and ../flavor = 'regular' and not(../interface-name = 'system')" {
                        description "only allowed in regular interfaces in router Base, and not in the system interface";
                    }
                    presence "Enable the untrusted context";
                    description "Enable the untrusted context";

                    ///nokia-conf/configure/router/interface/untrusted/default-forwarding
                    leaf default-forwarding {
                        type types-router:selective-ilm-default-forwarding;
                        default "forward";
                        description "Default action for selective ILM";
                    }

                } // container untrusted

                ///nokia-conf/configure/router/interface/network-domains
                container network-domains {
                    when "../../router-name = 'Base'" {
                        description "only allowed in the Base router";
                    }
                    description "Enter the network-domains context";

                    ///nokia-conf/configure/router/interface/network-domains/network-domain
                    list network-domain {
                        key "domain-name";
                        max-elements 5;
                        description "Add a list entry for network-domain";

                        ///nokia-conf/configure/router/interface/network-domains/network-domain/domain-name
                        leaf domain-name {
                            type leafref {
                                path "../../../../network-domains/network-domain/domain-name";
                            }
                            description "Network domain name";
                        }

                    } // list network-domain

                } // container network-domains


            } // list interface

            ///nokia-conf/configure/router/ipv6
            container ipv6 {
                description "Enter the ipv6 context";

                ///nokia-conf/configure/router/ipv6/neighbor-discovery
                container neighbor-discovery {
                    when "../../router-name = 'Base'" {
                        description "only allowed in the Base router";
                    }
                    description "Enter the neighbor-discovery context";

                    ///nokia-conf/configure/router/ipv6/neighbor-discovery/reachable-time
                    leaf reachable-time {
                        type types-router:neighbor-discovery-reachable-time;
                        default "30";
                        description "Neighbor reachability detection timer";
                    }

                    ///nokia-conf/configure/router/ipv6/neighbor-discovery/stale-time
                    leaf stale-time {
                        type types-router:neighbor-discovery-stale-time;
                        default "14400";
                        description "Neighbor discovery cache entry stale time";
                    }

                } // container neighbor-discovery

                ///nokia-conf/configure/router/ipv6/router-advertisement
                container router-advertisement {
                    when "../../router-name = 'Base'" {
                        description "only allowed in the Base router";
                    }
                    description "Enter the router-advertisement context";

                    ///nokia-conf/configure/router/ipv6/router-advertisement/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/ipv6/router-advertisement/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/ipv6/router-advertisement/dns-options
                    container dns-options {
                        presence "Enable the dns-options context";
                        description "Enable the dns-options context";

                        ///nokia-conf/configure/router/ipv6/router-advertisement/dns-options/rdnss-lifetime
                        leaf rdnss-lifetime {
                            type union {
                                type enumeration {
                                    enum infinite                       { value -1; }
                                }
                                type int32 {
                                    range "0|4..3600";
                                }
                            }
                            units "seconds";
                            default "infinite";
                            description "Maximum time over which the RDNSS address is valid";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/dns-options/server
                        leaf-list server {
                            type types-sros:ipv6-address;
                            max-elements 4;
                            ordered-by user;
                            description "RAs that are forwarded to IPv6 DNS servers";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/dns-options/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/dns-options/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container dns-options

                    ///nokia-conf/configure/router/ipv6/router-advertisement/interface
                    list interface {
                        key "ip-int-name";
                        description "Enter the interface list instance";

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/ip-int-name
                        leaf ip-int-name {
                            type types-sros:interface-name;
                            description "Router interface name";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/current-hop-limit
                        leaf current-hop-limit {
                            type uint16 {
                                range "0..255";
                            }
                            default "64";
                            description "Hop limit advertised in RA messages";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/managed-configuration
                        leaf managed-configuration {
                            type boolean;
                            default "false";
                            description "Set the managed address configuration flag";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/max-advertisement-interval
                        leaf max-advertisement-interval {
                            type uint32 {
                                range "4..1800";
                            }
                            units "seconds";
                            default "600";
                            description "Maximum time between sending advertisement messages";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/min-advertisement-interval
                        leaf min-advertisement-interval {
                            type uint32 {
                                range "3..1350";
                            }
                            units "seconds";
                            default "200";
                            description "Minimum interval between router advertisement messages";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/mtu
                        leaf mtu {
                            type uint32 {
                                range "1280..9800";
                            }
                            description "MTU for sending packets to the router";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/other-stateful-configuration
                        leaf other-stateful-configuration {
                            type boolean;
                            default "false";
                            description "Set the other configuration flag";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/reachable-time
                        leaf reachable-time {
                            type uint32 {
                                range "0..3600000";
                            }
                            units "milliseconds";
                            default "0";
                            description "Time the router is reachable by other hosts or nodes";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/retransmit-time
                        leaf retransmit-time {
                            type uint32 {
                                range "0..1800000";
                            }
                            units "milliseconds";
                            default "0";
                            description "Time to advertise neighbor advertisement messages";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/router-lifetime
                        leaf router-lifetime {
                            type uint32 {
                                range "0|4..9000";
                            }
                            units "seconds";
                            default "1800";
                            description "Lifetime value in neighbor advertisement messages";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of router advertisement";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/use-virtual-mac
                        leaf use-virtual-mac {
                            type boolean;
                            default "false";
                            description "Use VRRP virtual MAC address for advertisement message";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/dns-options
                        container dns-options {
                            presence "Enable the dns-options context";
                            description "Enable the dns-options context";

                            ///nokia-conf/configure/router/ipv6/router-advertisement/interface/dns-options/include-rdnss
                            leaf include-rdnss {
                                type boolean;
                                default "true";
                                description "Include the RDNSS option in the RA";
                            }

                            ///nokia-conf/configure/router/ipv6/router-advertisement/interface/dns-options/rdnss-lifetime
                            leaf rdnss-lifetime {
                                type union {
                                    type int32 {
                                        range "0|4..3600";
                                    }
                                    type enumeration {
                                        enum infinite                       { value -1; }
                                    }
                                }
                                units "seconds";
                                description "Maximum time over which the RDNSS address 25 is valid";
                            }

                            ///nokia-conf/configure/router/ipv6/router-advertisement/interface/dns-options/server
                            leaf-list server {
                                type types-sros:ipv6-address;
                                max-elements 4;
                                ordered-by user;
                                description "RAs that are forwarded to IPv6 DNS servers";
                            }

                            ///nokia-conf/configure/router/ipv6/router-advertisement/interface/dns-options/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/ipv6/router-advertisement/interface/dns-options/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // container dns-options

                        ///nokia-conf/configure/router/ipv6/router-advertisement/interface/prefix
                        list prefix {
                            key "ipv6-prefix";
                            max-elements 254;
                            description "Enter the prefix list instance";

                            ///nokia-conf/configure/router/ipv6/router-advertisement/interface/prefix/ipv6-prefix
                            leaf ipv6-prefix {
                                type types-sros:ipv6-prefix;
                                description "IPv6 address prefix";
                            }

                            ///nokia-conf/configure/router/ipv6/router-advertisement/interface/prefix/autonomous
                            leaf autonomous {
                                type boolean;
                                default "true";
                                description "Set the autonomous flag value";
                            }

                            ///nokia-conf/configure/router/ipv6/router-advertisement/interface/prefix/on-link
                            leaf on-link {
                                type boolean;
                                default "true";
                                description "Use prefix for on-link determination";
                            }

                            ///nokia-conf/configure/router/ipv6/router-advertisement/interface/prefix/preferred-lifetime
                            leaf preferred-lifetime {
                                type union {
                                    type enumeration {
                                        enum infinite                       { value -1; }
                                    }
                                    type uint32 {
                                        range "0..4294967294";
                                    }
                                }
                                units "seconds";
                                default "604800";
                                description "Remaining time that the prefix remains preferred";
                            }

                            ///nokia-conf/configure/router/ipv6/router-advertisement/interface/prefix/valid-lifetime
                            leaf valid-lifetime {
                                type union {
                                    type enumeration {
                                        enum infinite                       { value -1; }
                                    }
                                    type uint32 {
                                        range "0..4294967294";
                                    }
                                }
                                units "seconds";
                                default "2592000";
                                description "Remaining time in which the prefix is still valid";
                            }

                            ///nokia-conf/configure/router/ipv6/router-advertisement/interface/prefix/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/ipv6/router-advertisement/interface/prefix/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list prefix

                    } // list interface

                } // container router-advertisement

            } // container ipv6

            ///nokia-conf/configure/router/isa-service-chaining
            container isa-service-chaining {
                presence "Enable the isa-service-chaining context";
                description "Enable the isa-service-chaining context";

                ///nokia-conf/configure/router/isa-service-chaining/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/isa-service-chaining/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/router/isa-service-chaining/nat-group
                list nat-group {
                    key "id";
                    description "Add a list entry for nat-group";

                    ///nokia-conf/configure/router/isa-service-chaining/nat-group/id
                    leaf id {
                        type uint32;
                        description "ISA NAT group/ISA WLAN GW group ID for the EVPN pools";
                    }

                } // list nat-group

                ///nokia-conf/configure/router/isa-service-chaining/vxlan-vtep-range
                container vxlan-vtep-range {
                    presence "Enable the vxlan-vtep-range context";
                    description "Enable the vxlan-vtep-range context";

                    ///nokia-conf/configure/router/isa-service-chaining/vxlan-vtep-range/start
                    leaf start {
                        type types-sros:ip-unicast-without-local-address;
                        mandatory true;
                        description "Start address of the VXLAN VTEP range";
                    }

                    ///nokia-conf/configure/router/isa-service-chaining/vxlan-vtep-range/end
                    leaf end {
                        type types-sros:ip-unicast-without-local-address;
                        mandatory true;
                        description "End address of the VXLAN VTEP range";
                    }

                } // container vxlan-vtep-range

            } // container isa-service-chaining

            ///nokia-conf/configure/router/mpls-labels
            container mpls-labels {
                when "../router-name = 'Base'" {
                    description "only allowed in the Base router";
                }
                description "Enter the mpls-labels context";

                ///nokia-conf/configure/router/mpls-labels/bgp-labels-hold-timer
                leaf bgp-labels-hold-timer {
                    type uint32 {
                        range "0..255";
                    }
                    default "0";
                    description "BGP labels hold timer for the ingress router";
                }

                ///nokia-conf/configure/router/mpls-labels/static-label-range
                leaf static-label-range {
                    type uint32 {
                        range "0..1048544";
                    }
                    default "18400";
                    description "Static label range on the ingress router";
                }

                ///nokia-conf/configure/router/mpls-labels/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/mpls-labels/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/router/mpls-labels/sr-labels
                container sr-labels {
                    description "Enter the sr-labels context";

                    ///nokia-conf/configure/router/mpls-labels/sr-labels/start
                    leaf start {
                        type types-sros:mpls-label;
                        description "Start label value for the SRGB";
                    }

                    ///nokia-conf/configure/router/mpls-labels/sr-labels/end
                    leaf end {
                        type types-sros:mpls-label;
                        description "End label for the SRGB";
                    }

                } // container sr-labels

                ///nokia-conf/configure/router/mpls-labels/reserved-label-block
                list reserved-label-block {
                    key "label-block-name";
                    max-elements 16;
                    description "Enter the reserved-label-block list instance";

                    ///nokia-conf/configure/router/mpls-labels/reserved-label-block/start-label
                    leaf start-label {
                        type uint32 {
                            range "32..1048575";
                        }
                        description "Start label value for the reserved label block";
                    }

                    ///nokia-conf/configure/router/mpls-labels/reserved-label-block/end-label
                    leaf end-label {
                        type uint32 {
                            range "32..1048575";
                        }
                        description "End label for the reserved label block";
                    }

                    ///nokia-conf/configure/router/mpls-labels/reserved-label-block/label-block-name
                    leaf label-block-name {
                        type types-sros:named-item-64;
                        description "Name for the reserved label block";
                    }

                    ///nokia-conf/configure/router/mpls-labels/reserved-label-block/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/mpls-labels/reserved-label-block/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list reserved-label-block

            } // container mpls-labels

            ///nokia-conf/configure/router/mtrace2
            container mtrace2 {
                description "Enter the mtrace2 context";

                ///nokia-conf/configure/router/mtrace2/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of multicast path tracing";
                }

                ///nokia-conf/configure/router/mtrace2/udp-port
                leaf udp-port {
                    type uint32 {
                        range "1024..49151";
                    }
                    default "5000";
                    description "Destination and listening port for the mtrace2 command";
                }

            } // container mtrace2

            ///nokia-conf/configure/router/sgt-qos
            container sgt-qos {
                description "Enter the sgt-qos context";

                ///nokia-conf/configure/router/sgt-qos/dscp
                container dscp {
                    description "Enter the dscp context";

                    ///nokia-conf/configure/router/sgt-qos/dscp/application
                    list application {
                        key "dscp-app-name";
                        description "Enter the application list instance";

                        ///nokia-conf/configure/router/sgt-qos/dscp/application/dscp-app-name
                        leaf dscp-app-name {
                            type types-router:dscp-app-name;
                            description "DSCP application identifier on the NOKIA SR OS router that generates control traffic over IP";
                        }

                        ///nokia-conf/configure/router/sgt-qos/dscp/application/dscp
                        leaf dscp {
                            type union {
                                type types-qos:dscp-name;
                                type int32 {
                                    range "0..63";
                                }
                            }
                            description "DSCP value to the traffic generated by this application";
                        }

                        ///nokia-conf/configure/router/sgt-qos/dscp/application/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/sgt-qos/dscp/application/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list application

                    ///nokia-conf/configure/router/sgt-qos/dscp/dscp-map
                    list dscp-map {
                        when "../../../router-name = 'Base'" {
                            description "only allowed in the Base router";
                        }
                        key "dscp-name";
                        description "Enter the dscp-map list instance";

                        ///nokia-conf/configure/router/sgt-qos/dscp/dscp-map/dscp-name
                        leaf dscp-name {
                            type types-qos:dscp-name;
