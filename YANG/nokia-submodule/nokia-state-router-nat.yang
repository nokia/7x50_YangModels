         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-05-30";

    grouping state-router-nat {
        ///nokia-state/state/router/nat
        container nat {
            description "Enable the nat context";

            ///nokia-state/state/router/nat/outside-dnat-only-routes-counter
            leaf outside-dnat-only-routes-counter {
                type yang:gauge32;
                description
                    "the actual number of routes used to direct
                     downstream traffic from this outside virtual router instance
                     to a NAT ISA card in Destination NAT operation";
            }

            ///nokia-state/state/router/nat/inside
            container inside {
                description "Enter the inside context";

                ///nokia-state/state/router/nat/inside/large-scale
                container large-scale {
                    description "Enter the large-scale context";

                    ///nokia-state/state/router/nat/inside/large-scale/nat44
                    container nat44 {
                        description "Enter the nat44 context";

                        ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic
                        container deterministic {
                            description "Enter the deterministic context";

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/policy-map
                            list policy-map {
                                status obsolete;
                                key "source-prefix";
                                description "Enter the policy-map list instance";

                                ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/policy-map/source-prefix
                                leaf source-prefix {
                                    status obsolete;
                                    type types-sros:ipv4-unicast-prefix;
                                    description "Source prefix that adds traffic to NAT pool";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/policy-map/oper-state
                                leaf oper-state {
                                    status obsolete;
                                    type types-sros:up-down-status;
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/policy-map/map
                                list map {
                                    status obsolete;
                                    key "from to";
                                    description "Enter the map list instance";

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/policy-map/map/from
                                    leaf from {
                                        status obsolete;
                                        type types-sros:ipv4-address;
                                        description "Beginning of the range for IPv4 addresses";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/policy-map/map/to
                                    leaf to {
                                        status obsolete;
                                        type types-sros:ipv4-address;
                                        description "End of the range for IPv4 addresses";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/policy-map/map/oper-state
                                    leaf oper-state {
                                        status obsolete;
                                        type types-sros:up-down-status;
                                    }

                                } // list map

                            } // list policy-map

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/prefix-map
                            list prefix-map {
                                key "source-prefix nat-policy";
                                description "Enter the prefix-map list instance";

                                ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/prefix-map/source-prefix
                                leaf source-prefix {
                                    type types-sros:ipv4-unicast-prefix;
                                    description "Traffic sent from sources within this prefix will be NATed";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/prefix-map/nat-policy
                                leaf nat-policy {
                                    type leafref {
                                        path "../../../../../../../../service/nat/nat-policy/name";
                                    }
                                    description "NAT policy";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/prefix-map/oper-state
                                leaf oper-state {
                                    type types-sros:up-down-status;
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/prefix-map/map
                                list map {
                                    key "from to";
                                    description "Enter the map list instance";

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/prefix-map/map/from
                                    leaf from {
                                        type types-sros:ipv4-address;
                                        description "Beginning of the range for IPv4 addresses";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/prefix-map/map/to
                                    leaf to {
                                        type types-sros:ipv4-address;
                                        description "End of the range for IPv4 addresses";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/deterministic/prefix-map/map/oper-state
                                    leaf oper-state {
                                        type types-sros:up-down-status;
                                    }

                                } // list map

                            } // list prefix-map

                        } // container deterministic

                        ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber
                        list subscriber {
                            key "id nat-policy";
                            description "Enter the subscriber list instance";

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/id
                            leaf id {
                                type uint32;
                                description "Identifier of this subscriber";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/nat-policy
                            leaf nat-policy {
                                type types-sros:named-item;
                                description "NAT Policy associated with this subscriber";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/name
                            leaf name {
                                type types-sros:named-item;
                                description
                                    "Subscriber's identification string.

                                     A string enclosed by square brackets [] indicates an automatically system-generated name";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/type
                            leaf type {
                                type types-nat:sub-type;
                                description "Type of this subscriber";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/inside-ip-prefix
                            leaf inside-ip-prefix {
                                type types-sros:ip-prefix;
                                description "Inside IP prefix";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/nat-group
                            leaf nat-group {
                                type uint8;
                                description "Identifier of the NAT Integrated Service Adaptor group";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/group-member
                            leaf group-member {
                                type types-nat:bb-group-member;
                                description "Identifier of the NAT group member";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics
                            container statistics {
                                description "Enter the statistics context";

                                ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/protocol
                                container protocol {
                                    description "Enter the protocol context";

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/protocol/icmp
                                    container icmp {
                                        description "Enter the icmp context";

                                        ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/protocol/icmp/port-usage
                                        leaf port-usage {
                                            type types-sros:percent;
                                            description "ICMP port usage of this NAT subscriber and policy";
                                        }

                                        ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/protocol/icmp/port-usage-high
                                        leaf port-usage-high {
                                            type boolean;
                                            description "ICMP port usage has reached configured limit";
                                        }

                                    } // container icmp

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/protocol/tcp
                                    container tcp {
                                        description "Enter the tcp context";

                                        ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/protocol/tcp/port-usage
                                        leaf port-usage {
                                            type types-sros:percent;
                                            description "TCP port usage of this NAT subscriber and policy";
                                        }

                                        ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/protocol/tcp/port-usage-high
                                        leaf port-usage-high {
                                            type boolean;
                                            description "TCP port usage has reached configured limit";
                                        }

                                    } // container tcp

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/protocol/udp
                                    container udp {
                                        description "Enter the udp context";

                                        ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/protocol/udp/port-usage
                                        leaf port-usage {
                                            type types-sros:percent;
                                            description "UDP port usage of this NAT subscriber and policy";
                                        }

                                        ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/protocol/udp/port-usage-high
                                        leaf port-usage-high {
                                            type boolean;
                                            description "UDP port usage has reached configured limit";
                                        }

                                    } // container udp

                                } // container protocol

                                ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/sessions
                                container sessions {
                                    description "Enter the sessions context";

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/sessions/usage
                                    leaf usage {
                                        type types-sros:percent;
                                        description "Session usage of this NAT subscriber and policy";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/sessions/usage-high
                                    leaf usage-high {
                                        type boolean;
                                        description "Session usage has reached configured limit";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/sessions/active
                                    leaf active {
                                        type uint32;
                                        description "Number of active sessions of this NAT subscriber and policy";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/sessions/reserved
                                    leaf reserved {
                                        type uint32;
                                        description "Number of active prioritized sessions of this subscriber and policy";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/statistics/sessions/peak
                                    leaf peak {
                                        type uint32;
                                        description "Maximum number of sessions that were active together up to this point in time";
                                    }

                                } // container sessions

                            } // container statistics

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/outside-ip-addresses
                            container outside-ip-addresses {
                                description "Enter the outside-ip-addresses context";

                                ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/outside-ip-addresses/address
                                list address {
                                    key "address";
                                    description "Enter the address list instance";

                                    ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/outside-ip-addresses/address/address
                                    leaf address {
                                        type types-sros:ipv4-address;
                                        description "Outside IP address";
                                    }

                                } // list address

                            } // container outside-ip-addresses

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/port-block
                            list port-block {
                                key "outside-router-instance outside-ip-address start";
                                description "Enter the port-block list instance";

                                ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/port-block/outside-router-instance
                                leaf outside-router-instance {
                                    type types-sros:router-instance;
                                    description "Outside router instance";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/port-block/outside-ip-address
                                leaf outside-ip-address {
                                    type types-sros:ipv4-address;
                                    description "Outside IP address";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/port-block/start
                                leaf start {
                                    type uint32;
                                    description "Start of the port block allocated to the subscriber";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/nat44/subscriber/port-block/end
                                leaf end {
                                    type uint32;
                                    description "End of the port block allocated to the subscriber";
                                }

                            } // list port-block

                        } // list subscriber

                        ///nokia-state/state/router/nat/inside/large-scale/nat44/route
                        list route {
                            key "ip-prefix-length";
                            description "Enter the route list instance";

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/route/ip-prefix-length
                            leaf ip-prefix-length {
                                type types-sros:ipv4-unicast-prefix;
                                description "Route destination";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/route/nat-policy
                            leaf nat-policy {
                                type types-sros:named-item;
                                description "NAT policy associated with the route";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat44/route/type
                            leaf type {
                                type enumeration {
                                    enum dynamic                        { value 1; }
                                    enum static                         { value 2; }
                                }
                                description "Type of the route";
                            }

                        } // list route

                    } // container nat44

                    ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite
                    container dual-stack-lite {
                        description "Enter the dual-stack-lite context";

                        ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic
                        container deterministic {
                            description "Enter the deterministic context";

                            ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/policy-map
