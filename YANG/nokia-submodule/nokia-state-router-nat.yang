                                status obsolete;
                                key "source-prefix";
                                description "Enter the policy-map list instance";

                                ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/policy-map/source-prefix
                                leaf source-prefix {
                                    status obsolete;
                                    type types-sros:ipv6-unicast-prefix;
                                    description "Source prefix that adds traffic to NAT pool";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/policy-map/oper-state
                                leaf oper-state {
                                    status obsolete;
                                    type types-sros:up-down-status;
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/policy-map/map
                                list map {
                                    status obsolete;
                                    key "from to";
                                    description "Enter the map list instance";

                                    ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/policy-map/map/from
                                    leaf from {
                                        status obsolete;
                                        type types-sros:ipv6-prefix;
                                        description "Beginning of the range for IPv6 addresses";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/policy-map/map/to
                                    leaf to {
                                        status obsolete;
                                        type types-sros:ipv6-prefix;
                                        description "Inside end IPv6 address to map to outside IP addresses";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/policy-map/map/oper-state
                                    leaf oper-state {
                                        status obsolete;
                                        type types-sros:up-down-status;
                                    }

                                } // list map

                            } // list policy-map

                            ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/prefix-map
                            list prefix-map {
                                key "source-prefix nat-policy";
                                description "Enter the prefix-map list instance";

                                ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/prefix-map/source-prefix
                                leaf source-prefix {
                                    type types-sros:ipv6-unicast-prefix;
                                    description "List containing source IP addresses on the private side";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/prefix-map/nat-policy
                                leaf nat-policy {
                                    type leafref {
                                        path "../../../../../../../../service/nat/nat-policy/name";
                                    }
                                    description "NAT policy";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/prefix-map/oper-state
                                leaf oper-state {
                                    type types-sros:up-down-status;
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/prefix-map/map
                                list map {
                                    key "from to";
                                    description "Enter the map list instance";

                                    ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/prefix-map/map/from
                                    leaf from {
                                        type types-sros:ipv6-prefix;
                                        description "Beginning of the range for IPv6 addresses";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/prefix-map/map/to
                                    leaf to {
                                        type types-sros:ipv6-prefix;
                                        description "End of the range for IPv6 addresses";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/deterministic/prefix-map/map/oper-state
                                    leaf oper-state {
                                        type types-sros:up-down-status;
                                    }

                                } // list map

                            } // list prefix-map

                        } // container deterministic

                        ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber
                        list subscriber {
                            key "id nat-policy";
                            description "Enter the subscriber list instance";

                            ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/id
                            leaf id {
                                type uint32;
                                description "Identifier of this subscriber";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/nat-policy
                            leaf nat-policy {
                                type types-sros:named-item;
                                description "NAT Policy associated with this subscriber";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/name
                            leaf name {
                                type types-sros:named-item;
                                description
                                    "Subscriber's identification string.

                                     A string enclosed by square brackets [] indicates an automatically system-generated name";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/type
                            leaf type {
                                type types-nat:sub-type;
                                description "Type of this subscriber";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/inside-ip-prefix
                            leaf inside-ip-prefix {
                                type types-sros:ip-prefix;
                                description "Inside IP prefix";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/nat-group
                            leaf nat-group {
                                type uint8;
                                description "Identifier of the NAT Integrated Service Adaptor group";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/group-member
                            leaf group-member {
                                type types-nat:bb-group-member;
                                description "Identifier of the NAT group member";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/statistics
                            container statistics {
                                description "Enter the statistics context";

                                ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/statistics/protocol
                                container protocol {
                                    description "Enter the protocol context";

                                    ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/statistics/protocol/icmp
                                    container icmp {
                                        description "Enter the icmp context";

                                        ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/statistics/protocol/icmp/port-usage
                                        leaf port-usage {
                                            type types-sros:percent;
                                            description "ICMP port usage of this NAT subscriber and policy";
                                        }

                                        ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/statistics/protocol/icmp/port-usage-high
                                        leaf port-usage-high {
