                            ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/port-block
                            list port-block {
                                key "outside-router-instance outside-ip-address start";
                                description "Enter the port-block list instance";

                                ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/port-block/outside-router-instance
                                leaf outside-router-instance {
                                    type types-sros:router-instance;
                                    description "Outside router instance";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/port-block/outside-ip-address
                                leaf outside-ip-address {
                                    type types-sros:ipv4-address;
                                    description "Outside IP address";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/port-block/start
                                leaf start {
                                    type uint32;
                                    description "Start of the port block allocated to the subscriber";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/dual-stack-lite/subscriber/port-block/end
                                leaf end {
                                    type uint32;
                                    description "End of the port block allocated to the subscriber";
                                }

                            } // list port-block

                        } // list subscriber

                    } // container dual-stack-lite

                    ///nokia-state/state/router/nat/inside/large-scale/nat64
                    container nat64 {
                        description "Enable the nat64 context";

                        ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber
                        list subscriber {
                            key "id nat-policy";
                            description "Enter the subscriber list instance";

                            ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/id
                            leaf id {
                                type uint32;
                                description "Identifier of this subscriber";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/nat-policy
                            leaf nat-policy {
                                type types-sros:named-item;
                                description "NAT Policy associated with this subscriber";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/name
                            leaf name {
                                type types-sros:named-item;
                                description
                                    "Subscriber's identification string.

                                     A string enclosed by square brackets [] indicates an automatically system-generated name";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/type
                            leaf type {
                                type types-nat:sub-type;
                                description "Type of this subscriber";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/inside-ip-prefix
                            leaf inside-ip-prefix {
                                type types-sros:ip-prefix;
                                description "Inside IP prefix";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/nat-group
                            leaf nat-group {
                                type uint8;
                                description "Identifier of the NAT Integrated Service Adaptor group";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/group-member
                            leaf group-member {
                                type types-nat:bb-group-member;
                                description "Identifier of the NAT group member";
                            }

                            ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics
                            container statistics {
                                description "Enter the statistics context";

                                ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/protocol
                                container protocol {
                                    description "Enter the protocol context";

                                    ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/protocol/icmp
                                    container icmp {
                                        description "Enter the icmp context";

                                        ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/protocol/icmp/port-usage
                                        leaf port-usage {
                                            type types-sros:percent;
                                            description "ICMP port usage of this NAT subscriber and policy";
                                        }

                                        ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/protocol/icmp/port-usage-high
                                        leaf port-usage-high {
                                            type boolean;
                                            description "ICMP port usage has reached configured limit";
                                        }

                                    } // container icmp

                                    ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/protocol/tcp
                                    container tcp {
                                        description "Enter the tcp context";

                                        ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/protocol/tcp/port-usage
                                        leaf port-usage {
                                            type types-sros:percent;
                                            description "TCP port usage of this NAT subscriber and policy";
                                        }

                                        ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/protocol/tcp/port-usage-high
                                        leaf port-usage-high {
                                            type boolean;
                                            description "TCP port usage has reached configured limit";
                                        }

                                    } // container tcp

                                    ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/protocol/udp
                                    container udp {
                                        description "Enter the udp context";

                                        ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/protocol/udp/port-usage
                                        leaf port-usage {
                                            type types-sros:percent;
                                            description "UDP port usage of this NAT subscriber and policy";
                                        }

                                        ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/protocol/udp/port-usage-high
                                        leaf port-usage-high {
                                            type boolean;
                                            description "UDP port usage has reached configured limit";
                                        }

                                    } // container udp

                                } // container protocol

                                ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/sessions
                                container sessions {
                                    description "Enter the sessions context";

                                    ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/sessions/usage
                                    leaf usage {
                                        type types-sros:percent;
                                        description "Session usage of this NAT subscriber and policy";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/sessions/usage-high
                                    leaf usage-high {
                                        type boolean;
                                        description "Session usage has reached configured limit";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/sessions/active
                                    leaf active {
                                        type uint32;
                                        description "Number of active sessions of this NAT subscriber and policy";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/sessions/reserved
                                    leaf reserved {
                                        type uint32;
                                        description "Number of active prioritized sessions of this subscriber and policy";
                                    }

                                    ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/statistics/sessions/peak
                                    leaf peak {
                                        type uint32;
                                        description "Maximum number of sessions that were active together up to this point in time";
                                    }

                                } // container sessions

                            } // container statistics

                            ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/outside-ip-addresses
                            container outside-ip-addresses {
                                description "Enter the outside-ip-addresses context";

                                ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/outside-ip-addresses/address
                                list address {
                                    key "address";
                                    description "Enter the address list instance";

                                    ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/outside-ip-addresses/address/address
                                    leaf address {
                                        type types-sros:ipv4-address;
                                        description "Outside IP address";
                                    }

                                } // list address

                            } // container outside-ip-addresses

                            ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/port-block
                            list port-block {
                                key "outside-router-instance outside-ip-address start";
                                description "Enter the port-block list instance";

                                ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/port-block/outside-router-instance
                                leaf outside-router-instance {
                                    type types-sros:router-instance;
                                    description "Outside router instance";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/port-block/outside-ip-address
                                leaf outside-ip-address {
                                    type types-sros:ipv4-address;
                                    description "Outside IP address";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/port-block/start
                                leaf start {
                                    type uint32;
                                    description "Start of the port block allocated to the subscriber";
                                }

                                ///nokia-state/state/router/nat/inside/large-scale/nat64/subscriber/port-block/end
                                leaf end {
                                    type uint32;
                                    description "End of the port block allocated to the subscriber";
                                }

                            } // list port-block

                        } // list subscriber

                    } // container nat64

                } // container large-scale

            } // container inside

            ///nokia-state/state/router/nat/outside
            container outside {
                description "Enter the outside context";

                ///nokia-state/state/router/nat/outside/pool
                list pool {
                    key "name";
                    description "Enter the pool list instance";

                    ///nokia-state/state/router/nat/outside/pool/name
                    leaf name {
                        type types-sros:named-item;
                        description "NAT pool name";
                    }

                    ///nokia-state/state/router/nat/outside/pool/oper-mode
                    leaf oper-mode {
                        type types-nat:pool-mode;
                        description "Mode of operation";
                    }

                    ///nokia-state/state/router/nat/outside/pool/large-scale
                    container large-scale {
                        description "Enter the large-scale context";

                        ///nokia-state/state/router/nat/outside/pool/large-scale/redundancy
                        container redundancy {
                            description "Enter the redundancy context";

                            ///nokia-state/state/router/nat/outside/pool/large-scale/redundancy/state
                            leaf state {
                                type enumeration {
                                    enum down                           { value 0; }
                                    enum down-held                      { value 1; }
                                    enum standby                        { value 2; }
                                    enum standby-held                   { value 3; }
                                    enum active                         { value 4; }
                                    enum active-held                    { value 5; }
                                    enum active-sick                    { value 6; }
                                    enum needs-audit                    { value 7; }
                                    enum disabled-up                    { value 8; }
                                    enum disabled-down                  { value 9; }
                                }
                                description "Dual-homing state in a multi-chassis redundancy environment";
                            }

                        } // container redundancy

                        ///nokia-state/state/router/nat/outside/pool/large-scale/group-member
                        list group-member {
                            key "group-member";
                            description "Enter the group-member list instance";

                            ///nokia-state/state/router/nat/outside/pool/large-scale/group-member/group-member
                            leaf group-member {
                                type types-nat:bb-group-member;
                                description "group member ID";
                            }

                            ///nokia-state/state/router/nat/outside/pool/large-scale/group-member/block-usage
                            leaf block-usage {
                                type types-sros:percent;
                                description "Port block usage in large-scale NAT";
                            }

                            ///nokia-state/state/router/nat/outside/pool/large-scale/group-member/high-watermark-reached
                            leaf high-watermark-reached {
                                type boolean;
                                description "High watermark reached for the large-scale NAT block usage";
                            }

                        } // list group-member

                    } // container large-scale

                    ///nokia-state/state/router/nat/outside/pool/l2-aware
                    container l2-aware {
                        description "Enter the l2-aware context";

                        ///nokia-state/state/router/nat/outside/pool/l2-aware/block-usage
                        leaf block-usage {
                            type types-sros:percent;
                            description "Port block usage in L2-aware NAT";
                        }

                        ///nokia-state/state/router/nat/outside/pool/l2-aware/high-watermark-reached
                        leaf high-watermark-reached {
                            type boolean;
                            description "High watermark reached for the l2-aware block usage";
                        }

                        ///nokia-state/state/router/nat/outside/pool/l2-aware/port-block-extension
                        container port-block-extension {
                            description "Enable the port-block-extension context";

                            ///nokia-state/state/router/nat/outside/pool/l2-aware/port-block-extension/shared-port-blocks-per-ip
                            leaf shared-port-blocks-per-ip {
                                type uint32;
                                description "Number of shared port-blocks per IP";
                            }

                            ///nokia-state/state/router/nat/outside/pool/l2-aware/port-block-extension/subscriber
                            container subscriber {
                                description "Enter the subscriber context";

                                ///nokia-state/state/router/nat/outside/pool/l2-aware/port-block-extension/subscriber/usage
                                leaf usage {
                                    type types-sros:percent;
                                    description "Subscriber usage in L2-aware NAT";
                                }

                                ///nokia-state/state/router/nat/outside/pool/l2-aware/port-block-extension/subscriber/high-watermark-reached
                                leaf high-watermark-reached {
                                    type boolean;
                                    description "High watermark reached for the l2-aware subscriber usage";
                                }

                            } // container subscriber

                        } // container port-block-extension

                        ///nokia-state/state/router/nat/outside/pool/l2-aware/group-member
                        list group-member {
                            key "group-member";
                            description "Enter the group-member list instance";

                            ///nokia-state/state/router/nat/outside/pool/l2-aware/group-member/group-member
                            leaf group-member {
                                type types-nat:bb-group-member;
                                description "group member ID";
                            }

                            ///nokia-state/state/router/nat/outside/pool/l2-aware/group-member/block-usage
                            leaf block-usage {
                                type types-sros:percent;
                                description "Port block usage in l2-aware NAT";
                            }

                            ///nokia-state/state/router/nat/outside/pool/l2-aware/group-member/high-watermark-reached
                            leaf high-watermark-reached {
                                type boolean;
                                description "High watermark reached for the l2-aware NAT block usage";
                            }

                        } // list group-member

                    } // container l2-aware

                    ///nokia-state/state/router/nat/outside/pool/statistics
                    container statistics {
                        description "Enter the statistics context";

                        ///nokia-state/state/router/nat/outside/pool/statistics/default-host
                        container default-host {
                            description "Enter the default-host context";

                            ///nokia-state/state/router/nat/outside/pool/statistics/default-host/packets-forwarded
                            leaf packets-forwarded {
                                type uint64;
                                description "Number of packets forwarded to the default-host";
                            }

                            ///nokia-state/state/router/nat/outside/pool/statistics/default-host/packets-dropped
                            leaf packets-dropped {
                                type uint64;
                                description "Number of dropped packets destined to default-host";
                            }

                        } // container default-host

                    } // container statistics

                    ///nokia-state/state/router/nat/outside/pool/address-range
                    list address-range {
                        key "start end";
                        description "Enter the address-range list instance";

                        ///nokia-state/state/router/nat/outside/pool/address-range/start
                        leaf start {
                            type types-sros:ipv4-unicast-address;
                            description "Lower bound of the NAT address range";
                        }

                        ///nokia-state/state/router/nat/outside/pool/address-range/end
                        leaf end {
                            type types-sros:ipv4-unicast-address;
                            description "Upper bound of the NAT address range";
                        }

                        ///nokia-state/state/router/nat/outside/pool/address-range/allocated-blocks
                        leaf allocated-blocks {
                            type uint32;
                            description "Number of allocated blocks";
                        }

                        ///nokia-state/state/router/nat/outside/pool/address-range/allocated-subscribers
                        leaf allocated-subscribers {
                            type uint32;
                            description "Number of allocated L2-aware subscribers";
                        }

                    } // list address-range

                } // list pool

                ///nokia-state/state/router/nat/outside/large-scale
                container large-scale {
                    description "Enter the large-scale context";

                    ///nokia-state/state/router/nat/outside/large-scale/block
                    list block {
                        key "address first-port";
                        description "Enter the block list instance";

                        ///nokia-state/state/router/nat/outside/large-scale/block/address
                        leaf address {
                            type types-sros:ipv4-unicast-address;
                            description "Outside IP address";
                        }

                        ///nokia-state/state/router/nat/outside/large-scale/block/first-port
                        leaf first-port {
                            type types-sros:tcp-udp-port;
                            description "First outside port";
                        }

                        ///nokia-state/state/router/nat/outside/large-scale/block/last-port
                        leaf last-port {
                            type types-nat:port-number;
                            description "Last outside port";
                        }

                        ///nokia-state/state/router/nat/outside/large-scale/block/nat-policy
                        leaf nat-policy {
                            type types-sros:named-item;
                            description "NAT policy";
                        }

                        ///nokia-state/state/router/nat/outside/large-scale/block/nat-pool
                        leaf nat-pool {
                            type types-sros:named-item;
                            description "NAT pool";
                        }

                        ///nokia-state/state/router/nat/outside/large-scale/block/subscriber-id
                        leaf subscriber-id {
                            type uint32;
                            description "Subscriber ID";
                        }

                        ///nokia-state/state/router/nat/outside/large-scale/block/allocation-time
                        leaf allocation-time {
                            type types-sros:date-time;
                            description "Allocation time";
                        }

                        ///nokia-state/state/router/nat/outside/large-scale/block/inside
                        container inside {
                            description "Enter the inside context";

                            ///nokia-state/state/router/nat/outside/large-scale/block/inside/router-instance
                            leaf router-instance {
                                type types-sros:router-instance;
                                description "Inside service";
                            }

                            ///nokia-state/state/router/nat/outside/large-scale/block/inside/address
                            leaf address {
                                type types-sros:ip-address;
                                description "Inside address";
                            }

                        } // container inside

                    } // list block

                } // container large-scale

            } // container outside

            ///nokia-state/state/router/nat/map
            container map {
                description "Enter the map context";

                ///nokia-state/state/router/nat/map/map-domain
                list map-domain {
                    key "domain-name";
                    description "Add a list entry for map-domain";

                    ///nokia-state/state/router/nat/map/map-domain/domain-name
                    leaf domain-name {
                        type leafref {
                            path "../../../../../service/nat/map-t/domain/name";
                        }
                        description "MAP domain template name";
                    }

                    ///nokia-state/state/router/nat/map/map-domain/statistics
                    container statistics {
                        description "Enter the statistics context";

                        ///nokia-state/state/router/nat/map/map-domain/statistics/upstream
                        container upstream {
                            description "Enter the upstream context";

                            ///nokia-state/state/router/nat/map/map-domain/statistics/upstream/forwarded-packets
                            leaf forwarded-packets {
                                type yang:counter64;
                                description "Number of packets forwarded in the upstream direction";
                            }

                            ///nokia-state/state/router/nat/map/map-domain/statistics/upstream/forwarded-octets
                            leaf forwarded-octets {
                                type yang:counter64;
                                description "Number of octets forwarded in the upstream direction";
                            }

                            ///nokia-state/state/router/nat/map/map-domain/statistics/upstream/dropped-packets
                            leaf dropped-packets {
                                type yang:counter64;
                                description "Number of packets dropped in the upstream direction";
                            }

                            ///nokia-state/state/router/nat/map/map-domain/statistics/upstream/dropped-octets
                            leaf dropped-octets {
                                type yang:counter64;
                                description "Number of octets dropped in the upstream direction";
                            }

                        } // container upstream

                        ///nokia-state/state/router/nat/map/map-domain/statistics/downstream
                        container downstream {
                            description "Enter the downstream context";

                            ///nokia-state/state/router/nat/map/map-domain/statistics/downstream/forwarded-packets
                            leaf forwarded-packets {
                                type yang:counter64;
                                description "Number of packets forwarded in the downstream direction";
                            }

                            ///nokia-state/state/router/nat/map/map-domain/statistics/downstream/forwarded-octets
                            leaf forwarded-octets {
                                type yang:counter64;
                                description "Number of octets forwarded in the downstream direction";
                            }

                            ///nokia-state/state/router/nat/map/map-domain/statistics/downstream/dropped-packets
                            leaf dropped-packets {
                                type yang:counter64;
                                description "Number of packets dropped in the downstream direction";
                            }

                            ///nokia-state/state/router/nat/map/map-domain/statistics/downstream/dropped-octets
                            leaf dropped-octets {
                                type yang:counter64;
                                description "Number of octets dropped in the downstream direction";
                            }

                        } // container downstream

                    } // container statistics

                } // list map-domain

            } // container map

        } // container nat

    } // grouping state-router-nat

}
