submodule nokia-state-router-l2tp {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-inet-types                { prefix "inet"; }
    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-l2tp               { prefix "types-l2tp"; }
    import nokia-types-ppp                { prefix "types-ppp"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for l2tp on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-router-l2tp YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2022-11-18";

    grouping state-router-l2tp {
        ///nokia-state/state/router/l2tp
        container l2tp {
            description "Enable the l2tp context";

            ///nokia-state/state/router/l2tp/statistics
            container statistics {
                description "Enter the statistics context";

                ///nokia-state/state/router/l2tp/statistics/last-cleared
                leaf last-cleared {
                    type yang:date-and-time;
                    description "Indicates when the statistics were cleared.";
                }

                ///nokia-state/state/router/l2tp/statistics/total-tunnels
                leaf total-tunnels {
                    type yang:counter32;
                    description
                        "The total number of tunnels set up attempts since the last
                         re-initialization of the local network management subsystem,
                         or the last time the system statistics were cleared.";
                }

                ///nokia-state/state/router/l2tp/statistics/failed-tunnels
                leaf failed-tunnels {
                    type yang:counter32;
                    description
                        "The total number of tunnels that failed to reach the established
                         state since the last re-initialization of the local network
                         management subsystem, or the last time the system statistics were
                         cleared.";
                }

                ///nokia-state/state/router/l2tp/statistics/failed-authentication
                leaf failed-authentication {
                    type yang:counter32;
                    description
                        "The total number of tunnels that failed authentication
                         since the last re-initialization of the local network
                         management subsystem, or the last time the system statistics were
                         cleared.";
                }

                ///nokia-state/state/router/l2tp/statistics/actual-tunnels
                leaf actual-tunnels {
                    type yang:counter32;
                    description "The actual number of tunnels.";
                }

                ///nokia-state/state/router/l2tp/statistics/active-tunnels
                leaf active-tunnels {
                    type yang:counter32;
                    description "The number of tunnels currently established.";
                }

                ///nokia-state/state/router/l2tp/statistics/total-sessions
                leaf total-sessions {
                    type yang:counter32;
                    description
                        "The total number of session set up attempts since the last
                         re-initialization of the local network management subsystem,
                         or the last time the system statistics were cleared.";
                }

                ///nokia-state/state/router/l2tp/statistics/failed-sessions
                leaf failed-sessions {
                    type yang:counter32;
                    description
                        "The total number of sessions that failed to reach the established
                         state since the last re-initialization of the local network
                         management subsystem, or the last time the system statistics were
                         cleared.";
                }

                ///nokia-state/state/router/l2tp/statistics/actual-sessions
                leaf actual-sessions {
                    type yang:counter32;
                    description "The actual number of sessions.";
                }

                ///nokia-state/state/router/l2tp/statistics/active-sessions
                leaf active-sessions {
                    type yang:counter32;
                    description "The number of sessions currently established.";
                }

                ///nokia-state/state/router/l2tp/statistics/blacklist-len
                leaf blacklist-len {
                    type yang:gauge32;
                    description
                        "The actual number of tunnels and peers in the tunnel
                         selection blacklist.";
                }

                ///nokia-state/state/router/l2tp/statistics/unavailable-tunnel-ids
                leaf unavailable-tunnel-ids {
                    type yang:gauge32;
                    description
                        "The number of tunnel identifiers that is unavailable for the
                         L2TP protocol because they are used by some other application.";
                }

            } // container statistics

            ///nokia-state/state/router/l2tp/oper-group
            list oper-group {
                key "tunnel-group-name";
                description "Enter the oper-group list instance";

                ///nokia-state/state/router/l2tp/oper-group/tunnel-group-name
                leaf tunnel-group-name {
                    type types-sros:named-item-64 {
                        length "1..63";
                    }
                    description "Tunnel group name";
                }

                ///nokia-state/state/router/l2tp/oper-group/oper-state
                leaf oper-state {
                    type types-l2tp:group-oper-state;
                    description "The operational state of the tunnel group.";
                }

                ///nokia-state/state/router/l2tp/oper-group/session-limit
                leaf session-limit {
                    type types-l2tp:base-session-limit;
                    description "The administrative session limit of this tunnel group.";
                }

                ///nokia-state/state/router/l2tp/oper-group/oper-session-limit
                leaf oper-session-limit {
                    type uint32;
                    description "The operational session limit of this tunnel group.";
                }

                ///nokia-state/state/router/l2tp/oper-group/session-assign-method
                leaf session-assign-method {
                    type types-l2tp:base-session-assign-method;
                    description
                        "The latest actual method use for the authentication
                         of the tunnels in this tunnel group.";
                }

                ///nokia-state/state/router/l2tp/oper-group/statistics
                container statistics {
                    description "Enter the statistics context";

                    ///nokia-state/state/router/l2tp/oper-group/statistics/last-cleared
                    leaf last-cleared {
                        type yang:date-and-time;
                        description "Indicates when the statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/total-tunnels
                    leaf total-tunnels {
                        type yang:counter32;
                        description
                            "The total number of tunnel set up attempts
                             in this tunnel group since the last re-initialization
                             of the local network management subsystem, or the last
                             time the system statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/failed-tunnels
                    leaf failed-tunnels {
                        type yang:counter32;
                        description
                            "The total number of tunnels in this tunnel group
                             that failed to reach the established state since the last
                             re-initialization of the local network management subsystem,
                             or the last time the system statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/auth-failed-tunnels
                    leaf auth-failed-tunnels {
                        type yang:counter32;
                        description
                            "The total number of tunnels in this tunnel group
                             that failed authentication since the last re-initialization
                             of the local network management subsystem, or the last time
                             the system statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/active-tunnels
                    leaf active-tunnels {
                        type yang:gauge32;
                        description
                            "The total number of tunnels currently established
                             in this tunnel group.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/tunnels
                    leaf tunnels {
                        type yang:gauge32;
                        description "The actual number of tunnels in this tunnel group.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/total-sessions
                    leaf total-sessions {
                        type yang:counter32;
                        description
                            "The total number of session creation attempts
                             in this tunnel group since the last re-initialization
                             of the local network management subsystem, or the last
                             time the system statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/failed-sessions
                    leaf failed-sessions {
                        type yang:counter32;
                        description
                            "The total number of sessions in this tunnel group
                             that failed to reach the established state since the last
                             re-initialization of the local network management subsystem,
                             or the last time the system statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/active-sessions
                    leaf active-sessions {
                        type yang:gauge32;
                        description
                            "The total number of sessions currently established
                             in this tunnel group.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/sessions
                    leaf sessions {
                        type yang:gauge32;
                        description "The actual number of sessions in this tunnel group.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/rx-control-octets
                    leaf rx-control-octets {
                        type yang:counter64;
                        units "octets";
                        description
                            "The number of control-channel octets received by the
                             current tunnels in this tunnel group.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/rx-control-packets
                    leaf rx-control-packets {
                        type yang:counter32;
                        units "packets";
                        description
                            "The number of control-channel packets received by the
                             current tunnels in this tunnel group.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/errored-packets
                    leaf errored-packets {
                        type yang:gauge32;
                        units "packets";
                        description
                            "The accumulated number of control packets that were
                             received by the current tunnel endpoints in this tunnel group.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/tx-control-octets
                    leaf tx-control-octets {
                        type yang:counter64;
                        units "octets";
                        description
                            "The number of control-channel octets transmitted by the
                             current tunnels in this tunnel group.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/tx-control-packets
                    leaf tx-control-packets {
                        type yang:counter32;
                        units "packets";
                        description
                            "The number of control-channel packets transmitted by the
                             current tunnels in this tunnel group.";
                    }

                    ///nokia-state/state/router/l2tp/oper-group/statistics/transmission-errors
                    leaf transmission-errors {
                        type yang:gauge32;
                        units "packets";
                        description
                            "The accumulated number of control packet transmission errors
                             on the current tunnels in this tunnel group.";
                    }

                } // container statistics

            } // list oper-group

            ///nokia-state/state/router/l2tp/tunnel
            list tunnel {
                key "tunnel-conn-id";
                description "Enter the tunnel list instance";

                ///nokia-state/state/router/l2tp/tunnel/tunnel-conn-id
                leaf tunnel-conn-id {
                    type types-l2tp:connection-id;
                    description "The local connection-id of the control channel of the tunnel.";
                }

                ///nokia-state/state/router/l2tp/tunnel/oper-state
                leaf oper-state {
                    type types-l2tp:tunnel-oper-state;
                    description "The operational state of the tunnel.";
                }

                ///nokia-state/state/router/l2tp/tunnel/start-time
                leaf start-time {
                    type yang:date-and-time;
                    description "The time when this tunnel reached the state wait-reply or wait-connected.";
                }

                ///nokia-state/state/router/l2tp/tunnel/established-time
                leaf established-time {
                    type yang:date-and-time;
                    description "The time when this tunnel reached an active state.";
                }

                ///nokia-state/state/router/l2tp/tunnel/idle-time
                leaf idle-time {
                    type yang:date-and-time;
                    description "The time when this tunnel entered the state idle.";
                }

                ///nokia-state/state/router/l2tp/tunnel/closed-time
                leaf closed-time {
                    type yang:date-and-time;
                    description "The time when this tunnel entered the state closed or closed-by-peer.";
                }

                ///nokia-state/state/router/l2tp/tunnel/stop-ccn-result
                leaf stop-ccn-result {
                    type types-l2tp:stop-ccn-result;
                    description "The result of a transmitted or received StopCCN message.";
                }

                ///nokia-state/state/router/l2tp/tunnel/general-error
                leaf general-error {
                    type types-l2tp:general-error;
                    description "The general error of a transmitted or received StopCCN message.";
                }

                ///nokia-state/state/router/l2tp/tunnel/error-message
                leaf error-message {
                    type string {
                        length "1..255";
                    }
                    description "Describes the L2TP error condition in human readable form.";
                }

                ///nokia-state/state/router/l2tp/tunnel/act-receive-window-size
                leaf act-receive-window-size {
                    type uint32;
                    description "The actual receive window size.";
                }

                ///nokia-state/state/router/l2tp/tunnel/blacklist-state
                leaf blacklist-state {
                    type types-l2tp:blacklist-state;
                    description "Indicates if this tunnel is deemed reachable or not.";
                }

                ///nokia-state/state/router/l2tp/tunnel/blacklist-time
                leaf blacklist-time {
                    type yang:date-and-time;
                    description "The time when this tunnel was placed in the tunnel-selection-blacklist.";
                }

                ///nokia-state/state/router/l2tp/tunnel/blacklist-remain-time
                leaf blacklist-remain-time {
                    type uint32;
                    units "seconds";
                    description "Indicates how long the tunnel must still remain in the tunnel-selection-blacklist.";
                }

                ///nokia-state/state/router/l2tp/tunnel/oper-session-limit
                leaf oper-session-limit {
                    type uint32;
                    description "The operational session limit of this tunnel.";
                }

                ///nokia-state/state/router/l2tp/tunnel/connection
                container connection {
                    description "Enter the connection context";

                    ///nokia-state/state/router/l2tp/tunnel/connection/rem-conn-id
                    leaf rem-conn-id {
                        type types-l2tp:connection-id;
                        description "The remote connection-id of control channel.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/connection/loc-tunnel-id
                    leaf loc-tunnel-id {
                        type types-l2tp:tunnel-id;
                        description "The local tunnel-id.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/connection/rem-tunnel-id
                    leaf rem-tunnel-id {
                        type types-l2tp:tunnel-id;
                        description "The remote tunnel-id.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/connection/transport-type
                    leaf transport-type {
                        type types-l2tp:transport-type;
                        description "The transport type used to carry L2TP messages.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/connection/peer
                    leaf peer {
                        type types-sros:ip-address;
                        description "The peer address";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/connection/rem-udp-port
                    leaf rem-udp-port {
                        type inet:port-number;
                        description "The remote UDP port of this L2TP tunnel.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/connection/tx-dst-address
                    leaf tx-dst-address {
                        type types-sros:ip-address;
                        description
                            "The address used as destination IP address for transmitting
                             L2TP control messages";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/connection/tx-dst-udp-port
                    leaf tx-dst-udp-port {
                        type inet:port-number;
                        description "The destination UDP port used while transmitting L2TP control messages.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/connection/rx-src-address
                    leaf rx-src-address {
                        type types-sros:ip-address;
                        description
                            "The address found as source IP address in received L2TP control
                             messages";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/connection/rx-src-udp-port
                    leaf rx-src-udp-port {
                        type inet:port-number;
                        description "The source UDP port found in received L2TP control messages.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/connection/local-address
                    leaf local-address {
                        type types-sros:ip-address;
                        description "The local address";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/connection/loc-udp-port
                    leaf loc-udp-port {
                        type inet:port-number;
                        description "The local UDP port of this L2TP tunnel.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/connection/local-name
                    leaf local-name {
                        type types-sros:named-item-64;
                        description "The local host name.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/connection/remote-name
                    leaf remote-name {
                        type types-sros:named-item-64;
                        description "The remote host name.";
                    }

                } // container connection

                ///nokia-state/state/router/l2tp/tunnel/parameters
                container parameters {
                    description "Enter the parameters context";

                    ///nokia-state/state/router/l2tp/tunnel/parameters/protocol
                    leaf protocol {
                        type types-l2tp:protocol-version;
                        description "The protocol version being used";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/parameters/tunnel-group-name
                    leaf tunnel-group-name {
                        type types-sros:named-item-64;
                        description "The name of the tunnel group to which this tunnel belongs";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/parameters/assignment-id
                    leaf assignment-id {
                        type types-sros:named-item {
                            length "1..32";
                        }
                        description "The identifier assigned to this tunnel.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/parameters/preference
                    leaf preference {
                        type uint32;
                        description "The preference of the tunnel.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/parameters/hello-interval
                    leaf hello-interval {
                        type types-l2tp:base-hello-interval;
                        description "The hello interval.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/parameters/idle-timeout
                    leaf idle-timeout {
                        type types-l2tp:base-idle-timeout;
                        description "The idle timeout.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/parameters/destruct-timeout
                    leaf destruct-timeout {
                        type types-l2tp:base-destruct-timeout;
                        description "The destruction timeout.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/parameters/max-retries-estab
                    leaf max-retries-estab {
                        type types-l2tp:base-max-retries;
                        description "The maximum number of retries when the tunnel is established.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/parameters/max-retries-not-estab
                    leaf max-retries-not-estab {
                        type types-l2tp:base-max-retries;
                        description "The maximum number of retries when the tunnel is not established.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/parameters/radius-accounting-policy
                    leaf radius-accounting-policy {
                        type types-sros:named-item;
                        description "The RADIUS accounting policy.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/parameters/challenge
                    leaf challenge {
                        type boolean;
                        description "Indicates if challenge-response authentication is used or not.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/parameters/avp-hiding
                    leaf avp-hiding {
                        type types-l2tp:base-avp-hiding;
                        description "The AVP hiding.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/parameters/session-limit
                    leaf session-limit {
                        type types-l2tp:base-session-limit;
                        description "The administrative session limit of this L2TP tunnel";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/parameters/df-bit-lac
                    leaf df-bit-lac {
                        type boolean;
                        description "Indicates if the DF (don't fragment) bit is set in data traffic transmitted as LAC.";
                    }

                } // container parameters

                ///nokia-state/state/router/l2tp/tunnel/failover
                container failover {
                    description "Enter the failover context";

                    ///nokia-state/state/router/l2tp/tunnel/failover/failover-state
                    leaf failover-state {
                        type types-l2tp:failover-state;
                        description "The failover state of this tunnel.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/failover/recovery-conn-id
                    leaf recovery-conn-id {
                        type types-l2tp:connection-id;
                        description "The connection-id of the recovery tunnel.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/failover/recovery-state
                    leaf recovery-state {
                        type types-l2tp:recovery-state;
                        description "The failover state of a recovery tunnel.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/failover/recovered-conn-id
                    leaf recovered-conn-id {
                        type types-l2tp:connection-id;
                        description "The connection-id of the recovered tunnel.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/failover/recovery-method
                    leaf recovery-method {
                        type types-l2tp:base-failover-recovery-method;
                        description "The recovery method.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/failover/track-srrp
                    leaf track-srrp {
                        type uint32;
                        description "The SRRP instance used for recovery.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/failover/ctrl-msg-behavior
                    leaf ctrl-msg-behavior {
                        type types-l2tp:control-msg-behavior;
                        description "The behavior of the system with respect to L2TP control traffic.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/failover/requested-recovery-time
                    leaf requested-recovery-time {
                        type types-l2tp:recovery-time;
                        description "The recovery time requested to the peer system.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/failover/peer-recovery-time
                    leaf peer-recovery-time {
                        type types-l2tp:recovery-time;
                        description "The recovery time requested by the peer system.";
                    }

                } // container failover

                ///nokia-state/state/router/l2tp/tunnel/l2tpv3
                container l2tpv3 {
                    description "Enter the l2tpv3 context";

                    ///nokia-state/state/router/l2tp/tunnel/l2tpv3/cookie-length
                    leaf cookie-length {
                        type types-l2tp:base-v3-cookie-length;
                        description "The cookie length.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/l2tpv3/digest-type
                    leaf digest-type {
                        type types-l2tp:base-v3-digest-type;
                        description "The digest type";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/l2tpv3/local-nonce
                    leaf local-nonce {
                        type types-l2tp:nonce;
                        description "The nonce AVP added to the outgoing control messages.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/l2tpv3/remote-nonce
                    leaf remote-nonce {
                        type types-l2tp:nonce;
                        description "The nonce AVP found in incoming control messages.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/l2tpv3/rem-router-id
                    leaf rem-router-id {
                        type types-sros:ipv4-address;
                        description "The filter used on remote router ID.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/l2tpv3/public-tcp-mss-adjust
                    leaf public-tcp-mss-adjust {
                        type types-l2tp:base-v3-tcp-mss-adjust;
                        description "The TCP maximum segment size (MSS) on public network.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/l2tpv3/private-tcp-mss-adjust
                    leaf private-tcp-mss-adjust {
                        type types-l2tp:base-v3-tcp-mss-adjust;
                        description "The TCP maximum segment size (MSS) on private network.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/l2tpv3/pw-cap-list
                    container pw-cap-list {
                        description "Enter the pw-cap-list context";

                        ///nokia-state/state/router/l2tp/tunnel/l2tpv3/pw-cap-list/ethernet
                        leaf ethernet {
                            type boolean;
                            description "Ethernet pseudo-wire encapsulation is accepted";
                        }

                        ///nokia-state/state/router/l2tp/tunnel/l2tpv3/pw-cap-list/ethernet-vlan
                        leaf ethernet-vlan {
                            type boolean;
                            description "Ethernet VLAN pseudo-wire encapsulation is accepted";
                        }

                    } // container pw-cap-list

                } // container l2tpv3

                ///nokia-state/state/router/l2tp/tunnel/statistics
                container statistics {
                    description "Enter the statistics context";

                    ///nokia-state/state/router/l2tp/tunnel/statistics/last-cleared
                    leaf last-cleared {
                        type yang:date-and-time;
                        description "The time when the statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/statistics/total-sessions
                    leaf total-sessions {
                        type yang:counter32;
                        description
                            "The total number of session creation attempts
                             in this tunnel since the last re-initialization
                             of the local network management subsystem, or the last
                             time the system statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/tunnel/statistics/failed-sessions
                    leaf failed-sessions {
                        type yang:counter32;
                        description
                            "The total number of sessions in this tunnel
                             that failed to reach the established state since the last
                             re-initialization of the local network management subsystem,
