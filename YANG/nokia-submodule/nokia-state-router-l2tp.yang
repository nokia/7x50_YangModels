                ///nokia-state/state/router/l2tp/ip-peer/actual-roles
                leaf actual-roles {
                    type types-l2tp:peer-roles;
                    description "The roles the peer currently has.";
                }

                ///nokia-state/state/router/l2tp/ip-peer/statistics
                container statistics {
                    description "Enter the statistics context";

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/tunnels
                    leaf tunnels {
                        type yang:gauge32;
                        description "The actual number of tunnels associated with this peer.";
                    }

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/active-tunnels
                    leaf active-tunnels {
                        type yang:gauge32;
                        description "The actual number of tunnels associated with this peer that are currently established.";
                    }

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/sessions
                    leaf sessions {
                        type yang:gauge32;
                        description "The actual number of sessions associated with this peer.";
                    }

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/active-sessions
                    leaf active-sessions {
                        type yang:gauge32;
                        description "The actual number of sessions associated with this peer that are currently established.";
                    }

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/rx-control-octets
                    leaf rx-control-octets {
                        type yang:counter64;
                        units "octets";
                        description
                            "The number of control channel octets received from this peer since the last re-initialization
                             of the local network management subsystem, or the last time the tunnel statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/rx-control-packets
                    leaf rx-control-packets {
                        type yang:counter32;
                        units "packets";
                        description
                            "The number of control channel packets received from this peer since the last re-initialization
                             of the local network management subsystem, or the last time the tunnel statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/errored-packets
                    leaf errored-packets {
                        type yang:counter32;
                        units "packets";
                        description
                            "The number of errored packets received from this peer since the last re-initialization
                             of the local network management subsystem, or the last time the tunnel statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/tx-control-octets
                    leaf tx-control-octets {
                        type yang:counter64;
                        units "octets";
                        description
                            "The number of control channel octets transmitted to this peer since the last re-initialization
                             of the local network management subsystem, or the last time the tunnel statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/tx-control-packets
                    leaf tx-control-packets {
                        type yang:counter32;
                        units "packets";
                        description
                            "The number of control channel packets transmitted to this peer since the last re-initialization
                             of the local network management subsystem, or the last time the tunnel statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/transmission-errors
                    leaf transmission-errors {
                        type yang:counter32;
                        units "packets";
                        description
                            "The number of packets transmission error for this peer since the last re-initialization
                             of the local network management subsystem, or the last time the tunnel statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/fsm-msg-accepted
                    leaf fsm-msg-accepted {
                        type yang:counter32;
                        units "packets";
                        description
                            "The number of Finite State Machine messages from this peer accepted since the last re-initialization
                             of the local network management subsystem, or the last time the tunnel statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/fsm-msg-duplicates
                    leaf fsm-msg-duplicates {
                        type yang:counter32;
                        units "packets";
                        description
                            "The number of duplicate Finite State Machine messages from this peer received since the last re-initialization
                             of the local network management subsystem, or the last time the tunnel statistics were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/fsm-msg-out-of-rx-window
                    leaf fsm-msg-out-of-rx-window {
                        type yang:counter32;
                        units "packets";
                        description
                            "The number of Finite State Machine (FSM) messages that were received
                             out of the receive window from this peer since the last re-initialization of
                             the local network management subsystem, or the last time the tunnel statistics
                             were cleared.";
                    }

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/last-cleared
                    leaf last-cleared {
                        type yang:date-and-time;
                        description "The time when the statistics were cleared for the last time.";
                    }

                    ///nokia-state/state/router/l2tp/ip-peer/statistics/protocol
                    list protocol {
                        key "type id";
                        description "Enter the protocol list instance";

                        ///nokia-state/state/router/l2tp/ip-peer/statistics/protocol/type
                        leaf type {
                            type types-l2tp:proto-stats-type;
                            description "The statistics type.";
                        }

                        ///nokia-state/state/router/l2tp/ip-peer/statistics/protocol/id
                        leaf id {
                            type uint32 {
                                range "0..69|4294967294|4294967295";
                            }
                            description "The counter id.";
                        }

                        ///nokia-state/state/router/l2tp/ip-peer/statistics/protocol/name
                        leaf name {
                            type types-sros:display-string;
                            description "The human-readable identifier of the statistic.";
                        }

                        ///nokia-state/state/router/l2tp/ip-peer/statistics/protocol/value
                        leaf value {
                            type yang:counter32;
                            description "The counter value";
                        }

                    } // list protocol

                } // container statistics

            } // list ip-peer

            ///nokia-state/state/router/l2tp/vas-tunnel
            list vas-tunnel {
                key "tunnel-conn-id";
                description "Enter the vas-tunnel list instance";

                ///nokia-state/state/router/l2tp/vas-tunnel/tunnel-conn-id
                leaf tunnel-conn-id {
                    type types-l2tp:connection-id;
                    description "The connection-id of the VAS tunnel.";
                }

                ///nokia-state/state/router/l2tp/vas-tunnel/transport-type
                leaf transport-type {
                    type types-l2tp:transport-type;
                    description "The transport type used to carry L2TP messages.";
                }

                ///nokia-state/state/router/l2tp/vas-tunnel/local-address
                leaf local-address {
                    type types-sros:ip-address;
                    description "The local address";
                }

                ///nokia-state/state/router/l2tp/vas-tunnel/loc-udp-port
                leaf loc-udp-port {
                    type inet:port-number;
                    description "The local UDP port of this VAS tunnel.";
                }

                ///nokia-state/state/router/l2tp/vas-tunnel/remote-address
                leaf remote-address {
                    type types-sros:ip-address;
                    description "The remote address";
                }

                ///nokia-state/state/router/l2tp/vas-tunnel/rem-udp-port
                leaf rem-udp-port {
                    type inet:port-number;
                    description "The remote UDP port of this VAS tunnel.";
                }

                ///nokia-state/state/router/l2tp/vas-tunnel/base-router
                leaf base-router {
                    type types-sros:router-instance;
                    description "The virtual router instance used as the base of the VAS tunnel.";
                }

                ///nokia-state/state/router/l2tp/vas-tunnel/loc-tunnel-id
                leaf loc-tunnel-id {
                    type types-l2tp:tunnel-id;
                    description "The local tunnel-id.";
                }

                ///nokia-state/state/router/l2tp/vas-tunnel/rem-tunnel-id
                leaf rem-tunnel-id {
                    type types-l2tp:tunnel-id;
                    description "The remote tunnel-id.";
                }

                ///nokia-state/state/router/l2tp/vas-tunnel/rem-conn-id
                leaf rem-conn-id {
                    type types-l2tp:connection-id;
                    description "The remote connection-id of the VAS tunnel.";
                }

                ///nokia-state/state/router/l2tp/vas-tunnel/vas-session
                list vas-session {
                    key "session-conn-id";
                    description "Enter the vas-session list instance";

                    ///nokia-state/state/router/l2tp/vas-tunnel/vas-session/session-conn-id
                    leaf session-conn-id {
                        type types-l2tp:connection-id;
                        description "The connection-id of the VAS session.";
                    }

                    ///nokia-state/state/router/l2tp/vas-tunnel/vas-session/loc-session-id
                    leaf loc-session-id {
                        type types-l2tp:session-id;
                        description "The local session-id.";
                    }

                    ///nokia-state/state/router/l2tp/vas-tunnel/vas-session/rem-session-id
                    leaf rem-session-id {
                        type types-l2tp:session-id;
                        description "The remote session-id.";
                    }

                    ///nokia-state/state/router/l2tp/vas-tunnel/vas-session/rem-conn-id
                    leaf rem-conn-id {
                        type types-l2tp:connection-id;
                        description "The remote connection-id of the VAS session.";
                    }

                } // list vas-session

            } // list vas-tunnel

            ///nokia-state/state/router/l2tp/mlppp-bundle
            list mlppp-bundle {
                key "bundle-index";
                description "Enter the mlppp-bundle list instance";

                ///nokia-state/state/router/l2tp/mlppp-bundle/bundle-index
                leaf bundle-index {
                    type uint32;
                    description "The bundle index";
                }

                ///nokia-state/state/router/l2tp/mlppp-bundle/service-name
                leaf service-name {
                    type types-sros:named-item-64;
                    description "The service to which this MLPPP bundle belongs.";
                }

                ///nokia-state/state/router/l2tp/mlppp-bundle/forwarding-tunnel-id
                leaf forwarding-tunnel-id {
                    type types-l2tp:tunnel-id;
                    description
                        "The id of the LTS (L2TP Tunnel Stitching) tunnel if this bundle
                         is associated with PPP sessions that are stiched to another L2TP tunnel.";
                }

                ///nokia-state/state/router/l2tp/mlppp-bundle/user-name
                leaf user-name {
                    type types-sros:named-item;
                    description
                        "The user name of the subscriber associated with this MLPPP bundle or another
                         string used to identify that subscriber.";
                }

                ///nokia-state/state/router/l2tp/mlppp-bundle/local-endpoint
                container local-endpoint {
                    description "Enter the local-endpoint context";

                    ///nokia-state/state/router/l2tp/mlppp-bundle/local-endpoint/endpoint-class
                    leaf endpoint-class {
                        type types-ppp:mlppp-endpoint-class;
                        description "Endpoint class";
                    }

                    ///nokia-state/state/router/l2tp/mlppp-bundle/local-endpoint/endpoint-id
                    leaf endpoint-id {
                        type binary {
                            length "1..20";
                        }
                        description "Endpoint identifier";
                    }

                    ///nokia-state/state/router/l2tp/mlppp-bundle/local-endpoint/description
                    leaf description {
                        type types-sros:description;
                        description "The endpoint description.";
                    }

                } // container local-endpoint

                ///nokia-state/state/router/l2tp/mlppp-bundle/remote-endpoint
                container remote-endpoint {
                    description "Enter the remote-endpoint context";

                    ///nokia-state/state/router/l2tp/mlppp-bundle/remote-endpoint/endpoint-class
                    leaf endpoint-class {
                        type types-ppp:mlppp-endpoint-class;
                        description "Endpoint class";
                    }

                    ///nokia-state/state/router/l2tp/mlppp-bundle/remote-endpoint/endpoint-id
                    leaf endpoint-id {
                        type binary {
                            length "1..20";
                        }
                        description "Endpoint identifier";
                    }

                    ///nokia-state/state/router/l2tp/mlppp-bundle/remote-endpoint/description
                    leaf description {
                        type types-sros:description;
                        description "The endpoint description.";
                    }

                } // container remote-endpoint

                ///nokia-state/state/router/l2tp/mlppp-bundle/link
                list link {
                    key "session-conn-id";
                    description "Enter the link list instance";

                    ///nokia-state/state/router/l2tp/mlppp-bundle/link/session-conn-id
                    leaf session-conn-id {
                        type types-l2tp:connection-id;
                        description "The local connection-id of the session.";
                    }

                    ///nokia-state/state/router/l2tp/mlppp-bundle/link/oper-state
                    leaf oper-state {
                        type types-l2tp:session-oper-state;
                        description "The operational state of the session.";
                    }

                } // list link

            } // list mlppp-bundle

        } // container l2tp

    } // grouping state-router-l2tp

}
