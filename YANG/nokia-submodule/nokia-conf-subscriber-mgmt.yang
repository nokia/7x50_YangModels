                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/ppp-policy/mlppp/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/ppp-policy/mlppp/endpoint
                    container endpoint {
                        description "Enter the endpoint context";

                        ///nokia-conf/configure/subscriber-mgmt/ppp-policy/mlppp/endpoint/endpoint
                        choice endpoint {
                            ///nokia-conf/configure/subscriber-mgmt/ppp-policy/mlppp/endpoint/endpoint/ip
                            case ip {

                                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/mlppp/endpoint/endpoint/ip/ip
                                leaf ip {
                                    type types-ppp:mlppp-ip-endpoint;
                                    description "Endpoint ID as an IP address";
                                }

                            }
