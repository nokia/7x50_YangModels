                    ///nokia-conf/configure/subscriber-mgmt/router-advertisement-policy/options/retransmit-timer
                    leaf retransmit-timer {
                        type uint32 {
                            range "0..1800000";
                        }
                        units "seconds";
                        default "0";
                        description "Retransmit time in router advertisements from interface";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/router-advertisement-policy/options/router-lifetime
                    leaf router-lifetime {
                        type union {
                            type types-sros:time-duration {
                                range "2700..9000";
