                }

                ///nokia-conf/configure/subscriber-mgmt/ipoe-session-policy/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/ipoe-session-policy/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/ipoe-session-policy/session-key
                container session-key {
                    description "Enter the session-key context";

                    ///nokia-conf/configure/subscriber-mgmt/ipoe-session-policy/session-key/sap
                    leaf sap {
                        type boolean;
                        description "Use the SAP ID as part of the index";
