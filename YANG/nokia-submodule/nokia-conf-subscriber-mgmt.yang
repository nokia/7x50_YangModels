                    type boolean;
                    default "false";
                    description "Enable re-authentication on IPOE renewal";
                }

                ///nokia-conf/configure/subscriber-mgmt/radius-authentication-policy/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/radius-authentication-policy/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
