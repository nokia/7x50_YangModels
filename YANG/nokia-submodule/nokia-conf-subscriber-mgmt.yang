                    ///nokia-conf/configure/subscriber-mgmt/ipoe-session-policy/session-key/mac
                    leaf mac {
                        type boolean;
                        description "Use the MAC address as part of the index";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/ipoe-session-policy/session-key/cid
                    leaf cid {
                        type boolean;
                        sros-ext:immutable;
                        description "Use the Circuit ID as part of the index";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/ipoe-session-policy/session-key/rid
                    leaf rid {
                        type boolean;
                        sros-ext:immutable;
                        description "Use the remote ID as part of the index";
                    }

                } // container session-key

            } // list ipoe-session-policy

            ///nokia-conf/configure/subscriber-mgmt/sap-template
            list sap-template {
                key "name";
                max-elements 256;
                description "Enter the sap-template list instance";

                ///nokia-conf/configure/subscriber-mgmt/sap-template/name
