                }

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/allow-same-circuit-id-for-dhcp
                leaf allow-same-circuit-id-for-dhcp {
                    type boolean;
                    default "false";
                    description "Enable support for IPv4 address allocation on a SAP";
                }

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/pado-ac-name
                leaf pado-ac-name {
                    type types-sros:string-not-all-spaces {
                        length "1..128";
                    }
                    description "Access Concentrator name used in PPPoE PADO message";
                }

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/pado-delay
                leaf pado-delay {
                    type uint32 {
                        range "1..30";
                    }
                    units "deciseconds";
                    description "Delay timeout before sending a PADO";
                }

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/ppp-authentication
                leaf ppp-authentication {
                    type enumeration {
                        enum pap                            { value 1; }
                        enum chap                           { value 2; }
                        enum pref-chap                      { value 3; }
                        enum pref-pap                       { value 4; }
                    }
                    default "pref-chap";
                    description "PPP authentication protocol to negotiate";
                }

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/ppp-initial-delay
                leaf ppp-initial-delay {
                    type boolean;
                    default "false";
                    description "Select delay timeout before starting the PPP stack";
                }

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/ppp-mtu
                leaf ppp-mtu {
                    type uint32 {
                        range "512..9212";
                    }
                    description "Maximum PPP MTU size";
                }

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/re-establish-session
                leaf re-establish-session {
                    type boolean;
                    default "false";
                    description "Reconnect and override an existing session";
                }

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/reject-unconfigured-ncp
                leaf reject-unconfigured-ncp {
                    type boolean;
                    default "false";
                    description "Enable/disable reject unconfigured NCP.";
