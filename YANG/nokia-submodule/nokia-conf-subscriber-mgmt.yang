            ///nokia-conf/configure/subscriber-mgmt/pfcp
            container pfcp {
                description "Enter the pfcp context";

                ///nokia-conf/configure/subscriber-mgmt/pfcp/association
                list association {
                    key "name";
                    max-elements 1;
                    description "Enter the association list instance";

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/name
                    leaf name {
                        type types-sros:named-item;
                        description "PFCP association name";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the PFCP association";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/association-setup-retry
                    leaf association-setup-retry {
                        status obsolete;
                        type types-sros:time-duration {
                            range "1..36000";
                        }
                        units "seconds";
                        default "10";
                        description "Time to wait between successive association attempts";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/association-release-timeout
                    leaf association-release-timeout {
                        type union {
                            type enumeration {
                                enum none                           { value 0; }
                            }
                            type types-sros:time-duration {
                                range "30..3600";
                            }
                        }
                        units "seconds";
                        default "3600";
                        description "Time to wait for a response to a release request";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/path-restoration-time
                    leaf path-restoration-time {
                        type types-sros:time-duration {
                            range "1..11520";
                        }
                        units "minutes";
                        description "Time to wait before declaring the path down";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/python-policy
                    leaf python-policy {
                        type leafref {
                            path "../../../../python/python-policy/name";
                        }
                        description "Python policy to process PFCP messages";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/bfd-expedited-path-down
                    leaf bfd-expedited-path-down {
                        type boolean;
                        default "false";
                        description "Track BFD sessions to expedite PFCP path down detection";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/node-id
                    container node-id {
                        description "Enter the node-id context";

                        ///nokia-conf/configure/subscriber-mgmt/pfcp/association/node-id/node-id-type
                        choice node-id-type {
                            default "use-ip-address";
                            ///nokia-conf/configure/subscriber-mgmt/pfcp/association/node-id/node-id-type/fqdn
                            case fqdn {

                                ///nokia-conf/configure/subscriber-mgmt/pfcp/association/node-id/node-id-type/fqdn/fqdn
                                leaf fqdn {
                                    type types-sros:fully-qualified-domain-name;
                                    sros-ext:auto-restart-to-modify;
                                    description "FQDN that identifies the local system for PFCP";
                                }

                            }
                            ///nokia-conf/configure/subscriber-mgmt/pfcp/association/node-id/node-id-type/use-ip-address
                            case use-ip-address {

                                ///nokia-conf/configure/subscriber-mgmt/pfcp/association/node-id/node-id-type/use-ip-address/use-ip-address
                                leaf use-ip-address {
                                    type empty;
                                    sros-ext:auto-restart-to-modify;
                                    description "Use the IP address of interface for the association";
                                }

                            }
                        }
                    } // container node-id

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/interface
                    container interface {
                        description "Enter the interface context";

                        ///nokia-conf/configure/subscriber-mgmt/pfcp/association/interface/router-instance
                        leaf router-instance {
                            type string;
                            sros-ext:auto-restart-to-modify;
                            description "Router context for the PFCP association";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/pfcp/association/interface/name
                        leaf name {
                            type types-sros:interface-name;
                            sros-ext:auto-restart-to-modify;
                            description "Name of the interface to use for the association";
                        }

                    } // container interface

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/peer
                    container peer {
                        description "Enter the peer context";

                        ///nokia-conf/configure/subscriber-mgmt/pfcp/association/peer/node-id-type
                        choice node-id-type {
                            ///nokia-conf/configure/subscriber-mgmt/pfcp/association/peer/node-id-type/ip-address
                            case ip-address {

                                ///nokia-conf/configure/subscriber-mgmt/pfcp/association/peer/node-id-type/ip-address/ip-address
                                leaf ip-address {
                                    type types-sros:ip-unicast-address;
                                    sros-ext:auto-restart-to-modify;
                                    description "PFCP peer IP address";
                                }

                            }
                        }
                    } // container peer

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/heartbeat
                    container heartbeat {
                        description "Enter the heartbeat context";

                        ///nokia-conf/configure/subscriber-mgmt/pfcp/association/heartbeat/interval
                        leaf interval {
                            type types-sros:time-duration {
                                range "5..180";
                            }
                            units "seconds";
                            default "60";
                            description "Time between two successful heartbeat request messages";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/pfcp/association/heartbeat/timeout
                        leaf timeout {
                            type types-sros:time-duration {
                                range "1..20";
                            }
                            units "seconds";
                            default "5";
                            description "Time the system waits for a heartbeat response message";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/pfcp/association/heartbeat/retries
                        leaf retries {
                            type uint32 {
                                range "0..30";
                            }
                            default "4";
                            description "Number of retries after the heartbeat timeout expires";
                        }

                    } // container heartbeat

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/tx
                    container tx {
                        description "Enter the tx context";

                        ///nokia-conf/configure/subscriber-mgmt/pfcp/association/tx/timeout
                        leaf timeout {
                            type types-sros:time-duration {
                                range "1..30";
                            }
                            units "seconds";
                            default "1";
                            description "Retransmission timer";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/pfcp/association/tx/retries
                        leaf retries {
                            type uint32 {
                                range "0..7";
                            }
                            default "3";
                            description "Number of retries";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/pfcp/association/tx/ttl
                        leaf ttl {
                            type uint32 {
                                range "1..255";
                            }
                            default "255";
                            description "TTL value in the IP header of egress PFCP packets";
                        }

                    } // container tx

                    ///nokia-conf/configure/subscriber-mgmt/pfcp/association/nat
                    container nat {
                        description "Enter the nat context";

                        ///nokia-conf/configure/subscriber-mgmt/pfcp/association/nat/isa
                        choice isa {
                            ///nokia-conf/configure/subscriber-mgmt/pfcp/association/nat/isa/nat
                            case nat {

                                ///nokia-conf/configure/subscriber-mgmt/pfcp/association/nat/isa/nat/nat-group
                                leaf nat-group {
                                    type leafref {
                                        path "../../../../../isa/nat-group/id";
                                    }
                                    description "NAT group used for CUPS NAT functionality";
                                }

                            }
                            ///nokia-conf/configure/subscriber-mgmt/pfcp/association/nat/isa/wlan-gw
                            case wlan-gw {

                                ///nokia-conf/configure/subscriber-mgmt/pfcp/association/nat/isa/wlan-gw/wlan-gw-group
                                leaf wlan-gw-group {
                                    type leafref {
                                        path "../../../../../isa/wlan-gw-group/id";
                                    }
                                    description "WLAN gateway group used for CUPS NAT functionality";
                                }

                            }
                        }
                    } // container nat



                } // list association

            } // container pfcp

            ///nokia-conf/configure/subscriber-mgmt/pim-policy
            list pim-policy {
                key "name";
                max-elements 1;
                description "Enter the pim-policy list instance";

                ///nokia-conf/configure/subscriber-mgmt/pim-policy/name
                leaf name {
                    type types-sros:named-item;
                    description "Policy name";
                }

                ///nokia-conf/configure/subscriber-mgmt/pim-policy/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/subscriber-mgmt/pim-policy/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/pim-policy/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // list pim-policy

            ///nokia-conf/configure/subscriber-mgmt/sub-profile
            list sub-profile {
                key "name";
                description "Enter the sub-profile list instance";

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/name
                leaf name {
                    type types-sros:external-named-item;
                    description "Subscriber profile name";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/accounting-policy
                leaf accounting-policy {
                    type leafref {
                        path "../../../log/accounting-policy/policy-id";
                    }
                    description "Accounting policy";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/accu-stats-policy
                leaf accu-stats-policy {
                    type leafref {
                        path "../../accu-stats-policy/name";
                    }
                    description "Accumulated statistics policy";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/collect-stats
                leaf collect-stats {
                    type boolean;
                    default "false";
                    description "Collect statistics";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/firewall-policy
                leaf firewall-policy {
                    type leafref {
                        path "../../../service/nat/firewall-policy/name";
                    }
                    description "Firewall policy";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/host-tracking-policy
                leaf host-tracking-policy {
                    type leafref {
                        path "../../host-tracking-policy/name";
                    }
                    description "Host tracking policy";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/hs-sla-mode
                leaf hs-sla-mode {
                    type enumeration {
                        enum expanded                       { value 0; }
                        enum single                         { value 1; }
                    }
                    default "expanded";
                    description "HS board subscriber SLA mode";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/igmp-policy
                leaf igmp-policy {
                    type leafref {
                        path "../../igmp-policy/name";
                    }
                    description "IGMP policy";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/mld-policy
                leaf mld-policy {
                    type leafref {
                        path "../../mld-policy/name";
                    }
                    description "MLD policy";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/pim-policy
                leaf pim-policy {
                    type leafref {
                        path "../../pim-policy/name";
                    }
                    description "Protocol Independent Multicast (PIM) policy";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/preference
                leaf preference {
                    type uint32 {
                        range "1..10";
                    }
                    default "5";
                    description "Relative preference value for the subscriber profile";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/secondary-shaper-hashing
                leaf secondary-shaper-hashing {
                    type boolean;
                    default "false";
                    description "Include LAG hashing on secondary shaper";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/sub-mcac-policy
                leaf sub-mcac-policy {
                    type leafref {
                        path "../../sub-mcac-policy/name";
                    }
                    description "Subscriber MCAC policy";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/upnp-policy
                leaf upnp-policy {
                    type leafref {
                        path "../../../service/upnp/policy/name";
                    }
                    description "UPnP policy for the subscriber profile";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/volume-stats-type
                leaf volume-stats-type {
                    type enumeration {
                        enum l2                             { value 0; }
                        enum ip                             { value 1; }
                    }
                    default "l2";
                    description "Include non-IP headers in the accounting statistics";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/vport-hashing
                leaf vport-hashing {
                    type boolean;
                    default "false";
                    description "Include LAG hashing on Vport";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/control
                container control {
                    description "Enter the control context";

                    ///nokia-conf/configure/subscriber-mgmt/sub-profile/control/local
                    leaf local {
                        type boolean;
                        default "true";
                        description "Allow the local control plane to use the profile";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/sub-profile/control/cups
                    leaf cups {
                        type boolean;
                        default "false";
                        description "Allow the CUPS control plane to use the profile";
                    }

                } // container control

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/nat
                container nat {
                    description "Enter the nat context";

                    ///nokia-conf/configure/subscriber-mgmt/sub-profile/nat/policy
                    leaf policy {
                        type leafref {
                            path "../../../../service/nat/nat-policy/name";
                        }
                        description "NAT policy associated with the subscriber profile";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/sub-profile/nat/prefix-list
                    leaf prefix-list {
                        type leafref {
                            path "../../../../service/nat/prefix-list/name";
                        }
                        description "NAT prefix list name";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/sub-profile/nat/allow-bypass
                    leaf allow-bypass {
                        type boolean;
                        default "false";
                        description "Allow traffic to bypass NAT";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/sub-profile/nat/access-mode
                    leaf access-mode {
                        type enumeration {
                            enum auto {
                                value 1;
                                description "Automatically derived from the access SAP's anti-spoof type";
                            }
                            enum bridged {
                                value 2;
                                description "Force to bridged mode";
                            }
                        }
                        default "auto";
                        description "The subscriber NAT access mode";
                    }

                } // container nat

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/ancp
                container ancp {
                    description "Enable the ancp context";

                    ///nokia-conf/configure/subscriber-mgmt/sub-profile/ancp/ancp-policy
                    leaf ancp-policy {
                        type leafref {
                            path "../../../ancp/ancp-policy/name";
                        }
                        description "ANCP policy for this subscriber profile";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/sub-profile/ancp/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/sub-profile/ancp/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container ancp

                ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress
                container egress {
                    description "Enter the egress context";

                    ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/lag-per-link-hash
                    container lag-per-link-hash {
                        description "Enter the lag-per-link-hash context";

                        ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/lag-per-link-hash/class
                        leaf class {
                            type types-services:sap-lag-per-link-hash-class;
                            default "1";
                            description "Class used to select a LAG link";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/lag-per-link-hash/weight
                        leaf weight {
                            type types-services:sap-lag-per-link-hash-weight;
                            default "1";
                            description "Weight associated with SAP when selecting a LAG link";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/lag-per-link-hash/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/lag-per-link-hash/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container lag-per-link-hash

                    ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/qos/hs-agg-rate
                        leaf hs-agg-rate {
                            type int32 {
                                range "1..100000000";
                            }
                            units "kilobps";
                            description "Maximum rate for single egress HS queues";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/qos/hs-min-resv-bw
                        leaf hs-min-resv-bw {
                            type int32 {
                                range "0..100000000";
                            }
                            units "kilobps";
                            default "1";
                            description "Reserved minimum total rate of all egress queues";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/qos/hs-low-burst-max-class
                        leaf hs-low-burst-max-class {
                            type uint32 {
                                range "1..6";
                            }
                            default "6";
                            description "Scheduling classes to map the low burst limit threshold";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/qos/agg-rate
                        container agg-rate {
                            description "Enter the agg-rate context";

                            ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/qos/agg-rate/rate
                            leaf rate {
                                type int32 {
                                    range "1..800000000";
                                }
                                units "kilobps";
                                description "Maximum total rate of all egress queues";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/qos/agg-rate/min-resv-bw
                            leaf min-resv-bw {
                                type int32 {
                                    range "0..800000000";
                                }
                                units "kilobps";
                                default "1";
                                description "Reserved minimum total rate of all egress queues";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/qos/agg-rate/adaptation-rule
                            leaf adaptation-rule {
                                type types-qos:adaptation-rule;
                                default "closest";
                                description "Adaptation rule for operational PIR value of subscriber";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/qos/agg-rate/burst-limit
                            leaf burst-limit {
                                type union {
                                    type types-qos:bytes {
                                        range "1..14000000";
                                    }
                                    type enumeration {
                                        enum auto                           { value -1; }
                                    }
                                }
                                default "auto";
                                description "Burst limit of the subscriber aggregate rate";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/sub-profile/egress/qos/agg-rate/queue-frame-based-accounting
                            leaf queue-frame-based-accounting {
                                type boolean;
                                default "false";
                                description "Use frame-based accounting to evaluate rate limit";
                            }

