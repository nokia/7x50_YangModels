submodule nokia-conf-subscriber-mgmt {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-application-assurance { prefix "types-aa"; }
    import nokia-types-bgp                { prefix "types-bgp"; }
    import nokia-types-diameter           { prefix "types-diam"; }
    import nokia-types-filter             { prefix "types-filter"; }
    import nokia-types-igmp               { prefix "types-igmp"; }
    import nokia-types-mcast-cac          { prefix "types-mcast-cac"; }
    import nokia-types-ppp                { prefix "types-ppp"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-router             { prefix "types-router"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-subscriber-mgmt    { prefix "types-submgt"; }

    include nokia-conf-aa-group;
    include nokia-conf-aaa;
    include nokia-conf-application-assurance;
    include nokia-conf-filter;
    include nokia-conf-groups;
    include nokia-conf-isa;
    include nokia-conf-log;
    include nokia-conf-mcast-cac;
    include nokia-conf-mcast-mgmt;
    include nokia-conf-policy-options;
    include nokia-conf-python;
    include nokia-conf-qos;
    include nokia-conf-service;
    include nokia-conf-submgt-ancp;
    include nokia-conf-submgt-gtp;
    include nokia-conf-submgt-isa-filter;
    include nokia-conf-submgt-isa-policer;
    include nokia-conf-submgt-local-user-db;
    include nokia-conf-submgt-service-chaining;
    include nokia-conf-submgt-up-resiliency;
    include nokia-conf-submgt-vrgw;
    include nokia-conf-submgt-wlangw;
    include nokia-conf-svc-nat;
    include nokia-conf-svc-vpls;
    include nokia-conf-system;
    include nokia-conf-system-security;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure subscriber management on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-subscriber-mgmt YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-06-02";

    grouping conf-subscriber-mgmt {
        ///nokia-conf/configure/subscriber-mgmt
        container subscriber-mgmt {
            description "Enter the subscriber-mgmt context";

            ///nokia-conf/configure/subscriber-mgmt/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/subscriber-mgmt/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy
            list diameter-gx-policy {
                key "name";
                max-elements 32;
                description "Enter the diameter-gx-policy list instance";

                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/name
                leaf name {
                    type types-sros:named-item;
                    description "Diameter application policy name";
                }

                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/tx-timer
                leaf tx-timer {
                    type types-sros:time-duration {
                        range "10..1000";
                    }
                    units "seconds";
                    default "10";
                    description "Maximum wait time for a pending session request";
                }

                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/diameter-base
                choice diameter-base {
                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/diameter-base/node
                    case node {

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/diameter-base/node/node
                        container node {
                            description "Enter the node context";

                            ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/diameter-base/node/node/origin-host
                            leaf origin-host {
                                type leafref {
                                    path "../../../../aaa/diameter/node/origin-host";
                                }
                                sros-ext:immutable;
                                description "Origin-Host AVP used by the Diameter policy";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/diameter-base/node/node/destination-realm
                            leaf destination-realm {
                                type string {
                                    length "1..80";
                                }
                                description "Destination-Realm AVP used by the Diameter peer policy";
                            }

                        } // container node

                    }
                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/diameter-base/peer-policy
                    case peer-policy {
                        status obsolete;

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/diameter-base/peer-policy/peer-policy
                        leaf peer-policy {
                            status obsolete;
                            type leafref {
                                path "../../../aaa/diameter/peer-policy/name";
                            }
                            sros-ext:immutable;
                            description "Policy for the Diameter peers";
                        }

                    }
                }
                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/on-failure
                container on-failure {
                    description "Enter the on-failure context";

                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/on-failure/failover
                    leaf failover {
                        type boolean;
                        default "true";
                        description "Session peer failover";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/on-failure/handling
                    leaf handling {
                        type enumeration {
                            enum terminate                      { value 1; }
                            enum continue                       { value 2; }
                            enum retry-and-terminate            { value 3; }
                        }
                        default "terminate";
                        description "Session peer failure handling";
                    }

                } // container on-failure

                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx
                container gx {
                    description "Enter the gx context";

                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/mac-format
                    leaf mac-format {
                        type types-submgt:mac-specification;
                        default "aa:";
                        description "MAC address format";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/report-ip-address-event
                    leaf report-ip-address-event {
                        type boolean;
                        default "true";
                        description "Enable CCR-u messages on IP address allocation events";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/credit-mcs-interval
                    leaf credit-mcs-interval {
                        type types-sros:time-duration {
                            range "5..60";
                        }
                        units "minutes";
                        default "10";
                        description "Volume syncing interval for usage monitoring";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/destination-realm-learning
                    leaf destination-realm-learning {
                        type boolean;
                        default "true";
                        description "Learn the destination realm from the incoming messages";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/avp-subscription-id
                    container avp-subscription-id {
                        description "Enter the avp-subscription-id context";

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/avp-subscription-id/origin
                        leaf origin {
                            type enumeration {
                                enum subscriber-id                  { value 1; }
                                enum circuit-id                     { value 2; }
                                enum imsi                           { value 3; }
                                enum msisdn                         { value 4; }
                                enum imei                           { value 5; }
                                enum dual-stack-remote-id           { value 6; }
                                enum mac                            { value 7; }
                                enum username                       { value 8; }
                                enum nas-port-id                    { value 9; }
                            }
                            default "subscriber-id";
                            description "Format of the Subscription-Id-Data AVP";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/avp-subscription-id/type
                        leaf type {
                            type enumeration {
                                enum e164                           { value 0; }
                                enum imsi                           { value 1; }
                                enum nai                            { value 3; }
                                enum private                        { value 4; }
                            }
                            default "private";
                            description "Format of the Subscription-Id-Type AVP";
                        }

                    } // container avp-subscription-id

                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/ccrt-replay
                    container ccrt-replay {
                        presence "Enable the ccrt-replay context";
                        description "Enable the ccrt-replay context";

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/ccrt-replay/interval
                        leaf interval {
                            type uint32 {
                                range "60..86400";
                            }
                            default "3600";
                            description "Time between consecutive CCR-T replay attempts";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/ccrt-replay/max-lifetime
                        leaf max-lifetime {
                            type types-sros:time-duration {
                                range "1..24";
                            }
                            units "hours";
                            default "24";
                            description "Maximum time that CCR-t messages are replayed";
                        }

                    } // container ccrt-replay

                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp
                    container include-avp {
                        description "Enter the include-avp context";

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/an-gw-address
                        leaf an-gw-address {
                            type boolean;
                            default "false";
                            description "Include the IP address of this gateway";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/apn-ambr
                        leaf apn-ambr {
                            type boolean;
                            default "false";
                            description "Include the APN aggregate maximum bitrate";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/called-station-id
                        leaf called-station-id {
                            type boolean;
                            default "false";
                            description "Include the called station ID";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/ip-can-type
                        leaf ip-can-type {
                            type boolean;
                            default "false";
                            description "Include the IP CAN type";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/logical-access-id
                        leaf logical-access-id {
                            type boolean;
                            default "false";
                            description "Include the logical access id";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/pdn-connection-id
                        leaf pdn-connection-id {
                            type boolean;
                            default "false";
                            description "Include the PDN connection id";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/physical-access-id
                        leaf physical-access-id {
                            type boolean;
                            default "false";
                            description "Include the physical access ID";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/rai
                        leaf rai {
                            type boolean;
                            default "false";
                            description "Include the routing area identity";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/rat-type
                        leaf rat-type {
                            type boolean;
                            default "false";
                            description "Include the RAT type";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/sgsn-mcc-mnc
                        leaf sgsn-mcc-mnc {
                            type boolean;
                            default "false";
                            description "Include the 3GPP-SGSN-MCC-MNC AVP";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/supported-features
                        leaf supported-features {
                            type boolean;
                            default "false";
                            description "Include the supported features";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/user-location-info
                        leaf user-location-info {
                            type boolean;
                            default "false";
                            description "Include the 3GPP-User-Location-Information AVP";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/calling-station-id
                        container calling-station-id {
                            presence "Enable the calling-station-id context";
                            description "Enable the calling-station-id context";

                            ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/calling-station-id/type
                            leaf type {
                                type enumeration {
                                    enum sap-string                     { value 1; }
                                    enum mac                            { value 2; }
                                    enum sap-id                         { value 3; }
                                    enum remote-id                      { value 4; }
                                    enum llid                           { value 5; }
                                }
                                default "sap-string";
                                description "Format of the Calling-Station-ID AVP";
                            }

                        } // container calling-station-id

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port
                        container nas-port {
                            presence "Enable the nas-port context";
                            description "Enable the nas-port context";

                            ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port/bit-spec
                            leaf bit-spec {
                                type types-sros:binary-specification {
                                    pattern "([01]|(\\*[0123456789]+)?[oismpvc])+";
                                }
                                mandatory true;
                                description "NAS-Port AVP format";
                            }

                        } // container nas-port

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port-id
                        container nas-port-id {
                            presence "Enable the nas-port-id context";
                            description "Enable the nas-port-id context";

                            ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port-id/prefix-string
                            leaf prefix-string {
                                type string {
                                    length "1..8";
                                }
                                description "Prefix string for NAS-Port-Id AVP";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port-id/suffix
                            container suffix {
                                description "Enter the suffix context";

                                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port-id/suffix/suffix
                                choice suffix {
                                    default "not-included";
                                    description "Configure the format of the nas-port-id suffix.";
                                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port-id/suffix/suffix/not-included
                                    case not-included {

                                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port-id/suffix/suffix/not-included/not-included
                                        leaf not-included {
                                            type empty;
                                            description "Do not add a suffix";
                                        }

                                    }
                                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port-id/suffix/suffix/circuit-id
                                    case circuit-id {

                                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port-id/suffix/suffix/circuit-id/circuit-id
                                        leaf circuit-id {
                                            type empty;
                                            description "Add Circuit ID as suffix";
                                        }

                                    }
                                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port-id/suffix/suffix/remote-id
                                    case remote-id {

                                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port-id/suffix/suffix/remote-id/remote-id
                                        leaf remote-id {
                                            type empty;
                                            description "Add Remote ID as suffix";
                                        }

                                    }
                                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port-id/suffix/suffix/user-string
                                    case user-string {

                                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port-id/suffix/suffix/user-string/user-string
                                        leaf user-string {
                                            type string {
                                                length "1..64";
                                            }
                                            description "Add a user configurable string as suffix";
                                        }

                                    }
                                }
                            } // container suffix

                        } // container nas-port-id

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port-type
                        container nas-port-type {
                            presence "Enable the nas-port-type context";
                            description "Enable the nas-port-type context";

                            ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/nas-port-type/type
                            leaf type {
                                type uint32 {
                                    range "0..255";
                                }
                                description "NAS-Port-Type AVP value";
                            }

                        } // container nas-port-type

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/user-equipment-info
                        container user-equipment-info {
                            presence "Enable the user-equipment-info context";
                            description "Enable the user-equipment-info context";

                            ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/include-avp/user-equipment-info/type
                            leaf type {
                                type enumeration {
                                    enum imeisv                         { value 0; }
                                    enum mac                            { value 1; }
                                    enum eui64                          { value 2; }
                                    enum modified-eui64                 { value 3; }
                                }
                                default "mac";
                                description "Information for User-Equipment-Info attribute";
                            }

                        } // container user-equipment-info

                    } // container include-avp

                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping
                    container three-gpp-qos-mapping {
                        description "Enter the three-gpp-qos-mapping context";

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl
                        container apn-ambr-dl {
                            description "Enter the apn-ambr-dl context";

                            ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override
                            choice qos-override {
                                default "ignore-override";
                                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override/ignore-override
                                case ignore-override {

                                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override/ignore-override/ignore-override
                                    leaf ignore-override {
                                        type empty;
                                        description "no override on apn-ambr-dl";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override/arbiter
                                case arbiter {

                                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override/arbiter/arbiter
                                    leaf arbiter {
                                        type types-sros:named-item;
                                        description "Arbiter name to be overridden";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override/policer
                                case policer {

                                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override/policer/policer
                                    leaf policer {
                                        type types-qos:sap-egress-policer-id;
                                        description "Policer ID to be overidden";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override/queue
                                case queue {

                                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override/queue/queue
                                    leaf queue {
                                        type types-qos:egress-queue-id;
                                        description "Queue ID to be overridden";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override/scheduler
                                case scheduler {

                                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override/scheduler/scheduler
                                    leaf scheduler {
                                        type types-sros:named-item;
                                        description "Name of the scheduler to be overridden";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override/aggregate-rate
                                case aggregate-rate {

                                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override/aggregate-rate/aggregate-rate
                                    leaf aggregate-rate {
                                        type empty;
                                        description "Map to an aggregate rate";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override/hs-sla-agg-rate
                                case hs-sla-agg-rate {

                                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-dl/qos-override/hs-sla-agg-rate/hs-sla-agg-rate
                                    leaf hs-sla-agg-rate {
                                        type empty;
                                        description "Map to an HS SLA aggregate rate";
                                    }

                                }
                            }
                        } // container apn-ambr-dl

                        ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-ul
                        container apn-ambr-ul {
                            description "Enter the apn-ambr-ul context";

                            ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-ul/qos-override
                            choice qos-override {
                                default "ignore-override";
                                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-ul/qos-override/ignore-override
                                case ignore-override {

                                    ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-ul/qos-override/ignore-override/ignore-override
                                    leaf ignore-override {
                                        type empty;
                                        description "no override on apn-ambr-ul";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/diameter-gx-policy/gx/three-gpp-qos-mapping/apn-ambr-ul/qos-override/arbiter
                                case arbiter {

