                    }
                    default "terminate-session";
                    description "Behavior for subsequent NCP renegotiation messages";
                }

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/lcp-ignore-identifier
                leaf lcp-ignore-identifier {
                    type boolean;
                    default "false";
