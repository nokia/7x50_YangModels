                                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/ppp-options/custom-option/option-data/address/address
                                leaf address {
                                    type types-sros:ipv4-address;
                                    description "IP address of the custom PPP option";
                                }

                            }
                            ///nokia-conf/configure/subscriber-mgmt/ppp-policy/ppp-options/custom-option/option-data/hex-string
                            case hex-string {

                                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/ppp-options/custom-option/option-data/hex-string/hex-string
                                leaf hex-string {
                                    type types-sros:hex-string {
                                        length "1..256";
                                    }
                                    description "Hexadecimal string for the custom PPP option";
                                }

                            }
                            ///nokia-conf/configure/subscriber-mgmt/ppp-policy/ppp-options/custom-option/option-data/ascii-string
                            case ascii-string {

                                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/ppp-options/custom-option/option-data/ascii-string/ascii-string
                                leaf ascii-string {
                                    type types-sros:string-not-all-spaces {
