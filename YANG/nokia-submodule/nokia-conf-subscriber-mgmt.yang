                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/ppp-policy/ppp-options/custom-option/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/ppp-policy/ppp-options/custom-option/option-data
                        choice option-data {
                            mandatory true;
                            ///nokia-conf/configure/subscriber-mgmt/ppp-policy/ppp-options/custom-option/option-data/address
