                        enum pap-chap                       { value 3; }
                    }
                    default "padi";
                    description "PPPoE access method";
                }

                ///nokia-conf/configure/subscriber-mgmt/radius-authentication-policy/radius-server-policy
                leaf radius-server-policy {
                    type leafref {
                        path "../../../aaa/radius/server-policy/name";
