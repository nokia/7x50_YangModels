                        leaf peer-policy {
                            status obsolete;
                            type leafref {
                                path "../../../aaa/diameter/peer-policy/name";
                            }
                            sros-ext:immutable;
                            description "Policy for the Diameter peers";
                        }

                    }
                }
                ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/on-failure
                container on-failure {
                    description "Enter the on-failure context";

                    ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/on-failure/failover
                    leaf failover {
                        type boolean;
                        default "true";
                        description "Session peer failover";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/on-failure/handling
                    leaf handling {
                        type enumeration {
                            enum terminate                      { value 1; }
                            enum continue                       { value 2; }
                            enum retry-and-terminate            { value 3; }
                        }
                        default "terminate";
                        description "Session peer failure handling";
                    }

                } // container on-failure

                ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq
                container nasreq {
                    description "Enter the nasreq context";

                    ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/mac-format
                    leaf mac-format {
                        type types-submgt:mac-specification;
                        default "aa:";
                        description "MAC address format";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/password
                    leaf password {
                        type types-sros:encrypted-leaf {
                            length "1..115";
                        }
                        description "AA-request password";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/user-name
                    container user-name {
                        description "Enter the user-name context";

                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/user-name/format
                        leaf format {
                            type enumeration {
                                enum mac                            { value 1; }
                                enum circuit-id                     { value 2; }
                                enum tuple                          { value 3; }
                                enum ascii-converted-circuit-id     { value 4; }
                                enum ascii-converted-tuple          { value 5; }
                                enum dhcp-client-vendor-opts        { value 6; }
                                enum mac-giaddr                     { value 7; }
                                enum nas-port-id                    { value 8; }
                            }
                            default "mac";
                            description "Format type for username";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/user-name/operation
                        leaf operation {
                            type types-submgt:user-name-format-domain-operation;
                            description "Operation to perform";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/user-name/domain-name
                        leaf domain-name {
                            type string {
                                length "1..128";
                            }
                            description "Domain name";
                        }

                    } // container user-name

                    ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp
                    container include-avp {
                        description "Enter the include-avp context";

                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/called-station-id
                        leaf called-station-id {
                            type boolean;
                            default "false";
                            description "Include the called station ID AVP";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/circuit-id
                        leaf circuit-id {
                            type boolean;
                            default "false";
                            description "Include the Agent-Circuit-ID AVP";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/imei
                        leaf imei {
                            type boolean;
                            default "false";
                            description "Include the IMEI";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/rat-type
                        leaf rat-type {
                            type boolean;
                            default "false";
                            description "Include the Radio Access Technology (RAT) type AVP";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/remote-id
                        leaf remote-id {
                            type boolean;
                            default "false";
                            description "Include the agent remote ID AVP";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/user-location-info
                        leaf user-location-info {
                            type boolean;
                            default "false";
                            description "Include the 3GPP-User-Location-Information AVP";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/calling-station-id
                        container calling-station-id {
                            presence "Enable the calling-station-id context";
                            description "Enable the calling-station-id context";

                            ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/calling-station-id/type
                            leaf type {
                                type enumeration {
                                    enum sap-string                     { value 1; }
                                    enum mac                            { value 2; }
                                    enum sap-id                         { value 3; }
                                    enum remote-id                      { value 4; }
                                    enum llid                           { value 5; }
                                }
                                default "sap-string";
                                description "Format of the Calling-Station-ID AVP";
                            }

                        } // container calling-station-id

                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port
                        container nas-port {
                            presence "Enable the nas-port context";
                            description "Enable the nas-port context";

                            ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port/bit-spec
                            leaf bit-spec {
                                type types-sros:binary-specification {
                                    pattern "([01]|(\\*[0123456789]+)?[oismpvc])+";
                                }
                                mandatory true;
                                description "NAS-Port AVP format";
                            }

                        } // container nas-port

                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port-id
                        container nas-port-id {
                            presence "Enable the nas-port-id context";
                            description "Enable the nas-port-id context";

                            ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port-id/prefix-string
                            leaf prefix-string {
                                type string {
                                    length "1..8";
                                }
                                description "Prefix string for NAS-Port-Id AVP";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port-id/suffix
                            container suffix {
                                description "Enter the suffix context";

                                ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port-id/suffix/suffix
                                choice suffix {
                                    default "not-included";
                                    description "Configure the format of the nas-port-id suffix.";
                                    ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port-id/suffix/suffix/not-included
                                    case not-included {

                                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port-id/suffix/suffix/not-included/not-included
                                        leaf not-included {
                                            type empty;
                                            description "Do not add a suffix";
                                        }

                                    }
                                    ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port-id/suffix/suffix/circuit-id
                                    case circuit-id {

                                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port-id/suffix/suffix/circuit-id/circuit-id
                                        leaf circuit-id {
                                            type empty;
                                            description "Add Circuit ID as suffix";
                                        }

                                    }
                                    ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port-id/suffix/suffix/remote-id
                                    case remote-id {

                                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port-id/suffix/suffix/remote-id/remote-id
                                        leaf remote-id {
                                            type empty;
                                            description "Add Remote ID as suffix";
                                        }

                                    }
                                    ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port-id/suffix/suffix/user-string
                                    case user-string {

                                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port-id/suffix/suffix/user-string/user-string
                                        leaf user-string {
                                            type string {
                                                length "1..64";
                                            }
                                            description "Add a user configurable string as suffix";
                                        }

                                    }
                                }
                            } // container suffix

                        } // container nas-port-id

                        ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port-type
                        container nas-port-type {
                            presence "Enable the nas-port-type context";
                            description "Enable the nas-port-type context";

                            ///nokia-conf/configure/subscriber-mgmt/diameter-nasreq-policy/nasreq/include-avp/nas-port-type/type
                            leaf type {
                                type union {
                                    type enumeration {
                                        enum rfc-aligned                    { value 1000; }
                                    }
                                    type uint32 {
                                        range "0..255";
                                    }
                                }
                                default "rfc-aligned";
                                description "NAS-Port-Type AVP value";
                            }

                        } // container nas-port-type

                    } // container include-avp

                } // container nasreq

            } // list diameter-nasreq-policy

            ///nokia-conf/configure/subscriber-mgmt/group-interface-template
            list group-interface-template {
                key "name";
                max-elements 256;
                description "Enter the group-interface-template list instance";

                ///nokia-conf/configure/subscriber-mgmt/group-interface-template/name
                leaf name {
                    type types-sros:named-item;
                    description "Identifies this group interface template template";
                }

                ///nokia-conf/configure/subscriber-mgmt/group-interface-template/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ip-mtu
                leaf ip-mtu {
                    type types-router:ip-mtu-ext;
                    units "bytes";
                    description "IP MTU applied to outgoing packets";
                }

                ///nokia-conf/configure/subscriber-mgmt/group-interface-template/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/group-interface-template/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4
                container ipv4 {
                    description "Enter the ipv4 context";

                    ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp
                    container icmp {
                        description "Enter the icmp context";

                        ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/mask-reply
                        leaf mask-reply {
                            type boolean;
                            default "true";
                            description "Allow responses to ICMP mask requests on the interface";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/redirects
                        container redirects {
                            description "Enter the redirects context";

                            ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/redirects/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sending ICMP redirect messages";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/redirects/number
                            leaf number {
                                type uint16 {
                                    range "10..1000";
                                }
                                default "100";
                                description "Maximum number of ICMP redirect messages to send";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/redirects/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time used to limit the number of ICMP redirect messages";
                            }

                        } // container redirects

                        ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/ttl-expired
                        container ttl-expired {
                            description "Enter the ttl-expired context";

                            ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/ttl-expired/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sending TTL expired messages";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/ttl-expired/number
                            leaf number {
                                type uint16 {
                                    range "10..2000";
                                }
                                default "100";
                                description "Maximum number of TTL expired messages to send";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/ttl-expired/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time used to limit the number of TTL expired messages";
                            }

                        } // container ttl-expired

                        ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/unreachables
                        container unreachables {
                            description "Enter the unreachables context";

                            ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/unreachables/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sending unreachable messages";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/unreachables/number
                            leaf number {
                                type uint16 {
                                    range "10..2000";
                                }
                                default "100";
                                description "Maximum number of unreachable messages to send";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/unreachables/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time to limit the number of ICMP unreachable messages";
                            }

                        } // container unreachables

                        ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/param-problem
                        container param-problem {
                            description "Enter the param-problem context";

                            ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/param-problem/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sent Parameter Problem messages";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/param-problem/number
                            leaf number {
                                type uint32 {
                                    range "10..1000";
                                }
                                default "100";
                                description "Maximum number of Parameter Problem messages to send";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/icmp/param-problem/seconds
                            leaf seconds {
                                type uint32 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time used to limit number of Parameter Problem messages";
                            }

                        } // container param-problem

                    } // container icmp

                    ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/neighbor-discovery
                    container neighbor-discovery {
                        description "Enter the neighbor-discovery context";

                        ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/neighbor-discovery/remote-proxy-arp
                        leaf remote-proxy-arp {
                            type boolean;
                            default "false";
                            description "Enable remote proxy ARP on the interface";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/neighbor-discovery/proxy-arp-policy
                        leaf-list proxy-arp-policy {
                            type leafref {
                                path "../../../../../policy-options/policy-statement/name";
                            }
                            max-elements 5;
                            ordered-by user;
                            description "Proxy ARP policy name";
                        }

                    } // container neighbor-discovery

                    ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/urpf-check
                    container urpf-check {
                        presence "Enable the urpf-check context";
                        description "Enable the urpf-check context";

                        ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv4/urpf-check/mode
                        leaf mode {
                            type types-router:urpf-check-mode;
                            default "strict";
                            description "Unicast RPF check mode";
                        }

                    } // container urpf-check

                } // container ipv4

                ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv6
                container ipv6 {
                    description "Enter the ipv6 context";

                    ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv6/urpf-check
                    container urpf-check {
                        presence "Enable the urpf-check context";
                        description "Enable the urpf-check context";

                        ///nokia-conf/configure/subscriber-mgmt/group-interface-template/ipv6/urpf-check/mode
                        leaf mode {
                            type types-router:urpf-check-mode;
                            default "strict";
                            description "Unicast RPF check mode";
                        }

                    } // container urpf-check

                } // container ipv6

                ///nokia-conf/configure/subscriber-mgmt/group-interface-template/trigger-packet
                container trigger-packet {
                    description "Enter the trigger-packet context";

                    ///nokia-conf/configure/subscriber-mgmt/group-interface-template/trigger-packet/data
                    leaf data {
                        type boolean;
                        default "false";
                        description "Sample and forward data packets";
                    }

                } // container trigger-packet

            } // list group-interface-template

            ///nokia-conf/configure/subscriber-mgmt/ipoe-session-policy
            list ipoe-session-policy {
                key "name";
                max-elements 256;
                description "Enter the ipoe-session-policy list instance";

                ///nokia-conf/configure/subscriber-mgmt/ipoe-session-policy/name
                leaf name {
                    type types-sros:named-item;
                    description "IPoE session policy name";
                }

                ///nokia-conf/configure/subscriber-mgmt/ipoe-session-policy/circuit-id-from-auth
                leaf circuit-id-from-auth {
                    type boolean;
                    sros-ext:immutable;
                    default "false";
                    description "Use circuit ID from authentication server to identify session";
                }

