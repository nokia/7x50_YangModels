                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/force-ppp-mtu-gt-1492
                leaf force-ppp-mtu-gt-1492 {
                    type boolean;
                    default "false";
                    description "Enable PPPoE MRU negotiations greater than 1492 bytes";
                }

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/ipcp-subnet-negotiation
                leaf ipcp-subnet-negotiation {
                    type boolean;
                    default "false";
                    description "Allow IPCP subnet negotiation for PPPoE hosts";
                }

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/lcp-ignore-magic-numbers
                leaf lcp-ignore-magic-numbers {
                    type boolean;
                    default "false";
                    description "Ignore LCP peer magic number mismatch";
                }

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/max-sessions-per-mac
                leaf max-sessions-per-mac {
                    type uint32 {
                        range "1..8191";
                    }
