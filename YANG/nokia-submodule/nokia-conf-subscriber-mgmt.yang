                } // container options

                ///nokia-conf/configure/subscriber-mgmt/router-advertisement-policy/prefix-options
                container prefix-options {
                    description "Enter the prefix-options context";

                    ///nokia-conf/configure/subscriber-mgmt/router-advertisement-policy/prefix-options/stateful
                    container stateful {
                        description "Enter the stateful context";

                        ///nokia-conf/configure/subscriber-mgmt/router-advertisement-policy/prefix-options/stateful/auto-lifetimes
                        leaf auto-lifetimes {
                            type boolean;
                            default "false";
                            description "Use automatic lifetimes for stateful prefixes";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/router-advertisement-policy/prefix-options/stateful/on-link
                        leaf on-link {
                            type boolean;
                            default "true";
                            description "Assign the prefix to an interface on the specified link";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/router-advertisement-policy/prefix-options/stateful/preferred-lifetime
                        leaf preferred-lifetime {
                            type types-submgt:router-advertisement-prefix-options-lifetime;
                            units "seconds";
                            default "3600";
                            description "Time for a prefix to remain preferred";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/router-advertisement-policy/prefix-options/stateful/valid-lifetime
                        leaf valid-lifetime {
                            type types-submgt:router-advertisement-prefix-options-lifetime;
                            units "seconds";
                            default "86400";
                            description "Time for a prefix to remain valid";
                        }

                    } // container stateful

                    ///nokia-conf/configure/subscriber-mgmt/router-advertisement-policy/prefix-options/stateless
                    container stateless {
                        description "Enter the stateless context";

                        ///nokia-conf/configure/subscriber-mgmt/router-advertisement-policy/prefix-options/stateless/on-link
                        leaf on-link {
                            type boolean;
                            default "true";
                            description "Assign the prefix to an interface on the specified link";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/router-advertisement-policy/prefix-options/stateless/preferred-lifetime
                        leaf preferred-lifetime {
                            type types-submgt:router-advertisement-prefix-options-lifetime;
                            units "seconds";
                            default "3600";
                            description "Time for a prefix to remain preferred";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/router-advertisement-policy/prefix-options/stateless/valid-lifetime
                        leaf valid-lifetime {
                            type types-submgt:router-advertisement-prefix-options-lifetime;
                            units "seconds";
                            default "86400";
                            description "Time for a prefix to remain valid";
                        }

                    } // container stateless

                } // container prefix-options

            } // list router-advertisement-policy

            ///nokia-conf/configure/subscriber-mgmt/bgp-peering-policy
