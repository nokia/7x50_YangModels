                    list priority {
                        key "id";
                        description "Enter the priority list instance";

                        ///nokia-conf/configure/subscriber-mgmt/authentication-origin/overrides/priority/id
                        leaf id {
                            type uint32 {
                                range "1..4294967295";
                            }
                            description "Authentication origin priority override";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/authentication-origin/overrides/priority/source
                        leaf source {
                            type types-submgt:authentication-origin;
                            mandatory true;
                            description "Source of authentication priority";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/authentication-origin/overrides/priority/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/authentication-origin/overrides/priority/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list priority

                } // container overrides

            } // container authentication-origin

            ///nokia-conf/configure/subscriber-mgmt/radius-authentication-policy
            list radius-authentication-policy {
                key "name";
                max-elements 32;
                description "Enter the radius-authentication-policy list instance";

                ///nokia-conf/configure/subscriber-mgmt/radius-authentication-policy/name
                leaf name {
                    type types-sros:named-item;
                    description "RADIUS authentication policy name";
                }

                ///nokia-conf/configure/subscriber-mgmt/radius-authentication-policy/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/subscriber-mgmt/radius-authentication-policy/password
                leaf password {
                    type types-sros:encrypted-leaf {
                        length "1..115";
                    }
                    description "RADIUS request password";
                }

                ///nokia-conf/configure/subscriber-mgmt/radius-authentication-policy/pppoe-access-method
                leaf pppoe-access-method {
                    type enumeration {
                        enum none                           { value 1; }
