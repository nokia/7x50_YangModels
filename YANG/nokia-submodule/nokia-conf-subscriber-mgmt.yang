                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/max-sessions-per-cid
                container max-sessions-per-cid {
                    presence "Enable the max-sessions-per-cid context";
                    description "Enable the max-sessions-per-cid context";

                    ///nokia-conf/configure/subscriber-mgmt/ppp-policy/max-sessions-per-cid/limit
                    leaf limit {
                        type uint32 {
                            range "1..8190";
                        }
                        mandatory true;
                        description "Maximum sessions per Agent Circuit ID and per SAP";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/ppp-policy/max-sessions-per-cid/allow-sessions-without-cid
                    leaf allow-sessions-without-cid {
                        type boolean;
                        default "false";
                        description "Allow PPPoE sessions without an Agent Circuit ID";
                    }

                } // container max-sessions-per-cid

                ///nokia-conf/configure/subscriber-mgmt/ppp-policy/keepalive
                container keepalive {
                    description "Enter the keepalive context";

                    ///nokia-conf/configure/subscriber-mgmt/ppp-policy/keepalive/interval
                    leaf interval {
                        type types-sros:time-duration {
                            range "4..300";
                        }
                        units "seconds";
                        default "30";
                        description "PPP keepalive interval";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/ppp-policy/keepalive/hold-up-multiplier
                    leaf hold-up-multiplier {
                        type uint32 {
                            range "1..5";
                        }
                        default "3";
                        description "PPP keepalive multiplier";
                    }

