                    ///nokia-conf/configure/subscriber-mgmt/radius-authentication-policy/fallback/action
                    container action {
                        description "Enter the action context";

                        ///nokia-conf/configure/subscriber-mgmt/radius-authentication-policy/fallback/action/action
                        choice action {
                            ///nokia-conf/configure/subscriber-mgmt/radius-authentication-policy/fallback/action/action/accept
                            case accept {

                                ///nokia-conf/configure/subscriber-mgmt/radius-authentication-policy/fallback/action/action/accept/accept
                                leaf accept {
                                    type empty;
                                    description "Automatically accept all authentication requests";
                                }

                            }
                            ///nokia-conf/configure/subscriber-mgmt/radius-authentication-policy/fallback/action/action/user-db
                            case user-db {

                                ///nokia-conf/configure/subscriber-mgmt/radius-authentication-policy/fallback/action/action/user-db/user-db
                                leaf user-db {
                                    type types-sros:named-item;
                                    description "Local user database used to authenticate to the server";
                                }

                            }
                        }
