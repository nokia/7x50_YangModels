                    ///nokia-conf/configure/qos/port-qos-policy/wrr-weights/mc-weight
                    leaf mc-weight {
                        type types-qos:port-qos-policy-wrr-weight;
                        default "1";
                        description "Weight for the multicast queue";
                    }

                    ///nokia-conf/configure/qos/port-qos-policy/wrr-weights/uc-weight
                    leaf uc-weight {
                        type types-qos:port-qos-policy-wrr-weight;
                        default "1";
                        description "Weight for the unicast queue";
                    }

                    ///nokia-conf/configure/qos/port-qos-policy/wrr-weights/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/qos/port-qos-policy/wrr-weights/apply-groups-exclude
                    leaf-list apply-groups-exclude {
