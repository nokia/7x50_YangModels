                }

                ///nokia-conf/configure/qos/sap-egress/parent-location
                leaf parent-location {
                    type enumeration {
                        enum auto                           { value 1; }
                        enum sla                            { value 2; }
                    }
                    default "auto";
                    description "Location where queues look to find parent scheduler";
                }

                ///nokia-conf/configure/qos/sap-egress/policers-hqos-manageable
                leaf policers-hqos-manageable {
