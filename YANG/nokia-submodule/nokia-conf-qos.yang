                            ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/dynamic-queue/parent-mapping/scheduler-parent
                            case scheduler-parent {

                                ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/dynamic-queue/parent-mapping/scheduler-parent/scheduler-parent
                                container scheduler-parent {
                                    presence "Enable the scheduler-parent context";
