                                } // container port-redirect-group

                            } // container action

                        } // list entry

                    } // container ip-criteria

                    ///nokia-conf/configure/qos/network/egress/ipv6-criteria
                    container ipv6-criteria {
                        description "Enter the ipv6-criteria context";

                        ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry
                        list entry {
                            key "entry-id";
                            description "Enter the entry list instance";

                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/entry-id
                            leaf entry-id {
                                type types-qos:entry-id;
                                description "Entry ID for match criterion and corresponding action";
                            }

                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/description
                            leaf description {
                                type types-sros:description;
                                description "Text description";
                            }

                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match
                            container match {
                                description "Enter the match context";

                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/next-header
                                leaf next-header {
                                    type types-sros:match-ip-protocol;
                                    description "IP protocol to match";
                                }

                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dscp
                                leaf dscp {
                                    type types-qos:dscp-name;
                                    description "DSCP value to match in the packet";
                                }

                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/fragment
                                leaf fragment {
                                    type enumeration {
                                        enum false                          { value 2; }
                                        enum true                           { value 3; }
                                        enum first-only                     { value 4; }
                                        enum non-first-only                 { value 5; }
                                    }
                                    description "Fragmentation state as the match criterion";
                                }

                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/icmp-type
                                leaf icmp-type {
                                    type types-filter:ipv6-match-icmp-types;
                                    description "ICMPv6 type in ICMPv6 header of IPv6 packet to match";
                                }

                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-ip
                                container dst-ip {
                                    description "Enter the dst-ip context";

                                    ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-ip/dst-ip
                                    choice dst-ip {
                                        ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-ip/dst-ip/address-and-mask-or-prefix
                                        case address-and-mask-or-prefix {

                                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-ip/dst-ip/address-and-mask-or-prefix/address
                                            leaf address {
                                                type union {
                                                    type types-sros:ipv6-prefix-with-host-bits;
                                                    type types-sros:ipv6-address;
                                                }
                                                description "Destination IPv6 address to match";
                                            }

                                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-ip/dst-ip/address-and-mask-or-prefix/mask
                                            leaf mask {
                                                type types-sros:ipv6-address;
                                                description "Address mask";
                                            }

                                        }
                                    }
                                } // container dst-ip

                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-ip
                                container src-ip {
                                    description "Enter the src-ip context";

                                    ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-ip/src-ip
                                    choice src-ip {
                                        ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-ip/src-ip/address-and-mask-or-prefix
                                        case address-and-mask-or-prefix {

                                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-ip/src-ip/address-and-mask-or-prefix/address
                                            leaf address {
                                                type union {
                                                    type types-sros:ipv6-prefix-with-host-bits;
                                                    type types-sros:ipv6-address;
                                                }
                                                description "Source IPv6 address to match";
                                            }

                                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-ip/src-ip/address-and-mask-or-prefix/mask
                                            leaf mask {
                                                type types-sros:ipv6-address;
                                                description "Address mask";
                                            }

                                        }
                                    }
                                } // container src-ip

                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-port
                                container src-port {
                                    description "Enter the src-port context";

                                    ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-port/port-match-mapping
                                    choice port-match-mapping {
                                        ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-port/port-match-mapping/lt
                                        case lt {

                                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-port/port-match-mapping/lt/lt
                                            leaf lt {
                                                type int32 {
                                                    range "0..65535";
                                                }
                                                description "Value 'less than' assigned as match condition";
                                            }

                                        }
                                        ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-port/port-match-mapping/eq
                                        case eq {

                                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-port/port-match-mapping/eq/eq
                                            leaf eq {
                                                type int32 {
                                                    range "0..65535";
                                                }
                                                description "Value 'equal to' assigned as match condition";
                                            }

                                        }
                                        ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-port/port-match-mapping/gt
                                        case gt {

                                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-port/port-match-mapping/gt/gt
                                            leaf gt {
                                                type int32 {
                                                    range "0..65535";
                                                }
                                                description "Value 'greater than' assigned as match condition";
                                            }

                                        }
                                        ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-port/port-match-mapping/range
                                        case range {

                                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-port/port-match-mapping/range/range
                                            container range {
                                                description "Enter the range context";

                                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-port/port-match-mapping/range/range/start
                                                leaf start {
                                                    type int32 {
                                                        range "0..65535";
                                                    }
                                                    description "Lower bound of port range to match";
                                                }

                                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-port/port-match-mapping/range/range/end
                                                leaf end {
                                                    type int32 {
                                                        range "0..65535";
                                                    }
                                                    description "Upper bound of port range to match";
                                                }

                                            } // container range

                                        }
                                        ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-port/port-match-mapping/port-list
                                        case port-list {

                                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/src-port/port-match-mapping/port-list/port-list
                                            leaf port-list {
                                                type leafref {
                                                    path "../../../../../../../match-list/port-list/port-list-name";
                                                }
                                                description "Name of the port list as the match criterion";
                                            }

                                        }
                                    }
                                } // container src-port

                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-port
                                container dst-port {
                                    description "Enter the dst-port context";

                                    ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-port/port-match-mapping
                                    choice port-match-mapping {
                                        ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-port/port-match-mapping/lt
                                        case lt {

                                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-port/port-match-mapping/lt/lt
                                            leaf lt {
                                                type int32 {
                                                    range "0..65535";
                                                }
                                                description "Value 'less than' assigned as match condition";
                                            }

                                        }
                                        ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-port/port-match-mapping/eq
                                        case eq {

                                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-port/port-match-mapping/eq/eq
                                            leaf eq {
                                                type int32 {
                                                    range "0..65535";
                                                }
                                                description "Value 'equal to' assigned as match condition";
                                            }

                                        }
                                        ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-port/port-match-mapping/gt
                                        case gt {

                                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-port/port-match-mapping/gt/gt
                                            leaf gt {
                                                type int32 {
                                                    range "0..65535";
                                                }
                                                description "Value 'greater than' assigned as match condition";
                                            }

                                        }
                                        ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-port/port-match-mapping/range
                                        case range {

                                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-port/port-match-mapping/range/range
                                            container range {
                                                description "Enter the range context";

                                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-port/port-match-mapping/range/range/start
                                                leaf start {
                                                    type int32 {
                                                        range "0..65535";
                                                    }
                                                    description "Lower bound of port range to match";
                                                }

                                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-port/port-match-mapping/range/range/end
                                                leaf end {
                                                    type int32 {
                                                        range "0..65535";
                                                    }
                                                    description "Upper bound of port range to match";
                                                }

                                            } // container range

                                        }
                                        ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-port/port-match-mapping/port-list
                                        case port-list {

                                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/match/dst-port/port-match-mapping/port-list/port-list
                                            leaf port-list {
                                                type leafref {
                                                    path "../../../../../../../match-list/port-list/port-list-name";
                                                }
                                                description "Name of the port list as the match criterion";
                                            }

                                        }
                                    }
                                } // container dst-port

                            } // container match

                            ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/action
                            container action {
                                description "Enter the action context";

                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/action/type
                                leaf type {
                                    type types-qos:criteria-action-type;
                                    default "ignore-match";
                                    description "Action for criteria entry";
                                }

                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/action/fc
                                leaf fc {
                                    type types-sros:fc-name;
                                    description "Forwarding class";
                                }

                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/action/profile
                                leaf profile {
                                    type types-qos:egress-profile;
                                    description "Profile reclassification action";
                                }

                                ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/action/port-redirect-group
                                container port-redirect-group {
                                    description "Enter the port-redirect-group context";

                                    ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/action/port-redirect-group/queue
                                    leaf queue {
                                        type types-qos:egress-queue-id;
                                        description "Queue to be used for post-policed traffic";
                                    }

                                    ///nokia-conf/configure/qos/network/egress/ipv6-criteria/entry/action/port-redirect-group/policer
                                    leaf policer {
                                        type types-qos:qgroup-egress-policer-id;
                                        description "Policer ID to be used for the matched traffic";
                                    }

                                } // container port-redirect-group

                            } // container action

                        } // list entry

                    } // container ipv6-criteria

                } // container egress

            } // list network

            ///nokia-conf/configure/qos/port-qos-policy
            list port-qos-policy {
                key "port-qos-policy-name";
                max-elements 511;
                description "Enter the port-qos-policy list instance";

                ///nokia-conf/configure/qos/port-qos-policy/port-qos-policy-name
                leaf port-qos-policy-name {
                    type types-sros:named-item-64;
                    description "Port QoS policy name";
                }

                ///nokia-conf/configure/qos/port-qos-policy/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/qos/port-qos-policy/scope
                leaf scope {
                    type types-qos:item-scope;
                    default "template";
                    description "Scope of the scheduler policy";
                }

                ///nokia-conf/configure/qos/port-qos-policy/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/qos/port-qos-policy/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/qos/port-qos-policy/fc
                list fc {
                    key "fc-name";
                    description "Enter the fc list instance";

                    ///nokia-conf/configure/qos/port-qos-policy/fc/fc-name
                    leaf fc-name {
                        type types-sros:fc-name;
                        description "Forwarding class name";
                    }

                    ///nokia-conf/configure/qos/port-qos-policy/fc/queue
                    leaf queue {
                        type leafref {
                            path "../../queue/queue-id";
                        }
                        description "QoS policy queue for the forwarding class";
                    }

                    ///nokia-conf/configure/qos/port-qos-policy/fc/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/qos/port-qos-policy/fc/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list fc

                ///nokia-conf/configure/qos/port-qos-policy/queue
                list queue {
                    key "queue-id";
                    description "Enter the queue list instance";

                    ///nokia-conf/configure/qos/port-qos-policy/queue/queue-id
                    leaf queue-id {
                        type types-qos:egress-queue-id;
                        description "Port QoS policy queue identifier";
                    }

                    ///nokia-conf/configure/qos/port-qos-policy/queue/queue-mgmt
                    leaf queue-mgmt {
                        type leafref {
                            path "../../../queue-mgmt-policy/queue-mgmt-policy-name";
                        }
                        description "Queue management policy to attach to this queue";
                    }

                    ///nokia-conf/configure/qos/port-qos-policy/queue/wrr-weights
                    leaf wrr-weights {
                        type leafref {
                            path "../../wrr-weights/wrr-weights-id";
                        }
                        description "Weighted Round Robin (WRR) profile associated with this queue";
                    }

                    ///nokia-conf/configure/qos/port-qos-policy/queue/low-latency
                    leaf low-latency {
                        type boolean;
                        default "false";
                        description "Enable low-latency switching on the queue";
                    }

                    ///nokia-conf/configure/qos/port-qos-policy/queue/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/qos/port-qos-policy/queue/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/qos/port-qos-policy/queue/adaptation-rule
                    container adaptation-rule {
                        description "Enter the adaptation-rule context";

                        ///nokia-conf/configure/qos/port-qos-policy/queue/adaptation-rule/pir
                        leaf pir {
                            type types-qos:adaptation-rule;
                            default "closest";
                            description "Adaptation rule to use while computing operational PIR value";
                        }

                        ///nokia-conf/configure/qos/port-qos-policy/queue/adaptation-rule/cir
                        leaf cir {
                            type types-qos:adaptation-rule;
                            default "closest";
                            description "Adaptation rule to use while computing operational CIR value";
                        }

                    } // container adaptation-rule

                    ///nokia-conf/configure/qos/port-qos-policy/queue/scheduler-mode
                    container scheduler-mode {
                        description "Enter the scheduler-mode context";

                        ///nokia-conf/configure/qos/port-qos-policy/queue/scheduler-mode/wfq-or-strict-priority
                        choice wfq-or-strict-priority {
                            default "wfq";
                            ///nokia-conf/configure/qos/port-qos-policy/queue/scheduler-mode/wfq-or-strict-priority/wfq
                            case wfq {

                                ///nokia-conf/configure/qos/port-qos-policy/queue/scheduler-mode/wfq-or-strict-priority/wfq/wfq
                                container wfq {
                                    description "Enter the wfq context";

                                    ///nokia-conf/configure/qos/port-qos-policy/queue/scheduler-mode/wfq-or-strict-priority/wfq/wfq/pir-weight
                                    leaf pir-weight {
                                        type uint32 {
                                            range "1..255";
                                        }
                                        default "1";
                                        description "PIR weight for the queue";
                                    }

                                    ///nokia-conf/configure/qos/port-qos-policy/queue/scheduler-mode/wfq-or-strict-priority/wfq/wfq/percent-rate
                                    container percent-rate {
                                        description "Enter the percent-rate context";

                                        ///nokia-conf/configure/qos/port-qos-policy/queue/scheduler-mode/wfq-or-strict-priority/wfq/wfq/percent-rate/pir
                                        leaf pir {
                                            type decimal64 {
                                                range "0.01..100.00";
                                                fraction-digits 2;
                                            }
                                            default "100.00";
                                            description "Administrative PIR percent rate";
                                        }

                                        ///nokia-conf/configure/qos/port-qos-policy/queue/scheduler-mode/wfq-or-strict-priority/wfq/wfq/percent-rate/cir
                                        leaf cir {
                                            type decimal64 {
                                                range "0.00..100.00";
                                                fraction-digits 2;
                                            }
                                            default "0.00";
                                            description "Administrative CIR percent rate";
                                        }

                                    } // container percent-rate

                                } // container wfq

                            }
                        }
                    } // container scheduler-mode

                } // list queue

                ///nokia-conf/configure/qos/port-qos-policy/wrr-weights
                list wrr-weights {
                    key "wrr-weights-id";
                    max-elements 1;
