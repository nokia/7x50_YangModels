                    ///nokia-conf/configure/qos/network-ingress/policer/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/qos/network-ingress/policer/adaptation-rule
                    container adaptation-rule {
                        description "Enter the adaptation-rule context";

                        ///nokia-conf/configure/qos/network-ingress/policer/adaptation-rule/pir
                        leaf pir {
                            type types-qos:adaptation-rule;
                            default "closest";
                            description "Constraint used to derive operational PIR value";
                        }

                        ///nokia-conf/configure/qos/network-ingress/policer/adaptation-rule/cir
                        leaf cir {
                            type types-qos:adaptation-rule;
                            default "closest";
                            description "Constraint used to derive operational CIR value";
                        }

                    } // container adaptation-rule

                    ///nokia-conf/configure/qos/network-ingress/policer/rate
                    container rate {
                        description "Enter the rate context";

                        ///nokia-conf/configure/qos/network-ingress/policer/rate/pir
                        leaf pir {
                            type union {
                                type uint32 {
                                    range "144..1000000000";
