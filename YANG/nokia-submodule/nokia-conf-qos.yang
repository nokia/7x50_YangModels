                                leaf level {
                                    type types-qos:hierarchy-level;
                                    default "1";
                                    description "Level of priority while feeding to the parent";
                                }

                                ///nokia-conf/configure/qos/sap-egress/queue/parent-mapping/scheduler-parent/scheduler-parent/weight
                                leaf weight {
                                    type types-qos:weight;
                                    default "1";
