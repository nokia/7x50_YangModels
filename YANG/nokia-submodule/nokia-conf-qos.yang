                                    default "0";
                                    description "Level of priority while feeding to the parent";
                                }

                                ///nokia-conf/configure/qos/sap-egress/queue/parent-mapping/scheduler-parent/scheduler-parent/cir-weight
                                leaf cir-weight {
                                    type types-qos:cir-weight;
                                    default "1";
