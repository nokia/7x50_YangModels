                        ///nokia-conf/configure/qos/sap-egress/fc/queue-redirection-mapping/port-redirect-group-queue
                        case port-redirect-group-queue {

                            ///nokia-conf/configure/qos/sap-egress/fc/queue-redirection-mapping/port-redirect-group-queue/port-redirect-group-queue
                            container port-redirect-group-queue {
                                presence "Enable the port-redirect-group-queue context";
