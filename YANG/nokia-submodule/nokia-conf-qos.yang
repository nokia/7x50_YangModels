                } // list fc

                ///nokia-conf/configure/qos/sap-egress/hs-wrr-group
                list hs-wrr-group {
                    key "group-id";
                    description "Enter the hs-wrr-group list instance";

                    ///nokia-conf/configure/qos/sap-egress/hs-wrr-group/group-id
                    leaf group-id {
                        type types-qos:hs-wrr-group-id;
                        description "HS WRR group identifier";
                    }

                    ///nokia-conf/configure/qos/sap-egress/hs-wrr-group/hs-class-weight
                    leaf hs-class-weight {
                        type types-qos:hs-class-weight;
                        default "1";
                        description "Weight of scheduling class";
                    }

                    ///nokia-conf/configure/qos/sap-egress/hs-wrr-group/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/qos/sap-egress/hs-wrr-group/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/qos/sap-egress/hs-wrr-group/rate-or-percent-rate
                    choice rate-or-percent-rate {
                        default "rate";
                        ///nokia-conf/configure/qos/sap-egress/hs-wrr-group/rate-or-percent-rate/rate
                        case rate {

                            ///nokia-conf/configure/qos/sap-egress/hs-wrr-group/rate-or-percent-rate/rate/rate
                            leaf rate {
                                type union {
                                    type uint32 {
                                        range "1..2000000000";
                                    }
                                    type enumeration {
                                        enum max                            { value -1; }
                                    }
                                }
                                units "kilobps";
                                default "max";
                                description "Administrative PIR";
                            }

                        }
                        ///nokia-conf/configure/qos/sap-egress/hs-wrr-group/rate-or-percent-rate/percent-rate
                        case percent-rate {

                            ///nokia-conf/configure/qos/sap-egress/hs-wrr-group/rate-or-percent-rate/percent-rate/percent-rate
                            leaf percent-rate {
                                type decimal64 {
                                    range "0.01..100.00";
                                    fraction-digits 2;
                                }
                                description "Administrative PIR percent";
                            }

                        }
                    }
                    ///nokia-conf/configure/qos/sap-egress/hs-wrr-group/adaptation-rule
                    container adaptation-rule {
                        description "Enter the adaptation-rule context";

                        ///nokia-conf/configure/qos/sap-egress/hs-wrr-group/adaptation-rule/pir
                        leaf pir {
                            type types-qos:adaptation-rule;
                            default "closest";
