                                    ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/dynamic-queue/parent-mapping/port-parent/port-parent/weight
                                    leaf weight {
                                        type types-qos:weight;
                                        default "1";
                                        description "Weight queue uses to receive bandwidth from scheduler";
                                    }

                                    ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/dynamic-queue/parent-mapping/port-parent/port-parent/cir-level
                                    leaf cir-level {
                                        type types-qos:cir-level;
                                        default "0";
                                        description "Port priority queue uses to receive scheduler bandwidth";
                                    }

                                    ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/dynamic-queue/parent-mapping/port-parent/port-parent/cir-weight
                                    leaf cir-weight {
                                        type types-qos:cir-weight;
                                        default "0";
                                        description "Weight queue uses to receive scheduler bandwidth";
                                    }

                                } // container port-parent

                            }
                        }
                        ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/dynamic-queue/queue-id-range
                        container queue-id-range {
                            presence "Enable the queue-id-range context";
                            description "Enable the queue-id-range context";

                            ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/dynamic-queue/queue-id-range/start
