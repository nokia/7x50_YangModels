                } // list hs-wrr-group

                ///nokia-conf/configure/qos/sap-egress/ip-criteria
                container ip-criteria {
                    description "Enter the ip-criteria context";

                    ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry
                    list entry {
                        key "entry-id";
                        description "Enter the entry list instance";

                        ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/entry-id
                        leaf entry-id {
                            type uint32 {
                                range "1..65535";
                            }
                            description "IP Criteria Entry Index";
                        }

                        ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match
                        container match {
                            description "Enter the match context";

                            ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/protocol
                            leaf protocol {
                                type types-sros:match-ip-protocol;
                                description "IP protocol to match";
                            }

                            ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/dscp
                            leaf dscp {
                                type types-qos:dscp-name;
                                description "DSCP value to match in the packet";
                            }

                            ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/fragment
                            leaf fragment {
                                type enumeration {
                                    enum false                          { value 2; }
                                    enum true                           { value 3; }
                                }
                                description "Fragmented packets as the match criterion";
                            }

                            ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/src-port
                            container src-port {
                                description "Enter the src-port context";

                                ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/src-port/port-match-mapping
                                choice port-match-mapping {
                                    ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/src-port/port-match-mapping/lt
                                    case lt {

                                        ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/src-port/port-match-mapping/lt/lt
                                        leaf lt {
                                            type int32 {
                                                range "0..65535";
                                            }
                                            description "Value 'less than' as match condition";
                                        }

                                    }
                                    ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/src-port/port-match-mapping/eq
                                    case eq {

                                        ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/src-port/port-match-mapping/eq/eq
                                        leaf eq {
                                            type int32 {
                                                range "0..65535";
                                            }
                                            description "Value 'equal to' as match condition";
                                        }

                                    }
                                    ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/src-port/port-match-mapping/gt
                                    case gt {

                                        ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/src-port/port-match-mapping/gt/gt
                                        leaf gt {
                                            type int32 {
                                                range "0..65535";
                                            }
                                            description "Value 'greater than' as match condition";
                                        }

                                    }
                                    ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/src-port/port-match-mapping/range
                                    case range {

                                        ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/src-port/port-match-mapping/range/range
                                        container range {
                                            description "Enter the range context";

                                            ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/src-port/port-match-mapping/range/range/start
                                            leaf start {
                                                type int32 {
                                                    range "0..65535";
                                                }
                                                description "Lower bound of the port range";
                                            }

                                            ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/src-port/port-match-mapping/range/range/end
                                            leaf end {
                                                type int32 {
                                                    range "0..65535";
                                                }
                                                description "Upper bound of the port range";
                                            }

                                        } // container range

                                    }
                                }
                            } // container src-port

                            ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/dst-port
                            container dst-port {
                                description "Enter the dst-port context";

                                ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/dst-port/port-match-mapping
                                choice port-match-mapping {
                                    ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/dst-port/port-match-mapping/lt
                                    case lt {

                                        ///nokia-conf/configure/qos/sap-egress/ip-criteria/entry/match/dst-port/port-match-mapping/lt/lt
                                        leaf lt {
                                            type int32 {
                                                range "0..65535";
                                            }
                                            description "Value 'less than' as match condition";
                                        }

                                    }
