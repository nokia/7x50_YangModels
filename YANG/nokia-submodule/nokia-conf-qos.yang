                    ///nokia-conf/configure/qos/network-ingress/fc/fc-name
                    leaf fc-name {
                        type types-qos:sap-ingress-fc-name;
                        description "Forwarding class for which this mapping is performed";
                    }

                    ///nokia-conf/configure/qos/network-ingress/fc/multicast-policer
                    leaf multicast-policer {
                        type leafref {
                            path "../../policer/policer-id";
                        }
                        description "Policer identifier to use for multicast traffic";
                    }

                    ///nokia-conf/configure/qos/network-ingress/fc/policer
                    leaf policer {
                        type leafref {
                            path "../../policer/policer-id";
                        }
                        description "Policer identifier to use for normal traffic";
                    }

                    ///nokia-conf/configure/qos/network-ingress/fc/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
