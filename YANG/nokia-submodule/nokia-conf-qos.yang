                            description "Inner Dot1p value for in-profile frames";
                        }

                        ///nokia-conf/configure/qos/sap-egress/fc/dot1p-inner/out-profile
                        leaf out-profile {
                            type types-qos:dot1p-priority;
                            description "Inner Dot1p value for out-of-profile frames";
                        }

                        ///nokia-conf/configure/qos/sap-egress/fc/dot1p-inner/exceed-profile
                        leaf exceed-profile {
                            type types-qos:dot1p-priority;
                            description "Inner dot1p value for exceed-profile frames";
                        }

                    } // container dot1p-inner

                    ///nokia-conf/configure/qos/sap-egress/fc/dot1p-outer
                    container dot1p-outer {
                        description "Enter the dot1p-outer context";

                        ///nokia-conf/configure/qos/sap-egress/fc/dot1p-outer/in-profile
                        leaf in-profile {
