submodule nokia-conf-qos {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-filter             { prefix "types-filter"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-groups;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure QoS policies on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-qos YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-05-31";

    grouping conf-qos {
        ///nokia-conf/configure/qos
        container qos {
            description "Enter the qos context";

            ///nokia-conf/configure/qos/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/qos/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/qos/md-auto-id
            container md-auto-id {
                description "Enter the md-auto-id context";

                ///nokia-conf/configure/qos/md-auto-id/qos-policy-id-range
                container qos-policy-id-range {
                    presence "Enable the qos-policy-id-range context";
                    description "Enable the qos-policy-id-range context";

                    ///nokia-conf/configure/qos/md-auto-id/qos-policy-id-range/start
                    leaf start {
                        type types-qos:qos-policy-id {
                            range "2..65535";
                        }
                        sros-ext:immutable;
                        mandatory true;
                        description "Lower bound of the ID range";
                    }

                    ///nokia-conf/configure/qos/md-auto-id/qos-policy-id-range/end
                    leaf end {
                        type types-qos:qos-policy-id {
                            range "2..65535";
                        }
                        sros-ext:immutable;
                        mandatory true;
                        description "Upper bound of the ID range";
                    }

                    ///nokia-conf/configure/qos/md-auto-id/qos-policy-id-range/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/qos/md-auto-id/qos-policy-id-range/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container qos-policy-id-range

            } // container md-auto-id

            ///nokia-conf/configure/qos/sap-ingress
            list sap-ingress {
                key "sap-ingress-policy-name";
                description "Enter the sap-ingress list instance";

                ///nokia-conf/configure/qos/sap-ingress/sap-ingress-policy-name
                leaf sap-ingress-policy-name {
                    type types-qos:qos-policy-name;
                    description "Policy name";
                }

                ///nokia-conf/configure/qos/sap-ingress/policy-id
                leaf policy-id {
                    type types-qos:qos-policy-id;
                    sros-ext:immutable;
                    description "Ingress SAP QoS policy ID";
                }

                ///nokia-conf/configure/qos/sap-ingress/policer-allocation
                leaf policer-allocation {
                    type types-qos:sap-ingress-policer-alloc;
                    sros-ext:immutable;
                    description "FC policer mapping for SAP ingress policy";
                }

                ///nokia-conf/configure/qos/sap-ingress/scope
                leaf scope {
                    type types-qos:item-scope;
                    default "template";
                    description "Scope of the policy";
                }

                ///nokia-conf/configure/qos/sap-ingress/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/qos/sap-ingress/default-fc
                leaf default-fc {
                    type types-qos:sap-ingress-fc-name;
                    description "Default forwarding class for non-matching traffic";
                }

                ///nokia-conf/configure/qos/sap-ingress/default-priority
                leaf default-priority {
                    type types-qos:default-priority;
                    default "low";
                    description "Priority for packets received on an ingress SAP";
                }

                ///nokia-conf/configure/qos/sap-ingress/policers-hqos-manageable
                leaf policers-hqos-manageable {
                    type boolean;
                    default "false";
                    description "Manage policers through the hierarchical QoS process";
                }

                ///nokia-conf/configure/qos/sap-ingress/ingress-classification-policy
                leaf ingress-classification-policy {
                    type leafref {
                        path "../../ingress-classification-policy/ingress-classification-policy-name";
                    }
                    description "Attach ingress classification policy";
                }

                ///nokia-conf/configure/qos/sap-ingress/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/qos/sap-ingress/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt
                container subscriber-mgmt {
                    description "Enter the subscriber-mgmt context";

                    ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/pcc-rule-entry
                    container pcc-rule-entry {
                        description "Enter the pcc-rule-entry context";

                        ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/pcc-rule-entry/range
                        container range {
                            presence "Enable the range context";
                            description "Enable the range context";

                            ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/pcc-rule-entry/range/start
                            leaf start {
                                type types-qos:entry-id;
                                mandatory true;
                                description "Lower bound of the filter and QoS policy entries range";
                            }

                            ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/pcc-rule-entry/range/end
                            leaf end {
                                type types-qos:entry-id;
                                mandatory true;
                                description "Upper bound of the filter and QoS policy entries range";
                            }

                        } // container range

                    } // container pcc-rule-entry

                    ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/dynamic-policer
                    container dynamic-policer {
                        description "Enter the dynamic-policer context";

                        ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/dynamic-policer/cbs
                        leaf cbs {
                            type types-qos:plcr-burst-size;
                            description "Exceed threshold of the CIR leaky bucket of the policer";
                        }

                        ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/dynamic-policer/mbs
                        leaf mbs {
                            type types-qos:plcr-burst-size;
                            description "High priority violate threshold of PIR leaky bucket of this policer";
                        }

                        ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/dynamic-policer/packet-byte-offset
                        leaf packet-byte-offset {
                            type types-qos:per-packet-offset;
                            default "0";
                            description "Packet size modification for policing information";
                        }

                        ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/dynamic-policer/stat-mode
                        leaf stat-mode {
                            type types-qos:ingress-policer-stat-mode;
                            default "minimal";
                            description "Mode of statistics collected by the policer";
                        }

                        ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/dynamic-policer/policer-id-range
                        container policer-id-range {
                            presence "Enable the policer-id-range context";
                            description "Enable the policer-id-range context";

                            ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/dynamic-policer/policer-id-range/start
                            leaf start {
                                type types-qos:ingress-policer-id;
                                mandatory true;
                                description "Lower bound of the dynamic policer ID range";
                            }

                            ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/dynamic-policer/policer-id-range/end
                            leaf end {
                                type types-qos:ingress-policer-id;
                                mandatory true;
                                description "Upper bound of the dynamic policer ID range";
                            }

                        } // container policer-id-range

                        ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/dynamic-policer/arbiter-parent
                        container arbiter-parent {
                            description "Enter the arbiter-parent context";

                            ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/dynamic-policer/arbiter-parent/arbiter-name
                            leaf arbiter-name {
                                type types-sros:named-item;
                                description "Arbiter name";
                            }

                            ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/dynamic-policer/arbiter-parent/level
                            leaf level {
                                type types-qos:hierarchy-level;
                                default "1";
                                description "Level of priority while feeding to the parent";
                            }

                            ///nokia-conf/configure/qos/sap-ingress/subscriber-mgmt/dynamic-policer/arbiter-parent/weight
                            leaf weight {
                                type types-qos:weight {
                                    range "1..100";
                                }
                                default "1";
                                description "Weight used by the arbiter for feeding the policer";
                            }

                        } // container arbiter-parent

                    } // container dynamic-policer

                } // container subscriber-mgmt

                ///nokia-conf/configure/qos/sap-ingress/mac-criteria
                container mac-criteria {
                    description "Enter the mac-criteria context";

                    ///nokia-conf/configure/qos/sap-ingress/mac-criteria/type
                    leaf type {
                        type types-qos:mac-criteria-type;
                        sros-ext:immutable;
                        default "normal";
                        description "MAC criteria filter type";
                    }

                    ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry
                    list entry {
                        key "entry-id";
                        description "Enter the entry list instance";

                        ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/entry-id
                        leaf entry-id {
                            type types-qos:entry-id;
                            description "Entry ID";
                        }

                        ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match
                        container match {
                            description "Enter the match context";

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/frame-type
                            leaf frame-type {
                                type types-qos:mac-frame-type;
                                description "Match MAC criteria for ingress SAP QoS policy";
                            }

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/etype
                            leaf etype {
                                type types-services:etype-value;
                                description "Ethernet type";
                            }

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/snap-pid
                            leaf snap-pid {
                                type int32 {
                                    range "0..65535";
                                }
                                description "Parameter snap-pid as MAC filter match criteria";
                            }

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/snap-oui
                            leaf snap-oui {
                                type enumeration {
                                    enum zero                           { value 2; }
                                    enum non-zero                       { value 3; }
                                }
                                description "Parameter snap-oui as MAC filter match criteria";
                            }

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/src-mac
                            container src-mac {
                                presence "Enable the src-mac context";
                                description "Enable the src-mac context";

                                ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/src-mac/address
                                leaf address {
                                    type yang:mac-address;
                                    mandatory true;
                                    description "Source MAC address as QoS policy match criterion";
                                }

                                ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/src-mac/mask
                                leaf mask {
                                    type yang:mac-address;
                                    default "ff:ff:ff:ff:ff:ff";
                                    description "Mask for source MAC address";
                                }

                            } // container src-mac

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/dst-mac
                            container dst-mac {
                                presence "Enable the dst-mac context";
                                description "Enable the dst-mac context";

                                ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/dst-mac/address
                                leaf address {
                                    type yang:mac-address;
                                    mandatory true;
                                    description "Destination MAC address as QoS policy match criterion";
                                }

                                ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/dst-mac/mask
                                leaf mask {
                                    type yang:mac-address;
                                    default "ff:ff:ff:ff:ff:ff";
                                    description "Mask for destination MAC address";
                                }

                            } // container dst-mac

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/dot1p
                            container dot1p {
                                presence "Enable the dot1p context";
                                description "Enable the dot1p context";

                                ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/dot1p/priority
                                leaf priority {
                                    type types-qos:dot1p-priority;
                                    mandatory true;
                                    description "802.1P priority value to use as the match criterion";
                                }

                                ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/dot1p/mask
                                leaf mask {
                                    type types-qos:dot1p-priority;
                                    default "7";
                                    description "3-bit mask for the 802.1p value";
                                }

                            } // container dot1p

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/llc-dsap
                            container llc-dsap {
                                presence "Enable the llc-dsap context";
                                description "Enable the llc-dsap context";

                                ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/llc-dsap/dsap
                                leaf dsap {
                                    type types-qos:service-access-point;
                                    mandatory true;
                                    description "DSAP value";
                                }

                                ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/llc-dsap/mask
                                leaf mask {
                                    type types-qos:service-access-point;
                                    default "255";
                                    description "DSAP mask";
                                }

                            } // container llc-dsap

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/llc-ssap
                            container llc-ssap {
                                presence "Enable the llc-ssap context";
                                description "Enable the llc-ssap context";

                                ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/llc-ssap/ssap
                                leaf ssap {
                                    type types-qos:service-access-point;
                                    mandatory true;
                                    description "SSAP value";
                                }

                                ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/llc-ssap/mask
                                leaf mask {
                                    type types-qos:service-access-point;
                                    default "255";
                                    description "Source SAP mask";
                                }

                            } // container llc-ssap

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/inner-tag
                            container inner-tag {
                                presence "Enable the inner-tag context";
                                description "Enable the inner-tag context";

                                ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/inner-tag/vlan
                                leaf vlan {
                                    type int32 {
                                        range "0..4095";
                                    }
                                    mandatory true;
                                    description "Value to match against the VID of the second VLAN tag";
                                }

                                ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/inner-tag/mask
                                leaf mask {
                                    type uint32 {
                                        range "1..4095";
                                    }
                                    default "4095";
                                    description "Mask to VID of the inner VLAN tag before comparing it with the inner-tag or outer-tag value";
                                }

                            } // container inner-tag

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/outer-tag
                            container outer-tag {
                                presence "Enable the outer-tag context";
                                description "Enable the outer-tag context";

                                ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/outer-tag/vlan
                                leaf vlan {
                                    type int32 {
                                        range "0..4095";
                                    }
                                    mandatory true;
                                    description "Match against VID of the first VLAN tag in the packet carried transparently through service";
                                }

                                ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/match/outer-tag/mask
                                leaf mask {
                                    type uint32 {
                                        range "1..4095";
                                    }
                                    default "4095";
                                    description "First tag carried transparently through the service";
                                }

                            } // container outer-tag

                        } // container match

                        ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/action
                        container action {
                            description "Enter the action context";

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/action/type
                            leaf type {
                                type types-qos:criteria-action-type;
                                default "ignore-match";
                                description "Action for criteria entry";
                            }

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/action/fc
                            leaf fc {
                                type types-qos:sap-ingress-fc-name;
                                description "Forwarding class for traffic matching the criteria";
                            }

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/action/priority
                            leaf priority {
                                type types-qos:priority;
                                description "Priority for the packet";
                            }

                            ///nokia-conf/configure/qos/sap-ingress/mac-criteria/entry/action/policer
                            leaf policer {
                                type leafref {
                                    path "../../../../policer/policer-id";
                                }
                                description "Policer ID for traffic matching the criteria";
                            }

                        } // container action

                    } // list entry

                } // container mac-criteria

                ///nokia-conf/configure/qos/sap-ingress/ip-criteria
                container ip-criteria {
                    description "Enter the ip-criteria context";

                    ///nokia-conf/configure/qos/sap-ingress/ip-criteria/type
                    leaf type {
                        type types-qos:ip-filter-type;
                        sros-ext:immutable;
                        default "normal";
                        description "IPv4 criteria entry type";
                    }

                    ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry
                    list entry {
                        key "entry-id";
                        description "Enter the entry list instance";

                        ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/entry-id
                        leaf entry-id {
                            type types-qos:entry-id;
                            description "IP criteria entry to create or edit for the policy";
                        }

                        ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/tag
                        leaf tag {
                            type uint32 {
                                range "0..255";
                            }
                            default "0";
                            description "Tag ID associated with the criteria entry";
                        }

                        ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/match
                        container match {
                            description "Enter the match context";

                            ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/match/protocol
                            leaf protocol {
                                type types-sros:match-ip-protocol;
                                description "IP protocol to match";
                            }

                            ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/match/dscp
                            leaf dscp {
                                type types-qos:dscp-name;
                                description "DSCP value to match in the packet";
                            }

                            ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/match/fragment
                            leaf fragment {
                                type enumeration {
                                    enum false                          { value 2; }
                                    enum true                           { value 3; }
                                }
                                description "Fragmented packets as the match criterion";
                            }

                            ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/match/dst-ip
                            container dst-ip {
                                description "Enter the dst-ip context";

                                ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/match/dst-ip/dst-ip
                                choice dst-ip {
                                    ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/match/dst-ip/dst-ip/address-and-mask-or-prefix
                                    case address-and-mask-or-prefix {

                                        ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/match/dst-ip/dst-ip/address-and-mask-or-prefix/address
                                        leaf address {
                                            type union {
                                                type types-sros:ipv4-prefix-with-host-bits;
                                                type types-sros:ipv4-address;
                                            }
                                            description "Destination IPv4 address for SAP QoS policy match";
                                        }

                                        ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/match/dst-ip/dst-ip/address-and-mask-or-prefix/mask
                                        leaf mask {
                                            type types-sros:ipv4-address;
                                            description "IP address to match with source IP of the packet";
                                        }

                                    }
                                    ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/match/dst-ip/dst-ip/ip-prefix-list
                                    case ip-prefix-list {

                                        ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/match/dst-ip/dst-ip/ip-prefix-list/ip-prefix-list
                                        leaf ip-prefix-list {
                                            type leafref {
                                                path "../../../../../../match-list/ip-prefix-list/prefix-list-name";
                                            }
                                            description "IP-prefix list as match criterion";
                                        }

                                    }
                                }
                            } // container dst-ip

                            ///nokia-conf/configure/qos/sap-ingress/ip-criteria/entry/match/src-ip
                            container src-ip {
                                description "Enter the src-ip context";

