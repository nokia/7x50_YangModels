                        }
                        default "0.00";
                        description "Average packet-to-frame encapsulation overhead";
                    }

                    ///nokia-conf/configure/qos/sap-egress/queue/burst-limit
                    leaf burst-limit {
                        type union {
                            type types-qos:bytes {
