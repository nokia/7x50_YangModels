                }

                ///nokia-conf/configure/qos/network-ingress/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/qos/network-ingress/ingress-classification-policy
                leaf ingress-classification-policy {
                    type leafref {
