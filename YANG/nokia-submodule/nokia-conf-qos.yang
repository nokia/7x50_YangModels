                                        type types-sros:named-item;
                                        mandatory true;
                                        description "Scheduler to which the queue feeds";
                                    }

                                    ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/dynamic-queue/parent-mapping/scheduler-parent/scheduler-parent/level
                                    leaf level {
                                        type types-qos:hierarchy-level;
                                        default "1";
                                        description "Priority level while feeding to the parent";
