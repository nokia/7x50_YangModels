                leaf use-policer-result-marking-dot1p-inner {
                    type boolean;
                    default "false";
                    description "Enable using egress policer for dot1p inner marking";
                }

                ///nokia-conf/configure/qos/sap-egress/hs-attachment-policy
                leaf hs-attachment-policy {
                    type leafref {
                        path "../../hs-attachment-policy/name";
                    }
                    description "HS attachment policy applied";
                }

                ///nokia-conf/configure/qos/sap-egress/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/qos/sap-egress/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt
                container subscriber-mgmt {
                    description "Enter the subscriber-mgmt context";

                    ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/pcc-rule-entry
                    container pcc-rule-entry {
                        description "Enter the pcc-rule-entry context";

                        ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/pcc-rule-entry/range
                        container range {
                            presence "Enable the range context";
                            description "Enable the range context";

                            ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/pcc-rule-entry/range/start
                            leaf start {
                                type types-qos:entry-id;
                                mandatory true;
                                description "Lower bound of the filter and QoS policy entries range";
                            }

                            ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/pcc-rule-entry/range/end
                            leaf end {
                                type types-qos:entry-id;
                                mandatory true;
                                description "Upper bound of the filter and QoS policy entries range";
                            }

                        } // container range

                    } // container pcc-rule-entry

                    ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/dynamic-policer
                    container dynamic-policer {
                        description "Enter the dynamic-policer context";

                        ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/dynamic-policer/cbs
                        leaf cbs {
                            type types-qos:plcr-burst-size;
                            description "Exceed threshold of the CIR leaky bucket of the policer";
                        }

                        ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/dynamic-policer/mbs
                        leaf mbs {
                            type types-qos:plcr-burst-size;
                            description "High priority 'violate' threshold of PIR leaky bucket of this policer";
                        }

                        ///nokia-conf/configure/qos/sap-egress/subscriber-mgmt/dynamic-policer/packet-byte-offset
                        leaf packet-byte-offset {
                            type types-qos:egress-per-packet-offset;
                            default "0";
