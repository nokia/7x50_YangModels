                        leaf exceed-profile {
                            type types-qos:dot1p-priority;
                            description "Dot1p value for exceed-profile frames";
                        }

                    } // container dot1p

                    ///nokia-conf/configure/qos/sap-egress/fc/dot1p-inner
                    container dot1p-inner {
