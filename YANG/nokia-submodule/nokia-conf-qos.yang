                                    units "kilobps";
                                    default "max";
                                    description "Administrative PIR";
                                }

                                ///nokia-conf/configure/qos/sap-egress/queue/rate-or-percent-rate/rate/rate/cir
                                leaf cir {
                                    type types-qos:queue-cir-rate-maximum;
                                    units "kilobps";
                                    default "0";
                                    description "Administrative CIR";
