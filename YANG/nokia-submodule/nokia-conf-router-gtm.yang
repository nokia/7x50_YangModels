submodule nokia-conf-router-gtm {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-groups;
    include nokia-conf-router;
    include nokia-conf-router-mpls;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure GTM protocol on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-router-gtm YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2020-09-16";

    grouping conf-router-gtm {
        ///nokia-conf/configure/router/gtm
        container gtm {
            when "../router-name = 'Base'" {
                description "only allowed in the Base router";
            }
            description "Enter the gtm context";

            ///nokia-conf/configure/router/gtm/mvpn
            leaf mvpn {
                type boolean;
                default "false";
                description "Enable the configuration of MVPN-related parameters";
            }

            ///nokia-conf/configure/router/gtm/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/router/gtm/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/router/gtm/provider-tunnel
            container provider-tunnel {
                description "Enter the provider-tunnel context";

                ///nokia-conf/configure/router/gtm/provider-tunnel/inclusive
                container inclusive {
                    description "Enter the inclusive context";

                    ///nokia-conf/configure/router/gtm/provider-tunnel/inclusive/rsvp
                    container rsvp {
                        presence "Enable the rsvp context";
                        description "Enable the rsvp context";

                        ///nokia-conf/configure/router/gtm/provider-tunnel/inclusive/rsvp/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of P2MP RSVP as provider tunnel";
                        }

                        ///nokia-conf/configure/router/gtm/provider-tunnel/inclusive/rsvp/lsp-template
                        leaf lsp-template {
                            type leafref {
                                path "../../../../../../router/mpls/lsp-template/template-name";
                            }
                            description "LSP template name";
                        }

                    } // container rsvp

                } // container inclusive

                ///nokia-conf/configure/router/gtm/provider-tunnel/selective
                container selective {
                    description "Enter the selective context";

                    ///nokia-conf/configure/router/gtm/provider-tunnel/selective/data-delay-interval
                    leaf data-delay-interval {
                        type uint32 {
                            range "3..180";
                        }
                        default "3";
                        description "Delay before a PBR switches traffic to selective tunnel";
                    }

                    ///nokia-conf/configure/router/gtm/provider-tunnel/selective/maximum-p2mp-spmsi
                    leaf maximum-p2mp-spmsi {
                        type uint32 {
                            range "1..4000";
                        }
                        default "10";
                        description "Maximum number of RSVP P2MP or LDP P2MP S-PMSI tunnels";
                    }

                    ///nokia-conf/configure/router/gtm/provider-tunnel/selective/data-threshold
                    container data-threshold {
                        description "Enter the data-threshold context";

                        ///nokia-conf/configure/router/gtm/provider-tunnel/selective/data-threshold/group-prefix
                        list group-prefix {
                            key "ip-group-prefix";
                            description "Enter the group-prefix list instance";

                            ///nokia-conf/configure/router/gtm/provider-tunnel/selective/data-threshold/group-prefix/ip-group-prefix
                            leaf ip-group-prefix {
                                type types-sros:ip-multicast-prefix;
                                description "Multicast group-prefix";
                            }

                            ///nokia-conf/configure/router/gtm/provider-tunnel/selective/data-threshold/group-prefix/threshold
                            leaf threshold {
                                type uint32 {
                                    range "1..4294967294";
                                }
                                units "kilobps";
                                mandatory true;
                                description "Threshold for a group prefix";
                            }

                            ///nokia-conf/configure/router/gtm/provider-tunnel/selective/data-threshold/group-prefix/pe-threshold-add
                            leaf pe-threshold-add {
                                type uint32 {
                                    range "1..65535";
                                }
                                default "65535";
                                description "Number of receiver PBRs to create S-PMSI";
                            }

                            ///nokia-conf/configure/router/gtm/provider-tunnel/selective/data-threshold/group-prefix/pe-threshold-delete
                            leaf pe-threshold-delete {
                                type uint32 {
                                    range "2..65535";
                                }
                                default "65535";
                                description "Number of receiver PBRs to delete S-PMSI";
                            }

                            ///nokia-conf/configure/router/gtm/provider-tunnel/selective/data-threshold/group-prefix/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/gtm/provider-tunnel/selective/data-threshold/group-prefix/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list group-prefix

                    } // container data-threshold

                    ///nokia-conf/configure/router/gtm/provider-tunnel/selective/rsvp
                    container rsvp {
                        presence "Enable the rsvp context";
                        description "Enable the rsvp context";

                        ///nokia-conf/configure/router/gtm/provider-tunnel/selective/rsvp/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of P2MP RSVP as provider tunnel";
                        }

                        ///nokia-conf/configure/router/gtm/provider-tunnel/selective/rsvp/lsp-template
                        leaf lsp-template {
                            type leafref {
                                path "../../../../../../router/mpls/lsp-template/template-name";
                            }
                            description "LSP template name";
                        }

                    } // container rsvp

                } // container selective

            } // container provider-tunnel

        } // container gtm

    } // grouping conf-router-gtm

}
