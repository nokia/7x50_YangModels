submodule nokia-conf-submgt-vrgw {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-aaa;
    include nokia-conf-groups;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure subscriber management Virtual Residential Gateway on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-submgt-vrgw YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2021-11-05";

    grouping conf-submgt-vrgw {
        ///nokia-conf/configure/subscriber-mgmt/vrgw
        container vrgw {
            description "Enter the vrgw context";

            ///nokia-conf/configure/subscriber-mgmt/vrgw/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/subscriber-mgmt/vrgw/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/subscriber-mgmt/vrgw/lanext
            container lanext {
                description "Enable the lanext context";

                ///nokia-conf/configure/subscriber-mgmt/vrgw/lanext/router-target-as-number
                leaf router-target-as-number {
                    type uint16 {
                        range "1..65535";
                    }
                    description "AS number to derive RT and RD for the HLE service";
                }

                ///nokia-conf/configure/subscriber-mgmt/vrgw/lanext/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/vrgw/lanext/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container lanext

            ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile
            list brg-profile {
                key "brg-profile-name";
                max-elements 16;
                description "Enter the brg-profile list instance";

                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/brg-profile-name
                leaf brg-profile-name {
                    type types-sros:named-item;
                    description "BRG profile name";
                }

                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/sla-profile-string
                leaf sla-profile-string {
                    type types-sros:named-item-or-empty;
                    description "Default for SLA-profile lookup";
                }

                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/sub-profile-string
                leaf sub-profile-string {
                    type types-sros:named-item-or-empty;
                    description "Default for subscriber-profile lookup";
                }

                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/hold-time
                leaf hold-time {
                    type types-sros:time-duration {
                        range "0|30..2592000";
                    }
                    units "seconds";
                    default "0";
                    description "The time to hold on to a BRG after it went down";
                }

                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/initial-hold-time
                leaf initial-hold-time {
                    type types-sros:time-duration {
                        range "0..900";
                    }
                    units "seconds";
                    default "300";
                    description "Hold time for BRG instances created without any hosts";
                }

                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/uplink-initial-wait
                leaf uplink-initial-wait {
                    type types-sros:time-duration {
                        range "10..300";
                    }
                    units "seconds";
                    default "30";
                    description "Wait time for non-routed uplink to become operational";
                }

                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/connectivity-verification
                container connectivity-verification {
                    description "Enter the connectivity-verification context";

                    ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/connectivity-verification/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "enable";
                        description "Administrative state of BRG connectivity verification";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/connectivity-verification/count
                    leaf count {
                        when "../admin-state = 'enable'" {
                            description "admin-state must be enabled";
                        }
                        type uint32 {
                            range "1..5";
                        }
                        description "Connection attempts before BRG is considered down";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/connectivity-verification/timeout
                    leaf timeout {
                        when "../admin-state = 'enable'" {
                            description "admin-state must be enabled";
                        }
                        type uint32 {
                            range "5..60";
                        }
                        description "Timeout after which a message is considered unanswered";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/connectivity-verification/retry-time
                    leaf retry-time {
                        when "../admin-state = 'enable'" {
                            description "admin-state must be enabled";
                        }
                        type uint32 {
                            range "300..3600";
                        }
                        description "Wait time before a new connectivity verification cycle";
                    }

                } // container connectivity-verification

                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/radius-proxy-server
                list radius-proxy-server {
                    key "router-instance name";
                    max-elements 4;
                    description "Add a list entry for radius-proxy-server";

                    ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/radius-proxy-server/router-instance
                    leaf router-instance {
                        type string;
                        description "router-instance";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/radius-proxy-server/name
                    leaf name {
                        type types-sros:named-item;
                        description "RADIUS proxy server name";
                    }

                } // list radius-proxy-server

                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool
                container home-pool {
                    description "Enter the home-pool context";

                    ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/lease-time
                    leaf lease-time {
                        type types-sros:time-duration {
                            range "300..315446399";
                        }
                        units "seconds";
                        default "21600";
                        description "Lease time used when allocating addresses from the pool";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/standby-ip-lifetime
                    leaf standby-ip-lifetime {
                        type types-sros:time-duration {
                            range "300..315446399";
                        }
                        units "seconds";
                        default "21600";
                        description "The lifetime of the standby IP addresses";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/subnet
                    container subnet {
                        description "Enter the subnet context";

                        ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/subnet/prefix
                        leaf prefix {
                            type types-sros:ipv4-prefix-with-host-bits;
                            default "192.168.0.1/24";
                            description "Prefix and default-gateway to be used inside the home";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/subnet/start
                        leaf start {
                            type types-sros:ipv4-unicast-address;
                            default "192.168.0.2";
                            description "Lower bound of the IP address range";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/subnet/end
                        leaf end {
                            type types-sros:ipv4-unicast-address;
                            default "192.168.0.254";
                            description "Upper bound of the IP address range";
                        }

                    } // container subnet

                    ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option
                    list option {
                        key "number";
                        description "Enter the option list instance";

                        ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/number
                        leaf number {
                            type union {
                                type uint32 {
                                    range "1..254";
                                }
                                type enumeration {
                                    enum subnet-mask                    { value 1; }
                                    enum default-router                 { value 3; }
                                    enum dns-server                     { value 6; }
                                    enum domain-name                    { value 15; }
                                    enum netbios-name-server            { value 44; }
                                    enum netbios-node-type              { value 46; }
                                    enum lease-time                     { value 51; }
                                    enum lease-renew-time               { value 58; }
                                    enum lease-rebind-time              { value 59; }
                                }
                            }
                            description "DHCP option to send identification strings to client";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/value
                        choice value {
                            mandatory true;
                            description "The value of the DHCP option.";
                            ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/value/ascii-string
                            case ascii-string {

                                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/value/ascii-string/ascii-string
                                leaf ascii-string {
                                    when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                        description "ascii-string not supported for this option";
                                    }
                                    type types-sros:string-not-all-spaces {
                                        length "1..127";
                                    }
                                    description "ASCII string of the DHCP option";
                                }

                            }
                            ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/value/hex-string
                            case hex-string {

                                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/value/hex-string/hex-string
                                leaf hex-string {
                                    when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                        description "hex-string not supported for this option";
                                    }
                                    type types-sros:hex-string {
                                        length "1..256";
                                    }
                                    description "DHCP option as a hexadecimal string";
                                }

                            }
                            ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/value/netbios-node-type
                            case netbios-node-type {

                                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/value/netbios-node-type/netbios-node-type
                                leaf netbios-node-type {
                                    when "../number = 46 or ../number = 'netbios-node-type'" {
                                        description "netbios-node-type not supported for this option";
                                    }
                                    type enumeration {
                                        enum b-node                         { value 1; }
                                        enum p-node                         { value 2; }
                                        enum m-node                         { value 4; }
                                        enum h-node                         { value 8; }
                                    }
                                    description "DHCP option as NetBIOS node type.";
                                }

                            }
                            ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/value/duration
                            case duration {

                                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/value/duration/duration
                                leaf duration {
                                    when "../number = 51 or ../number = 'lease-time' or ../number = 58 or ../number = 'lease-renew-time' or ../number = 59 or ../number = 'lease-rebind-time'" {
                                        description "duration not supported for this option";
                                    }
                                    type types-sros:time-duration {
                                        range "10..315446399";
                                    }
                                    units "seconds";
                                    description "DHCP option as time duration.";
                                }

                            }
                            ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/value/ipv4-address
                            case ipv4-address {

                                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/value/ipv4-address/ipv4-address
                                leaf-list ipv4-address {
                                    when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                        description "ipv4-address not supported for this option";
                                    }
                                    type types-sros:ipv4-address;
                                    max-elements 4;
                                    ordered-by user;
                                    description "DHCP option as a list of IPv4 addresses";
                                }

                            }
                            ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/value/empty
                            case empty {

                                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/home-pool/option/value/empty/empty
                                leaf empty {
                                    when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                        description "empty not supported for this option";
                                    }
                                    type empty;
                                    description "Empty DHCP option.";
                                }

                            }
                        }
                    } // list option

                } // container home-pool

                ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/radius-authentication
                container radius-authentication {
                    description "Enter the radius-authentication context";

                    ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/radius-authentication/server-policy
                    leaf server-policy {
                        type leafref {
                            path "../../../../../aaa/radius/server-policy/name";
                        }
                        description "Radius servers to be used for authentication";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/vrgw/brg-profile/radius-authentication/password
                    leaf password {
                        type types-sros:encrypted-leaf {
                            length "1..115";
                        }
                        description "Password used for authentication";
                    }

                } // container radius-authentication

            } // list brg-profile


        } // container vrgw

    } // grouping conf-submgt-vrgw

}
