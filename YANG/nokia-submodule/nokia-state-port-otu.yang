submodule nokia-state-port-otu {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-port               { prefix "types-port"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for optical transport unit ports on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-port-otu YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2022-09-06";

    grouping state-port-otu {
        ///nokia-state/state/port/otu
        container otu {
            description "Enable the otu context";

            ///nokia-state/state/port/otu/current-alarms
            leaf current-alarms {
                type types-port:otu-report-alarms;
                description "Indicates the current alarms on the OTU port.";
            }

            ///nokia-state/state/port/otu/reported-alarms
            leaf reported-alarms {
                type types-port:otu-report-alarms;
                description
                    "Indicates the monitored alarms currently being raised on the OTU port.
                     This leaf reports any 'current-alarms' that respect what is configured
                     by 'report-alarm'.";
            }

            ///nokia-state/state/port/otu/operational-data-rate
            leaf operational-data-rate {
                type uint32;
                units "megabps";
                description "Indicates the rate at which the OTU data is transmitted on the line.";
            }

            ///nokia-state/state/port/otu/path-monitoring
            container path-monitoring {
                description "Enter the path-monitoring context";

                ///nokia-state/state/port/otu/path-monitoring/trail-trace-identifier
                container trail-trace-identifier {
                    description "Enter the trail-trace-identifier context";

                    ///nokia-state/state/port/otu/path-monitoring/trail-trace-identifier/receive
                    leaf receive {
                        type string;
                        description "Indicates the TTI received OTU overhead.";
                    }

                } // container trail-trace-identifier

            } // container path-monitoring

            ///nokia-state/state/port/otu/payload-structure-identifier
            container payload-structure-identifier {
                description "Enter the payload-structure-identifier context";

                ///nokia-state/state/port/otu/payload-structure-identifier/payload
                container payload {
                    description "Enter the payload context";

                    ///nokia-state/state/port/otu/payload-structure-identifier/payload/receive
                    leaf receive {
                        type types-port:otu-psi-payload-type;
                        description "Indicates the payload type in the received OTU PSI overhead.";
                    }

                } // container payload

            } // container payload-structure-identifier

            ///nokia-state/state/port/otu/section-monitoring
            container section-monitoring {
                description "Enter the section-monitoring context";

                ///nokia-state/state/port/otu/section-monitoring/trail-trace-identifier
                container trail-trace-identifier {
                    description "Enter the trail-trace-identifier context";

                    ///nokia-state/state/port/otu/section-monitoring/trail-trace-identifier/receive
                    leaf receive {
                        type string;
                        description "Indicates the TTI received OTU overhead.";
                    }

                } // container trail-trace-identifier

            } // container section-monitoring

            ///nokia-state/state/port/otu/statistics
            container statistics {
                description "Enter the statistics context";

                ///nokia-state/state/port/otu/statistics/raw
                container raw {
                    description "Enter the raw context";

                    ///nokia-state/state/port/otu/statistics/raw/elapsed-seconds
                    leaf elapsed-seconds {
                        type yang:counter32;
                        description
                            "Indicates the number of elapsed seconds since the last OTU raw
                             statistics clearing.";
                    }

                    ///nokia-state/state/port/otu/statistics/raw/negative-pointer-justifications
                    leaf negative-pointer-justifications {
                        type yang:counter32;
                        description
                            "Indicates the number of Negative Pointer Justifications (NPJ) that
                             occurred during the asynchronous demapping of the received payload
                             inside the OTU frame.";
                    }

                    ///nokia-state/state/port/otu/statistics/raw/positive-pointer-justifications
                    leaf positive-pointer-justifications {
                        type yang:counter32;
                        description
                            "Indicates the number of Positive Pointer Justifications (PPJ) that
                             occurred during the asynchronous demapping of the received payload
                             inside the OTU frame.";
                    }

                    ///nokia-state/state/port/otu/statistics/raw/forward-error-correction
                    container forward-error-correction {
                        description "Enter the forward-error-correction context";

                        ///nokia-state/state/port/otu/statistics/raw/forward-error-correction/corrected-bits
                        leaf corrected-bits {
                            type yang:counter64;
                            description "Indicates the number of corrected bits.";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/forward-error-correction/corrected-ones
                        leaf corrected-ones {
                            type yang:counter64;
                            description "Indicates the number of corrected ones.";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/forward-error-correction/corrected-zeros
                        leaf corrected-zeros {
                            type yang:counter64;
                            description "Indicates the number of corrected zeros.";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/forward-error-correction/uncorrectable-sub-rows
                        leaf uncorrectable-sub-rows {
                            type yang:counter64;
                            description "Indicates the number of uncorrectable sub-rows.";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/forward-error-correction/corrected-ber
                        leaf corrected-ber {
                            type string {
                                length "1..32";
                            }
                            description "Indicates the corrected Bit Error Rate (BER).";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/forward-error-correction/uncorrected-ber
                        leaf uncorrected-ber {
                            type string {
                                length "1..32";
                            }
                            description "Indicates the uncorrected Bit Error Rate (BER).";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/forward-error-correction/errored-seconds
                        leaf errored-seconds {
                            type yang:counter32;
                            description "Indicates the number of Errored Seconds (SES).";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/forward-error-correction/severely-errored-seconds
                        leaf severely-errored-seconds {
                            type yang:counter32;
                            description "Indicates the number of Severely Errored Seconds (SES).";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/forward-error-correction/unavailable-seconds
                        leaf unavailable-seconds {
                            type yang:counter32;
                            description "Indicates the number of Unavailable Seconds (UAS).";
                        }

                    } // container forward-error-correction

                    ///nokia-state/state/port/otu/statistics/raw/path-monitoring
                    container path-monitoring {
                        description "Enter the path-monitoring context";

                        ///nokia-state/state/port/otu/statistics/raw/path-monitoring/bei-errors
                        leaf bei-errors {
                            type yang:counter64;
                            description "Indicates the number of Backward Error Indication (BEI) errors.";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/path-monitoring/bip8-ber
                        leaf bip8-ber {
                            type string {
                                length "1..32";
                            }
                            description "Indicates the BIP8 Bit Error Rate (BER).";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/path-monitoring/bip8-errors
                        leaf bip8-errors {
                            type yang:counter64;
                            description "Indicates the number of BIP8 errors.";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/path-monitoring/errored-seconds
                        leaf errored-seconds {
                            type yang:counter32;
                            description "Indicates the number of Errored Seconds (SES).";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/path-monitoring/severely-errored-seconds
                        leaf severely-errored-seconds {
                            type yang:counter32;
                            description "Indicates the number of Severely Errored Seconds (SES).";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/path-monitoring/unavailable-seconds
                        leaf unavailable-seconds {
                            type yang:counter32;
                            description "Indicates the number of Unavailable Seconds (UAS).";
                        }

                    } // container path-monitoring

                    ///nokia-state/state/port/otu/statistics/raw/section-monitoring
                    container section-monitoring {
                        description "Enter the section-monitoring context";

                        ///nokia-state/state/port/otu/statistics/raw/section-monitoring/bei-errors
                        leaf bei-errors {
                            type yang:counter64;
                            description "Indicates the number of Backward Error Indication (BEI) errors.";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/section-monitoring/bip8-ber
                        leaf bip8-ber {
                            type string {
                                length "1..32";
                            }
                            description "Indicates the BIP8 Bit Error Rate (BER).";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/section-monitoring/bip8-errors
                        leaf bip8-errors {
                            type yang:counter64;
                            description "Indicates the number of BIP8 errors.";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/section-monitoring/errored-seconds
                        leaf errored-seconds {
                            type yang:counter32;
                            description "Indicates the number of Errored Seconds (SES).";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/section-monitoring/severely-errored-seconds
                        leaf severely-errored-seconds {
                            type yang:counter32;
                            description "Indicates the number of Severely Errored Seconds (SES).";
                        }

                        ///nokia-state/state/port/otu/statistics/raw/section-monitoring/unavailable-seconds
                        leaf unavailable-seconds {
                            type yang:counter32;
                            description "Indicates the number of Unavailable Seconds (UAS).";
                        }

                    } // container section-monitoring

                } // container raw

            } // container statistics

        } // container otu

    } // grouping state-port-otu

}
