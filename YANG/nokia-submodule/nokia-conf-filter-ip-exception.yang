                                leaf address {
                                    type union {
                                        type types-sros:ipv6-prefix-with-host-bits;
                                        type types-sros:ipv6-address;
                                    }
                                    description "IPv6 address used as the match criterion";
                                }

                                ///nokia-conf/configure/filter/ipv6-exception/entry/match/src-ip/match-address-choice/address-and-prefix-or-mask/mask
                                leaf mask {
