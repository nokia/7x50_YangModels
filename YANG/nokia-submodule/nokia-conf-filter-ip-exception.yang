                                    ///nokia-conf/configure/filter/ipv6-exception/entry/match/port-selector/port/port/port/port-list
                                    case port-list {

                                        ///nokia-conf/configure/filter/ipv6-exception/entry/match/port-selector/port/port/port/port-list/port-list
                                        leaf port-list {
                                            type leafref {
                                                path "../../../../../match-list/port-list/port-list-name";
                                            }
                                            description "Name of the port list as the match criterion";
                                        }

                                    }
                                }
                            } // container port

                        }
                    }
                    ///nokia-conf/configure/filter/ipv6-exception/entry/match/src-ip
                    container src-ip {
                        description "Enter the src-ip context";

                        ///nokia-conf/configure/filter/ipv6-exception/entry/match/src-ip/match-address-choice
                        choice match-address-choice {
                            ///nokia-conf/configure/filter/ipv6-exception/entry/match/src-ip/match-address-choice/address-and-prefix-or-mask
                            case address-and-prefix-or-mask {

