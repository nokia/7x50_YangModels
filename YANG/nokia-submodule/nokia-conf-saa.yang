submodule nokia-conf-saa {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-oam                { prefix "types-oam"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-groups;
    include nokia-conf-log;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure Service Assurance Agent (SAA) on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-saa YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-05-26";

    grouping conf-saa {
        ///nokia-conf/configure/saa
        container saa {
            description "Enter the saa context";

            ///nokia-conf/configure/saa/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/saa/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/saa/owner
            list owner {
                key "owner-name test";
                description "Enter the owner list instance";

                ///nokia-conf/configure/saa/owner/owner-name
                leaf owner-name {
                    type types-sros:named-item;
                    description "Optional owner name of the SAA operation";
                }

                ///nokia-conf/configure/saa/owner/test
                leaf test {
                    type types-sros:named-item;
                    description "SAA test name";
                }

                ///nokia-conf/configure/saa/owner/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of the SAA test";
                }

                ///nokia-conf/configure/saa/owner/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/saa/owner/accounting-policy
                leaf accounting-policy {
                    type leafref {
                        path "../../../log/accounting-policy/policy-id";
                    }
                    sros-ext:auto-restart-to-modify;
                    description "Accounting policy associated with the SAA test";
                }

                ///nokia-conf/configure/saa/owner/continuous
                leaf continuous {
                    type boolean;
                    sros-ext:auto-restart-to-modify;
                    default "false";
                    description "Execute the SAA tests as continuous";
                }

                ///nokia-conf/configure/saa/owner/probe-history
                leaf probe-history {
                    type enumeration {
                        enum keep                           { value 1; }
                        enum drop                           { value 2; }
                        enum auto                           { value 3; }
                    }
                    sros-ext:auto-restart-to-modify;
                    default "auto";
                    description "History probe behavior";
                }

                ///nokia-conf/configure/saa/owner/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/saa/owner/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/saa/owner/jitter-event
                list jitter-event {
                    key "direction threshold-type";
                    description "Enter the jitter-event list instance";

                    ///nokia-conf/configure/saa/owner/jitter-event/direction
                    leaf direction {
                        type types-oam:saa-threshold-direction;
                        description "Direction for OAM ping responses for OAM ping test run";
                    }

                    ///nokia-conf/configure/saa/owner/jitter-event/threshold-type
                    leaf threshold-type {
                        type enumeration {
                            enum rising                         { value 1; }
                            enum falling                        { value 2; }
                        }
                        description "Threshold type";
                    }

                    ///nokia-conf/configure/saa/owner/jitter-event/threshold
                    leaf threshold {
                        type uint32 {
                            range "0..2147483647";
                        }
                        units "microseconds";
                        mandatory true;
                        description "Threshold jitter value";
                    }

                    ///nokia-conf/configure/saa/owner/jitter-event/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/saa/owner/jitter-event/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list jitter-event

                ///nokia-conf/configure/saa/owner/latency-event
                list latency-event {
                    key "direction threshold-type";
                    description "Enter the latency-event list instance";

                    ///nokia-conf/configure/saa/owner/latency-event/direction
                    leaf direction {
                        type types-oam:saa-threshold-direction;
                        description "Direction for OAM ping responses for OAM ping test run";
                    }

                    ///nokia-conf/configure/saa/owner/latency-event/threshold-type
                    leaf threshold-type {
                        type enumeration {
                            enum rising                         { value 1; }
                            enum falling                        { value 2; }
                        }
                        description "Threshold type";
                    }

                    ///nokia-conf/configure/saa/owner/latency-event/threshold
                    leaf threshold {
                        type uint32 {
                            range "0..2147483647";
                        }
                        units "microseconds";
                        mandatory true;
                        description "Threshold latency value";
                    }

                    ///nokia-conf/configure/saa/owner/latency-event/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/saa/owner/latency-event/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list latency-event

                ///nokia-conf/configure/saa/owner/loss-event
                list loss-event {
                    key "direction threshold-type";
                    description "Enter the loss-event list instance";

                    ///nokia-conf/configure/saa/owner/loss-event/direction
                    leaf direction {
                        type types-oam:saa-threshold-direction;
                        description "Direction for OAM ping responses for OAM ping test run";
                    }

                    ///nokia-conf/configure/saa/owner/loss-event/threshold-type
                    leaf threshold-type {
                        type enumeration {
                            enum rising                         { value 1; }
                            enum falling                        { value 2; }
                        }
                        description "Threshold type";
                    }

                    ///nokia-conf/configure/saa/owner/loss-event/threshold
                    leaf threshold {
                        type uint32 {
                            range "0..2147483647";
                        }
                        units "packets";
                        mandatory true;
                        description "Threshold loss event value";
                    }

                    ///nokia-conf/configure/saa/owner/loss-event/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/saa/owner/loss-event/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list loss-event

                ///nokia-conf/configure/saa/owner/type
                container type {
                    description "Enter the type context";

                    ///nokia-conf/configure/saa/owner/type/saa-types
                    choice saa-types {
                        ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns
                        case saa-dns {

                            ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns
                            container dns {
                                presence "Enable the dns context";
                                description "Enable the dns context";

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/target-address
                                leaf target-address {
                                    type types-sros:string-not-all-spaces {
                                        length "1..255";
                                    }
                                    mandatory true;
                                    description "DNS name to be resolved to an IP address";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/name-server
                                leaf name-server {
                                    type types-sros:ip-address;
                                    mandatory true;
                                    description "IP address of the DNS name server used for the test";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/count
                                leaf count {
                                    type uint32 {
                                        range "1..100";
                                    }
                                    units "packets";
                                    default "1";
                                    description "Number of messages to send in a sample window";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/interval
                                leaf interval {
                                    type uint32 {
                                        range "1..10";
                                    }
                                    units "seconds";
                                    default "1";
                                    description "Minimum time before the next message request is sent";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/source-address
                                leaf source-address {
                                    type types-sros:ip-address;
                                    description "IP address on the local system";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/timeout
                                leaf timeout {
                                    type uint32 {
                                        range "1..120";
                                    }
                                    units "seconds";
                                    default "5";
                                    description "Maximum time to wait for a reply message";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/router-instance
                                leaf router-instance {
                                    type string;
                                    default "_Base_and_management";
                                    description "Router name or VPRN service name";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/record-type
                                leaf record-type {
                                    type enumeration {
                                        enum ipv4-a-record                  { value 1; }
                                        enum ipv6-aaaa-record               { value 2; }
                                    }
                                    default "ipv4-a-record";
                                    description "Record type";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/trap-generation
                                container trap-generation {
                                    description "Enter the trap-generation context";

                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/trap-generation/probe-fail
                                    leaf probe-fail {
                                        type boolean;
                                        default "false";
                                        description "Enable the generation of probe fail notifications";
                                    }

                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/trap-generation/test-complete
                                    leaf test-complete {
                                        type boolean;
                                        default "false";
                                        description "Enable generation of test complete notifications";
                                    }

                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/trap-generation/test-fail
                                    leaf test-fail {
                                        type boolean;
                                        default "false";
                                        description "Enable the generation of test fail notifications";
                                    }

                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/trap-generation/probe-fail-threshold
                                    leaf probe-fail-threshold {
                                        type uint32 {
                                            range "0..15";
                                        }
                                        default "1";
                                        description "Consecutive probe failure count";
                                    }

                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-dns/dns/trap-generation/test-fail-threshold
                                    leaf test-fail-threshold {
                                        type uint32 {
                                            range "0..15";
                                        }
                                        default "1";
                                        description "Total probe failures for trap generation";
                                    }

                                } // container trap-generation

                            } // container dns

                        }
                        ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping
                        case saa-icmp-ping {

                            ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping
                            container icmp-ping {
                                presence "Enable the icmp-ping context";
                                description "Enable the icmp-ping context";

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/destination-address
                                leaf destination-address {
                                    type union {
                                        type types-sros:ip-address;
                                        type types-sros:string-not-all-spaces {
                                            length "1..128";
                                        }
                                    }
                                    mandatory true;
                                    description "Destination IP address or DNS name to send ping request";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/count
                                leaf count {
                                    type uint32 {
                                        range "1..100000";
                                    }
                                    units "packets";
                                    default "5";
                                    description "ICMP Echo Request packets to send in a sample window";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/do-not-fragment
                                leaf do-not-fragment {
                                    type boolean;
                                    default "false";
                                    description "Prevent fragmentation of the ICMP Echo Request packet";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/interval
                                leaf interval {
                                    type union {
                                        type uint32 {
                                            range "1..10000";
                                        }
                                        type decimal64 {
                                            range "0.01..100.00";
                                            fraction-digits 2;
                                        }
                                    }
                                    units "seconds";
                                    default "1";
                                    description "Minimum time to expire before message request is sent";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/pattern
                                leaf pattern {
                                    type types-oam:padding-pattern;
                                    default "sequential";
                                    description "Pattern to fill the data portion in a ping packet";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/router-instance
                                leaf router-instance {
                                    type string;
                                    default "Base";
                                    description "Router name or VPRN service name";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/size
                                leaf size {
                                    type uint32 {
                                        range "0..16384";
                                    }
                                    units "bytes";
                                    default "56";
                                    description "Packet padding size";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/source-address
                                leaf source-address {
                                    type types-sros:ip-address;
                                    description "Source IP address";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/timeout
                                leaf timeout {
                                    type uint32 {
                                        range "1..10";
                                    }
                                    units "seconds";
                                    default "5";
                                    description "Time router waits for a reply after sending last probe";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/ttl
                                leaf ttl {
                                    type uint32 {
                                        range "1..128";
                                    }
                                    default "64";
                                    description "Time to live in the IP header";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/saa-icmp-ping-routing
                                choice saa-icmp-ping-routing {
                                    default "case-bypass-routing";
                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/saa-icmp-ping-routing/case-bypass-routing
                                    case case-bypass-routing {

                                        ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/saa-icmp-ping-routing/case-bypass-routing/bypass-routing
                                        leaf bypass-routing {
                                            type boolean;
                                            default "false";
                                            description "Bypass routing table for directly connected interfaces";
                                        }

                                    }
                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/saa-icmp-ping-routing/case-interface
                                    case case-interface {

                                        ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/saa-icmp-ping-routing/case-interface/interface
                                        leaf interface {
                                            type types-sros:interface-name;
                                            description "Interface name";
                                        }

                                    }
                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/saa-icmp-ping-routing/case-next-hop-address
                                    case case-next-hop-address {

                                        ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/saa-icmp-ping-routing/case-next-hop-address/next-hop-address
                                        leaf next-hop-address {
                                            type types-sros:ip-address;
                                            description "Next-hop IP address to reach destination";
                                        }

                                    }
                                }
                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/qos
                                container qos {
                                    description "Enter the qos context";

                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/qos/fc
                                    leaf fc {
                                        type types-sros:fc-name;
                                        default "nc";
                                        description "Forwarding class";
                                    }

                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/qos/tos
                                    leaf tos {
                                        type uint32 {
                                            range "0..255";
                                        }
                                        default "0";
                                        description "Type of service";
                                    }

                                } // container qos

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/trap-generation
                                container trap-generation {
                                    description "Enter the trap-generation context";

                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/trap-generation/probe-fail
                                    leaf probe-fail {
                                        type boolean;
                                        default "false";
                                        description "Enable the generation of probe fail notifications";
                                    }

                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/trap-generation/test-complete
                                    leaf test-complete {
                                        type boolean;
                                        default "false";
                                        description "Enable generation of test complete notifications";
                                    }

                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/trap-generation/test-fail
                                    leaf test-fail {
                                        type boolean;
                                        default "false";
                                        description "Enable the generation of test fail notifications";
                                    }

                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/trap-generation/probe-fail-threshold
                                    leaf probe-fail-threshold {
                                        type uint32 {
                                            range "0..15";
                                        }
                                        default "1";
                                        description "Consecutive probe failure count";
                                    }

                                    ///nokia-conf/configure/saa/owner/type/saa-types/saa-icmp-ping/icmp-ping/trap-generation/test-fail-threshold
                                    leaf test-fail-threshold {
                                        type uint32 {
                                            range "0..15";
                                        }
                                        default "1";
                                        description "Total probe failures for trap generation";
                                    }

                                } // container trap-generation

                            } // container icmp-ping

                        }
                        ///nokia-conf/configure/saa/owner/type/saa-types/saa-lsp-ping
                        case saa-lsp-ping {

                            ///nokia-conf/configure/saa/owner/type/saa-types/saa-lsp-ping/lsp-ping
                            container lsp-ping {
                                presence "Enable the lsp-ping context";
                                description "Enable the lsp-ping context";

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-lsp-ping/lsp-ping/interval
                                leaf interval {
                                    type uint32 {
                                        range "1..10";
                                    }
                                    units "seconds";
                                    default "1";
                                    description "Minimum time before the next lsp-ping request is sent";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-lsp-ping/lsp-ping/send-count
                                leaf send-count {
                                    type uint32 {
                                        range "1..100";
                                    }
                                    units "packets";
                                    default "1";
                                    description "Number of request packets to send in a sample window";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-lsp-ping/lsp-ping/size
                                leaf size {
                                    type uint32 {
                                        range "1..9786";
                                    }
                                    units "bytes";
                                    default "1";
                                    description "Packet pad size";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-lsp-ping/lsp-ping/source-ip-address
                                leaf source-ip-address {
                                    type types-sros:ip-address;
                                    description "Source IP address";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-lsp-ping/lsp-ping/timeout
                                leaf timeout {
                                    type uint32 {
                                        range "1..10";
                                    }
                                    units "seconds";
                                    default "5";
                                    description "Time router waits for a reply to an echo request";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-lsp-ping/lsp-ping/ttl
                                leaf ttl {
                                    type uint32 {
                                        range "1..255";
                                    }
                                    default "255";
                                    description "TTL value for the echo request MPLS label";
                                }

                                ///nokia-conf/configure/saa/owner/type/saa-types/saa-lsp-ping/lsp-ping/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

