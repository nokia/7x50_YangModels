submodule nokia-state-chassis {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-chassis            { prefix "types-chassis"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-system             { prefix "types-system"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for Physical Chassis related parameters on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-chassis YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-03-14";

    grouping state-chassis {
        ///nokia-state/state/chassis
        list chassis {
            key "chassis-class chassis-number";
            description "Enter the chassis list instance";

            ///nokia-state/state/chassis/chassis-class
            leaf chassis-class {
                type types-chassis:chassis-class;
                description "Functional use of the physical chassis";
            }

            ///nokia-state/state/chassis/chassis-number
            leaf chassis-number {
                type uint32 {
                    range "1..100";
                }
                description "Unique index to identify this physical chassis";
            }

            ///nokia-state/state/chassis/name
            leaf name {
                type types-sros:named-item;
                description "The name for the physical chassis.";
            }

            ///nokia-state/state/chassis/role
            leaf role {
                type enumeration {
                    enum standalone                     { value 1; }
                    enum master                         { value 2; }
                    enum extension                      { value 3; }
                    enum ethernet-satellite             { value 6; }
                    enum tdm-satellite                  { value 7; }
                    enum power-shelf                    { value 10; }
                }
                description
                    "The role in which the physical chassis performs while
                     in the system.";
            }

            ///nokia-state/state/chassis/state
            leaf state {
                type types-system:device-state;
                description "The device state of the physical chassis.";
            }

            ///nokia-state/state/chassis/power-supplies
            leaf power-supplies {
                type uint32;
                description
                    "The total number of power supplies that can be installed in this
                     physical chassis.";
            }

            ///nokia-state/state/chassis/fan-trays
            leaf fan-trays {
                type uint32;
                description
                    "The total number of fan trays installed in this
                     physical chassis.";
            }

            ///nokia-state/state/chassis/filter-door-state
            leaf filter-door-state {
                type enumeration {
                    enum unknown                        { value 0; }
                    enum not-monitored                  { value 1; }
                    enum closed                         { value 2; }
                    enum open                           { value 3; }
                }
                description
                    "The value of filter-door-state indicates the state of
                     the filter door.";
            }

            ///nokia-state/state/chassis/hardware-data
            container hardware-data {
                description "Enter the hardware-data context";

                ///nokia-state/state/chassis/hardware-data/part-number
                leaf part-number {
                    type string {
                        length "0..32";
                    }
                    description "Indicates the part number information.";
                }

                ///nokia-state/state/chassis/hardware-data/serial-number
                leaf serial-number {
                    type string {
                        length "0..32";
                    }
                    description "Indicates the serial number of the hardware component.";
                }

                ///nokia-state/state/chassis/hardware-data/manufacturing-date
                leaf manufacturing-date {
                    type string {
                        length "0..8";
                    }
                    description "Indicates the manufacturing date of the hardware component.";
                }

                ///nokia-state/state/chassis/hardware-data/common-language-equipment-identifier
                leaf common-language-equipment-identifier {
                    type string {
                        length "0..10";
                    }
                    description
                        "Indicates the CLEI, a unique 10-character identifier fixed
                         by the manufacturer.";
                }

                ///nokia-state/state/chassis/hardware-data/field-replaceable-unit
                leaf field-replaceable-unit {
                    type boolean;
                    description
                        "Indicates whether the hardware component is a field
                         replaceable unit or not.";
                }

                ///nokia-state/state/chassis/hardware-data/oper-state
                leaf oper-state {
                    type types-chassis:hw-oper-state;
                    description "Indicates the operational status of the hardware component.";
                }

                ///nokia-state/state/chassis/hardware-data/failure-reason
                leaf failure-reason {
                    type string {
                        length "0..255";
                    }
                    description
                        "Indicates the reason why a hardware component 'failed' as
                         indicated in oper-state.";
                }

                ///nokia-state/state/chassis/hardware-data/alarm-state
                leaf alarm-state {
                    type enumeration {
                        enum unknown                        { value 0; }
                        enum alarm-active                   { value 1; }
                        enum alarm-cleared                  { value 2; }
                    }
                    description
                        "Indicates the current alarm state for this hardware
                         component.";
                }

                ///nokia-state/state/chassis/hardware-data/manufacturing-deviations
                leaf manufacturing-deviations {
                    type string {
                        length "0..255";
                    }
                    description
                        "Indicates the record of changes done by the manfacturing
                         to the hardware or software which is outside the normal
                         revision control process.";
                }

                ///nokia-state/state/chassis/hardware-data/equipped-platform-type
                leaf equipped-platform-type {
                    type types-chassis:equipped-platform-type;
                    description
                        "Indicates the platform type of the installed hardware
                         component, as read from the EEPROM.";
                }

                ///nokia-state/state/chassis/hardware-data/manufacturing-assembly-number
                leaf manufacturing-assembly-number {
                    type string {
                        length "0..32";
                    }
                    description
                        "Indicates an assembly number indicating manufacturing
                         differences for a given part number.";
                }

                ///nokia-state/state/chassis/hardware-data/power-zone-location
                leaf power-zone-location {
                    type uint32 {
                        range "0..32";
                    }
                    description
                        "Indicates the managed power zone that this hardware
                         component resides.";
                }

                ///nokia-state/state/chassis/hardware-data/base-mac-address
                leaf base-mac-address {
                    type yang:mac-address;
                    description "Indicates the base MAC address of the hardware component.";
                }

                ///nokia-state/state/chassis/hardware-data/boot-code-version
                leaf boot-code-version {
                    type string {
                        length "0..255";
                    }
                    description "Indicates the version number of boot eprom.";
                }

                ///nokia-state/state/chassis/hardware-data/software-code-version
                leaf software-code-version {
                    type string {
                        length "0..255";
                    }
                    description
                        "Indicates the software product release version number for the
                         software image currently running.";
                }

                ///nokia-state/state/chassis/hardware-data/software-last-boot-time
                leaf software-last-boot-time {
                    type yang:date-and-time;
                    description
                        "This indicates the date and time the software running on this
                         hardware component was last rebooted.";
                }

            } // container hardware-data

            ///nokia-state/state/chassis/chassis-control-module
            list chassis-control-module {
                key "ccm-slot";
                description "Enter the chassis-control-module list instance";

                ///nokia-state/state/chassis/chassis-control-module/ccm-slot
                leaf ccm-slot {
                    type string {
                        length "1";
                    }
                    description "The unique value which identifies this CCM slot.";
                }

                ///nokia-state/state/chassis/chassis-control-module/equipped-type
                leaf equipped-type {
                    type enumeration {
                        enum unknown                        { value 1; }
                        enum ccm-x20                        { value 6; }
                        enum ccm-x16                        { value 7; }
                        enum ccm-e                          { value 8; }
                        enum ccm-1                          { value 9; }
                        enum ccm-1s                         { value 10; }
                        enum ccm-2s                         { value 11; }
                        enum ccm2-x20                       { value 12; }
                    }
                    description "Indicates the CCM type that is equipped.";
                }

                ///nokia-state/state/chassis/chassis-control-module/hardware-data
                container hardware-data {
                    description "Enter the hardware-data context";

                    ///nokia-state/state/chassis/chassis-control-module/hardware-data/part-number
                    leaf part-number {
                        type string {
                            length "0..32";
                        }
                        description "Indicates the part number information.";
                    }

                    ///nokia-state/state/chassis/chassis-control-module/hardware-data/serial-number
                    leaf serial-number {
                        type string {
                            length "0..32";
                        }
                        description "Indicates the serial number of the hardware component.";
                    }

                    ///nokia-state/state/chassis/chassis-control-module/hardware-data/manufacturing-date
                    leaf manufacturing-date {
                        type string {
                            length "0..8";
                        }
                        description "Indicates the manufacturing date of the hardware component.";
                    }

                    ///nokia-state/state/chassis/chassis-control-module/hardware-data/common-language-equipment-identifier
                    leaf common-language-equipment-identifier {
                        type string {
                            length "0..10";
                        }
                        description
                            "Indicates the CLEI, a unique 10-character identifier fixed
                             by the manufacturer.";
                    }

                    ///nokia-state/state/chassis/chassis-control-module/hardware-data/field-replaceable-unit
                    leaf field-replaceable-unit {
                        type boolean;
                        description
                            "Indicates whether the hardware component is a field
                             replaceable unit or not.";
                    }

                    ///nokia-state/state/chassis/chassis-control-module/hardware-data/oper-state
                    leaf oper-state {
                        type types-chassis:hw-oper-state;
                        description "Indicates the operational status of the hardware component.";
                    }

                    ///nokia-state/state/chassis/chassis-control-module/hardware-data/contains-temperature-sensor
                    leaf contains-temperature-sensor {
                        type boolean;
                        description
                            "Indicates whether or not this hardware component contains
                             a temperature sensor.";
                    }

                    ///nokia-state/state/chassis/chassis-control-module/hardware-data/temperature
                    leaf temperature {
                        type int32;
                        description
                            "Indicates the current temperature in degrees celsius from
                             the temperature sensor.";
                    }

                    ///nokia-state/state/chassis/chassis-control-module/hardware-data/temperature-threshold
                    leaf temperature-threshold {
                        type int32;
                        description
                            "Indicates the temperature threshold for this hardware
                             component in degrees celsius.";
                    }

                    ///nokia-state/state/chassis/chassis-control-module/hardware-data/over-temperature-status
                    leaf over-temperature-status {
                        type enumeration {
                            enum ok                             { value 1; }
                            enum over-temp                      { value 2; }
                        }
                        description
                            "Indicates whether the temperature of the hardware component has exceeded
                             its threshold. If that temperature has been exceeded, a value of
                             'over-temp' (2) will be reported.";
                    }

                    ///nokia-state/state/chassis/chassis-control-module/hardware-data/alarm-state
                    leaf alarm-state {
                        type enumeration {
                            enum unknown                        { value 0; }
                            enum alarm-active                   { value 1; }
                            enum alarm-cleared                  { value 2; }
                        }
                        description
                            "Indicates the current alarm state for this hardware
                             component.";
                    }

                    ///nokia-state/state/chassis/chassis-control-module/hardware-data/manufacturing-deviations
                    leaf manufacturing-deviations {
                        type string {
                            length "0..255";
                        }
                        description
                            "Indicates the record of changes done by the manfacturing
                             to the hardware or software which is outside the normal
                             revision control process.";
                    }

                    ///nokia-state/state/chassis/chassis-control-module/hardware-data/manufacturing-assembly-number
                    leaf manufacturing-assembly-number {
                        type string {
                            length "0..32";
                        }
                        description
                            "Indicates an assembly number indicating manufacturing
                             differences for a given part number.";
                    }

                    ///nokia-state/state/chassis/chassis-control-module/hardware-data/power-zone-location
                    leaf power-zone-location {
                        type uint32 {
                            range "0..32";
                        }
                        description
                            "Indicates the managed power zone that this hardware
                             component resides.";
                    }

                } // container hardware-data

                ///nokia-state/state/chassis/chassis-control-module/hardware-resource
                container hardware-resource {
                    description "Enter the hardware-resource context";

                    ///nokia-state/state/chassis/chassis-control-module/hardware-resource/amperage
                    container amperage {
                        description "Enter the amperage context";

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/amperage/current
                        leaf current {
                            type int32;
                            units "milliamperes";
                            description
                                "Indicates the current amperage use of the managed hardware component
                                 in milli-Amperes (mA).";
                        }

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/amperage/minimum
                        leaf minimum {
                            type int32;
                            units "milliamperes";
                            description
                                "Indicates the lowest amperage use of the managed hardware component
                                 in milli-Amperes (mA), recorded at the time indicated by
                                 minimum-time";
                        }

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/amperage/minimum-time
                        leaf minimum-time {
                            type yang:date-and-time;
                            description "Indicates the time and date at which minimum amperage was last updated.";
                        }

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/amperage/peak
                        leaf peak {
                            type int32;
                            units "milliamperes";
                            description
                                "Indicates the peak amperage use of the managed hardware component
                                 in milli-Amperes (mA), recorded at the time indicated by
                                 peak-time.";
                        }

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/amperage/peak-time
                        leaf peak-time {
                            type yang:date-and-time;
                            description "Indicates the time and date at which peak amperage was last updated";
                        }

                    } // container amperage

                    ///nokia-state/state/chassis/chassis-control-module/hardware-resource/voltage
                    container voltage {
                        description "Enter the voltage context";

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/voltage/current
                        leaf current {
                            type int32;
                            units "millivolts";
                            description
                                "Indicates the current voltage use of the managed hardware component
                                 in milli-Volts (mV).";
                        }

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/voltage/minimum
                        leaf minimum {
                            type int32;
                            units "millivolts";
                            description
                                "Indicates the lowest voltage use of the managed hardware component
                                 in milli-Volts (mV), last recorded at the time indicated by
                                 minimum-time";
                        }

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/voltage/minimum-time
                        leaf minimum-time {
                            type yang:date-and-time;
                            description "Indicates the time and date at which minimum voltage was last updated.";
                        }

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/voltage/peak
                        leaf peak {
                            type int32;
                            units "millivolts";
                            description
                                "Indicates the peak voltage use of the managed hardware component
                                 in milli-Volts (mV), last recorded at the time indicated by
                                 peak-time.";
                        }

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/voltage/peak-time
                        leaf peak-time {
                            type yang:date-and-time;
                            description "Indicates the time and date at which peak voltage was last updated";
                        }

                    } // container voltage

                    ///nokia-state/state/chassis/chassis-control-module/hardware-resource/wattage
                    container wattage {
                        description "Enter the wattage context";

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/wattage/current
                        leaf current {
                            type int32;
                            units "milliwatts";
                            description
                                "Indicates the current wattage use of the managed hardware component
                                 in milli-Watts (mW).";
                        }

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/wattage/minimum
                        leaf minimum {
                            type int32;
                            units "milliwatts";
                            description
                                "Indicates the lowest wattage use of the managed hardware component
                                 in milli-Watts (mW), last recorded at the time indicated by
                                 minimum-time";
                        }

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/wattage/minimum-time
                        leaf minimum-time {
                            type yang:date-and-time;
                            description "Indicates the time and date at which minimum wattage was last updated.";
                        }

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/wattage/peak
                        leaf peak {
                            type int32;
                            units "milliwatts";
                            description
                                "Indicates the peak wattage use of the managed hardware component
                                 in milli-Watts (mW), last recorded at the time indicated by
                                 peak-time.";
                        }

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/wattage/peak-time
                        leaf peak-time {
                            type yang:date-and-time;
                            description "Indicates the time and date at which was last updated";
                        }

                        ///nokia-state/state/chassis/chassis-control-module/hardware-resource/wattage/required
                        leaf required {
                            type int32;
                            units "milliwatts";
                            description
                                "Indicates the wattage required by the managed hardware component
                                 in milli-Watts (mW).";
                        }

                    } // container wattage

                } // container hardware-resource

            } // list chassis-control-module

            ///nokia-state/state/chassis/cpm-module-adapter
            container cpm-module-adapter {
                description "Enter the cpm-module-adapter context";

                ///nokia-state/state/chassis/cpm-module-adapter/hardware-data
                container hardware-data {
                    description "Enter the hardware-data context";

                    ///nokia-state/state/chassis/cpm-module-adapter/hardware-data/part-number
                    leaf part-number {
                        type string {
                            length "0..32";
                        }
                        description "Indicates the part number information.";
                    }

                    ///nokia-state/state/chassis/cpm-module-adapter/hardware-data/serial-number
                    leaf serial-number {
                        type string {
                            length "0..32";
                        }
                        description "Indicates the serial number of the hardware component.";
                    }

                    ///nokia-state/state/chassis/cpm-module-adapter/hardware-data/manufacturing-date
                    leaf manufacturing-date {
                        type string {
                            length "0..8";
                        }
                        description "Indicates the manufacturing date of the hardware component.";
                    }

                    ///nokia-state/state/chassis/cpm-module-adapter/hardware-data/common-language-equipment-identifier
                    leaf common-language-equipment-identifier {
                        type string {
                            length "0..10";
                        }
                        description
                            "Indicates the CLEI, a unique 10-character identifier fixed
                             by the manufacturer.";
                    }

                    ///nokia-state/state/chassis/cpm-module-adapter/hardware-data/field-replaceable-unit
                    leaf field-replaceable-unit {
                        type boolean;
                        description
                            "Indicates whether the hardware component is a field
                             replaceable unit or not.";
                    }

                    ///nokia-state/state/chassis/cpm-module-adapter/hardware-data/contains-temperature-sensor
                    leaf contains-temperature-sensor {
                        type boolean;
                        description
                            "Indicates whether or not this hardware component contains
                             a temperature sensor.";
                    }

                    ///nokia-state/state/chassis/cpm-module-adapter/hardware-data/alarm-state
                    leaf alarm-state {
                        type enumeration {
                            enum unknown                        { value 0; }
                            enum alarm-active                   { value 1; }
                            enum alarm-cleared                  { value 2; }
                        }
                        description
                            "Indicates the current alarm state for this hardware
                             component.";
                    }

                    ///nokia-state/state/chassis/cpm-module-adapter/hardware-data/manufacturing-deviations
                    leaf manufacturing-deviations {
                        type string {
                            length "0..255";
                        }
                        description
                            "Indicates the record of changes done by the manfacturing
                             to the hardware or software which is outside the normal
                             revision control process.";
                    }

                    ///nokia-state/state/chassis/cpm-module-adapter/hardware-data/manufacturing-assembly-number
                    leaf manufacturing-assembly-number {
                        type string {
                            length "0..32";
                        }
                        description
                            "Indicates an assembly number indicating manufacturing
                             differences for a given part number.";
                    }

                } // container hardware-data

                ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource
                container hardware-resource {
                    description "Enter the hardware-resource context";

                    ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/amperage
                    container amperage {
                        description "Enter the amperage context";

                        ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/amperage/current
                        leaf current {
                            type int32;
                            units "milliamperes";
                            description
                                "Indicates the current amperage use of the managed hardware component
                                 in milli-Amperes (mA).";
                        }

                        ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/amperage/minimum
                        leaf minimum {
                            type int32;
                            units "milliamperes";
                            description
                                "Indicates the lowest amperage use of the managed hardware component
                                 in milli-Amperes (mA), recorded at the time indicated by
                                 minimum-time";
                        }

                        ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/amperage/minimum-time
                        leaf minimum-time {
                            type yang:date-and-time;
                            description "Indicates the time and date at which minimum amperage was last updated.";
                        }

                        ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/amperage/peak
                        leaf peak {
                            type int32;
                            units "milliamperes";
                            description
                                "Indicates the peak amperage use of the managed hardware component
                                 in milli-Amperes (mA), recorded at the time indicated by
                                 peak-time.";
                        }

                        ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/amperage/peak-time
                        leaf peak-time {
                            type yang:date-and-time;
                            description "Indicates the time and date at which peak amperage was last updated";
                        }

                    } // container amperage

                    ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/voltage
                    container voltage {
                        description "Enter the voltage context";

                        ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/voltage/current
                        leaf current {
                            type int32;
                            units "millivolts";
                            description
                                "Indicates the current voltage use of the managed hardware component
                                 in milli-Volts (mV).";
                        }

                        ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/voltage/minimum
                        leaf minimum {
                            type int32;
                            units "millivolts";
                            description
                                "Indicates the lowest voltage use of the managed hardware component
                                 in milli-Volts (mV), last recorded at the time indicated by
                                 minimum-time";
                        }

                        ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/voltage/minimum-time
                        leaf minimum-time {
                            type yang:date-and-time;
                            description "Indicates the time and date at which minimum voltage was last updated.";
                        }

                        ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/voltage/peak
                        leaf peak {
                            type int32;
                            units "millivolts";
                            description
                                "Indicates the peak voltage use of the managed hardware component
                                 in milli-Volts (mV), last recorded at the time indicated by
                                 peak-time.";
                        }

                        ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/voltage/peak-time
                        leaf peak-time {
                            type yang:date-and-time;
                            description "Indicates the time and date at which peak voltage was last updated";
                        }

                    } // container voltage

                    ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/wattage
                    container wattage {
                        description "Enter the wattage context";

                        ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/wattage/current
                        leaf current {
                            type int32;
                            units "milliwatts";
                            description
                                "Indicates the current wattage use of the managed hardware component
                                 in milli-Watts (mW).";
                        }

                        ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/wattage/minimum
                        leaf minimum {
                            type int32;
                            units "milliwatts";
                            description
                                "Indicates the lowest wattage use of the managed hardware component
                                 in milli-Watts (mW), last recorded at the time indicated by
                                 minimum-time";
                        }

                        ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/wattage/minimum-time
                        leaf minimum-time {
                            type yang:date-and-time;
                            description "Indicates the time and date at which minimum wattage was last updated.";
                        }

                        ///nokia-state/state/chassis/cpm-module-adapter/hardware-resource/wattage/peak
                        leaf peak {
                            type int32;
                            units "milliwatts";
                            description
                                "Indicates the peak wattage use of the managed hardware component
                                 in milli-Watts (mW), last recorded at the time indicated by
