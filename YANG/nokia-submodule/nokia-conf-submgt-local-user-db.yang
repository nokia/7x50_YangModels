                                    leaf hex-string {
                                        type types-sros:hex-string {
                                            length "1..256";
                                        }
                                        description "DHCP option specified as hexadecimal string";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv4-option/value/netbios-node-type
                                case netbios-node-type {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv4-option/value/netbios-node-type/netbios-node-type
                                    leaf netbios-node-type {
                                        when "../number = 46 or ../number = 'netbios-node-type'" {
                                            description "netbios-node-type is supported only when the option is netbios-node-type(46)";
                                        }
                                        type enumeration {
                                            enum b-node                         { value 1; }
                                            enum p-node                         { value 2; }
                                            enum m-node                         { value 4; }
                                            enum h-node                         { value 8; }
                                        }
                                        description "DHCP option as NetBIOS node type";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv4-option/value/duration
