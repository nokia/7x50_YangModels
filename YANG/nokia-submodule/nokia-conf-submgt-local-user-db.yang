                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/address/address/ip-address/prefix-length
                                    leaf prefix-length {
                                        type uint32 {
                                            range "1..32";
                                        }
                                        default "32";
                                        description "IPv4 host address prefix length";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/address/address/gi-address
                                case gi-address {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/address/address/gi-address/gi-address
                                    leaf gi-address {
                                        type enumeration {
                                            enum subnet-scope                   { value 0; }
                                            enum pool-scope                     { value 1; }
                                        }
                                        description "Use gi-address to select a pool with the given scope";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/address/address/pool
                                case pool {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/address/address/pool/pool
                                    container pool {
                                        presence "Enable the pool context";
                                        description "Enable the pool context";

                                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/address/address/pool/pool/primary
                                        leaf primary {
                                            type types-sros:named-item;
                                            mandatory true;
                                            description "Primary pool name";
                                        }

                                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/address/address/pool/pool/secondary
                                        leaf secondary {
                                            type types-sros:named-item;
                                            description "Secondary pool name";
                                        }

                                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/address/address/pool/pool/delimiter
                                        leaf delimiter {
                                            type string {
                                                length "1";
                                            }
                                            description "Delimiter separating primary and secondary pool names";
                                        }

                                    } // container pool

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/address/address/use-pool-from-client
                                case use-pool-from-client {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/address/address/use-pool-from-client/use-pool-from-client
                                    container use-pool-from-client {
                                        presence "Enable the use-pool-from-client context";
                                        description "Enable the use-pool-from-client context";

                                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/address/address/use-pool-from-client/use-pool-from-client/delimiter
                                        leaf delimiter {
                                            type string {
                                                length "1";
                                            }
                                            description "Delimiter character to combine primary and secondary pool names";
                                        }

                                    } // container use-pool-from-client

                                }
                            }
                        } // container address

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option
                        list option {
                            key "number";
                            description "Enter the option list instance";

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/number
                            leaf number {
                                type union {
                                    type uint32 {
                                        range "1..254";
                                    }
                                    type enumeration {
                                        enum subnet-mask                    { value 1; }
                                        enum default-router                 { value 3; }
                                        enum dns-server                     { value 6; }
                                        enum domain-name                    { value 15; }
                                        enum netbios-name-server            { value 44; }
                                        enum netbios-node-type              { value 46; }
                                        enum lease-time                     { value 51; }
                                        enum lease-renew-time               { value 58; }
                                        enum lease-rebind-time              { value 59; }
                                    }
                                }
                                description "DHCP option to send identification strings to client";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/value
                            choice value {
                                mandatory true;
                                description "The value of the DHCP option.";
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/value/ascii-string
                                case ascii-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/value/ascii-string/ascii-string
                                    leaf ascii-string {
                                        when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                            description "ascii-string is supported only when the option is neither netbios-node-type(46), lease-time(51), lease-renew-time(58) nor lease-rebind-time(59)";
                                        }
                                        type types-sros:string-not-all-spaces {
                                            length "1..127";
                                        }
                                        description "DHCP option specified as an ASCII string";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/value/hex-string
                                case hex-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/value/hex-string/hex-string
                                    leaf hex-string {
                                        when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                            description "hex-string is supported only when the option is neither netbios-node-type(46), lease-time(51), lease-renew-time(58) nor lease-rebind-time(59)";
                                        }
                                        type types-sros:hex-string {
                                            length "1..256";
                                        }
                                        description "DHCP option specified as hexadecimal string";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/value/netbios-node-type
                                case netbios-node-type {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/value/netbios-node-type/netbios-node-type
                                    leaf netbios-node-type {
                                        when "../number = 46 or ../number = 'netbios-node-type'" {
                                            description "netbios-node-type is supported only when the option is netbios-node-type(46)";
                                        }
                                        type enumeration {
                                            enum b-node                         { value 1; }
                                            enum p-node                         { value 2; }
                                            enum m-node                         { value 4; }
                                            enum h-node                         { value 8; }
                                        }
                                        description "DHCP option as NetBIOS node type";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/value/duration
                                case duration {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/value/duration/duration
                                    leaf duration {
                                        when "../number = 51 or ../number = 'lease-time' or ../number = 58 or ../number = 'lease-renew-time' or ../number = 59 or ../number = 'lease-rebind-time'" {
                                            description "duration is supported only when the option is either lease-time(51), lease-renew-time(58) or lease-rebind-time(59)";
                                        }
                                        type types-sros:time-duration {
                                            range "10..315446399";
                                        }
                                        units "seconds";
                                        description "DHCP option as time duration";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/value/ipv4-address
                                case ipv4-address {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/value/ipv4-address/ipv4-address
                                    leaf-list ipv4-address {
                                        when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                            description "ipv4-address is supported only when the option is neither netbios-node-type(46), lease-time(51), lease-renew-time(58) nor lease-rebind-time(59)";
                                        }
                                        type types-sros:ipv4-address;
                                        max-elements 4;
                                        ordered-by user;
                                        description "DHCP option as a list of IPv4 addresses";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/value/empty
                                case empty {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv4/option/value/empty/empty
                                    leaf empty {
                                        when "(not(../number = 46) and not(../number = 'netbios-node-type')) and (not(../number = 51) and not(../number = 'lease-time')) and (not(../number = 58) and not(../number = 'lease-renew-time')) and (not(../number = 59) and not(../number = 'lease-rebind-time'))" {
                                            description "empty is supported only when the option is neither netbios-node-type(46), lease-time(51), lease-renew-time(58) nor lease-rebind-time(59)";
                                        }
                                        type empty;
                                        description "Empty DHCP option.";
                                    }

                                }
                            }
                        } // list option

                    } // container ipv4

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6
                    container ipv6 {
                        description "Enter the ipv6 context";

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/address
                        leaf address {
                            type types-sros:ipv6-unicast-address;
                            description "Fixed IPv6 address of the host";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/address-pool
                        leaf address-pool {
                            type types-sros:named-item;
                            description "IPv6 address pool name";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/delegated-prefix
                        leaf delegated-prefix {
                            type types-sros:ipv6-unicast-prefix;
                            description "IPv6 delegated prefix of this host";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/delegated-prefix-length
                        leaf delegated-prefix-length {
                            type uint32 {
                                range "48..64";
                            }
                            description "Delegated prefix length";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/delegated-prefix-pool
                        leaf delegated-prefix-pool {
                            type types-sros:named-item;
                            description "Delegated prefix pool";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/slaac-prefix
                        leaf slaac-prefix {
                            type types-sros:ipv6-unicast-prefix;
                            description "IPv6 SLAAC prefix of this host";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/slaac-prefix-pool
                        leaf slaac-prefix-pool {
                            type types-sros:named-item;
                            description "IPv6 SLAAC prefix pool of this host";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/router-advertisement-policy
                        leaf router-advertisement-policy {
                            type types-sros:named-item;
                            description "IPv6 router advertisement policy";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/force-ipv6cp
                        leaf force-ipv6cp {
                            type boolean;
                            default "false";
                            description "Force IPv6 control protocol";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/timers
                        container timers {
                            description "Enter the timers context";

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/timers/preferred-lifetime
                            leaf preferred-lifetime {
                                type union {
                                    type types-sros:time-duration {
                                        range "300..315446399";
                                    }
                                    type enumeration {
                                        enum infinite                       { value -1; }
                                    }
                                }
                                units "seconds";
                                description "Time for a lease to remain preferred";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/timers/rebind
                            leaf rebind {
                                type types-sros:time-duration {
                                    range "0..1209600";
                                }
                                units "seconds";
                                description "Rebind timer (T2)";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/timers/renew
                            leaf renew {
                                type types-sros:time-duration {
                                    range "0..604800";
                                }
                                units "seconds";
                                description "Renew timer (T1)";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/timers/valid-lifetime
                            leaf valid-lifetime {
                                type union {
                                    type types-sros:time-duration {
                                        range "300..315446399";
                                    }
                                    type enumeration {
                                        enum infinite                       { value -1; }
                                    }
                                }
                                units "seconds";
                                description "Time for a lease to remain valid";
                            }

                        } // container timers

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/option
                        list option {
                            key "number";
                            description "Enter the option list instance";

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/option/number
                            leaf number {
                                type union {
                                    type uint32 {
                                        range "23";
                                    }
                                    type enumeration {
                                        enum dns-server                     { value 23; }
                                    }
                                }
                                description "The number of the DHCPv6 option.";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/option/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/option/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/option/value
                            choice value {
                                mandatory true;
                                description "The value of the DHCP option.";
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/option/value/hex-string
                                case hex-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/option/value/hex-string/hex-string
                                    leaf hex-string {
                                        type types-sros:hex-string {
                                            length "1..256";
                                        }
                                        description "DHCP option specified as hexadecimal string";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/option/value/ipv6-address
                                case ipv6-address {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ipv6/option/value/ipv6-address/ipv6-address
                                    leaf-list ipv6-address {
                                        type types-sros:ipv6-address;
                                        max-elements 4;
                                        ordered-by user;
                                        description "DHCP option specified as a list of IPv6 addresses";
                                    }

                                }
                            }
                        } // list option

                    } // container ipv6

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/mld-import
                    list mld-import {
                        key "policy-name";
                        max-elements 14;
                        description "Add a list entry for mld-import";

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/mld-import/policy-name
                        leaf policy-name {
                            type types-sros:named-item;
                            description "MLD import policy used to control the multicast group";
                        }

                    } // list mld-import

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ppp-policy-parameters
                    container ppp-policy-parameters {
                        description "Enter the ppp-policy-parameters context";

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ppp-policy-parameters/max-sessions-per-mac
                        leaf max-sessions-per-mac {
                            type uint32 {
                                range "1..8191";
                            }
                            description "Maximum number of sessions per MAC address";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ppp-policy-parameters/keepalive
                        container keepalive {
                            presence "Enable the keepalive context";
                            description "Enable the keepalive context";

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ppp-policy-parameters/keepalive/hold-up-multiplier
                            leaf hold-up-multiplier {
                                type uint32 {
                                    range "1..5";
                                }
                                default "3";
                                description "PPP LCP keepalive hold up multiplier override";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/ppp-policy-parameters/keepalive/interval
                            leaf interval {
                                type types-sros:time-duration {
                                    range "4..300";
                                }
                                units "seconds";
                                default "30";
                                description "PPP LCP keepalive interval override";
                            }

                        } // container keepalive

                    } // container ppp-policy-parameters

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options
                    container to-client-options {
                        description "Enter the to-client-options context";

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option
                        list ipv6-option {
                            key "number";
                            description "Enter the ipv6-option list instance";

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option/number
                            leaf number {
                                type union {
                                    type uint32 {
                                        range "1..65535";
                                    }
                                    type enumeration {
                                        enum dns-server                     { value 23; }
                                        enum domain-name                    { value 24; }
                                    }
                                }
                                description "DHCP option to send as identification string";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option/value
                            choice value {
                                mandatory true;
                                description "The value of the DHCP option.";
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option/value/ascii-string
                                case ascii-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option/value/ascii-string/ascii-string
                                    leaf ascii-string {
                                        type types-sros:string-not-all-spaces {
                                            length "1..127";
                                        }
                                        description "DHCP option specified as an ASCII string";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option/value/hex-string
                                case hex-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option/value/hex-string/hex-string
                                    leaf hex-string {
                                        type types-sros:hex-string {
                                            length "1..256";
                                        }
                                        description "DHCP option specified as hexadecimal string";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option/value/domain-string
                                case domain-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option/value/domain-string/domain-string
                                    leaf domain-string {
                                        type string {
                                            length "1..127";
                                            pattern "[a-z0-9\\-]+(\\.[a-z0-9\\-]+)*";
                                        }
                                        description "DHCP option specified as a domain name";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option/value/ipv6-address
                                case ipv6-address {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option/value/ipv6-address/ipv6-address
                                    leaf-list ipv6-address {
                                        type types-sros:ipv6-address;
                                        max-elements 4;
                                        ordered-by user;
                                        description "DHCP option specified as a list of IPv6 addresses";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option/value/empty
                                case empty {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/to-client-options/ipv6-option/value/empty/empty
                                    leaf empty {
                                        type empty;
                                        description "Empty DHCP option";
                                    }

                                }
                            }
                        } // list ipv6-option

                    } // container to-client-options

                } // list host

            } // container ppp

        } // list local-user-db

    } // grouping conf-submgt-local-user-db

}
