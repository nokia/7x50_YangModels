                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv6-option/value
                            choice value {
                                mandatory true;
                                description "The value of the DHCP option.";
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv6-option/value/ascii-string
                                case ascii-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv6-option/value/ascii-string/ascii-string
                                    leaf ascii-string {
                                        type types-sros:string-not-all-spaces {
                                            length "1..127";
                                        }
                                        description "DHCP option specified as an ASCII string";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv6-option/value/hex-string
                                case hex-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv6-option/value/hex-string/hex-string
                                    leaf hex-string {
                                        type types-sros:hex-string {
                                            length "1..256";
                                        }
                                        description "DHCP option specified as hexadecimal string";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv6-option/value/domain-string
                                case domain-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv6-option/value/domain-string/domain-string
                                    leaf domain-string {
                                        type string {
                                            length "1..127";
                                            pattern "[a-z0-9\\-]+(\\.[a-z0-9\\-]+)*";
                                        }
                                        description "DHCP option specified as a domain name";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv6-option/value/ipv6-address
                                case ipv6-address {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv6-option/value/ipv6-address/ipv6-address
                                    leaf-list ipv6-address {
                                        type types-sros:ipv6-address;
                                        max-elements 4;
                                        ordered-by user;
                                        description "DHCP option specified as a list of IPv6 addresses";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv6-option/value/empty
                                case empty {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv6-option/value/empty/empty
                                    leaf empty {
                                        type empty;
                                        description "Empty DHCP option";
                                    }

                                }
                            }
                        } // list ipv6-option

