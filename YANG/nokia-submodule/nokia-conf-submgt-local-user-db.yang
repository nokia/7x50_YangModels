                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/host-identification/sap-id
                        leaf sap-id {
                            type types-sros:display-string;
                            sros-ext:auto-restart-to-modify;
                            description "SAP ID";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/host-identification/service-name
                        leaf service-name {
                            type types-sros:display-string;
                            sros-ext:auto-restart-to-modify;
                            description "Administrative service name";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/host-identification/circuit-id
                        container circuit-id {
                            description "Enter the circuit-id context";

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/host/host-identification/circuit-id/format
                            choice format {
