                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv4-option/value/ascii-string/ascii-string
                                    leaf ascii-string {
                                        type types-sros:string-not-all-spaces {
                                            length "1..127";
                                        }
                                        description "DHCP option specified as an ASCII string";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/to-client-options/ipv4-option/value/hex-string
                                case hex-string {

