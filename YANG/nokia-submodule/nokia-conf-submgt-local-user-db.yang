                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/mask/suffix
                    container suffix {
                        presence "Enable the suffix context";
                        description "Enable the suffix context";

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/mask/suffix/suffix
                        choice suffix {
                            mandatory true;
                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/mask/suffix/suffix/length
                            case length {

                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/mask/suffix/suffix/length/length
                                leaf length {
                                    type uint32 {
                                        range "1..127";
                                    }
                                    description "PPP mask suffix length";
                                }

