submodule nokia-conf-submgt-local-user-db {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-subscriber-mgmt    { prefix "types-submgt"; }

    include nokia-conf-groups;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure subscriber management local user database on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-submgt-local-user-db YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-01-30";

    grouping conf-submgt-local-user-db {
        ///nokia-conf/configure/subscriber-mgmt/local-user-db
        list local-user-db {
            key "name";
            max-elements 127;
            description "Enter the local-user-db list instance";

            ///nokia-conf/configure/subscriber-mgmt/local-user-db/name
            leaf name {
                type types-sros:named-item;
                description "Local user database name";
            }

            ///nokia-conf/configure/subscriber-mgmt/local-user-db/admin-state
            leaf admin-state {
                type types-sros:admin-state;
                default "disable";
                description "Administrative state of the local user database";
            }

            ///nokia-conf/configure/subscriber-mgmt/local-user-db/description
            leaf description {
                type types-sros:description;
                description "Text description";
            }

            ///nokia-conf/configure/subscriber-mgmt/local-user-db/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/subscriber-mgmt/local-user-db/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe
            container ipoe {
                description "Enter the ipoe context";

                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/match-list
                leaf-list match-list {
                    type enumeration {
                        enum circuit-id                     { value 1; }
                        enum mac                            { value 2; }
                        enum remote-id                      { value 3; }
                        enum sap-id                         { value 4; }
                        enum service-id                     { value 5; }
                        enum string                         { value 6; }
                        enum system-id                      { value 7; }
                        enum option60                       { value 8; }
                        enum encap-tag-range                { value 9; }
                        enum dual-stack-remote-id           { value 10; }
                        enum derived-id                     { value 11; }
                        enum ip                             { value 12; }
                        enum encap-tag-separate-range       { value 13; }
                    }
                    max-elements 4;
                    ordered-by user;
                    sros-ext:auto-restart-to-modify;
                    description "IPoE host match type";
                }

                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask
                list mask {
                    key "type";
                    description "Enter the mask list instance";

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/type
                    leaf type {
                        type enumeration {
                            enum circuit-id                     { value 1; }
                            enum remote-id                      { value 3; }
                            enum sap-id                         { value 4; }
                            enum string                         { value 6; }
                            enum system-id                      { value 7; }
                            enum option60                       { value 8; }
                        }
                        description "Matching type to identify a host";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/prefix
                    container prefix {
                        presence "Enable the prefix context";
                        description "Enable the prefix context";

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/prefix/prefix
                        choice prefix {
                            mandatory true;
                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/prefix/prefix/length
                            case length {

                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/prefix/prefix/length/length
                                leaf length {
                                    type uint32 {
                                        range "1..127";
                                    }
                                    description "PPP mask prefix length";
                                }

                            }
                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/prefix/prefix/string
                            case string {

                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/prefix/prefix/string/string
                                leaf string {
                                    type string {
                                        length "1..127";
                                    }
                                    description "Prefix string";
                                }

                            }
                        }
                    } // container prefix

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/suffix
                    container suffix {
                        presence "Enable the suffix context";
                        description "Enable the suffix context";

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/suffix/suffix
                        choice suffix {
                            mandatory true;
                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/suffix/suffix/length
                            case length {

                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/suffix/suffix/length/length
                                leaf length {
                                    type uint32 {
                                        range "1..127";
                                    }
                                    description "PPP mask suffix length";
                                }

                            }
                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/suffix/suffix/string
                            case string {

                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/mask/suffix/suffix/string/string
                                leaf string {
                                    type string {
                                        length "1..127";
                                        pattern "[^*].*";
                                    }
                                    description "Suffix string";
                                }

                            }
                        }
                    } // container suffix

                } // list mask

                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host
                list host {
                    key "host-name";
                    description "Enter the host list instance";

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-name
                    leaf host-name {
                        type types-sros:named-item;
                        description "Host name";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the host";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/retail-service-id
                    leaf retail-service-id {
                        type types-services:service-id-maf;
                        description "Configure the retail service-id";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/rip-policy
                    leaf rip-policy {
                        type types-sros:named-item;
                        description "RIP policy";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification
                    container host-identification {
                        description "Enter the host-identification context";

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/derived-id
                        leaf derived-id {
                            type types-sros:display-string;
                            sros-ext:auto-restart-to-modify;
                            description "Derived ID string";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/mac
                        leaf mac {
                            type types-sros:mac-unicast-address-no-zero;
                            sros-ext:auto-restart-to-modify;
                            description "MAC address";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/sap-id
                        leaf sap-id {
                            type types-sros:display-string;
                            sros-ext:auto-restart-to-modify;
                            description "SAP ID";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/ip-prefix
                        leaf ip-prefix {
                            type types-sros:ip-prefix-with-host-bits;
                            sros-ext:auto-restart-to-modify;
                            description "IP interface name";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/string
                        leaf string {
                            type types-sros:display-string;
                            sros-ext:auto-restart-to-modify;
                            description "VSO string";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/service-id
                        leaf service-id {
                            type types-services:service-id-maf;
                            sros-ext:auto-restart-to-modify;
                            description "Service ID";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/system-id
                        leaf system-id {
                            type types-sros:display-string;
                            sros-ext:auto-restart-to-modify;
                            description "System ID";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/circuit-id
                        container circuit-id {
                            description "Enter the circuit-id context";

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/circuit-id/format
                            choice format {
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/circuit-id/format/ascii-string
                                case ascii-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/circuit-id/format/ascii-string/ascii-string
                                    leaf ascii-string {
                                        type string {
                                            length "1..127";
                                        }
                                        sros-ext:auto-restart-to-modify;
                                        description "Circuit ID as ASCII string";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/circuit-id/format/hex-string
                                case hex-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/circuit-id/format/hex-string/hex-string
                                    leaf hex-string {
                                        type types-sros:hex-string {
                                            length "1..256";
                                        }
                                        sros-ext:auto-restart-to-modify;
                                        description "Circuit ID as hexadecimal string";
                                    }

                                }
                            }
                        } // container circuit-id

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/remote-id
                        container remote-id {
                            description "Enter the remote-id context";

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/remote-id/format
                            choice format {
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/remote-id/format/ascii-string
                                case ascii-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/remote-id/format/ascii-string/ascii-string
                                    leaf ascii-string {
                                        type string {
                                            length "1..255";
                                        }
                                        sros-ext:auto-restart-to-modify;
                                        description "Remote ID as ASCII string";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/remote-id/format/hex-string
                                case hex-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/remote-id/format/hex-string/hex-string
                                    leaf hex-string {
                                        type types-sros:hex-string {
                                            length "1..512";
                                        }
                                        sros-ext:auto-restart-to-modify;
                                        description "Remote ID as hexadecimal string";
                                    }

                                }
                            }
                        } // container remote-id

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/option60
                        container option60 {
                            description "Enter the option60 context";

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/option60/format
                            choice format {
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/option60/format/ascii-string
                                case ascii-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/option60/format/ascii-string/ascii-string
                                    leaf ascii-string {
                                        type types-sros:named-item;
                                        sros-ext:auto-restart-to-modify;
                                        description "Option 60 as ASCII string";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/option60/format/hex-string
                                case hex-string {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/option60/format/hex-string/hex-string
                                    leaf hex-string {
                                        type types-sros:hex-string {
                                            length "1..64";
                                        }
                                        sros-ext:auto-restart-to-modify;
                                        description "Option as a hexadecimal string";
                                    }

                                }
                            }
                        } // container option60

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/encap-tag-range
                        container encap-tag-range {
                            presence "Enable the encap-tag-range context";
                            description "Enable the encap-tag-range context";

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/encap-tag-range/from
                            leaf from {
                                type types-sros:atm-or-vlan-encap;
                                sros-ext:auto-restart-to-modify;
                                mandatory true;
                                description "Start tag";
                            }

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/encap-tag-range/to
                            leaf to {
                                type types-sros:atm-or-vlan-encap;
                                sros-ext:auto-restart-to-modify;
                                mandatory true;
                                description "End tag";
                            }

                        } // container encap-tag-range

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/encap-tag-separate-range
                        container encap-tag-separate-range {
                            description "Enter the encap-tag-separate-range context";

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/encap-tag-separate-range/outer
                            container outer {
                                description "Enter the outer context";

                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/encap-tag-separate-range/outer/start
                                leaf start {
                                    type int32 {
                                        range "0..4094";
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    description "Lower bound of the outer encapsulation tag range";
                                }

                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/encap-tag-separate-range/outer/end
                                leaf end {
                                    type int32 {
                                        range "0..4094";
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    description "Upper bound of the outer encapsulation tag range";
                                }

                            } // container outer

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/encap-tag-separate-range/inner
                            container inner {
                                description "Enter the inner context";

                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/encap-tag-separate-range/inner/start
                                leaf start {
                                    type int32 {
                                        range "0..4094";
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    description "Lower bound of the inner encapsulation tag range";
                                }

                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/host-identification/encap-tag-separate-range/inner/end
                                leaf end {
                                    type int32 {
                                        range "0..4094";
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    description "Upper bound of the inner encapsulation tag range";
                                }

                            } // container inner

                        } // container encap-tag-separate-range

                    } // container host-identification

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/authentication
                    container authentication {
                        description "Enter the authentication context";

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/authentication/radius-auth-policy
                        leaf radius-auth-policy {
                            type types-sros:named-item;
                            description "Host authentication policy";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/authentication/gx-policy
                        leaf gx-policy {
                            type types-sros:named-item;
                            description "Diameter application policy";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/authentication/nasreq-auth-policy
                        leaf nasreq-auth-policy {
                            type types-sros:named-item;
                            description "Diameter NASREQ application policy for authentication";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/authentication/auth-domain-name
                        leaf auth-domain-name {
                            type types-sros:named-item;
                            description "Domain name appended to the username for the host";
                        }

                    } // container authentication

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/radius-accounting-policy
                    container radius-accounting-policy {
                        description "Enter the radius-accounting-policy context";

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/radius-accounting-policy/name
                        leaf name {
                            type types-sros:named-item;
                            description "RADIUS accounting policy name";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/radius-accounting-policy/duplicate
                        leaf duplicate {
                            type types-sros:named-item;
                            description "RADIUS accounting policy for generating duplicate accounting information";
                        }

                    } // container radius-accounting-policy

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/gtp-interface
                    container gtp-interface {
                        presence "Enable the gtp-interface context";
                        description "Enable the gtp-interface context";

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/gtp-interface/name
                        leaf name {
                            type types-sros:named-item;
                            mandatory true;
                            description "IP interface name";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/gtp-interface/service-id
                        leaf service-id {
                            type types-services:service-id-maf;
                            mandatory true;
                            description "Service ID";
                        }

                    } // container gtp-interface

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/identification
                    container identification {
                        presence "Enable the identification context";
                        description "Enable the identification context";

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/identification/option-number
                        leaf option-number {
                            type uint32 {
                                range "1..254";
                            }
                            sros-ext:immutable;
                            default "254";
                            description "Identification strings for the subscriber";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/identification/ancp-string
                        leaf ancp-string {
                            type string {
                                length "1..63";
                            }
                            description "ANCP string";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/identification/app-profile-string
                        leaf app-profile-string {
                            type string {
                                length "1..16";
                            }
                            description "Application profile string";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/identification/category-map-name
                        leaf category-map-name {
                            type types-sros:named-item;
                            description "Category map name";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/identification/int-dest-id
                        leaf int-dest-id {
                            type types-submgt:int-dest-id;
                            description "Intermediate destination ID";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/identification/sla-profile-string
                        leaf sla-profile-string {
                            type string {
                                length "1..32";
                            }
                            description "SLA profile string";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/identification/sub-profile-string
                        leaf sub-profile-string {
                            type string {
                                length "1..32";
                            }
                            description "Sub profile string";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/identification/subscriber-id
                        leaf subscriber-id {
                            type types-sros:named-item-64;
                            description "Subscriber ID";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/identification/spi-sharing-group-id
                        leaf spi-sharing-group-id {
                            type int32 {
                                range "0..65535";
                            }
                            description "SPI sharing group ID";
                        }

                    } // container identification

                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/match-radius-proxy-cache
                    container match-radius-proxy-cache {
                        description "Enter the match-radius-proxy-cache context";

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/match-radius-proxy-cache/fail-action
                        leaf fail-action {
                            type enumeration {
                                enum continue                       { value 1; }
                                enum drop                           { value 2; }
                            }
                            default "drop";
                            description "Action when no match is found";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/match-radius-proxy-cache/mac-format
                        leaf mac-format {
                            type types-submgt:mac-specification;
                            default "aa:";
                            description "MAC address format";
                        }

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/match-radius-proxy-cache/match
                        container match {
                            description "Enter the match context";

                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/match-radius-proxy-cache/match/format
                            choice format {
                                default "mac";
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/match-radius-proxy-cache/match/format/mac
                                case mac {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/match-radius-proxy-cache/match/format/mac/mac
                                    leaf mac {
                                        type boolean;
                                        default "true";
                                        description "Match MAC address of DHCP client";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/match-radius-proxy-cache/match/format/circuit-id
                                case circuit-id {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/match-radius-proxy-cache/match/format/circuit-id/circuit-id
                                    leaf circuit-id {
                                        type boolean;
                                        description "Match circuit ID in DHCP Option 82";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/match-radius-proxy-cache/match/format/remote-id
                                case remote-id {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/match-radius-proxy-cache/match/format/remote-id/remote-id
                                    leaf remote-id {
                                        type boolean;
                                        description "Match remote ID in DHCP Option 82";
                                    }

                                }
                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/match-radius-proxy-cache/match/format/option
                                case option {

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/match-radius-proxy-cache/match/format/option/ipv4-option
                                    leaf ipv4-option {
                                        type uint8 {
                                            range "1..254";
                                        }
                                        description "DHCP option number";
                                    }

                                    ///nokia-conf/configure/subscriber-mgmt/local-user-db/ipoe/host/match-radius-proxy-cache/match/format/option/ipv6-option
                                    leaf ipv6-option {
                                        type uint32 {
                                            range "1..65535";
