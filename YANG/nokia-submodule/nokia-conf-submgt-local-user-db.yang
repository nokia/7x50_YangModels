                        presence "Enable the prefix context";
                        description "Enable the prefix context";

                        ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/mask/prefix/prefix
                        choice prefix {
                            mandatory true;
                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/mask/prefix/prefix/length
                            case length {

                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/mask/prefix/prefix/length/length
                                leaf length {
                                    type uint32 {
                                        range "1..127";
                                    }
                                    description "PPP mask prefix length";
                                }

                            }
                            ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/mask/prefix/prefix/string
                            case string {

                                ///nokia-conf/configure/subscriber-mgmt/local-user-db/ppp/mask/prefix/prefix/string/string
                                leaf string {
                                    type string {
                                        length "1..127";
                                    }
                                    description "Prefix string";
                                }

                            }
