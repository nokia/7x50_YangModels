submodule nokia-state-mcast-mgmt {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-sros               { prefix "types-sros"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for mcast mgmt on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-mcast-mgmt YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2022-07-18";

    grouping state-mcast-mgmt-multicast-management {
        ///nokia-state/state/multicast-management
        container multicast-management {
            description "Enter the multicast-management context";

            ///nokia-state/state/multicast-management/multicast-reporting-destination
            list multicast-reporting-destination {
                key "name";
                description "Enter the multicast-reporting-destination list instance";

                ///nokia-state/state/multicast-management/multicast-reporting-destination/name
                leaf name {
                    type types-sros:named-item;
                    description "Multicast reporting destination name";
                }

                ///nokia-state/state/multicast-management/multicast-reporting-destination/statistics
                container statistics {
                    description "Enter the statistics context";

                    ///nokia-state/state/multicast-management/multicast-reporting-destination/statistics/frames-sent
                    leaf frames-sent {
                        type yang:counter32;
                        description "The number of frames sent to this multicast reporting destination.";
                    }

                    ///nokia-state/state/multicast-management/multicast-reporting-destination/statistics/frames-lost
                    leaf frames-lost {
                        type yang:counter32;
                        description "The number of frames lost for this multicast reporting destination.";
                    }

                    ///nokia-state/state/multicast-management/multicast-reporting-destination/statistics/records-sent
                    leaf records-sent {
                        type yang:counter32;
                        description "The number of records sent to this multicast reporting destination.";
                    }

                    ///nokia-state/state/multicast-management/multicast-reporting-destination/statistics/records-lost
                    leaf records-lost {
                        type yang:counter32;
                        description "The number of records lost to this multicast reporting destination.";
                    }

                } // container statistics

            } // list multicast-reporting-destination

            ///nokia-state/state/multicast-management/chassis-level
            container chassis-level {
                description "Enter the chassis-level context";

                ///nokia-state/state/multicast-management/chassis-level/mmrp-oper-mode
                leaf mmrp-oper-mode {
                    type boolean;
                    description "Enable/Disable managing MMRP traffic by IMPM.";
                }

                ///nokia-state/state/multicast-management/chassis-level/mcast-plane-total-capacity
                leaf mcast-plane-total-capacity {
                    type uint32;
                    description "The total bandwidth of the chassis forwarding plane currently in use";
                }

            } // container chassis-level

        } // container multicast-management

    } // grouping state-mcast-mgmt-multicast-management

}
