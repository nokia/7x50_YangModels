submodule nokia-state-submgt-gtp {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-cellular           { prefix "types-cellular"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-wlangw             { prefix "types-wlangw"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for subscriber management gtp on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-submgt-gtp YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2021-07-01";

    grouping state-submgt-gtp {
        ///nokia-state/state/subscriber-mgmt/gtp
        container gtp {
            description "Enter the gtp context";

            ///nokia-state/state/subscriber-mgmt/gtp/statistics
            container statistics {
                description "Enter the statistics context";

                ///nokia-state/state/subscriber-mgmt/gtp/statistics/rx-packets
                leaf rx-packets {
                    type yang:counter64;
                    description "Total number of GTP control-plane packets received.";
                }

                ///nokia-state/state/subscriber-mgmt/gtp/statistics/tx-packets
                leaf tx-packets {
                    type yang:counter64;
                    description "Total Number of GTP control-plane packets sent.";
                }

                ///nokia-state/state/subscriber-mgmt/gtp/statistics/connections
                container connections {
                    description "Enter the connections context";

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/connections/mme-peers
                    leaf mme-peers {
                        type yang:gauge32;
                        description "Number of MME peers";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/connections/enodeb-peers
                    leaf enodeb-peers {
                        type yang:gauge32;
                        description "Number of ENODE-B peers";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/connections/s11-sessions
                    leaf s11-sessions {
                        type yang:gauge32;
                        description "Number of S11 sessions";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/connections/s11-idle-sessions
                    leaf s11-idle-sessions {
                        type yang:gauge32;
                        description "Number of S11 idle sessions";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/connections/mobile-gateway-peers
                    leaf mobile-gateway-peers {
                        type yang:gauge32;
                        description "Number of mobile gateway (GGS or PGW) peerings active for GTP uplink";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/connections/uplink-sessions
                    leaf uplink-sessions {
                        type yang:gauge32;
                        description "Number of uplink sessions";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/connections/current-held-uplinks
                    leaf current-held-uplinks {
                        type yang:gauge32;
                        description "Number of uplinks in hold";
                    }

                } // container connections

                ///nokia-state/state/subscriber-mgmt/gtp/statistics/path-management
                container path-management {
                    description "Enter the path-management context";

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/path-management/tx-echo-requests
                    leaf tx-echo-requests {
                        type yang:counter64;
                        description "Number of echo requests transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/path-management/tx-echo-responses
                    leaf tx-echo-responses {
                        type yang:counter64;
                        description "Number of echo responses transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/path-management/rx-echo-requests
                    leaf rx-echo-requests {
                        type yang:counter64;
                        description "Number of echo requests received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/path-management/rx-echo-responses
                    leaf rx-echo-responses {
                        type yang:counter64;
                        description "Number of echo responses received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/path-management/path-faults
                    leaf path-faults {
                        type yang:counter64;
                        description "Number of path faults detected.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/path-management/path-restarts
                    leaf path-restarts {
                        type yang:counter64;
                        description "Number of path restarts.";
                    }

                } // container path-management

                ///nokia-state/state/subscriber-mgmt/gtp/statistics/errors
                container errors {
                    description "Enter the errors context";

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/errors/tx-errors
                    leaf tx-errors {
                        type yang:counter64;
                        description "All other transmission errors.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/errors/tx-discarded-packets
                    leaf tx-discarded-packets {
                        type yang:counter64;
                        description "Number of 'GTP version not supported' packets and 'responses with error cause' packets.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/errors/rx-errors
                    leaf rx-errors {
                        type yang:counter64;
                        description "Total number of errors during hadling received packets.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/errors/rx-version-not-supported
                    leaf rx-version-not-supported {
                        type yang:counter64;
                        description "Received GTP version not supported message.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/errors/rx-zero-teid-responses
                    leaf rx-zero-teid-responses {
                        type yang:counter64;
                        description "Received response message with TEID 0.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/errors/rx-invalid-packet-length
                    leaf rx-invalid-packet-length {
                        type yang:counter64;
                        description "Number of packets received with an invalid packet length.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/errors/rx-unknown-packets
                    leaf rx-unknown-packets {
                        type yang:counter64;
                        description "Number of packets received that could not be recognized.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/errors/rx-missing-ie-packets
                    leaf rx-missing-ie-packets {
                        type yang:counter64;
                        description "Number of packets received that contain missing IEs.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/errors/rx-bad-udp-headers
                    leaf rx-bad-udp-headers {
                        type yang:counter64;
                        description "Number of packets received with incorrect UDP headers.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/errors/rx-discarded-packets
                    leaf rx-discarded-packets {
                        type yang:counter64;
                        description "Number of discarded packets received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/errors/rx-udp-checksum-errors
                    leaf rx-udp-checksum-errors {
                        type yang:counter64;
                        description "Number of packets received with failed UDP checksums.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/errors/rx-invalid-teid
                    leaf rx-invalid-teid {
                        type yang:counter64;
                        description "Number of packets received with an invalid teid.";
                    }

                } // container errors

                ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv1
                container gtpv1 {
                    description "Enter the gtpv1 context";

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv1/tx-create-pdp-context-requests
                    leaf tx-create-pdp-context-requests {
                        type yang:counter64;
                        description "Number of PDP context requests transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv1/tx-create-pdp-context-responses
                    leaf tx-create-pdp-context-responses {
                        type yang:counter64;
                        description "Number of create PDP context responses transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv1/tx-delete-pdp-context-requests
                    leaf tx-delete-pdp-context-requests {
                        type yang:counter64;
                        description "Number of delete PDP context requests transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv1/tx-delete-pdp-context-responses
                    leaf tx-delete-pdp-context-responses {
                        type yang:counter64;
                        description "Number of delete PDP context responses transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv1/rx-create-pdp-context-requests
                    leaf rx-create-pdp-context-requests {
                        type yang:counter64;
                        description "Number of create PDP context requests received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv1/rx-create-pdp-context-responses
                    leaf rx-create-pdp-context-responses {
                        type yang:counter64;
                        description "Number of create PDP context responses received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv1/rx-delete-pdp-context-requests
                    leaf rx-delete-pdp-context-requests {
                        type yang:counter64;
                        description "Number of delete PDP context requests received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv1/rx-delete-pdp-context-responses
                    leaf rx-delete-pdp-context-responses {
                        type yang:counter64;
                        description "Number of delete PDP context responses received.";
                    }

                } // container gtpv1

                ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2
                container gtpv2 {
                    description "Enter the gtpv2 context";

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/tx-create-session-requests
                    leaf tx-create-session-requests {
                        type yang:counter64;
                        description "Number of create session requests transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/tx-create-session-responses
                    leaf tx-create-session-responses {
                        type yang:counter64;
                        description "Number of create session responses transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/tx-delete-session-requests
                    leaf tx-delete-session-requests {
                        type yang:counter64;
                        description "Number of delete session requests transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/tx-delete-session-responses
                    leaf tx-delete-session-responses {
                        type yang:counter64;
                        description "Number of delete session responses transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/tx-delete-bearer-requests
                    leaf tx-delete-bearer-requests {
                        type yang:counter64;
                        description "Number of delete bearer requests transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/tx-delete-bearer-responses
                    leaf tx-delete-bearer-responses {
                        type yang:counter64;
                        description "Number of delete bearer responses transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/tx-create-bearer-responses
                    leaf tx-create-bearer-responses {
                        type yang:counter64;
                        description "Number of create bearer responses transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/tx-update-bearer-responses
                    leaf tx-update-bearer-responses {
                        type yang:counter64;
                        description "Number of update bearer responses transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/tx-modify-bearer-requests
                    leaf tx-modify-bearer-requests {
                        type yang:counter64;
                        description "Number of modify bearer requests transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/tx-modify-bearer-responses
                    leaf tx-modify-bearer-responses {
                        type yang:counter64;
                        description "Number of modify bearer responses transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/tx-change-notification-responses
                    leaf tx-change-notification-responses {
                        type yang:counter64;
                        description "Number of change notification responses transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/tx-release-access-bearer-responses
                    leaf tx-release-access-bearer-responses {
                        type yang:counter64;
                        description "Number of release access bearer responses transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/tx-downlink-notifications
                    leaf tx-downlink-notifications {
                        type yang:counter64;
                        description "Number of downlink notifications transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/tx-stop-paging-indications
                    leaf tx-stop-paging-indications {
                        type yang:counter64;
                        description "Number of stop downlink indications transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/rx-create-session-requests
                    leaf rx-create-session-requests {
                        type yang:counter64;
                        description "Number of create session requests received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/rx-create-session-responses
                    leaf rx-create-session-responses {
                        type yang:counter64;
                        description "Number of create session responses received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/rx-delete-session-requests
                    leaf rx-delete-session-requests {
                        type yang:counter64;
                        description "Number of delete session requests received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/rx-delete-session-responses
                    leaf rx-delete-session-responses {
                        type yang:counter64;
                        description "Number of delete session responses received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/rx-delete-bearer-requests
                    leaf rx-delete-bearer-requests {
                        type yang:counter64;
                        description "Number of delete bearer requests received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/rx-delete-bearer-responses
                    leaf rx-delete-bearer-responses {
                        type yang:counter64;
                        description "Number of delete bearer responses received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/rx-create-bearer-requests
                    leaf rx-create-bearer-requests {
                        type yang:counter64;
                        description "Number of create bearer responses received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/rx-update-bearer-requests
                    leaf rx-update-bearer-requests {
                        type yang:counter64;
                        description "Number of update bearer responses received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/rx-modify-bearer-requests
                    leaf rx-modify-bearer-requests {
                        type yang:counter64;
                        description "Number of modify bearer requests received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/rx-modify-bearer-responses
                    leaf rx-modify-bearer-responses {
                        type yang:counter64;
                        description "Number of modify bearer responses received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/rx-change-notification-requests
                    leaf rx-change-notification-requests {
                        type yang:counter64;
                        description "Number of change notification requests received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/rx-release-access-bearer-requests
                    leaf rx-release-access-bearer-requests {
                        type yang:counter64;
                        description "Number of release access bearer requests received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/rx-downlink-notification-ack
                    leaf rx-downlink-notification-ack {
                        type yang:counter64;
                        description "Number of downlink notification ACK received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtpv2/rx-downlink-notification-failure
                    leaf rx-downlink-notification-failure {
                        type yang:counter64;
                        description "Number of downlink notification failures received.";
                    }

                } // container gtpv2

                ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtp-u
                container gtp-u {
                    description "Enter the gtp-u context";

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtp-u/rx-error-indications
                    leaf rx-error-indications {
                        type yang:counter64;
                        description "Number of GTP-U error indications received.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtp-u/tx-error-indications
                    leaf tx-error-indications {
                        type yang:counter64;
                        description "Number of GTP-U error indications transmitted.";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/statistics/gtp-u/tx-end-markers
                    leaf tx-end-markers {
                        type yang:counter64;
                        description "Number of end markers transmitted.";
                    }

                } // container gtp-u

            } // container statistics

            ///nokia-state/state/subscriber-mgmt/gtp/uplink
            container uplink {
                description "Enter the uplink context";

                ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context
                list pdp-context {
                    key "imsi apn";
                    description "Enter the pdp-context list instance";

                    ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/imsi
                    leaf imsi {
                        type types-cellular:imsi;
                        description "International Mobile Subscriber Identity";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/apn
                    leaf apn {
                        type types-cellular:apn;
                        description "Access point name";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/interface-type
                    leaf interface-type {
                        type types-wlangw:mobile-gateway-connection;
                        description "Interface type";
                    }

                    ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/ggsn
                    container ggsn {
                        description "Enter the ggsn context";

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/ggsn/data-address
                        leaf data-address {
                            type types-sros:ip-address;
                            description "IP address";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/ggsn/data-teid
                        leaf data-teid {
                            type uint32;
                            description "Data Transport Endpoint Identifier (TEID)";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/ggsn/control-address
                        leaf control-address {
                            type types-sros:ip-address;
                            description "Control IP address";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/ggsn/control-teid
                        leaf control-teid {
                            type uint32;
                            description "Control Transport Endpoint Identifier (TEID)";
                        }

                    } // container ggsn

                    ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/local
                    container local {
                        description "Enter the local context";

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/local/data-teid
                        leaf data-teid {
                            type uint32;
                            description "Data Transport Endpoint Identifier (TEID)";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/local/router
                        leaf router {
                            type types-sros:router-instance;
                            description "Router";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/local/control-address
                        leaf control-address {
                            type types-sros:ip-address;
                            description "Control IP address";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/local/control-teid
                        leaf control-teid {
                            type uint32;
                            description "Control Transport Endpoint Identifier (TEID)";
                        }

                    } // container local

                    ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/qos/uplink-gbr
                        leaf uplink-gbr {
                            type uint32;
                            units "kilobps";
                            description "Uplink Guaranteed Bit Rate";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/qos/uplink-mbr
                        leaf uplink-mbr {
                            type uint32;
                            units "kilobps";
                            description "Uplink Maximum Bit Rate";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/qos/downlink-gbr
                        leaf downlink-gbr {
                            type uint32;
                            units "kilobps";
                            description "Downlinklink Guaranteed Bit Rate";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/qos/downlink-mbr
                        leaf downlink-mbr {
                            type uint32;
                            units "kilobps";
                            description "Downlink Maximum Bit Rate";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/qos/qci
                        leaf qci {
                            type uint32;
                            description "QoS Class ID";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/qos/arp
                        leaf arp {
                            type uint32;
                            description "Allocation/Retention Priority";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/qos/uplink-ambr
                        leaf uplink-ambr {
                            type uint32;
                            units "kilobps";
                            description "Uplink Aggregate Maximum Bit Rate";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/qos/downlink-ambr
                        leaf downlink-ambr {
                            type uint32;
                            units "kilobps";
                            description "Downlink Aggregate Maximum Bit Rate";
                        }

                    } // container qos

                    ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/charging-characteristics
                    container charging-characteristics {
                        description "Enter the charging-characteristics context";

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/charging-characteristics/roaming
                        leaf roaming {
                            type boolean;
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/charging-characteristics/bit0
                        leaf bit0 {
                            type boolean;
                            description "Set bit in charging characteristics information element";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/charging-characteristics/bit1
                        leaf bit1 {
                            type boolean;
                            description "Set bit in charging characteristics information element";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/charging-characteristics/bit2
                        leaf bit2 {
                            type boolean;
                            description "Set bit in charging characteristics information element";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/charging-characteristics/bit3
                        leaf bit3 {
                            type boolean;
                            description "Set bit in charging characteristics information element";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/charging-characteristics/bit4
                        leaf bit4 {
                            type boolean;
                            description "Set bit in charging characteristics information element";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/charging-characteristics/bit5
                        leaf bit5 {
                            type boolean;
                            description "Set bit in charging characteristics information element";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/charging-characteristics/bit6
                        leaf bit6 {
                            type boolean;
                            description "Set bit in charging characteristics information element";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/charging-characteristics/bit7
                        leaf bit7 {
                            type boolean;
                            description "Set bit in charging characteristics information element";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/charging-characteristics/bit8
                        leaf bit8 {
                            type boolean;
                            description "Set bit in charging characteristics information element";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/charging-characteristics/bit9
                        leaf bit9 {
                            type boolean;
                            description "Set bit in charging characteristics information element";
                        }

                        ///nokia-state/state/subscriber-mgmt/gtp/uplink/pdp-context/charging-characteristics/bit10
                        leaf bit10 {
                            type boolean;
