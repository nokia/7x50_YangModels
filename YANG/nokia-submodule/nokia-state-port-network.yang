submodule nokia-state-port-network {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-sros               { prefix "types-sros"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for network port parameters on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-port-network YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-05-30";

    grouping state-port-network {
        ///nokia-state/state/port/network
        container network {
            description "Enable the network context";

            ///nokia-state/state/port/network/egress
            container egress {
                description "Enter the egress context";

                ///nokia-state/state/port/network/egress/pool
                list pool {
                    key "name";
                    description "Enter the pool list instance";

                    ///nokia-state/state/port/network/egress/pool/name
                    leaf name {
                        type types-sros:named-item;
                        description "Pool name";
                    }

                    ///nokia-state/state/port/network/egress/pool/pool-usage
                    container pool-usage {
                        description "Enter the pool-usage context";

                        ///nokia-state/state/port/network/egress/pool/pool-usage/total-pool-size
                        leaf total-pool-size {
                            type uint32;
                            units "kilobytes";
                            description "Total buffer pools size.";
                        }

                        ///nokia-state/state/port/network/egress/pool/pool-usage/total-pool-in-use
                        leaf total-pool-in-use {
                            type uint32;
                            units "kilobytes";
                            description "Total buffer pools size in use.";
                        }

                        ///nokia-state/state/port/network/egress/pool/pool-usage/shared-pool-size
                        leaf shared-pool-size {
                            type uint32;
                            units "kilobytes";
                            description "Total shared pools size.";
                        }

                        ///nokia-state/state/port/network/egress/pool/pool-usage/shared-pool-in-use
                        leaf shared-pool-in-use {
                            type uint32;
                            units "kilobytes";
                            description "Total shared pools size in use.";
                        }

                        ///nokia-state/state/port/network/egress/pool/pool-usage/reserved-pool-size
                        leaf reserved-pool-size {
                            type uint32;
                            units "kilobytes";
                            description "Total reserved pools size.";
                        }

                        ///nokia-state/state/port/network/egress/pool/pool-usage/reserved-pool-in-use
                        leaf reserved-pool-in-use {
                            type uint32;
                            units "kilobytes";
                            description "Total reserved pools size in use.";
                        }

                    } // container pool-usage


                } // list pool

                ///nokia-state/state/port/network/egress/queue
                list queue {
                    key "queue-id";
                    description "Enter the queue list instance";

                    ///nokia-state/state/port/network/egress/queue/queue-id
                    leaf queue-id {
                        type uint32 {
                            range "1..16";
                        }
                        description
                            "The identifier which uniquely identifies a network egress queue
                             for the port.";
                    }

                    ///nokia-state/state/port/network/egress/queue/statistics
                    container statistics {
                        description "Enter the statistics context";

                        ///nokia-state/state/port/network/egress/queue/statistics/in-profile-forwarded-packets
                        leaf in-profile-forwarded-packets {
                            type yang:counter64;
                            description
                                "Indicates the number of conforming network packets forwarded on the
                                 port for the queue.";
                        }

                        ///nokia-state/state/port/network/egress/queue/statistics/in-profile-forwarded-octets
                        leaf in-profile-forwarded-octets {
                            type yang:counter64;
                            description
                                "Indicates the number of conforming network octets forwarded on the
                                 port for the queue.";
                        }

                        ///nokia-state/state/port/network/egress/queue/statistics/in-profile-dropped-packets
                        leaf in-profile-dropped-packets {
                            type yang:counter64;
                            description
                                "Indicates the number of conforming network packets dropped on the
                                 port for the queue.";
                        }

                        ///nokia-state/state/port/network/egress/queue/statistics/in-profile-dropped-octets
                        leaf in-profile-dropped-octets {
                            type yang:counter64;
                            description
                                "Indicates the number of conforming network octets dropped on the
                                 port for the queue.";
                        }

                        ///nokia-state/state/port/network/egress/queue/statistics/out-profile-forwarded-packets
                        leaf out-profile-forwarded-packets {
                            type yang:counter64;
                            description
                                "Indicates the number of exceeding network packets forwarded on the
                                 port for the queue.";
                        }

                        ///nokia-state/state/port/network/egress/queue/statistics/out-profile-forwarded-octets
                        leaf out-profile-forwarded-octets {
                            type yang:counter64;
                            description
                                "Indicates the number of exceeding network octets forwarded on the
                                 port for the queue.";
                        }

                        ///nokia-state/state/port/network/egress/queue/statistics/out-profile-dropped-packets
                        leaf out-profile-dropped-packets {
                            type yang:counter64;
                            description
                                "Indicates the number of exceeding network packets dropped on the
                                 port for the queue.";
                        }

                        ///nokia-state/state/port/network/egress/queue/statistics/out-profile-dropped-octets
                        leaf out-profile-dropped-octets {
                            type yang:counter64;
                            description
                                "Indicates the number of exceeding network octets dropped on the
                                 port for the queue.";
                        }

                    } // container statistics

                    ///nokia-state/state/port/network/egress/queue/hardware-queue
                    container hardware-queue {
                        description "Enter the hardware-queue context";

                        ///nokia-state/state/port/network/egress/queue/hardware-queue/adapted-admin-mbs
                        leaf adapted-admin-mbs {
                            type types-qos:bytes;
                            units "bytes";
                            description "Admin MBS.";
                        }

                        ///nokia-state/state/port/network/egress/queue/hardware-queue/exceed-droptail
                        leaf exceed-droptail {
                            type types-qos:queue-burst-size-64-override-max;
                            description "Exceed droptail.";
                        }

                        ///nokia-state/state/port/network/egress/queue/hardware-queue/high-droptail
                        leaf high-droptail {
                            type types-qos:queue-burst-size-64-override-max;
                            description "High droptail.";
                        }

                        ///nokia-state/state/port/network/egress/queue/hardware-queue/high-plus-droptail
                        leaf high-plus-droptail {
                            type union {
                                type int64 {
                                    range "0..max";
                                }
                                type enumeration {
                                    enum auto                           { value -1; }
                                }
                            }
                            description "Highplus droptail.";
                        }

                        ///nokia-state/state/port/network/egress/queue/hardware-queue/low-droptail
                        leaf low-droptail {
                            type types-qos:queue-burst-size-64-override-max;
                            description "Low droptail.";
                        }

                        ///nokia-state/state/port/network/egress/queue/hardware-queue/operational-cbs
                        leaf operational-cbs {
                            type types-qos:bytes;
                            units "bytes";
                            description "Oper CBS.";
                        }

                        ///nokia-state/state/port/network/egress/queue/hardware-queue/operational-cir
                        leaf operational-cir {
                            type types-qos:int-max;
                            description "Oper CIR.";
                        }

                        ///nokia-state/state/port/network/egress/queue/hardware-queue/operational-mbs
                        leaf operational-mbs {
                            type types-qos:bytes;
                            units "bytes";
                            description "Oper MBS.";
                        }

                        ///nokia-state/state/port/network/egress/queue/hardware-queue/operational-pir
                        leaf operational-pir {
                            type types-qos:int-max;
                            description "Oper PIR.";
                        }

                        ///nokia-state/state/port/network/egress/queue/hardware-queue/burst-limit
                        leaf burst-limit {
                            type union {
                                type int32;
                                type enumeration {
                                    enum auto                           { value -1; }
                                }
                            }
                            description "Oper burst limit.";
                        }

                        ///nokia-state/state/port/network/egress/queue/hardware-queue/operational-exceed-burst
                        leaf operational-exceed-burst {
                            type types-qos:int64-max;
                            description "Oper burst exceed.";
                        }

                        ///nokia-state/state/port/network/egress/queue/hardware-queue/operational-max-transmit-data-size
                        leaf operational-max-transmit-data-size {
                            type types-qos:int64-max;
                            description "Oper max transmit data size.";
                        }

                        ///nokia-state/state/port/network/egress/queue/hardware-queue/operational-burst-fir
                        leaf operational-burst-fir {
                            type types-qos:int64-max;
                            description "Oper burst FIR.";
                        }

                    } // container hardware-queue

                    ///nokia-state/state/port/network/egress/queue/queue-depth
                    container queue-depth {
                        description "Enter the queue-depth context";

                        ///nokia-state/state/port/network/egress/queue/queue-depth/elapsed-time
                        leaf elapsed-time {
                            type types-sros:time-interval;
                            description "Indicated the elapsed time.";
                        }

                        ///nokia-state/state/port/network/egress/queue/queue-depth/depth-0-to-10-percent
                        leaf depth-0-to-10-percent {
                            type decimal64 {
                                fraction-digits 2;
                            }
                            description "Indicated the depth 0 to 10 percent.";
                        }

                        ///nokia-state/state/port/network/egress/queue/queue-depth/depth-11-to-20-percent
                        leaf depth-11-to-20-percent {
                            type decimal64 {
                                fraction-digits 2;
                            }
                            description "Indicated the depth 0 to 20 percent.";
                        }

                        ///nokia-state/state/port/network/egress/queue/queue-depth/depth-21-to-30-percent
                        leaf depth-21-to-30-percent {
                            type decimal64 {
                                fraction-digits 2;
                            }
                            description "Indicated the depth 0 to 30 percent.";
                        }

                        ///nokia-state/state/port/network/egress/queue/queue-depth/depth-31-to-40-percent
                        leaf depth-31-to-40-percent {
                            type decimal64 {
                                fraction-digits 2;
                            }
                            description "Indicated the depth 0 to 40 percent.";
                        }

                        ///nokia-state/state/port/network/egress/queue/queue-depth/depth-41-to-50-percent
                        leaf depth-41-to-50-percent {
                            type decimal64 {
                                fraction-digits 2;
                            }
                            description "Indicated the depth 0 to 50 percent.";
                        }

                        ///nokia-state/state/port/network/egress/queue/queue-depth/depth-51-to-60-percent
                        leaf depth-51-to-60-percent {
                            type decimal64 {
                                fraction-digits 2;
                            }
                            description "Indicated the depth 0 to 60 percent.";
                        }

                        ///nokia-state/state/port/network/egress/queue/queue-depth/depth-61-to-70-percent
                        leaf depth-61-to-70-percent {
                            type decimal64 {
                                fraction-digits 2;
                            }
                            description "Indicated the depth 0 to 70 percent.";
                        }

                        ///nokia-state/state/port/network/egress/queue/queue-depth/depth-71-to-80-percent
                        leaf depth-71-to-80-percent {
                            type decimal64 {
                                fraction-digits 2;
                            }
                            description "Indicated the depth 0 to 80 percent.";
                        }

                        ///nokia-state/state/port/network/egress/queue/queue-depth/depth-81-to-90-percent
                        leaf depth-81-to-90-percent {
                            type decimal64 {
                                fraction-digits 2;
                            }
                            description "Indicated the depth 0 to 90 percent.";
                        }

                        ///nokia-state/state/port/network/egress/queue/queue-depth/depth-91-to-100-percent
                        leaf depth-91-to-100-percent {
                            type decimal64 {
                                fraction-digits 2;
                            }
                            description "Indicated the depth 0 to 100 percent.";
                        }

                        ///nokia-state/state/port/network/egress/queue/queue-depth/queue-polling-interval
                        leaf queue-polling-interval {
                            type types-sros:time-interval;
                            description "Indicates the queue polling interval.";
                        }

                        ///nokia-state/state/port/network/egress/queue/queue-depth/last-violation
                        leaf last-violation {
                            type yang:date-and-time;
                            description "Indicates the last violation time.";
                        }

                        ///nokia-state/state/port/network/egress/queue/queue-depth/total-threshold-violations
                        leaf total-threshold-violations {
                            type yang:counter32;
                            description "Indicates the total threshold violations.";
                        }

                        ///nokia-state/state/port/network/egress/queue/queue-depth/high-water-mark-polling-interval
                        leaf high-water-mark-polling-interval {
                            type types-sros:time-duration;
                            units "microseconds";
                            description "Indicates the high water mark polling interval.";
                        }

                    } // container queue-depth

                } // list queue

            } // container egress

            ///nokia-state/state/port/network/ingress
            container ingress {
                description "Enter the ingress context";

                ///nokia-state/state/port/network/ingress/queue
                list queue {
                    key "queue-id";
                    description "Enter the queue list instance";

                    ///nokia-state/state/port/network/ingress/queue/queue-id
                    leaf queue-id {
                        type uint32 {
                            range "1..16";
                        }
                        description
                            "The identifier which uniquely identifies a network ingress queue
                             on the port.";
                    }

                    ///nokia-state/state/port/network/ingress/queue/statistics
                    container statistics {
                        description "Enter the statistics context";

                        ///nokia-state/state/port/network/ingress/queue/statistics/in-profile-forwarded-packets
                        leaf in-profile-forwarded-packets {
                            type yang:counter64;
                            description
                                "Indicates the number of conforming network packets forwarded on the
                                 port for the queue.";
                        }

                        ///nokia-state/state/port/network/ingress/queue/statistics/in-profile-forwarded-octets
                        leaf in-profile-forwarded-octets {
                            type yang:counter64;
                            description
                                "Indicates the number of conforming network octets forwarded on the
                                 port for the queue.";
                        }

                        ///nokia-state/state/port/network/ingress/queue/statistics/in-profile-dropped-packets
                        leaf in-profile-dropped-packets {
                            type yang:counter64;
                            description
                                "Indicates the number of conforming network packets dropped on the
                                 port for the queue.";
                        }

                        ///nokia-state/state/port/network/ingress/queue/statistics/in-profile-dropped-octets
                        leaf in-profile-dropped-octets {
                            type yang:counter64;
                            description
                                "Indicates the number of conforming network octets dropped on the
                                 port for the queue.";
                        }

                        ///nokia-state/state/port/network/ingress/queue/statistics/out-profile-forwarded-packets
                        leaf out-profile-forwarded-packets {
                            type yang:counter64;
                            description
                                "Indicates the number of exceeding network packets forwarded on the
                                 port for the queue.";
                        }

                        ///nokia-state/state/port/network/ingress/queue/statistics/out-profile-forwarded-octets
                        leaf out-profile-forwarded-octets {
                            type yang:counter64;
                            description
                                "Indicates the number of exceeding network octets forwarded on the
                                 port for the queue.";
                        }

                        ///nokia-state/state/port/network/ingress/queue/statistics/out-profile-dropped-packets
                        leaf out-profile-dropped-packets {
                            type yang:counter64;
                            description
                                "Indicates the number of exceeding network packets dropped on the
                                 port for the queue.";
                        }

                        ///nokia-state/state/port/network/ingress/queue/statistics/out-profile-dropped-octets
                        leaf out-profile-dropped-octets {
                            type yang:counter64;
                            description
                                "Indicates the number of exceeding network octets dropped on the
                                 port for the queue.";
                        }

                    } // container statistics

                } // list queue

            } // container ingress

        } // container network

    } // grouping state-port-network

}
