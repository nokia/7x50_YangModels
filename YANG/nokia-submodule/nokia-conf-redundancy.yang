                                description "Administrative state of the multi-chassis ring";
                            }

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/type
                            leaf type {
                                type enumeration {
                                    enum layer-2                        { value 1; }
                                    enum layer-3                        { value 2; }
                                }
                                sros-ext:immutable;
                                mandatory true;
                                description "Type of the multi-chassis ring";
                            }

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/in-band-control-path
                            container in-band-control-path {
                                description "Enter the in-band-control-path context";

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/in-band-control-path/debounce
                                leaf debounce {
                                    type boolean;
                                    default "true";
                                    description "Enable inband control path debouncing";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/in-band-control-path/max-debounce-time
                                leaf max-debounce-time {
                                    type uint32 {
                                        range "5..200";
                                    }
                                    units "seconds";
                                    default "10";
                                    description "Maximum delay after a failure when debouncing is active";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/in-band-control-path/dst-ip
                                leaf dst-ip {
                                    type types-sros:ipv4-unicast-address;
                                    description "IP address of the in-band control path peer";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/in-band-control-path/service-name
                                leaf service-name {
                                    type types-services:service-name;
                                    description "Administrative service name";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/in-band-control-path/interface
                                leaf interface {
                                    type types-sros:interface-name;
                                    description "Interface to verify in-band control path";
                                }

                            } // container in-band-control-path

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/path-b
                            container path-b {
                                when "../type = 'layer-2'" {
                                    description "path-b must be configured on a layer-2 ring";
                                }
                                description "Enable the path-b context";

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/path-b/wildcard-saps
                                leaf wildcard-saps {
                                    type boolean;
                                    default "false";
                                    description "Include the SAPs starting with a wildcard '*'";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/path-b/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/path-b/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/path-b/range
                                list range {
                                    key "start end";
                                    description "Add a list entry for range";

                                    ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/path-b/range/start
                                    leaf start {
                                        type uint32 {
                                            range "0..4094";
                                        }
                                        description "Lower bound of the VLAN range";
                                    }

                                    ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/path-b/range/end
                                    leaf end {
                                        type uint32 {
                                            range "0..4094";
                                        }
                                        description "Upper bound of the VLAN range";
                                    }

                                } // list range

                            } // container path-b

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/path-excl
                            container path-excl {
                                when "../type = 'layer-2'" {
                                    description "path-excl must be configured on a layer-2 ring";
                                }
                                description "Enter the path-excl context";

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/path-excl/wildcard-saps
                                leaf wildcard-saps {
                                    type boolean;
                                    default "false";
                                    description "Include the SAPs starting with a wildcard '*'";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/path-excl/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/path-excl/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/path-excl/range
                                list range {
                                    key "start end";
                                    description "Add a list entry for range";

                                    ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/path-excl/range/start
                                    leaf start {
                                        type uint32 {
                                            range "0..4094";
                                        }
                                        description "Lower bound of the VLAN range";
                                    }

                                    ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/path-excl/range/end
                                    leaf end {
                                        type uint32 {
                                            range "0..4094";
                                        }
                                        description "Upper bound of the VLAN range";
                                    }

                                } // list range

                            } // container path-excl

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/srrp-instance
                            list srrp-instance {
                                when "../type = 'layer-3'" {
                                    description "srrp-instance must be configured on a layer-3 ring";
                                }
                                key "id";
                                max-elements 2;
                                description "Add a list entry for srrp-instance";

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/srrp-instance/id
                                leaf id {
                                    type uint32 {
                                        range "1..4294967295";
                                    }
                                    description "SRRP instance ID";
                                }

                            } // list srrp-instance

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/ring-node
                            list ring-node {
                                key "name";
                                description "Enter the ring-node list instance";

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/name
                                leaf name {
                                    type types-sros:named-item;
                                    description "Name of the multi-chassis ring access node";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    default "disable";
                                    description "Administrative state of the ring node verification";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/interval
                                leaf interval {
                                    type uint32 {
                                        range "1..6000";
                                    }
                                    units "minutes";
                                    default "5";
                                    description "Connection verification interval";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/service-name
                                leaf service-name {
                                    type types-services:service-name;
                                    description "Administrative service name";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/dst-ip
                                leaf dst-ip {
                                    type types-sros:ipv4-unicast-address;
                                    description "Destination IP address for access node connection";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/src-ip
                                leaf src-ip {
                                    type types-sros:ipv4-unicast-address;
                                    description "Source IP address for access node connection";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/src-mac
                                leaf src-mac {
                                    type union {
                                        type enumeration {
                                            enum system-mac-address             { value 0; }
                                        }
                                        type types-mcr:ring-node-mac-address;
                                    }
                                    default "system-mac-address";
                                    description "Source MAC address for the access node connection";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/vlan
                                leaf vlan {
                                    type types-sros:ext-vlan-encap;
                                    description "VLAN ID for access node connection";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-ring/ring/ring-node/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                            } // list ring-node

                        } // list ring

                    } // container mc-ring

                } // list peer

                ///nokia-conf/configure/redundancy/multi-chassis/options
                container options {
                    description "Enter the options context";

                    ///nokia-conf/configure/redundancy/multi-chassis/options/sub-mgmt
                    container sub-mgmt {
                        description "Enable the sub-mgmt context";

                        ///nokia-conf/configure/redundancy/multi-chassis/options/sub-mgmt/dhcp-leasetime-threshold
                        leaf dhcp-leasetime-threshold {
                            type types-sros:time-duration {
                                range "0..86400";
                            }
                            description "DHCP lease threshold to skip MCS synchronization";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/options/sub-mgmt/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/options/sub-mgmt/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container sub-mgmt

                } // container options

            } // container multi-chassis

            ///nokia-conf/configure/redundancy/bgp-mh
            container bgp-mh {
                description "Enable the bgp-mh context";

                ///nokia-conf/configure/redundancy/bgp-mh/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/redundancy/bgp-mh/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/redundancy/bgp-mh/site
                container site {
                    description "Enter the site context";

                    ///nokia-conf/configure/redundancy/bgp-mh/site/boot-timer
                    leaf boot-timer {
                        type int32 {
                            range "0..600";
                        }
                        units "seconds";
                        default "10";
                        description "Wait time after reboot to run the DF election algorithm";
                    }

                    ///nokia-conf/configure/redundancy/bgp-mh/site/activation-timer
                    leaf activation-timer {
                        type int32 {
                            range "0..100";
                        }
                        units "seconds";
                        default "2";
                        description "Time to wait for BGP updates from remote PEs";
                    }

                    ///nokia-conf/configure/redundancy/bgp-mh/site/min-down-timer
                    leaf min-down-timer {
                        type int32 {
                            range "1..100";
                        }
                        units "seconds";
                        description "Min downtime for BGP multi-homing site after transition";
                    }

                } // container site

            } // container bgp-mh

            ///nokia-conf/configure/redundancy/bgp-evpn
            container bgp-evpn {
                description "Enter the bgp-evpn context";

                ///nokia-conf/configure/redundancy/bgp-evpn/ethernet-segment
                container ethernet-segment {
                    description "Enter the ethernet-segment context";

                    ///nokia-conf/configure/redundancy/bgp-evpn/ethernet-segment/boot-timer
                    leaf boot-timer {
                        type uint32 {
                            range "0..1800";
                        }
                        units "seconds";
                        default "10";
                        description "Time before BGP EVPN multi-homing DF election algorithm";
                    }

                    ///nokia-conf/configure/redundancy/bgp-evpn/ethernet-segment/activation-timer
                    leaf activation-timer {
                        type uint32 {
                            range "0..100";
                        }
                        units "seconds";
                        default "3";
                        description "Time before Ethernet segment activated on DF-elected PE";
                    }

                    ///nokia-conf/configure/redundancy/bgp-evpn/ethernet-segment/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/redundancy/bgp-evpn/ethernet-segment/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container ethernet-segment

            } // container bgp-evpn

        } // container redundancy

    } // grouping conf-redundancy

}
