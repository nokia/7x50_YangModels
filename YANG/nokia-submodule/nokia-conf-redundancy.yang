submodule nokia-conf-redundancy {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-l2tp               { prefix "types-l2tp"; }
    import nokia-types-mcr                { prefix "types-mcr"; }
    import nokia-types-redundancy         { prefix "types-redundancy"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-aaa;
    include nokia-conf-groups;
    include nokia-conf-isa;
    include nokia-conf-lag;
    include nokia-conf-port;
    include nokia-conf-pw-port;
    include nokia-conf-system;
    include nokia-conf-system-security;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure redundancy configuration on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-redundancy YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-01-09";

    grouping conf-redundancy {
        ///nokia-conf/configure/redundancy
        container redundancy {
            description "Enable the redundancy context";

            ///nokia-conf/configure/redundancy/cert-sync
            leaf cert-sync {
                type boolean;
                default "true";
                description "Automatically synchronize certificate files";
            }

            ///nokia-conf/configure/redundancy/rollback-sync
            leaf rollback-sync {
                type enumeration {
                    enum rollback-single                { value 2; }
                    enum rollback-all                   { value 3; }
                }
                description "Parameters for redundancy rollback synchronization";
            }

            ///nokia-conf/configure/redundancy/synchronize
            leaf synchronize {
                type enumeration {
                    enum off                            { value 1; }
                    enum config                         { value 2; }
                    enum boot-env                       { value 3; }
                }
                description "Parameters for redundancy synchronization";
            }

            ///nokia-conf/configure/redundancy/switchover-exec
            leaf switchover-exec {
                type types-sros:url {
                    length "1..100";
                }
                description "CLI script file executed after a redundancy switchover";
            }

            ///nokia-conf/configure/redundancy/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/redundancy/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/redundancy/mgmt-ethernet
            container mgmt-ethernet {
                presence "Enable the mgmt-ethernet context";
                description "Enable the mgmt-ethernet context";

                ///nokia-conf/configure/redundancy/mgmt-ethernet/revert
                leaf revert {
                    type uint32 {
                        range "1..300";
                    }
                    units "seconds";
                    default "5";
                    description "Time before reverting standby CPM management Ethernet port to active CPM";
                }

            } // container mgmt-ethernet

            ///nokia-conf/configure/redundancy/srrp
            container srrp {
                description "Enable the srrp context";

                ///nokia-conf/configure/redundancy/srrp/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/redundancy/srrp/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/redundancy/srrp/auto-srrp-id-range
                container auto-srrp-id-range {
                    presence "Enable the auto-srrp-id-range context";
                    description "Enable the auto-srrp-id-range context";

                    ///nokia-conf/configure/redundancy/srrp/auto-srrp-id-range/start
                    leaf start {
                        type uint32 {
                            range "1..4294967294";
                        }
                        default "2147483648";
                        description "Lower bound of the ID range";
                    }

                    ///nokia-conf/configure/redundancy/srrp/auto-srrp-id-range/end
                    leaf end {
                        type uint32 {
                            range "2..4294967295";
                        }
                        default "4294967295";
                        description "Upper bound of the ID range";
                    }

                } // container auto-srrp-id-range

            } // container srrp

            ///nokia-conf/configure/redundancy/multi-chassis
            container multi-chassis {
                description "Enter the multi-chassis context";

                ///nokia-conf/configure/redundancy/multi-chassis/ipsec-domain
                list ipsec-domain {
                    key "id";
                    max-elements 64;
                    description "Enter the ipsec-domain list instance";

                    ///nokia-conf/configure/redundancy/multi-chassis/ipsec-domain/id
                    leaf id {
                        type uint32 {
                            range "1..255";
                        }
                        description "IPsec domain ID";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/ipsec-domain/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the IPsec domain";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/ipsec-domain/designated-role
                    leaf designated-role {
                        type enumeration {
                            enum standby                        { value 1; }
                            enum active                         { value 2; }
                        }
                        sros-ext:auto-restart-to-modify;
                        default "standby";
                        description "Designated role for the IPsec domain";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/ipsec-domain/priority
                    leaf priority {
                        type uint32 {
                            range "0..255";
                        }
                        default "100";
                        description "Priority for the IPsec domain";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/ipsec-domain/revertive
                    leaf revertive {
                        type boolean;
                        sros-ext:auto-restart-to-modify;
                        default "false";
                        description "Enable revertive behavior for the tunnel group";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/ipsec-domain/tunnel-group
                    leaf tunnel-group {
                        type leafref {
                            path "../../../../isa/tunnel-group/id";
                        }
                        sros-ext:auto-restart-to-modify;
                        description "Tunnel group ID for an IPsec domain";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/ipsec-domain/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/ipsec-domain/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list ipsec-domain

                ///nokia-conf/configure/redundancy/multi-chassis/peer
                list peer {
                    key "ip-address";
                    max-elements 20;
                    description "Enter the peer list instance";

                    ///nokia-conf/configure/redundancy/multi-chassis/peer/ip-address
                    leaf ip-address {
                        type types-sros:ip-unicast-address;
                        description "Peer IP address";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/peer/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the MC redundancy peer";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/peer/authentication-key
                    leaf authentication-key {
                        type types-sros:encrypted-leaf {
                            length "1..54";
                        }
                        sros-ext:auto-restart-to-modify;
                        description "Authentication key used between the node and MC peer";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/peer/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/peer/peer-name
                    leaf peer-name {
                        type types-sros:named-item;
                        description "Multi-chassis peer name";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/peer/source-address
                    leaf source-address {
                        type types-sros:ip-unicast-address;
                        sros-ext:auto-restart-to-modify;
                        description "Source address to communicate with the MC peer";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/peer/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/peer/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-endpoint
                    container mc-endpoint {
                        presence "Enable the mc-endpoint context";
                        description "Enable the mc-endpoint context";

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-endpoint/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the MC-EP";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-endpoint/system-priority
                        leaf system-priority {
                            type uint32 {
                                range "0..255";
                            }
                            default "0";
                            description "System priority of the MC-EP";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-endpoint/keep-alive-interval
                        leaf keep-alive-interval {
                            type uint32 {
                                range "5..500";
                            }
                            default "10";
                            description "Interval for exchange of keepalive messages";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-endpoint/hold-on-neighbor-failure
                        leaf hold-on-neighbor-failure {
                            type uint32 {
                                range "2..25";
                            }
                            default "3";
                            description "Number of keepalive intervals to wait for packets";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-endpoint/boot-timer
                        leaf boot-timer {
                            type uint32 {
                                range "1..600";
                            }
                            units "seconds";
                            default "300";
                            description "Time to attempt connection before declaring failure";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-endpoint/passive-mode
                        leaf passive-mode {
                            type boolean;
                            default "false";
                            description "Enable passive mode for the MC-EP tunnel";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-endpoint/bfd
                        leaf bfd {
                            type boolean;
                            default "false";
                            description "Enable BFD detection for the MC-EP peering tunnel";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-endpoint/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/mc-endpoint/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container mc-endpoint

                    ///nokia-conf/configure/redundancy/multi-chassis/peer/sync
                    container sync {
                        presence "Enable the sync context";
                        description "Enable the sync context";

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the peer synchronization";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/diameter-proxy
                        leaf diameter-proxy {
                            status obsolete;
                            type boolean;
                            default "false";
                            description "Synchronize the Diameter proxy data with the multi-chassis peer";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/igmp
                        leaf igmp {
                            type boolean;
                            default "false";
                            description "Synchronize IGMP protocol information with the MC peer";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/igmp-snooping
                        leaf igmp-snooping {
                            type boolean;
                            default "false";
                            description "Synchronize IGMP snooping information";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/ipsec
                        leaf ipsec {
                            type boolean;
                            default "false";
                            description "Synchronize IPsec information";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/l2tp
                        leaf l2tp {
                            type boolean;
                            default "false";
                            description "Synchronize Layer Two Tunnel Protocol (L2TP)";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/local-dhcp-server
                        leaf local-dhcp-server {
                            type boolean;
                            default "false";
                            description "Synchronize DHCP Server information";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/mc-ring
                        leaf mc-ring {
                            type boolean;
                            default "false";
                            description "Synchronize ring information";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/mld
                        leaf mld {
                            type boolean;
                            default "false";
                            description "Synchronize MLD protocol information";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/mld-snooping
                        leaf mld-snooping {
                            type boolean;
                            default "false";
                            description "Synchronize MLD snooping information";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/python
                        leaf python {
                            type boolean;
                            default "false";
                            description "Synchronize Python";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/srrp
                        leaf srrp {
                            type boolean;
                            default "false";
                            description "Synchronize SRRP information";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/sub-host-trk
                        leaf sub-host-trk {
                            type boolean;
                            default "false";
                            description "Synchronize subscriber host tracking information";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/nat
                        container nat {
                            presence "Enable the nat context";
                            description "Enable the nat context";

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/nat/nat-group
                            list nat-group {
                                key "id";
                                description "Enter the nat-group list instance";

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/nat/nat-group/id
                                leaf id {
                                    type leafref {
                                        path "../../../../../../../isa/nat-group/id";
                                    }
                                    description "NAT group ID";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/nat/nat-group/sync-tag
                                leaf sync-tag {
                                    type types-sros:named-item;
                                    sros-ext:immutable;
                                    mandatory true;
                                    description "NAT group synchronization tag";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/nat/nat-group/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/nat/nat-group/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                            } // list nat-group

                        } // container nat

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/pim-snooping
                        container pim-snooping {
                            description "Enter the pim-snooping context";

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/pim-snooping/saps
                            leaf saps {
                                type boolean;
                                default "false";
                                description "Synchronize PIM snooping data with the multi-chassis peer on SDP endpoints";
                            }

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/pim-snooping/spoke-sdps
                            leaf spoke-sdps {
                                type boolean;
                                default "false";
                                description "Synchronize spoke SDPs with the multi-chassis peer";
                            }

                        } // container pim-snooping

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/sub-mgmt
                        container sub-mgmt {
                            description "Enter the sub-mgmt context";

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/sub-mgmt/ipoe
                            leaf ipoe {
                                type boolean;
                                default "false";
                                description "Synchronize subscriber management IPoE information";
                            }

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/sub-mgmt/pppoe
                            leaf pppoe {
                                type boolean;
                                default "false";
                                description "Synchronize subscriber management PPPoE information";
                            }

                        } // container sub-mgmt

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/transport-encryption
                        container transport-encryption {
                            description "Enter the transport-encryption context";

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/transport-encryption/application
                            list application {
                                key "application-id";
                                description "Enter the application list instance";

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/transport-encryption/application/application-id
                                leaf application-id {
                                    type types-redundancy:mc-application;
                                    description "Application using transport encryption";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/transport-encryption/application/keychain
                                leaf keychain {
                                    type leafref {
                                        path "../../../../../../../system/security/keychains/keychain/keychain-name";
                                    }
                                    mandatory true;
                                    description "Keychain containing the authentication keys";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/transport-encryption/application/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/transport-encryption/application/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                            } // list application

                        } // container transport-encryption

                        ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags
                        container tags {
                            description "Enter the tags context";

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/port
                            list port {
                                key "id";
                                description "Enter the port list instance";

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/port/id
                                leaf id {
                                    type leafref {
                                        path "../../../../../../../port/port-id";
                                    }
                                    description "ID of port to be synchronized with multi-chassis peer";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/port/sync-tag
                                leaf sync-tag {
                                    type types-sros:named-item;
                                    sros-ext:immutable;
                                    description "Synchronization tag";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/port/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/port/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/port/range
                                list range {
                                    key "start end";
                                    description "Enter the range list instance";

                                    ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/port/range/start
                                    leaf start {
                                        type types-sros:vlan-encap;
                                        description "First encapsulation value";
                                    }

                                    ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/port/range/end
                                    leaf end {
                                        type types-sros:vlan-encap;
                                        description "Last encapsulation value";
                                    }

                                    ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/port/range/sync-tag
                                    leaf sync-tag {
                                        type types-sros:named-item;
                                        sros-ext:immutable;
                                        mandatory true;
                                        description "Synchronization tag";
                                    }

                                    ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/port/range/apply-groups
                                    leaf-list apply-groups {
                                        type leafref {
                                            path "../../../../../../../../groups/group/name";
                                        }
                                        max-elements 8;
                                        ordered-by user;
                                        description "Apply a configuration group at this level";
                                    }

                                    ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/port/range/apply-groups-exclude
                                    leaf-list apply-groups-exclude {
                                        type leafref {
                                            path "../../../../../../../../groups/group/name";
                                        }
                                        max-elements 8;
                                        ordered-by user;
                                        description "Exclude a configuration group at this level";
                                    }

                                } // list range

                            } // list port

                            ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/lag
                            list lag {
                                key "lag-name";
                                description "Enter the lag list instance";

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/lag/lag-name
                                leaf lag-name {
                                    type leafref {
                                        path "../../../../../../../lag/lag-name";
                                    }
                                    description "LAG name";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/lag/sync-tag
                                leaf sync-tag {
                                    type types-sros:named-item;
                                    sros-ext:immutable;
                                    description "Synchronization tag";
                                }

                                ///nokia-conf/configure/redundancy/multi-chassis/peer/sync/tags/lag/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

