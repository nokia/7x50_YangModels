                            bit admin-locked                   { position 29; }
                            bit evpn-p2mp-conflict             { position 30; }
                            bit label-stack-limit-exceeded     { position 31; }
                            bit vccv-bfd-down                  { position 32; }
                            bit stitching-svc-pw-fault         { position 33; }
                        }
                        description
                            "This object indicates all the conditions that affect the
                             operating status of this SDP Bind.";
                    }

                    ///nokia-state/state/mirror/mirror-dest/spoke-sdp/ingress
                    container ingress {
                        description "Enter the ingress context";

                        ///nokia-state/state/mirror/mirror-dest/spoke-sdp/ingress/oper-label
                        leaf oper-label {
                            type uint32;
                            description "MPLS service label used by the far-end device to send packets to the local PEs sdp-bind.";
                        }

                    } // container ingress

                    ///nokia-state/state/mirror/mirror-dest/spoke-sdp/egress
                    container egress {
                        description "Enter the egress context";

                        ///nokia-state/state/mirror/mirror-dest/spoke-sdp/egress/oper-label
                        leaf oper-label {
                            type uint32;
                            description "MPLS service label used to send packets to the far-end device via this sdp-bind.";
                        }

                    } // container egress

                } // list spoke-sdp

                ///nokia-state/state/mirror/mirror-dest/pcap
                list pcap {
                    key "session-name";
                    description "Enter the pcap list instance";

                    ///nokia-state/state/mirror/mirror-dest/pcap/session-name
                    leaf session-name {
                        type types-sros:named-item;
                        description "PCAP session name";
                    }

                    ///nokia-state/state/mirror/mirror-dest/pcap/session-state
                    leaf session-state {
                        type enumeration {
                            enum failed                         { value 0; }
                            enum init                           { value 1; }
                            enum ready                          { value 2; }
                            enum start                          { value 3; }
                            enum in-progress                    { value 4; }
                            enum stopped                        { value 5; }
                            enum file-error                     { value 6; }
                            enum buffer-full                    { value 7; }
                            enum buffer-high-watermark          { value 8; }
                        }
                        description "The capture state of the PCAP session.";
                    }

                    ///nokia-state/state/mirror/mirror-dest/pcap/buffer-size
                    leaf buffer-size {
                        type uint32;
                        units "bytes";
                        description "The total buffer size, in bytes, currently in use by the PCAP session.";
                    }

                    ///nokia-state/state/mirror/mirror-dest/pcap/file-size
                    leaf file-size {
                        type uint32;
                        units "bytes";
                        description "The total number of bytes that have been written to the file specified by file-url.";
                    }

                    ///nokia-state/state/mirror/mirror-dest/pcap/last-file-write-time
                    leaf last-file-write-time {
                        type types-sros:date-time;
                        description "The last time the file specified by file-url was written to.";
                    }

                    ///nokia-state/state/mirror/mirror-dest/pcap/buffer-write-failures
                    leaf buffer-write-failures {
                        type uint32;
                        description "The total number of times that the PCAP session has failed to write to its buffer.";
                    }

                    ///nokia-state/state/mirror/mirror-dest/pcap/buffer-read-failures
                    leaf buffer-read-failures {
                        type uint32;
                        description "The total number of times that the PCAP session has failed to read its buffer.";
                    }

                    ///nokia-state/state/mirror/mirror-dest/pcap/dropped-packets
                    leaf dropped-packets {
                        type uint32;
                        units "packets";
                        description "The total number of packets that were dropped by this PCAP session.";
                    }

                    ///nokia-state/state/mirror/mirror-dest/pcap/processing-time-bailouts
                    leaf processing-time-bailouts {
                        type uint32;
                        description "The total number of processing time bailouts that have occurred for the PCAP session.";
                    }

                    ///nokia-state/state/mirror/mirror-dest/pcap/application-id-ixr
                    leaf application-id-ixr {
                        type uint32 {
                            range "0..255";
                        }
                        description "The application ID for the IXR PCAP session.";
                    }

                    ///nokia-state/state/mirror/mirror-dest/pcap/captured-packets
                    leaf captured-packets {
                        type uint32;
                        units "packets";
                        description "The total number of packets that were captured by this PCAP session.";
                    }

                } // list pcap

            } // list mirror-dest

            ///nokia-state/state/mirror/mirror-source
            list mirror-source {
                key "service-name";
                description "Enter the mirror-source list instance";

                ///nokia-state/state/mirror/mirror-source/service-name
                leaf service-name {
                    type types-services:service-name;
                    description "Administrative service name";
                }

                ///nokia-state/state/mirror/mirror-source/creation-origin
                leaf creation-origin {
                    type enumeration {
                        enum debug                          { value 1; }
                        enum li                             { value 2; }
                        enum config                         { value 3; }
                    }
                    description "The protocol or mechanism that created this mirror-source entry";
                }

                ///nokia-state/state/mirror/mirror-source/sap-shim
                list sap-shim {
                    key "sap-id";
                    description "Enter the sap-shim list instance";

                    ///nokia-state/state/mirror/mirror-source/sap-shim/sap-id
                    leaf sap-id {
                        type types-sros:sap;
                        description "SAP ID";
                    }

                    ///nokia-state/state/mirror/mirror-source/sap-shim/sap-shim-id
                    leaf sap-shim-id {
                        type uint32;
                        description "The shim-id for mirror-source SAP traffic collectors";
                    }

                } // list sap-shim

                ///nokia-state/state/mirror/mirror-source/statistics
                container statistics {
                    description "Enter the statistics context";

                    ///nokia-state/state/mirror/mirror-source/statistics/total-sources
                    leaf total-sources {
                        type uint32;
                        description "The total number of sources configured under this mirror-source entry";
                    }

                } // container statistics

            } // list mirror-source

        } // container mirror

    } // grouping state-mirror

}
