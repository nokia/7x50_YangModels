                        description
                            "If a static-route hold timer is configured and this is non-zero, it indicates
                             the static-route is being held from becoming active and is the remaining time
                             in seconds for static route to become active.";
                    }

                    ///nokia-state/state/router/static-routes/route/indirect/backup-status
                    leaf backup-status {
                        type enumeration {
                            enum inactive                       { value 1; }
                            enum active-as-backup               { value 2; }
                            enum active-as-primary              { value 3; }
                        }
                        description "Indicates the backup route status of the static route entry.";
                    }

                    ///nokia-state/state/router/static-routes/route/indirect/cpe-check
                    list cpe-check {
                        key "address";
                        description "Enter the cpe-check list instance";

                        ///nokia-state/state/router/static-routes/route/indirect/cpe-check/address
                        leaf address {
                            type types-sros:ip-address;
                            description "IP address of the target CPE device";
                        }

                        ///nokia-state/state/router/static-routes/route/indirect/cpe-check/up-time
                        leaf up-time {
                            type yang:timeticks;
                            description
                                "Indicates how long (in centiseconds) the CPE has been
                                 available.";
                        }

                        ///nokia-state/state/router/static-routes/route/indirect/cpe-check/in-packet-count
                        leaf in-packet-count {
                            type yang:counter32;
                            description "Indicates the number of echo-reply packets received.";
                        }

                        ///nokia-state/state/router/static-routes/route/indirect/cpe-check/out-packet-count
                        leaf out-packet-count {
                            type yang:counter32;
                            description "Indicates the number of echo-request packets sent.";
                        }

                        ///nokia-state/state/router/static-routes/route/indirect/cpe-check/down-transition
                        leaf down-transition {
                            type yang:gauge32;
                            description
                                "Indicates the number of times the CPE has transitioned
                                 to the unavailable state.";
                        }

                        ///nokia-state/state/router/static-routes/route/indirect/cpe-check/up-transition
                        leaf up-transition {
                            type yang:gauge32;
                            description
                                "Indicates the number of times the CPE has transitioned
                                 to the available state.";
                        }

                        ///nokia-state/state/router/static-routes/route/indirect/cpe-check/ttl
                        leaf ttl {
                            type uint32;
                            units "seconds";
                            description "Indicates the time, in seconds, before the CPE will be declared down.";
                        }

                    } // list cpe-check

                } // list indirect

                ///nokia-state/state/router/static-routes/route/interface
                list interface {
                    key "interface-name";
                    description "Enter the interface list instance";

                    ///nokia-state/state/router/static-routes/route/interface/interface-name
                    leaf interface-name {
                        type types-sros:interface-name;
                        description "Router interface name";
                    }

                    ///nokia-state/state/router/static-routes/route/interface/last-enabled-time
                    leaf last-enabled-time {
                        type types-sros:date-time;
                        description
                            "Indicates the time when 'status' was last set to active
                             to allow IP forwarding on this static route entry.";
                    }

                    ///nokia-state/state/router/static-routes/route/interface/status
                    leaf status {
