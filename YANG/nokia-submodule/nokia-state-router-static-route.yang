    grouping state-router-static-route-static-routes {
        ///nokia-state/state/router/static-routes
        container static-routes {
            description "Enter the static-routes context";

            ///nokia-state/state/router/static-routes/route
            list route {
                key "ip-prefix route-type";
                description "Enter the route list instance";

                ///nokia-state/state/router/static-routes/route/ip-prefix
                leaf ip-prefix {
                    type types-sros:ip-prefix;
                    description "IP prefix and prefix length for the static routes";
                }

                ///nokia-state/state/router/static-routes/route/route-type
                leaf route-type {
                    type types-router:unicast-multicast;
                    description "Static route type for unicast or multicast RPF";
                }

                ///nokia-state/state/router/static-routes/route/creation-origin
                leaf creation-origin {
                    type types-sros:creation-origin;
                    description "Indicates the mechanism used to create this entry.";
                }

                ///nokia-state/state/router/static-routes/route/next-hops-count
                leaf next-hops-count {
                    type yang:counter32;
                    description "The number of next-hops configured under this prefix row entry.";
                }

                ///nokia-state/state/router/static-routes/route/blackhole
                container blackhole {
                    description "Enable the blackhole context";

                    ///nokia-state/state/router/static-routes/route/blackhole/last-enabled-time
                    leaf last-enabled-time {
                        type types-sros:date-time;
                        description
                            "Indicates the time when 'status' was last set to active
                             to allow IP forwarding on this static route entry.";
                    }

                    ///nokia-state/state/router/static-routes/route/blackhole/status
                    leaf status {
                        type enumeration {
                            enum active                         { value 1; }
                            enum inactive                       { value 2; }
                        }
                        description "Indicates whether or not this route entry is active in the system.";
                    }

                    ///nokia-state/state/router/static-routes/route/blackhole/egress-interface-index
                    leaf egress-interface-index {
                        type int32 {
                            range "0..max";
                        }
                        description
                            "Indicates the outgoing interface index for this route as computed by the
                             static-route module.";
                    }

                    ///nokia-state/state/router/static-routes/route/blackhole/creation-origin
                    leaf creation-origin {
                        type types-sros:creation-origin;
                        description "Indicates the mechanism used to create this entry.";
                    }

                    ///nokia-state/state/router/static-routes/route/blackhole/dynamic-bgp-next-hop
                    leaf dynamic-bgp-next-hop {
                        type types-sros:ip-address;
                        description
                            "This is the next-hop of the BGP prefix that matches the static-route prefix in
                             a blackhole static-route. The static-route is programmed in the route-table
                             with this next-hop, if present.";
                    }

                    ///nokia-state/state/router/static-routes/route/blackhole/hold-time-left
                    leaf hold-time-left {
                        type uint32;
                        description
                            "If a static-route hold timer is configured and this is non-zero, it indicates
                             the static-route is being held from becoming active and is the remaining time
                             in seconds for static route to become active.";
                    }

                    ///nokia-state/state/router/static-routes/route/blackhole/backup-status
                    leaf backup-status {
                        type enumeration {
                            enum inactive                       { value 1; }
                            enum active-as-backup               { value 2; }
                            enum active-as-primary              { value 3; }
                        }
                        description "Indicates the backup route status of the static route entry.";
                    }

                } // container blackhole

                ///nokia-state/state/router/static-routes/route/indirect
                list indirect {
                    key "ip-address";
                    description "Enter the indirect list instance";

                    ///nokia-state/state/router/static-routes/route/indirect/ip-address
                    leaf ip-address {
                        type types-sros:ip-address;
                        description "Next-hop IP address used to reach the destination";
                    }

                    ///nokia-state/state/router/static-routes/route/indirect/last-enabled-time
                    leaf last-enabled-time {
                        type types-sros:date-time;
