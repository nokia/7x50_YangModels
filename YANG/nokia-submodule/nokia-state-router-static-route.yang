                        leaf out-packet-count {
                            type yang:counter32;
                            description "Indicates the number of echo-request packets sent.";
                        }

                        ///nokia-state/state/router/static-routes/route/interface/cpe-check/down-transition
                        leaf down-transition {
                            type yang:gauge32;
                            description
                                "Indicates the number of times the CPE has transitioned
                                 to the unavailable state.";
                        }

                        ///nokia-state/state/router/static-routes/route/interface/cpe-check/up-transition
                        leaf up-transition {
                            type yang:gauge32;
                            description
                                "Indicates the number of times the CPE has transitioned
                                 to the available state.";
                        }

                        ///nokia-state/state/router/static-routes/route/interface/cpe-check/ttl
                        leaf ttl {
                            type uint32;
                            units "seconds";
                            description "Indicates the time, in seconds, before the CPE will be declared down.";
                        }

                    } // list cpe-check

                } // list interface

                ///nokia-state/state/router/static-routes/route/ipsec-tunnel
                list ipsec-tunnel {
                    key "ipsec-tunnel-name";
                    description "Enter the ipsec-tunnel list instance";

                    ///nokia-state/state/router/static-routes/route/ipsec-tunnel/ipsec-tunnel-name
                    leaf ipsec-tunnel-name {
                        type types-sros:named-item;
                        description "IPsec tunnel name.";
                    }

                    ///nokia-state/state/router/static-routes/route/ipsec-tunnel/last-enabled-time
                    leaf last-enabled-time {
                        type types-sros:date-time;
                        description
                            "Indicates the time when 'status' was last set to active
                             to allow IP forwarding on this static route entry.";
                    }

                    ///nokia-state/state/router/static-routes/route/ipsec-tunnel/status
                    leaf status {
                        type enumeration {
                            enum active                         { value 1; }
                            enum inactive                       { value 2; }
                        }
                        description "Indicates whether or not this route entry is active in the system.";
