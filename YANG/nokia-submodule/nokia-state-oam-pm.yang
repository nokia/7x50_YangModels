submodule nokia-state-oam-pm {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-oam                { prefix "types-oam"; }
    import nokia-types-sros               { prefix "types-sros"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for OAM-PM configuration on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-oam-pm YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-06-16";

    grouping state-oam-pm {
        ///nokia-state/state/oam-pm
        container oam-pm {
            description "Enable the oam-pm context";

            ///nokia-state/state/oam-pm/session
            list session {
                key "session-name";
                description "Enter the session list instance";

                ///nokia-state/state/oam-pm/session/session-name
                leaf session-name {
                    type types-sros:named-item;
                    description "OAM-PM session name";
                }

                ///nokia-state/state/oam-pm/session/test-family-choice
                choice test-family-choice {
                    mandatory true;
                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case
                    case ethernet-case {

                        ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet
                        container ethernet {
                            description "Enable the ethernet context";

                            ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm
                            container dmm {
                                description "Enable the dmm context";

                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/oper-state
                                leaf oper-state {
                                    type enumeration {
                                        enum up                             { value 1; }
                                        enum down                           { value 2; }
                                    }
                                    description "The operational state of OAM-PM tests for the session.";
                                }

                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/detected-tx-error
                                leaf detected-tx-error {
                                    type types-oam:oam-pm-session-test-detected-tx-error;
                                    description
                                        "Reason for the specified test's current inability (if any) to launch request frames.

                                         For example, 'eth-parent-admin-down(4)' could be returned for an SLM test
                                         if the MEP to be tested is associated with a SAP which is
                                         administratively down.

                                         Not all request frame transmit failures are detected by this mechanism.
                                         It is possible that 'none' will be returned when the test is unable
                                         to transmit frames.";
                                }

                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/delay-events
                                list delay-events {
                                    key "bin-metric direction";
                                    description "Enter the delay-events list instance";

                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/delay-events/bin-metric
                                    leaf bin-metric {
                                        type enumeration {
                                            enum fd                             { value 1; }
                                            enum fdr                            { value 2; }
                                            enum ifdv                           { value 3; }
                                        }
                                        description "Delay metric bin structure";
                                    }

                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/delay-events/direction
                                    leaf direction {
                                        type types-oam:direction;
                                        description "Traffic flow direction of the OAM-PM test or metric";
                                    }

                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/delay-events/last-tca-time
                                    leaf last-tca-time {
                                        type yang:date-and-time;
                                        description
                                            "UTC date and time at the start of the measurement interval which generated
                                             the most recent delay-based Raise or Clear TCA for the specified session,
                                             test-type, bin-type and direction.";
                                    }

                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/delay-events/oper-state
                                    leaf oper-state {
                                        type enumeration {
                                            enum pending                        { value 1; }
                                            enum active                         { value 2; }
                                            enum not-active                     { value 3; }
                                        }
                                        description
                                            "Operational state of the Threshold Crossing Alerts (TCAs) for the specified session,
                                             test-type, bin-type and direction.

                                             The code points are as follows.  The conditions are evaluated in the
                                             order shown.
                                             pending
                                                is returned if the threshold configuration for the
                                                specified indices changed during the current measurement interval.
                                             active
                                                is returned if the specified indices have a default clear threshold configured,
                                                and the current measurement interval has generated a Raise TCA.
                                                is returned if the specified indices have a non-default clear threshold
                                                configured, and the most recent TCA generated was a Raise.
                                             not-active
                                                is returned for all other conditions (e.g. thresholding is not
                                                configured for the specified indices).";
                                    }

                                } // list delay-events

                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics
                                container statistics {
                                    description "Enter the statistics context";

                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval
                                    list measurement-interval {
                                        key "duration";
                                        description "Enter the measurement-interval list instance";

                                        ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/duration
                                        leaf duration {
                                            type types-oam:statistics-measurement-interval-duration;
                                            description "The duration of the measurement interval";
                                        }

                                        ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/newest-index
                                        leaf newest-index {
                                            type uint32 {
                                                range "1..max";
                                            }
                                            description "The interval number of the newest interval for the specified session and test type";
                                        }

                                        ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number
                                        list number {
                                            key "mi-number";
                                            description "Enter the number list instance";

                                            ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/mi-number
                                            leaf mi-number {
                                                type uint32 {
                                                    range "1..max";
                                                }
                                                description "Measurement interval number (the most recent interval is newest-index)";
                                            }

                                            ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/oper-state
                                            leaf oper-state {
                                                type enumeration {
                                                    enum in-progress                    { value 1; }
                                                    enum completed                      { value 2; }
                                                }
                                                description "Operational state of the specified measurement interval data set";
                                            }

                                            ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/suspect
                                            leaf suspect {
                                                type boolean;
                                                description "Indicates whether or not the specified measurement interval data set contains suspect information";
                                            }

                                            ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/start-time
                                            leaf start-time {
                                                type yang:date-and-time;
                                                description "Start time (in UTC) of the specified measurement interval data set";
                                            }

                                            ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/elapsed-time
                                            leaf elapsed-time {
                                                type uint32;
                                                units "seconds";
                                                description "Time elapsed since data collection started for the specified measurement interval data set";
                                            }

                                            ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/frames-sent
                                            leaf frames-sent {
                                                type uint32;
                                                description "Number of test frames sent for the specified measurement interval data set";
                                            }

                                            ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/frames-received
                                            leaf frames-received {
                                                type uint32;
                                                description "Number of test frames received for the specified measurement interval data set";
                                            }

                                            ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type
                                            list bin-type {
                                                key "bin-metric";
                                                description "Enter the bin-type list instance";

                                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/bin-metric
                                                leaf bin-metric {
                                                    type enumeration {
                                                        enum fd                             { value 1; }
                                                        enum fdr                            { value 2; }
                                                        enum ifdv                           { value 3; }
                                                    }
                                                    description "Delay metric bin structure";
                                                }

                                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/forward
                                                container forward {
                                                    description "Enter the forward context";

                                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/forward/minimum
                                                    leaf minimum {
                                                        type uint32;
                                                        units "microseconds";
                                                        description "Minimum delay for the specified direction, session, interval duration, interval number, and bin type";
                                                    }

                                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/forward/maximum
                                                    leaf maximum {
                                                        type uint32;
                                                        units "microseconds";
                                                        description "Maximum delay for the specified direction, session, interval duration, interval number, and bin type";
                                                    }

                                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/forward/average
                                                    leaf average {
                                                        type uint32;
                                                        units "microseconds";
                                                        description "Average delay for the specified direction, session, interval duration, interval number, and bin type";
                                                    }

                                                } // container forward

                                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/backward
                                                container backward {
                                                    description "Enter the backward context";

                                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/backward/minimum
                                                    leaf minimum {
                                                        type uint32;
                                                        units "microseconds";
                                                        description "Minimum delay for the specified direction, session, interval duration, interval number, and bin type";
                                                    }

                                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/backward/maximum
                                                    leaf maximum {
                                                        type uint32;
                                                        units "microseconds";
                                                        description "Maximum delay for the specified direction, session, interval duration, interval number, and bin type";
                                                    }

                                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/backward/average
                                                    leaf average {
                                                        type uint32;
                                                        units "microseconds";
                                                        description "Average delay for the specified direction, session, interval duration, interval number, and bin type";
                                                    }

                                                } // container backward

                                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/round-trip
                                                container round-trip {
                                                    description "Enter the round-trip context";

                                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/round-trip/minimum
                                                    leaf minimum {
                                                        type uint32;
                                                        units "microseconds";
                                                        description "Minimum delay for the specified direction, session, interval duration, interval number, and bin type";
                                                    }

                                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/round-trip/maximum
                                                    leaf maximum {
                                                        type uint32;
                                                        units "microseconds";
                                                        description "Maximum delay for the specified direction, session, interval duration, interval number, and bin type";
                                                    }

                                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/round-trip/average
                                                    leaf average {
                                                        type uint32;
                                                        units "microseconds";
                                                        description "Average delay for the specified direction, session, interval duration, interval number, and bin type";
                                                    }

                                                } // container round-trip

                                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/bin
                                                list bin {
                                                    key "bin-number";
                                                    description "Enter the bin list instance";

                                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/bin/bin-number
                                                    leaf bin-number {
                                                        type uint32 {
                                                            range "0..9";
                                                        }
                                                        description "Bin number";
                                                    }

                                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/bin/forward-measurements
                                                    leaf forward-measurements {
                                                        type uint32;
                                                        description "Number of frame delay measurements in the forward direction that fall within the configured range of the specified bin";
                                                    }

                                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/bin/backward-measurements
                                                    leaf backward-measurements {
                                                        type uint32;
                                                        description "Number of frame delay measurements in the backward direction that fall within the configured range of the specified bin";
                                                    }

                                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/measurement-interval/number/bin-type/bin/round-trip-measurements
                                                    leaf round-trip-measurements {
                                                        type uint32;
                                                        description "Number of round trip frame delay measurements that fall within the configured range of the specified bin";
                                                    }

                                                } // list bin

                                            } // list bin-type

                                        } // list number

                                    } // list measurement-interval

                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/streaming
                                    container streaming {
                                        description "Enter the streaming context";

                                        ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/streaming/metric
                                        list metric {
                                            key "metric-id";
                                            description "Enter the metric list instance";

                                            ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/streaming/metric/metric-id
                                            leaf metric-id {
                                                type enumeration {
                                                    enum fd-average                     { value 1; }
                                                    enum ifdv-average                   { value 2; }
                                                }
                                                description "The OAM-PM streaming metric to be displayed";
                                            }

                                            ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/streaming/metric/newest-closed
                                            list newest-closed {
                                                key "direction";
                                                description "Enter the newest-closed list instance";

                                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/streaming/metric/newest-closed/direction
                                                leaf direction {
                                                    type types-oam:direction;
                                                    description "Traffic flow direction of the OAM-PM test or metric";
                                                }

                                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/streaming/metric/newest-closed/close-time
                                                leaf close-time {
                                                    type yang:date-and-time;
                                                    description "The timestamp taken when the streaming sample window closed";
                                                }

                                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/streaming/metric/newest-closed/sample-count
                                                leaf sample-count {
                                                    type uint32;
                                                    description "The number of successful measurements used to compute the statistics in the streaming sample window";
                                                }

                                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/streaming/metric/newest-closed/suspect
                                                leaf suspect {
                                                    type boolean;
                                                    description "Indicates whether or not the streaming sample window contains suspect information";
                                                }

                                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/dmm/statistics/streaming/metric/newest-closed/delay
                                                leaf delay {
                                                    type uint32;
                                                    units "microseconds";
                                                    description "The delay statistic for the streaming sample window";
                                                }

                                            } // list newest-closed

                                        } // list metric

                                    } // container streaming

                                } // container statistics

                            } // container dmm

                            ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm
                            container lmm {
                                description "Enable the lmm context";

                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/oper-state
                                leaf oper-state {
                                    type enumeration {
                                        enum up                             { value 1; }
                                        enum down                           { value 2; }
                                    }
                                    description "The operational state of OAM-PM tests for the session.";
                                }

                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/detected-tx-error
                                leaf detected-tx-error {
                                    type types-oam:oam-pm-session-test-detected-tx-error;
                                    description
                                        "Reason for the specified test's current inability (if any) to launch request frames.

                                         For example, 'eth-parent-admin-down(4)' could be returned for an SLM test
                                         if the MEP to be tested is associated with a SAP which is
                                         administratively down.

                                         Not all request frame transmit failures are detected by this mechanism.
                                         It is possible that 'none' will be returned when the test is unable
                                         to transmit frames.";
                                }

                                ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events
                                container loss-events {
                                    description "Enter the loss-events context";

                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/avg-flr-event
                                    list avg-flr-event {
                                        key "direction";
                                        description "Enter the avg-flr-event list instance";

                                        ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/avg-flr-event/direction
                                        leaf direction {
                                            type enumeration {
                                                enum forward                        { value 1; }
                                                enum backward                       { value 2; }
                                            }
                                            description "Traffic flow direction";
                                        }

                                        ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/avg-flr-event/last-tca-time
                                        leaf last-tca-time {
                                            type yang:date-and-time;
                                            description
                                                "UTC date and time at the start of the measurement interval which generated
                                                 the most recent delay-based Raise or Clear TCA for the specified session,
                                                 test-type, bin-type and direction.";
                                        }

                                        ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/avg-flr-event/oper-state
                                        leaf oper-state {
                                            type enumeration {
                                                enum pending                        { value 1; }
                                                enum active                         { value 2; }
                                                enum not-active                     { value 3; }
                                            }
                                            description
                                                "Operational state of the Threshold Crossing Alerts (TCAs) for the specified session,
                                                 test-type, bin-type and direction.

                                                 The code points are as follows.  The conditions are evaluated in the
                                                 order shown.
                                                 pending
                                                    is returned if the threshold configuration for the
                                                    specified indices changed during the current measurement interval.
                                                 active
                                                    is returned if the specified indices have a default clear threshold configured,
                                                    and the current measurement interval has generated a Raise TCA.
                                                    is returned if the specified indices have a non-default clear threshold
                                                    configured, and the most recent TCA generated was a Raise.
                                                 not-active
                                                    is returned for all other conditions (e.g. thresholding is not
                                                    configured for the specified indices).";
                                        }

                                    } // list avg-flr-event

                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/chli-event
                                    list chli-event {
                                        key "direction";
                                        description "Enter the chli-event list instance";

                                        ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/chli-event/direction
                                        leaf direction {
                                            type enumeration {
                                                enum forward                        { value 1; }
                                                enum backward                       { value 2; }
                                                enum aggregate                      { value 3; }
                                            }
                                            description "Traffic flow direction";
                                        }

                                        ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/chli-event/last-tca-time
                                        leaf last-tca-time {
                                            type yang:date-and-time;
                                            description
                                                "UTC date and time at the start of the measurement interval which generated
                                                 the most recent delay-based Raise or Clear TCA for the specified session,
                                                 test-type, bin-type and direction.";
                                        }

                                        ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/chli-event/oper-state
                                        leaf oper-state {
                                            type enumeration {
                                                enum pending                        { value 1; }
                                                enum active                         { value 2; }
                                                enum not-active                     { value 3; }
                                            }
                                            description
                                                "Operational state of the Threshold Crossing Alerts (TCAs) for the specified session,
                                                 test-type, bin-type and direction.

                                                 The code points are as follows.  The conditions are evaluated in the
                                                 order shown.
                                                 pending
                                                    is returned if the threshold configuration for the
                                                    specified indices changed during the current measurement interval.
                                                 active
                                                    is returned if the specified indices have a default clear threshold configured,
                                                    and the current measurement interval has generated a Raise TCA.
                                                    is returned if the specified indices have a non-default clear threshold
                                                    configured, and the most recent TCA generated was a Raise.
                                                 not-active
                                                    is returned for all other conditions (e.g. thresholding is not
                                                    configured for the specified indices).";
                                        }

                                    } // list chli-event

                                    ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/hli-event
                                    list hli-event {
                                        key "direction";
                                        description "Enter the hli-event list instance";

                                        ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/hli-event/direction
                                        leaf direction {
                                            type enumeration {
                                                enum forward                        { value 1; }
                                                enum backward                       { value 2; }
                                                enum aggregate                      { value 3; }
                                            }
                                            description "Traffic flow direction";
                                        }

                                        ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/hli-event/last-tca-time
                                        leaf last-tca-time {
                                            type yang:date-and-time;
                                            description
                                                "UTC date and time at the start of the measurement interval which generated
                                                 the most recent delay-based Raise or Clear TCA for the specified session,
                                                 test-type, bin-type and direction.";
                                        }

                                        ///nokia-state/state/oam-pm/session/test-family-choice/ethernet-case/ethernet/lmm/loss-events/hli-event/oper-state
                                        leaf oper-state {
                                            type enumeration {
                                                enum pending                        { value 1; }
                                                enum active                         { value 2; }
                                                enum not-active                     { value 3; }
                                            }
                                            description
                                                "Operational state of the Threshold Crossing Alerts (TCAs) for the specified session,
                                                 test-type, bin-type and direction.

                                                 The code points are as follows.  The conditions are evaluated in the
                                                 order shown.
                                                 pending
                                                    is returned if the threshold configuration for the
                                                    specified indices changed during the current measurement interval.
                                                 active
                                                    is returned if the specified indices have a default clear threshold configured,
