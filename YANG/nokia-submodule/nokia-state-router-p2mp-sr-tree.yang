submodule nokia-state-router-p2mp-sr-tree {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-sros               { prefix "types-sros"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for Base router p2mp-sr-tree on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-router-p2mp-sr-tree YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2022-03-22";

    grouping state-router-p2mp-sr-tree {
        ///nokia-state/state/router/p2mp-sr-tree
        container p2mp-sr-tree {
            description "Enable the p2mp-sr-tree context";

            ///nokia-state/state/router/p2mp-sr-tree/oper-state
            leaf oper-state {
                type enumeration {
                    enum unknown                        { value 1; }
                    enum in-service                     { value 2; }
                    enum out-of-service                 { value 3; }
                    enum transition                     { value 4; }
                }
                description "oper-state for p2mp-sr-tree status entry.";
            }

            ///nokia-state/state/router/p2mp-sr-tree/statistics
            container statistics {
                description "Enter the statistics context";

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-static-policies
                leaf total-static-policies {
                    type uint32;
                    description "This value indicates the total number of p2mp static policies.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-pce-policies
                leaf total-pce-policies {
                    type uint32;
                    description "This value indicates the total number of p2mp pce policies.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-sr-policies
                leaf total-sr-policies {
                    type uint32;
                    description "This value indicates the total number of p2mp sr policies.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-static-candidate-paths
                leaf total-static-candidate-paths {
                    type uint32;
                    description "This value indicates the total number of p2mp static candidate paths.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-pce-candidate-paths
                leaf total-pce-candidate-paths {
                    type uint32;
                    description "This value indicates the total number of pce candidate paths.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-sr-plcy-candidate-paths
                leaf total-sr-plcy-candidate-paths {
                    type uint32;
                    description "This value indicates the total number of sr policy candidate paths.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-static-replication-seg
                leaf total-static-replication-seg {
                    type uint32;
                    description "This value indicates the total number of static replication segments.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-pce-replication-seg
                leaf total-pce-replication-seg {
                    type uint32;
                    description "This value indicates the total number of pce replication segments.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-sr-plcy-replication-seg
                leaf total-sr-plcy-replication-seg {
                    type uint32;
                    description "This value indicates the total number of sr policy replication segments.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-static-nh
                leaf total-static-nh {
                    type uint32;
                    description "This value indicates the total number of static next-hops.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-pce-nh
                leaf total-pce-nh {
                    type uint32;
                    description "This value indicates the total number of pce next-hops.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-sr-plcy-nh
                leaf total-sr-plcy-nh {
                    type uint32;
                    description "This value indicates the total number of sr-plcy next-hops.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-num-push
                leaf total-num-push {
                    type uint32;
                    description "This value indicates the total number of push.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-num-swap
                leaf total-num-swap {
                    type uint32;
                    description "This value indicates the total number of swap.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-num-pop
                leaf total-num-pop {
                    type uint32;
                    description "This value indicates the total number of pop.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-num-tnls-allocated
                leaf total-num-tnls-allocated {
                    type uint32;
                    description "This value indicates the total number of tunnels allocated.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-num-nhif-insvc
                leaf total-num-nhif-insvc {
                    type uint32;
                    description "This value indicates the total number of in-service next-hop interfaces.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-num-nhif-outsvc
                leaf total-num-nhif-outsvc {
                    type uint32;
                    description "This value indicates the total number of out-of-service next-hop interfaces.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-num-nhif-unsupported
                leaf total-num-nhif-unsupported {
                    type uint32;
                    description "This value indicates the total number of unsupported next-hop interfaces.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-num-nhif-mismatched
                leaf total-num-nhif-mismatched {
                    type uint32;
                    description "This value indicates the total number of mismatched next-hop interfaces.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/total-num-nhif-nobfd
                leaf total-num-nhif-nobfd {
                    type uint32;
                    description "This value indicates the total number of no-bfd next-hop interfaces.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/dataplane-backpressure-active
                leaf dataplane-backpressure-active {
                    type uint32;
                    description "This value indicates whether dataplane backpressure active or not.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/dataplane-backpressure-count
                leaf dataplane-backpressure-count {
                    type uint32;
                    description "This value indicates dataplane backpressure count.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/unknown-originator-owner-error-count
                leaf unknown-originator-owner-error-count {
                    type uint32;
                    description "This value indicates unknown originator owner error count.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/duplicate-treeids-error-count
                leaf duplicate-treeids-error-count {
                    type uint32;
                    description "This value indicates duplicate treeids error count.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/progfail-nhidx-error-count
                leaf progfail-nhidx-error-count {
                    type uint32;
                    description "This value indicates progfail nexthop index error count.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/progfail-labels-error-count
                leaf progfail-labels-error-count {
                    type uint32;
                    description "This value indicates progfail labels error count.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/progfail-tunnels-error-count
                leaf progfail-tunnels-error-count {
                    type uint32;
                    description "This value indicates progfail tunnels error count.";
                }

                ///nokia-state/state/router/p2mp-sr-tree/statistics/progfail-protgroup-error-count
                leaf progfail-protgroup-error-count {
                    type uint32;
                    description "This value indicates progfail protgroup error count.";
                }

            } // container statistics

            ///nokia-state/state/router/p2mp-sr-tree/labels
            container labels {
                description "Enter the labels context";

                ///nokia-state/state/router/p2mp-sr-tree/labels/label
                list label {
                    key "label-id";
                    description "Enter the label list instance";

                    ///nokia-state/state/router/p2mp-sr-tree/labels/label/label-id
                    leaf label-id {
                        type int32;
                        description "This value indicates the owner of this label.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/labels/label/owner
                    leaf owner {
                        type enumeration {
                            enum none                           { value 0; }
                            enum static                         { value 1; }
                            enum pce                            { value 2; }
                            enum sr-pol                         { value 3; }
                        }
                        description "This value indicates the owner of this label.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/labels/label/in-use
                    leaf in-use {
                        type enumeration {
                            enum no                             { value 0; }
                            enum yes                            { value 1; }
                        }
                        description "This value indicates whether the label is in use";
                    }

                } // list label

            } // container labels

            ///nokia-state/state/router/p2mp-sr-tree/database
            container database {
                description "Enter the database context";

                ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy
                list p2mp-policy {
                    key "root-address tree-id";
                    description "Enter the p2mp-policy list instance";

                    ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/root-address
                    leaf root-address {
                        type types-sros:ip-address;
                        description "root-address for replication-segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/tree-id
                    leaf tree-id {
                        type uint32;
                        description "tree-id for replication-segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/num-paths
                    leaf num-paths {
                        type uint32;
                        description "num-paths for replication segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/oper-state
                    leaf oper-state {
                        type enumeration {
                            enum unknown                        { value 1; }
                            enum in-service                     { value 2; }
                            enum out-of-service                 { value 3; }
                            enum transition                     { value 4; }
                        }
                        description "operation state for replication segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/policy-name
                    leaf policy-name {
                        type types-sros:named-item;
                        description "policy-name for replication segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/tunnel-id
                    leaf tunnel-id {
                        type uint32;
                        description "tunnel index for replication segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/active-candidate-path
                    leaf active-candidate-path {
                        type types-sros:named-item-or-empty;
                        description "active-candidate-path for replication segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/candidate-path
                    list candidate-path {
                        key "protocol-id originator-asn originator discriminator";
                        description "Enter the candidate-path list instance";

                        ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/candidate-path/protocol-id
                        leaf protocol-id {
                            type enumeration {
                                enum pcep                           { value 10; }
                                enum bgp-sr-policy                  { value 20; }
                                enum configuration                  { value 30; }
                            }
                            description "protocol-id for the p2mp policy candidate path entry.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/candidate-path/originator-asn
                        leaf originator-asn {
                            type uint32;
                            description "originator asn value for the p2mp policy candidate path entry.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/candidate-path/originator
                        leaf originator {
                            type types-sros:ipv4-unicast-or-zero-address;
                            description "originator node address for the p2mp policy candidate path entry.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/candidate-path/discriminator
                        leaf discriminator {
                            type uint32;
                            description "discriminator value for the p2mp policy candidate path entry.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/candidate-path/oper-state
                        leaf oper-state {
                            type enumeration {
                                enum unknown                        { value 1; }
                                enum in-service                     { value 2; }
                                enum out-of-service                 { value 3; }
                                enum transition                     { value 4; }
                            }
                            description "operational state for the p2mp policy candidate path entry.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/candidate-path/path-plsp-id
                        leaf path-plsp-id {
                            type uint32;
                            description "path-plsp-id for the p2mp policy candidate path entry.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/candidate-path/active-instance
                        leaf active-instance {
                            type uint32;
                            description "active instance for the p2mp policy candidate path entry.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/candidate-path/standby-instance
                        leaf standby-instance {
                            type uint32;
                            description "standby instance for the p2mp policy candidate path entry.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/candidate-path/preference
                        leaf preference {
                            type uint32;
                            description "preference for the p2mp policy candidate path entry.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/p2mp-policy/candidate-path/create-time
                        leaf create-time {
                            type types-sros:time-interval;
                            description
                                "The value indicates the sysUpTime when this candidate path
                                 was created.";
                        }

                    } // list candidate-path

                } // list p2mp-policy

                ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment
                list replication-segment {
                    key "root-address tree-id instance-id protocol-id";
                    description "Enter the replication-segment list instance";

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/root-address
                    leaf root-address {
                        type types-sros:ip-address;
                        description "root-address for replication-segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/tree-id
                    leaf tree-id {
                        type uint32;
                        description "tree-id for replication-segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/instance-id
                    leaf instance-id {
                        type uint32;
                        description "instance-id for replication-segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/protocol-id
                    leaf protocol-id {
                        type enumeration {
                            enum pcep                           { value 10; }
                            enum bgp-sr-policy                  { value 20; }
                            enum configuration                  { value 30; }
                        }
                        description "protocol-id for replication-segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/num-hops
                    leaf num-hops {
                        type uint32;
                        description "num-paths for replication segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/oper-state
                    leaf oper-state {
                        type enumeration {
                            enum unknown                        { value 1; }
                            enum in-service                     { value 2; }
                            enum out-of-service                 { value 3; }
                            enum transition                     { value 4; }
                        }
                        description "operation state for replication segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/policy-name
                    leaf policy-name {
                        type types-sros:named-item;
                        description "policy-name for replication segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/tunnel-id
                    leaf tunnel-id {
                        type uint32;
                        description "tunnel index for replication segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/down-reason
                    leaf down-reason {
                        type enumeration {
                            enum none                           { value 0; }
                            enum idx-alloc-fail                 { value 1; }
                            enum all-nh-down                    { value 2; }
                            enum label-reg-fail                 { value 3; }
                            enum iom-pgrm-fail                  { value 4; }
                            enum iom-ack-pend                   { value 5; }
                        }
                        description "down-reason for replication segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/operation
                    leaf operation {
                        type enumeration {
                            enum none                           { value 0; }
                            enum push                           { value 1; }
                            enum pop                            { value 2; }
                            enum swap                           { value 3; }
                        }
                        description "operation for replication-segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/replication-sid
                    leaf replication-sid {
                        type uint32;
                        description "replication-sid for replication-segment entries.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/ltn
                    leaf ltn {
                        type boolean;
                        description
                            "The value of ltn indicates whether this replication policy is rooted
                             in this node(ingress LSP).";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/is-local
                    leaf is-local {
                        type boolean;
                        description
                            "The value of is-local indicates whether this replication policy has
                             any MVPN leaf(egress LSP).";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/ccid
                    leaf ccid {
                        type uint32;
                        description
                            "The value of ccid indicates the referenced CCI(central controler
                             instruction) object ID in PCC.";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/update-id
                    leaf update-id {
                        type uint32;
                        description
                            "The value of update-id indicates the referenced update id among the list
                             of CCID objects in PCC. ";
                    }

                    ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes
                    list downstream-nodes {
                        key "downstream-index";
                        description "Enter the downstream-nodes list instance";

                        ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/downstream-index
                        leaf downstream-index {
                            type uint32;
                            description
                                "The value of downstream-index indicates the downstream-nodes group index within a
                                 candidate path entry inside a tree-side replication policy instance.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/next-hop-address
                        leaf next-hop-address {
                            type types-sros:ip-address-with-zone;
                            description "next-hop-address for a replication segment next hop entry.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/oper-state
                        leaf oper-state {
                            type enumeration {
                                enum unknown                        { value 1; }
                                enum in-service                     { value 2; }
                                enum out-of-service                 { value 3; }
                                enum transition                     { value 4; }
                            }
                            description "operation state for replication segment next-hop entries.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/down-reason
                        leaf down-reason {
                            type enumeration {
                                enum none                           { value 0; }
                                enum invalid-nhop                   { value 1; }
                                enum pip-if-not-found               { value 2; }
                                enum pip-if-down                    { value 3; }
                                enum pip-if-v6-down                 { value 4; }
                                enum rtm-unresolved                 { value 5; }
                                enum if-not-unnum                   { value 6; }
                                enum if-not-ipv6-link-local         { value 7; }
                                enum bfd-down                       { value 8; }
                                enum idx-alloc-fail                 { value 9; }
                                enum invalid-label-stack            { value 10; }
                                enum sr-tunnel-pgrm-fail            { value 11; }
                                enum sr-tunnel-down                 { value 12; }
                            }
                            description "down-reason for replication segment next-hop entries.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/protect-nexthop-address
                        leaf protect-nexthop-address {
                            type types-sros:ip-address-with-zone;
                            description "protect-nexthop-address for replication segment next-hop entries.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/nexthop-interface-name
                        leaf nexthop-interface-name {
                            type types-sros:named-item-or-empty;
                            description "protect interface name for replication-segment next-hop entries.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/protect-oper-state
                        leaf protect-oper-state {
                            type enumeration {
                                enum unknown                        { value 1; }
                                enum in-service                     { value 2; }
                                enum out-of-service                 { value 3; }
                                enum transition                     { value 4; }
                            }
                            description "protect operational state for replication-segment next-hop entries.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/protect-oper-down-reason
                        leaf protect-oper-down-reason {
                            type enumeration {
                                enum none                           { value 0; }
                                enum invalid-nhop                   { value 1; }
                                enum pip-if-not-found               { value 2; }
                                enum pip-if-down                    { value 3; }
                                enum pip-if-v6-down                 { value 4; }
                                enum rtm-unresolved                 { value 5; }
                                enum if-not-unnum                   { value 6; }
                                enum if-not-ipv6-link-local         { value 7; }
                                enum bfd-down                       { value 8; }
                                enum idx-alloc-fail                 { value 9; }
                                enum invalid-label-stack            { value 10; }
                                enum sr-tunnel-pgrm-fail            { value 11; }
                                enum sr-tunnel-down                 { value 12; }
                            }
                            description "protect-oper-down-reason for the replication-segment next-hop entry.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/protect-nexthop-id
                        leaf protect-nexthop-id {
                            type uint32;
                            description "protect nexthop id for the replication-segment next-hop entry.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/protect-nexthop-if-name
                        leaf protect-nexthop-if-name {
                            type types-sros:named-item-or-empty;
                            description "protect nexthop interface name for the replication-segment next-hop entry.";
                        }

                        ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/label
                        container label {
                            description "Enter the label context";

                            ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/label/sid-list
                            list sid-list {
                                key "index";
                                description "Enter the sid-list list instance";

                                ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/label/sid-list/index
                                leaf index {
                                    type uint32 {
                                        range "0..10";
                                    }
                                    description "index for the replication segment next hop outgoing replication sid entry.";
                                }

                                ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/label/sid-list/replication-sid
                                leaf replication-sid {
                                    type uint32;
                                    description "replication-sid for the replication segment next hop outgoing replication sid entry.";
                                }

                            } // list sid-list

                            ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/label/protect-sid-list
                            list protect-sid-list {
                                key "index";
                                description "Enter the protect-sid-list list instance";

                                ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/label/protect-sid-list/index
                                leaf index {
                                    type uint32 {
                                        range "0..10";
                                    }
                                    description "index for the replication segment next hop outgoing protect replication sid entry.";
                                }

                                ///nokia-state/state/router/p2mp-sr-tree/database/replication-segment/downstream-nodes/label/protect-sid-list/replication-sid
                                leaf replication-sid {
                                    type uint32;
                                    description "replication-sid for the replication segment downstream nodes outgoing replication sid entry.";
                                }

                            } // list protect-sid-list

                        } // container label

                    } // list downstream-nodes

                } // list replication-segment

            } // container database

        } // container p2mp-sr-tree

    } // grouping state-router-p2mp-sr-tree

}
