                            ///nokia-conf/configure/router/bgp/group/dynamic-neighbor/match/prefix/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/bgp/group/dynamic-neighbor/match/prefix/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list prefix

                    } // container match



                    ///nokia-conf/configure/router/bgp/group/dynamic-neighbor/interface
                    list interface {
                        key "interface-name";
                        description "Enter the interface list instance";

                        ///nokia-conf/configure/router/bgp/group/dynamic-neighbor/interface/interface-name
                        leaf interface-name {
                            type types-sros:interface-name;
                            description "Name of the dynamic neighbor interface";
                        }

                        ///nokia-conf/configure/router/bgp/group/dynamic-neighbor/interface/allowed-peer-as
                        leaf-list allowed-peer-as {
                            type string {
                                pattern "(([1-9]|[1-9][0-9]{1,8}|[1-3][0-9]{9}|4[0-1][0-9]{8}|42[0-8][0-9]{7}|429[0-3][0-9]{6}|4294[0-8][0-9]{5}|42949[0-5][0-9]{4}|429496[0-6][0-9]{3}|4294967[0-1][0-9]{2}|42949672[0-8][0-9]|429496729[0-5]))|(([1-9]|[1-9][0-9]{1,8}|[1-3][0-9]{9}|4[0-1][0-9]{8}|42[0-8][0-9]{7}|429[0-3][0-9]{6}|4294[0-8][0-9]{5}|42949[0-5][0-9]{4}|429496[0-6][0-9]{3}|4294967[0-1][0-9]{2}|42949672[0-8][0-9]|429496729[0-5])\\.\\.([1-9]|[1-9][0-9]{1,8}|[1-3][0-9]{9}|4[0-1][0-9]{8}|42[0-8][0-9]{7}|429[0-3][0-9]{6}|4294[0-8][0-9]{5}|42949[0-5][0-9]{4}|429496[0-6][0-9]{3}|4294967[0-1][0-9]{2}|42949672[0-8][0-9]|429496729[0-5]))";
                            }
                            max-elements 32;
                            ordered-by user;
                            description "Allowed peer AS value or range of acceptable values";
                        }

                        ///nokia-conf/configure/router/bgp/group/dynamic-neighbor/interface/max-sessions
                        leaf max-sessions {
                            type uint32 {
                                range "1..255";
                            }
                            default "1";
                            description "Maximum number of dynamic sessions allowed";
                        }

                        ///nokia-conf/configure/router/bgp/group/dynamic-neighbor/interface/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/router/bgp/group/dynamic-neighbor/interface/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list interface

                } // container dynamic-neighbor

                ///nokia-conf/configure/router/bgp/group/monitor
                container monitor {
                    presence "Enable the monitor context";
                    description "Enable the monitor context";

                    ///nokia-conf/configure/router/bgp/group/monitor/all-stations
                    leaf all-stations {
                        type boolean;
                        default "false";
                        description "Send BMP messages to all configured stations";
                    }

                    ///nokia-conf/configure/router/bgp/group/monitor/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of BMP monitoring";
                    }

                    ///nokia-conf/configure/router/bgp/group/monitor/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/bgp/group/monitor/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/bgp/group/monitor/route-monitoring
                    container route-monitoring {
                        description "Enter the route-monitoring context";

                        ///nokia-conf/configure/router/bgp/group/monitor/route-monitoring/pre-policy
                        leaf pre-policy {
                            type boolean;
                            default "false";
                            description "Allow pre-policy route-monitoring messages to be sent";
                        }

                        ///nokia-conf/configure/router/bgp/group/monitor/route-monitoring/post-policy
                        leaf post-policy {
                            type boolean;
                            default "false";
                            description "Allow post-policy route-monitoring messages to be sent";
                        }

                    } // container route-monitoring

                    ///nokia-conf/configure/router/bgp/group/monitor/station
                    list station {
                        key "station-name";
                        max-elements 8;
                        description "Add a list entry for station";

                        ///nokia-conf/configure/router/bgp/group/monitor/station/station-name
                        leaf station-name {
                            type leafref {
                                path "../../../../../../bmp/station/name";
                            }
                            description "BMP monitoring station";
                        }

                    } // list station

                } // container monitor

                ///nokia-conf/configure/router/bgp/group/egress-engineering
                container egress-engineering {
                    presence "Enable the egress-engineering context";
                    description "Enable the egress-engineering context";

                    ///nokia-conf/configure/router/bgp/group/egress-engineering/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of BGP egress engineering";
                    }

                    ///nokia-conf/configure/router/bgp/group/egress-engineering/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/bgp/group/egress-engineering/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container egress-engineering

                ///nokia-conf/configure/router/bgp/group/segment-routing-v6
                container segment-routing-v6 {
                    presence "Enable the segment-routing-v6 context";
                    description "Enable the segment-routing-v6 context";

                    ///nokia-conf/configure/router/bgp/group/segment-routing-v6/route-advertisement
                    container route-advertisement {
                        presence "Enable the route-advertisement context";
                        description "Enable the route-advertisement context";

                        ///nokia-conf/configure/router/bgp/group/segment-routing-v6/route-advertisement/drop-routes-with-srv6-tlvs
                        leaf drop-routes-with-srv6-tlvs {
                            type boolean;
                            default "false";
                            description "Drop BGP routes with SRv6 TLVs";
                        }

                        ///nokia-conf/configure/router/bgp/group/segment-routing-v6/route-advertisement/family
                        list family {
                            key "family-type";
                            description "Enter the family list instance";

                            ///nokia-conf/configure/router/bgp/group/segment-routing-v6/route-advertisement/family/family-type
                            leaf family-type {
                                type enumeration {
                                    enum ipv4                           { value 1; }
                                    enum ipv6                           { value 3; }
                                }
                                description "IP address type that SRv6 route attributes apply to";
                            }

                            ///nokia-conf/configure/router/bgp/group/segment-routing-v6/route-advertisement/family/strip-srv6-tlvs
                            leaf strip-srv6-tlvs {
                                type boolean;
                                default "false";
                                description "Strip SRv6 TLVs from BGP routes advertised to peers";
                            }

                            ///nokia-conf/configure/router/bgp/group/segment-routing-v6/route-advertisement/family/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/bgp/group/segment-routing-v6/route-advertisement/family/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list family

                    } // container route-advertisement

                } // container segment-routing-v6

            } // list group

            ///nokia-conf/configure/router/bgp/neighbor
            list neighbor {
                key "ip-address";
                description "Enter the neighbor list instance";

                ///nokia-conf/configure/router/bgp/neighbor/ip-address
                leaf ip-address {
                    type types-sros:ip-address-with-zone;
                    description "IP address of the BGP peer router";
                }

                ///nokia-conf/configure/router/bgp/neighbor/advertise-ldp-prefix
                leaf advertise-ldp-prefix {
                    type boolean;
                    default "false";
                    description "Advertise active /32 LDP FEC prefixes to BGP peers";
                }

                ///nokia-conf/configure/router/bgp/neighbor/ttl-security
                leaf ttl-security {
                    type uint32 {
                        range "1..255";
                    }
                    description "Minimum TTL value for an incoming BGP packet";
                }

                ///nokia-conf/configure/router/bgp/neighbor/default-route-target
                leaf default-route-target {
                    type boolean;
                    description "Send default RTC route (zero prefix length) to peers";
                }

                ///nokia-conf/configure/router/bgp/neighbor/aigp
                leaf aigp {
                    type boolean;
                    description "Add AIGP attribute to advertised routes";
                }

                ///nokia-conf/configure/router/bgp/neighbor/advertise-inactive
                leaf advertise-inactive {
                    type boolean;
                    description "Advertise an inactive BGP route to peers";
                }

                ///nokia-conf/configure/router/bgp/neighbor/authentication-keychain
                leaf authentication-keychain {
                    type leafref {
                        path "../../../../system/security/keychains/keychain/keychain-name";
                    }
                    description "TCP authentication keychain for the session";
                }

                ///nokia-conf/configure/router/bgp/neighbor/bfd-liveness
                leaf bfd-liveness {
                    type boolean;
                    description "Enable BFD";
                }

                ///nokia-conf/configure/router/bgp/neighbor/fast-external-failover
                leaf fast-external-failover {
                    type boolean;
                    description "Drop external BGP session immediately when link fails";
                }

                ///nokia-conf/configure/router/bgp/neighbor/peer-ip-tracking
                leaf peer-ip-tracking {
                    type boolean;
                    description "Enable BGP peer tracking";
                }

                ///nokia-conf/configure/router/bgp/neighbor/split-horizon
                leaf split-horizon {
                    type boolean;
                    description "Prevent routes being reflected back to best-route peer";
                }

                ///nokia-conf/configure/router/bgp/neighbor/third-party-nexthop
                leaf third-party-nexthop {
                    type boolean;
                    description "Apply third-party next-hop processing to EBGP peers";
                }

                ///nokia-conf/configure/router/bgp/neighbor/label-preference
                leaf label-preference {
                    type uint32 {
                        range "1..255";
                    }
                    description "Route preference for routes from labeled-unicast peers";
                }

                ///nokia-conf/configure/router/bgp/neighbor/def-recv-evpn-encap
                leaf def-recv-evpn-encap {
                    type types-bgp:encap;
                    description "Default EVPN encapsulation type";
