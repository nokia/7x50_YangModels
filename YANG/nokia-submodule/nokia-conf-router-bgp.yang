submodule nokia-conf-router-bgp {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-bgp                { prefix "types-bgp"; }
    import nokia-types-router             { prefix "types-router"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-bmp;
    include nokia-conf-groups;
    include nokia-conf-policy-options;
    include nokia-conf-router;
    include nokia-conf-router-segment-routing;
    include nokia-conf-system;
    include nokia-conf-system-security;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure bgp protocol on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-router-bgp YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-04-28";

    grouping conf-router-bgp {
        ///nokia-conf/configure/router/bgp
        container bgp {
            presence "Enable the bgp context";
            description "Enable the bgp context";

            ///nokia-conf/configure/router/bgp/description
            leaf description {
                type types-sros:description;
                description "Text description";
            }

            ///nokia-conf/configure/router/bgp/connect-retry
            leaf connect-retry {
                type int32 {
                    range "1..65535";
                }
                default "120";
                description "BGP connect retry timer value";
            }

            ///nokia-conf/configure/router/bgp/admin-state
            leaf admin-state {
                type types-sros:admin-state;
                default "enable";
                description "Administrative state of the BGP instance";
            }

            ///nokia-conf/configure/router/bgp/keepalive
            leaf keepalive {
                type int32 {
                    range "0..21845";
                }
                default "30";
                description "Time after which the BGP KEEPALIVE message is sent";
            }

            ///nokia-conf/configure/router/bgp/damping
            leaf damping {
                type boolean;
                default "false";
                description "Use BGP route damping to reduce route flap";
            }

            ///nokia-conf/configure/router/bgp/local-preference
            leaf local-preference {
                type uint32;
                default "100";
                description "Default local preference if not in incoming routes";
            }

            ///nokia-conf/configure/router/bgp/loop-detect
            leaf loop-detect {
                type types-bgp:loop-detect;
                default "ignore-loop";
                description "Strategy for loop detection in the AS path";
            }

            ///nokia-conf/configure/router/bgp/loop-detect-threshold
            leaf loop-detect-threshold {
                type uint32 {
                    range "0..15";
                }
                default "0";
                description "Threshold for the global ASN in a received AS path";
            }

            ///nokia-conf/configure/router/bgp/tcp-mss
            leaf tcp-mss {
                type uint32 {
                    range "384..9746";
                }
                description "TCP maximum segment size override";
            }

            ///nokia-conf/configure/router/bgp/selective-label-ipv4-install
            leaf selective-label-ipv4-install {
                type boolean;
                default "false";
                description "Enable selective download for BGP label-ipv4 routes";
            }

            ///nokia-conf/configure/router/bgp/selective-label-ip-prioritization
            leaf selective-label-ip-prioritization {
                type boolean;
                default "false";
                description "Enable BGP selective prioritization for labeled routes";
            }

            ///nokia-conf/configure/router/bgp/min-route-advertisement
            leaf min-route-advertisement {
                type int16 {
                    range "1..255";
                }
                default "30";
                description "Minimum time before a prefix can be advertised to peer";
            }

            ///nokia-conf/configure/router/bgp/aggregator-id-zero
            leaf aggregator-id-zero {
                type boolean;
                default "false";
                description "Set router ID in the BGP AGGREGATOR attribute to 0";
            }

            ///nokia-conf/configure/router/bgp/preference
            leaf preference {
                type uint32 {
                    range "1..255";
                }
                default "170";
                description "Route preference for routes learned from all peers";
            }

            ///nokia-conf/configure/router/bgp/block-prefix-sid
            leaf block-prefix-sid {
                type boolean;
                default "false";
                description "Block the prefix SID attribute";
            }

            ///nokia-conf/configure/router/bgp/multihop
            leaf multihop {
                type int32 {
                    range "1..255";
                }
                description "TTL in IP packet headers for EBGP peers multi-hops away";
            }

            ///nokia-conf/configure/router/bgp/med-out
            leaf med-out {
                type union {
                    type uint32;
                    type enumeration {
                        enum igp-cost                       { value -1; }
                    }
                }
                description "Default MED attribute value to advertise to peers";
            }

            ///nokia-conf/configure/router/bgp/authentication-key
            leaf authentication-key {
                type types-sros:encrypted-leaf {
                    length "1..370";
                }
                description "BGP authentication key for all peers";
            }

            ///nokia-conf/configure/router/bgp/client-reflect
            leaf client-reflect {
                type boolean;
                default "true";
                description "Allow client reflection of routes by route reflector";
            }

            ///nokia-conf/configure/router/bgp/vpn-apply-export
            leaf vpn-apply-export {
                type boolean;
                default "false";
                description "Apply base-instance BGP export policies to VPN routes";
            }

            ///nokia-conf/configure/router/bgp/vpn-apply-import
            leaf vpn-apply-import {
                type boolean;
                default "false";
                description "Apply base-instance import route policies to VPN routes";
            }

            ///nokia-conf/configure/router/bgp/asn-4-byte
            leaf asn-4-byte {
                type boolean;
                default "true";
                description "Advertise support for 4-byte ASNs";
            }

            ///nokia-conf/configure/router/bgp/path-mtu-discovery
            leaf path-mtu-discovery {
                type boolean;
                default "false";
                description "Enable Path MTU Discovery";
            }

            ///nokia-conf/configure/router/bgp/enforce-first-as
            leaf enforce-first-as {
                type boolean;
                default "false";
                description "Enforce the configured peer AS value in received routes";
            }

            ///nokia-conf/configure/router/bgp/initial-send-delay-zero
            leaf initial-send-delay-zero {
                type boolean;
                default "false";
                description "Send BGP updates as soon as session comes up";
            }

            ///nokia-conf/configure/router/bgp/router-id
            leaf router-id {
                type types-sros:ipv4-address;
                description "Router ID for the BGP instance in the AS";
            }

            ///nokia-conf/configure/router/bgp/inter-as-vpn
            leaf inter-as-vpn {
                type boolean;
                default "false";
                description "Allow advertisement of VPN routes across AS boundaries";
            }

            ///nokia-conf/configure/router/bgp/purge-timer
            leaf purge-timer {
                type uint32 {
                    range "1..60";
                }
                default "10";
                description "Maximum time before stale routes are purged";
            }

            ///nokia-conf/configure/router/bgp/route-table-install
            leaf route-table-install {
                type boolean;
                default "true";
                description "Install all IPv4 and IPv6 BGP routes in FIB and RTM";
            }

            ///nokia-conf/configure/router/bgp/ibgp-multipath
            leaf ibgp-multipath {
                type boolean;
                default "false";
                description "Enable IBGP multipath load balancing";
            }

            ///nokia-conf/configure/router/bgp/rr-vpn-forwarding
            leaf rr-vpn-forwarding {
                type boolean;
                default "false";
                description "Allow route reflector to forward VPN-IP routes";
            }

            ///nokia-conf/configure/router/bgp/subconfed-vpn-forwarding
            leaf subconfed-vpn-forwarding {
                type boolean;
                default "false";
                description "Allow subconfed route resolution, next hop and labels";
            }

            ///nokia-conf/configure/router/bgp/bfd-liveness
            leaf bfd-liveness {
                type boolean;
                default "false";
                description "Enable BFD";
            }

            ///nokia-conf/configure/router/bgp/fast-external-failover
            leaf fast-external-failover {
                type boolean;
                default "true";
                description "Drop external BGP session immediately when link fails";
            }

            ///nokia-conf/configure/router/bgp/advertise-inactive
            leaf advertise-inactive {
                type boolean;
                default "false";
                description "Advertise inactive BGP routes to peers";
            }

            ///nokia-conf/configure/router/bgp/authentication-keychain
            leaf authentication-keychain {
                type leafref {
                    path "../../../system/security/keychains/keychain/keychain-name";
                }
                description "TCP authentication keychain for the session";
            }

            ///nokia-conf/configure/router/bgp/mvpn-vrf-import-subtype-new
            leaf mvpn-vrf-import-subtype-new {
                type boolean;
                default "false";
                description "Encode the IANA value of 0x010b in advertised routes";
            }

            ///nokia-conf/configure/router/bgp/rapid-withdrawal
            leaf rapid-withdrawal {
                type boolean;
                default "false";
                description "Send BGP withdrawal UPDATE messages immediately";
            }

            ///nokia-conf/configure/router/bgp/mp-bgp-keep
            leaf mp-bgp-keep {
                type boolean;
                default "false";
                description "Keep rejected VPN-IP routes in RIB-IN";
            }

            ///nokia-conf/configure/router/bgp/peer-ip-tracking
            leaf peer-ip-tracking {
                type boolean;
                default "false";
                description "Enable BGP peer tracking";
            }

            ///nokia-conf/configure/router/bgp/split-horizon
            leaf split-horizon {
                type boolean;
                default "false";
                description "Prevent routes being reflected back to best-route peer";
            }

            ///nokia-conf/configure/router/bgp/override-tunnel-elc
            leaf override-tunnel-elc {
                type boolean;
                default "false";
                description "Enable entropy label capability on BGP tunnels";
            }

            ///nokia-conf/configure/router/bgp/link-state-route-export
            leaf link-state-route-export {
                type boolean;
                default "false";
                description "Allow the export of the TED NLRI database";
            }

            ///nokia-conf/configure/router/bgp/link-state-route-import
            leaf link-state-route-import {
                type boolean;
                default "false";
                description "Allow the import of the TED NLRI database";
            }

            ///nokia-conf/configure/router/bgp/third-party-nexthop
            leaf third-party-nexthop {
                type boolean;
                default "false";
                description "Apply third-party next-hop processing to EBGP peers";
            }

            ///nokia-conf/configure/router/bgp/dynamic-neighbor-limit
            leaf dynamic-neighbor-limit {
                type uint32 {
                    range "1..8192";
                }
                description "Max dynamic BGP sessions to accept from remote peers";
            }

            ///nokia-conf/configure/router/bgp/label-preference
            leaf label-preference {
                type uint32 {
                    range "1..255";
                }
                default "170";
                description "Route preference for routes from labeled-unicast peers";
            }

            ///nokia-conf/configure/router/bgp/peer-tracking-policy
            leaf peer-tracking-policy {
                type leafref {
                    path "../../../policy-options/policy-statement/name";
                }
                description "Policy for BGP peer tracking on router instance";
            }

            ///nokia-conf/configure/router/bgp/def-recv-evpn-encap
            leaf def-recv-evpn-encap {
                type types-bgp:encap;
                default "mpls";
                description "Default EVPN encapsulation type";
            }

            ///nokia-conf/configure/router/bgp/sr-policy-import
            leaf sr-policy-import {
                type boolean;
                default "false";
                description "Import all segment routing policies into the BGP RIB";
            }

            ///nokia-conf/configure/router/bgp/bgp-tunnel-preference
            leaf bgp-tunnel-preference {
                type uint32 {
                    range "1..255";
                }
                default "12";
                description "BGP tunnel table preference";
            }

            ///nokia-conf/configure/router/bgp/selective-label-ip
            leaf selective-label-ip {
                type enumeration {
                    enum no-install                     { value 1; }
                    enum route-table-install-only       { value 2; }
                }
                description "Label IP routes installed for local use on NHS router";
            }

            ///nokia-conf/configure/router/bgp/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/router/bgp/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/router/bgp/hold-time
            container hold-time {
                description "Enter the hold-time context";

                ///nokia-conf/configure/router/bgp/hold-time/seconds
                leaf seconds {
                    type int32 {
                        range "0|3..65535";
                    }
                    default "90";
                    description "Maximum time BGP waits between successive messages";
                }

                ///nokia-conf/configure/router/bgp/hold-time/minimum-hold-time
                leaf minimum-hold-time {
                    type int32 {
                        range "0|3..65535";
                    }
                    default "0";
                    description "Minimum hold time between successive messages";
                }

            } // container hold-time

            ///nokia-conf/configure/router/bgp/family
            container family {
                description "Enter the family context";

                ///nokia-conf/configure/router/bgp/family/ipv4
                leaf ipv4 {
                    type boolean;
                    default "true";
                    description "Advertise MP-BGP support for the IPv4 address family";
                }

                ///nokia-conf/configure/router/bgp/family/vpn-ipv4
                leaf vpn-ipv4 {
                    type boolean;
                    default "false";
                    description "Advertise MP-BGP support for IPv4 VPN address family";
                }

                ///nokia-conf/configure/router/bgp/family/ipv6
                leaf ipv6 {
                    type boolean;
                    default "false";
                    description "Advertise MP-BGP support for the IPv6 address family";
                }

                ///nokia-conf/configure/router/bgp/family/mcast-ipv4
                leaf mcast-ipv4 {
                    type boolean;
                    default "false";
                    description "Advertise support for the MCAST-IPv4 address family";
                }

                ///nokia-conf/configure/router/bgp/family/vpn-ipv6
                leaf vpn-ipv6 {
                    type boolean;
                    default "false";
                    description "Advertise MP-BGP support for IPv6 VPN address family";
                }

                ///nokia-conf/configure/router/bgp/family/l2-vpn
                leaf l2-vpn {
                    type boolean;
                    default "false";
                    description "Advertise MP-BGP support for the L2-VPN address family";
                }

                ///nokia-conf/configure/router/bgp/family/mvpn-ipv4
                leaf mvpn-ipv4 {
                    type boolean;
                    default "false";
                    description "Advertise support for the IPv4 MCAST VPN address family";
                }

                ///nokia-conf/configure/router/bgp/family/mdt-safi
                leaf mdt-safi {
                    type boolean;
                    default "false";
                    description "Advertise MP-BGP support for MDT-SAFI address family";
                }

                ///nokia-conf/configure/router/bgp/family/ms-pw
                leaf ms-pw {
                    type boolean;
                    default "false";
                    description "Advertise support for multi-segment PW address family";
                }

                ///nokia-conf/configure/router/bgp/family/flow-ipv4
                leaf flow-ipv4 {
                    type boolean;
                    default "false";
                    description "Advertise support for the flowspec-IPv4 address family";
                }

                ///nokia-conf/configure/router/bgp/family/route-target
                leaf route-target {
                    type boolean;
                    default "false";
                    description "Advertise MP-BGP support for RT constraint routes";
                }

                ///nokia-conf/configure/router/bgp/family/mcast-vpn-ipv4
                leaf mcast-vpn-ipv4 {
                    type boolean;
                    default "false";
                    description "Advertise support for the IPv4 VPN MCAST address family";
                }

                ///nokia-conf/configure/router/bgp/family/mvpn-ipv6
                leaf mvpn-ipv6 {
                    type boolean;
                    default "false";
                    description "Advertise support for the IPv6 MCAST VPN address family";
                }

                ///nokia-conf/configure/router/bgp/family/flow-ipv6
                leaf flow-ipv6 {
                    type boolean;
                    default "false";
                    description "Advertise support for the flowspec-IPv6 address family";
                }

                ///nokia-conf/configure/router/bgp/family/evpn
                leaf evpn {
                    type boolean;
                    default "false";
                    description "Advertise MP-BGP support for the EVPN address family";
                }

                ///nokia-conf/configure/router/bgp/family/mcast-ipv6
                leaf mcast-ipv6 {
                    type boolean;
                    default "false";
                    description "Advertise support for the MCAST-IPv6 address family";
                }

                ///nokia-conf/configure/router/bgp/family/label-ipv4
                leaf label-ipv4 {
                    type boolean;
                    default "false";
                    description "Advertise support for the label-IPv4 address family";
                }

                ///nokia-conf/configure/router/bgp/family/label-ipv6
                leaf label-ipv6 {
                    type boolean;
                    default "false";
                    description "Advertise support for the label-IPv6 address family";
                }

                ///nokia-conf/configure/router/bgp/family/bgp-ls
                leaf bgp-ls {
                    type boolean;
                    default "false";
                    description "Advertise MP-BGP support for the BGP-LS address family";
                }

                ///nokia-conf/configure/router/bgp/family/mcast-vpn-ipv6
                leaf mcast-vpn-ipv6 {
                    type boolean;
                    default "false";
                    description "Advertise support for the IPv6 VPN MCAST address family";
                }

                ///nokia-conf/configure/router/bgp/family/sr-policy-ipv4
                leaf sr-policy-ipv4 {
                    type boolean;
                    default "false";
                    description "Advertise MP-BGP support for the SR-policy-IPv4 family";
                }

                ///nokia-conf/configure/router/bgp/family/sr-policy-ipv6
                leaf sr-policy-ipv6 {
                    type boolean;
                    default "false";
                    description "Advertise MP-BGP support for the SR-policy-IPv6 family";
                }

                ///nokia-conf/configure/router/bgp/family/flow-vpn-ipv4
                leaf flow-vpn-ipv4 {
                    type boolean;
                    default "false";
                    description "Advertise support for FlowSpec-VPN IPv4 address family";
                }

                ///nokia-conf/configure/router/bgp/family/flow-vpn-ipv6
                leaf flow-vpn-ipv6 {
                    type boolean;
                    default "false";
                    description "Advertise support for FlowSpec-VPN IPv6 address family";
                }

            } // container family

            ///nokia-conf/configure/router/bgp/label-allocation
            container label-allocation {
                description "Enter the label-allocation context";

                ///nokia-conf/configure/router/bgp/label-allocation/label-ipv6
                container label-ipv6 {
                    description "Enter the label-ipv6 context";

                    ///nokia-conf/configure/router/bgp/label-allocation/label-ipv6/explicit-null
                    leaf explicit-null {
                        type boolean;
                        default "true";
                        description "Apply advertised label and associated programming rules";
                    }

                } // container label-ipv6

            } // container label-allocation

            ///nokia-conf/configure/router/bgp/send-default
            container send-default {
                description "Enter the send-default context";

                ///nokia-conf/configure/router/bgp/send-default/ipv4
                leaf ipv4 {
                    type boolean;
                    default "false";
                    description "Enable IPv4 family type";
                }

                ///nokia-conf/configure/router/bgp/send-default/ipv6
                leaf ipv6 {
                    type boolean;
                    default "false";
                    description "Enable IPv6 family type";
                }

                ///nokia-conf/configure/router/bgp/send-default/export-policy
                leaf export-policy {
                    type leafref {
                        path "../../../../policy-options/policy-statement/name";
                    }
                    description "Export policy name";
                }

            } // container send-default

            ///nokia-conf/configure/router/bgp/default-preference
            container default-preference {
                description "Enter the default-preference context";

                ///nokia-conf/configure/router/bgp/default-preference/ebgp
                leaf ebgp {
                    type uint32 {
                        range "0..255";
                    }
                    default "0";
                    description "Default preference for EBGP";
                }

                ///nokia-conf/configure/router/bgp/default-preference/ibgp
                leaf ibgp {
                    type uint32 {
                        range "0..255";
                    }
                    default "0";
                    description "Default preference for IBGP";
                }

            } // container default-preference

            ///nokia-conf/configure/router/bgp/default-label-preference
            container default-label-preference {
                description "Enter the default-label-preference context";

                ///nokia-conf/configure/router/bgp/default-label-preference/ebgp
                leaf ebgp {
                    type uint32 {
                        range "0..255";
                    }
                    default "0";
                    description "Default preference for EBGP";
                }

                ///nokia-conf/configure/router/bgp/default-label-preference/ibgp
                leaf ibgp {
                    type uint32 {
                        range "0..255";
                    }
                    default "0";
                    description "Default preference for IBGP";
                }

            } // container default-label-preference

            ///nokia-conf/configure/router/bgp/remove-private
            container remove-private {
                presence "Enable the remove-private context";
                description "Enable the remove-private context";

                ///nokia-conf/configure/router/bgp/remove-private/limited
                leaf limited {
                    type boolean;
                    default "false";
                    description "Remove private ASNs up to first public ASN encountered";
                }

                ///nokia-conf/configure/router/bgp/remove-private/skip-peer-as
                leaf skip-peer-as {
                    type boolean;
                    default "false";
                    description "Keep private ASN if AS-PATH contains eBGP peer's ASN";
                }

                ///nokia-conf/configure/router/bgp/remove-private/replace
                leaf replace {
                    type boolean;
                    default "false";
                    description "Replace private ASN with global ASN before advertising";
                }

            } // container remove-private

            ///nokia-conf/configure/router/bgp/cluster
            container cluster {
                description "Enter the cluster context";

                ///nokia-conf/configure/router/bgp/cluster/cluster-id
                leaf cluster-id {
                    type types-sros:ipv4-address;
                    description "Route reflector cluster ID";
                }

                ///nokia-conf/configure/router/bgp/cluster/orr-location
                leaf orr-location {
                    type uint32 {
                        range "1..255";
                    }
                    description "Optimal route reflection location for the cluster";
                }

                ///nokia-conf/configure/router/bgp/cluster/allow-local-fallback
                leaf allow-local-fallback {
                    type boolean;
                    default "false";
                    description "Allow fallback to RR topology location for ORR";
                }

            } // container cluster

            ///nokia-conf/configure/router/bgp/ebgp-default-reject-policy
            container ebgp-default-reject-policy {
                description "Enter the ebgp-default-reject-policy context";

                ///nokia-conf/configure/router/bgp/ebgp-default-reject-policy/import
                leaf import {
                    type boolean;
                    default "true";
                    description "Enable default reject import policy for external peers";
                }

                ///nokia-conf/configure/router/bgp/ebgp-default-reject-policy/export
                leaf export {
                    type boolean;
                    default "true";
                    description "Enable default reject export policy for external peers";
                }

            } // container ebgp-default-reject-policy

            ///nokia-conf/configure/router/bgp/bfd-strict-mode
            container bfd-strict-mode {
                description "Enter the bfd-strict-mode context";

                ///nokia-conf/configure/router/bgp/bfd-strict-mode/next-hop-reachability
                leaf next-hop-reachability {
                    type boolean;
                    default "false";
                    description "Consider next hop unreachable if BFD session is down";
                }

                ///nokia-conf/configure/router/bgp/bfd-strict-mode/advertise
                container advertise {
                    presence "Enable the advertise context";
                    description "Enable the advertise context";

                    ///nokia-conf/configure/router/bgp/bfd-strict-mode/advertise/holdtime
                    leaf holdtime {
                        type uint16 {
                            range "1..65535";
                        }
                        units "seconds";
                        default "30";
                        description "Maximum time BGP waits for the BFD session to come up";
                    }

                } // container advertise

            } // container bfd-strict-mode

            ///nokia-conf/configure/router/bgp/local-as
            container local-as {
                description "Enter the local-as context";

                ///nokia-conf/configure/router/bgp/local-as/as-number
                leaf as-number {
                    type uint32 {
                        range "1..4294967295";
                    }
                    description "Local (or virtual) BGP AS number";
                }

                ///nokia-conf/configure/router/bgp/local-as/private
