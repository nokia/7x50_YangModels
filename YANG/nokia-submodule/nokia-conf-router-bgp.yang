                    choice type {
                        ///nokia-conf/configure/router/bgp/segment-routing/prefix-sid-range/type/global
                        case global {

                            ///nokia-conf/configure/router/bgp/segment-routing/prefix-sid-range/type/global/global
                            leaf global {
                                type empty;
                                description "Allow label allocation from entire SRBG space";
                            }

                        }
                        ///nokia-conf/configure/router/bgp/segment-routing/prefix-sid-range/type/local
                        case local {

                            ///nokia-conf/configure/router/bgp/segment-routing/prefix-sid-range/type/local/start-label
                            leaf start-label {
                                type uint32 {
                                    range "0..524287";
                                }
                                sros-ext:auto-restart-to-modify;
                                default "0";
                                description "First label value available in a contiguous range";
