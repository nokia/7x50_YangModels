                ///nokia-conf/configure/router/bgp/neighbor/aggregator-id-zero
                leaf aggregator-id-zero {
                    type boolean;
                    description "Set router ID in the BGP AGGREGATOR attribute to zero";
                }

                ///nokia-conf/configure/router/bgp/neighbor/preference
                leaf preference {
                    type uint32 {
                        range "1..255";
