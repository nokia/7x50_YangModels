                ///nokia-conf/configure/router/bgp/neighbor/peer-as
                leaf peer-as {
                    type uint32 {
                        range "1..4294967295";
                    }
                    description "Peer AS number";
                }

                ///nokia-conf/configure/router/bgp/neighbor/capability-negotiation
                leaf capability-negotiation {
                    type boolean;
                    description "Enable capability negotiation";
                }

