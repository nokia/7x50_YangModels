                                description "Strip SRv6 TLVs from BGP routes advertised to peers";
                            }

                            ///nokia-conf/configure/router/bgp/neighbor/segment-routing-v6/route-advertisement/family/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/router/bgp/neighbor/segment-routing-v6/route-advertisement/family/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list family

                    } // container route-advertisement

                } // container segment-routing-v6

            } // list neighbor

            ///nokia-conf/configure/router/bgp/bgp-tunnel-metric
            container bgp-tunnel-metric {
                description "Enter the bgp-tunnel-metric context";

                ///nokia-conf/configure/router/bgp/bgp-tunnel-metric/value
                leaf value {
                    type uint32 {
                        range "0..4294967295";
                    }
                    default "1000";
                    description "BGP tunnel metric value";
                }

                ///nokia-conf/configure/router/bgp/bgp-tunnel-metric/prefer-aigp
                leaf prefer-aigp {
                    type boolean;
                    default "false";
                    description "Use the AIGP attribute as tunnel metric when present";
                }

                ///nokia-conf/configure/router/bgp/bgp-tunnel-metric/prefer-med
                leaf prefer-med {
                    type boolean;
                    default "false";
                    description "Use the MED attribute as tunnel metric when present";
                }

            } // container bgp-tunnel-metric

            ///nokia-conf/configure/router/bgp/segment-routing-v6
            container segment-routing-v6 {
                presence "Enable the segment-routing-v6 context";
                description "Enable the segment-routing-v6 context";

                ///nokia-conf/configure/router/bgp/segment-routing-v6/source-address
                leaf source-address {
                    type types-sros:ipv6-unicast-address;
                    description "Source address used in SRv6 packet";
                }

                ///nokia-conf/configure/router/bgp/segment-routing-v6/family
                list family {
                    key "family-type";
                    description "Enter the family list instance";

                    ///nokia-conf/configure/router/bgp/segment-routing-v6/family/family-type
                    leaf family-type {
                        type enumeration {
                            enum ipv4                           { value 1; }
                            enum ipv6                           { value 3; }
                        }
                        description "IP address type that SRv6 route attributes apply to";
                    }

                    ///nokia-conf/configure/router/bgp/segment-routing-v6/family/ignore-received-srv6-tlvs
                    leaf ignore-received-srv6-tlvs {
                        type boolean;
                        default "true";
                        description "Ignore SRv6 TLVs for received routes";
                    }

                    ///nokia-conf/configure/router/bgp/segment-routing-v6/family/resolution
                    leaf resolution {
                        type types-sros:srv6-route-resolution;
                        default "route-table";
                        description "Resolution options for routes";
                    }

                    ///nokia-conf/configure/router/bgp/segment-routing-v6/family/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/bgp/segment-routing-v6/family/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/router/bgp/segment-routing-v6/family/add-srv6-tlvs
                    container add-srv6-tlvs {
                        presence "Enable the add-srv6-tlvs context";
                        description "Enable the add-srv6-tlvs context";

                        ///nokia-conf/configure/router/bgp/segment-routing-v6/family/add-srv6-tlvs/locator-name
                        leaf locator-name {
                            type leafref {
                                path "../../../../../segment-routing/segment-routing-v6/base-routing-instance/locator/locator-name";
                            }
                            description "SRv6 locator name";
                        }

                        ///nokia-conf/configure/router/bgp/segment-routing-v6/family/add-srv6-tlvs/micro-segment-locator-name
                        leaf micro-segment-locator-name {
                            type leafref {
                                path "../../../../../../router/segment-routing/segment-routing-v6/base-routing-instance/micro-segment-locator/locator-name";
                            }
                            description "Micro-segment SRv6 locator name";
                        }

                    } // container add-srv6-tlvs

                } // list family

            } // container segment-routing-v6

        } // container bgp

    } // grouping conf-router-bgp

}
