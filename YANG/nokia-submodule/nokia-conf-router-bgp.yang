                ///nokia-conf/configure/router/bgp/neighbor/connect-retry
                leaf connect-retry {
                    type int32 {
                        range "1..65535";
                    }
                    description "BGP connect retry timer value";
                }

                ///nokia-conf/configure/router/bgp/neighbor/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
