                    type int32 {
                        range "0..21845";
                    }
                    description "Time after which the BGP KEEPALIVE message is sent";
                }

                ///nokia-conf/configure/router/bgp/group/damping
                leaf damping {
                    type boolean;
                    description "Use BGP route damping to reduce route flap";
                }

                ///nokia-conf/configure/router/bgp/group/local-preference
