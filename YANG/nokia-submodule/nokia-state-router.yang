                                leaf nexthop-index {
                                    type uint32;
                                    description "Index of ECMP next-hop or backup next-hop.";
                                }

                                ///nokia-state/state/router/route-table/unicast/ipv6/route/nexthop/flags
                                leaf flags {
                                    type bits {
                                        bit nh-is-indirect                 { position 0; }
