                        ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/name
                        leaf name {
                            type types-sros:named-item;
                            description "IPsec tunnel name";
                        }

                        ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/host-mda
                        leaf host-mda {
                            type types-isa:slot-mda;
                            description "Active ISA that is being used to host this tunnel.";
