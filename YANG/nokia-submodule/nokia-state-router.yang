                        ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa
                        list ipsec-sa {
                            key "security-association direction index";
                            description "Enter the ipsec-sa list instance";

                            ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/security-association
                            leaf security-association {
                                type uint32 {
                                    range "1..16";
                                }
                                description "The unique identifier of an security association (SA).";
                            }

                            ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/direction
                            leaf direction {
                                type enumeration {
                                    enum inbound                        { value 1; }
                                    enum outbound                       { value 2; }
                                }
                                description "The direction on the IPsec tunnel to which this SA can be applied.";
                            }

                            ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/index
                            leaf index {
                                type uint32 {
                                    range "1..2";
                                }
                                description "The unique identifier of an security association (SA).";
                            }

                            ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/spi
                            leaf spi {
                                type uint32;
                                description
                                    "The SPI (Security Parameter Index) used to lookup the instruction to verify and decrypt
                                     the incoming IPsec packets when the value of direction is 'inbound'.";
                            }

                            ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/authentication-algorithm
                            leaf authentication-algorithm {
                                type types-ipsec:auth-algorithms;
                                description "The authentication algorithm used with this SA.";
                            }

                            ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/encryption-algorithm
                            leaf encryption-algorithm {
                                type types-ipsec:encryption-algorithms;
                                description "The encryption algorithm used with this SA.";
                            }

                            ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/established-time
                            leaf established-time {
                                type yang:date-and-time;
                                description "The time when the IPsec SA negotiation completed.";
                            }

                            ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/negotiated-lifetime
                            leaf negotiated-lifetime {
                                type types-sros:time-duration;
                                description "The lifetime negotiated for the IPsec SA.";
                            }

                            ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/security-association-type
                            leaf security-association-type {
                                type types-ipsec:keying-type;
                                description
                                    "Whether this SA entry is created manually by the user or
                                     dynamically by the IPsec subsystem.";
                            }

                            ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa
                            container isa {
                                description "Enter the isa context";

                                ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/multicast-interface-name
                                leaf multicast-interface-name {
                                    type types-sros:named-item;
                                    description "The multicast interface name associated with this SA";
                                }

                                ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/multicast-protocol
                                leaf multicast-protocol {
                                    type types-ipsec:multicast-protocol;
                                    description "The supported protocol types of the multicast interface associated to this SA";
                                }

                                ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/pfs-dh-group
                                leaf pfs-dh-group {
                                    type types-ipsec:dh-group;
                                    description "The Diffie-Hellman (DH) group used with this SA";
                                }

                                ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/extended-sequence-number
                                leaf extended-sequence-number {
                                    type boolean;
                                    description "The value 'true' indicates ESN is enabled for this SA";
                                }

                                ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/fragmentation
                                container fragmentation {
                                    description "Enter the fragmentation context";

                                    ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/fragmentation/encapsulation-overhead
                                    leaf encapsulation-overhead {
                                        type uint32;
                                        description "The encapsulation overhead for this outbound SA.";
                                    }

                                    ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/fragmentation/temporary-private-mtu
                                    leaf temporary-private-mtu {
                                        type uint32;
                                        units "bytes";
                                        description "The size of temporary private MTU for this SA.";
                                    }

                                    ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/fragmentation/pre-encapsulation
                                    container pre-encapsulation {
                                        description "Enter the pre-encapsulation context";

                                        ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/fragmentation/pre-encapsulation/last-packet-size
                                        leaf last-packet-size {
                                            type uint32;
                                            units "bytes";
                                            description
                                                "The size of the last packet which caused a pre-encapsulation fragmentation
                                                 to occur for this SA.";
                                        }

                                    } // container pre-encapsulation

                                    ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/fragmentation/post-encapsulation
                                    container post-encapsulation {
                                        description "Enter the post-encapsulation context";

                                        ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/fragmentation/post-encapsulation/last-packet-size
                                        leaf last-packet-size {
                                            type uint32;
                                            units "bytes";
                                            description
                                                "The size of the last encapsulated packet which caused a post-encapsulation
                                                 fragmentation to occur for this SA.";
                                        }

                                    } // container post-encapsulation

                                } // container fragmentation

                                ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/statistics
                                container statistics {
                                    description "Enter the statistics context";

                                    ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/statistics/bytes-processed
                                    leaf bytes-processed {
                                        type yang:counter64;
                                        description "The number of bytes successfully processed for this SA";
                                    }

                                    ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/statistics/packets-processed
                                    leaf packets-processed {
                                        type yang:counter64;
                                        description "The the number of packets successfully processed for this SA";
                                    }

                                    ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/statistics/errors
                                    container errors {
                                        description "Enter the errors context";

                                        ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/statistics/errors/crypto
                                        leaf crypto {
                                            type yang:counter32;
                                            description
                                                "The number of crypto errors encountered on this SA.
                                                     
                                                 When the value of direction is 'inbound', the crypto-errors will be set for the following errors:
                                                     MAC miscompare
                                                     Pad errors
                                                     Illegal configure algorithm
                                                     Illegal authentication algorithm
                                                     Inner IP checksum errors
                                                     Payload alignment errors
                                                     Sequence number errors
                                                     Protocol errors
                                                      
                                                 When the value of direction is 'outbound', the crypto-errors will be set for the following errors:
                                                     Sequence wrap errors
                                                     Illegal configure algorithm
                                                     Illegal authentication algorithm
                                                     Expanded packet too big
                                                     TTL decrement errors.";
                                        }

                                        ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/statistics/errors/replay
                                        leaf replay {
                                            type yang:counter32;
                                            description "The number of replay errors encountered on this SA";
                                        }

                                        ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/statistics/errors/security-association
                                        leaf security-association {
                                            type yang:counter32;
                                            description
                                                "The number of SA errors encountered on this SA.
                                                 The SA errors means ISA tried to use a CHILD SA that is marked for deletion.";
                                        }

                                        ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/statistics/errors/policy
                                        leaf policy {
                                            type yang:counter32;
                                            description
                                                "The number of policy errors encountered on this SA.
                                                 The policy errors include bundled SA, selector check and policy direction error.";
                                        }

                                    } // container errors

                                    ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/statistics/fragmentation
                                    container fragmentation {
                                        description "Enter the fragmentation context";

                                        ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/statistics/fragmentation/pre-encapsulation
                                        container pre-encapsulation {
                                            description "Enter the pre-encapsulation context";

                                            ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/statistics/fragmentation/pre-encapsulation/count
                                            leaf count {
                                                type yang:counter64;
                                                description
                                                    "The number of fragmentations that occurred prior to encapsulation for this outbound SA.
                                                     Pre-encapsulation fragmentation occurs for IPv4 packets whose size exceeds tmnxIPsecTunnelIpMtu.";
                                            }

                                        } // container pre-encapsulation

                                        ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/statistics/fragmentation/post-encapsulation
                                        container post-encapsulation {
                                            description "Enter the post-encapsulation context";

                                            ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/ipsec-sa/isa/statistics/fragmentation/post-encapsulation/count
                                            leaf count {
                                                type yang:counter64;
                                                description
                                                    "The number of fragmentations that occurred after encapsulation for this SA.
                                                     Post-encapsulation fragmentation occurs when the encapsulated packet size exceeds tmnxIPsecTunnelEncapIpMtu.";
                                            }

                                        } // container post-encapsulation

                                    } // container fragmentation

                                } // container statistics

                            } // container isa

                        } // list ipsec-sa


