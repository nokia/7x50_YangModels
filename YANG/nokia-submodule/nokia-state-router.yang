                                            ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/dh-group
                                            leaf dh-group {
                                                type types-ipsec:dh-group;
                                                description "The Diffie-Hellman (DH) group of the IKE SA for this IPsec tunnel.";
                                            }

                                            ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/cert-profile-entry-id
                                            leaf cert-profile-entry-id {
                                                type uint32;
                                                description "The matching certificate profile entry id used for this tunnel";
