                                                ///nokia-state/state/router/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-sa/statistics/ctrl-packets-received
                                                leaf ctrl-packets-received {
                                                    type yang:counter32;
