submodule nokia-state-router-msdp {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-sros               { prefix "types-sros"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for  The Multicast Source Discovery Protocol (MSDP) is used to connect multicast routing domains. on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-router-msdp YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2020-09-16";

    grouping state-router-msdp {
        ///nokia-state/state/router/msdp
        container msdp {
            description "Enable the msdp context";

            ///nokia-state/state/router/msdp/source-active-reject-export-policy
            leaf source-active-reject-export-policy {
                type yang:counter32;
                description "The number of SA messages that were not sent due to export policy";
            }

            ///nokia-state/state/router/msdp/source-active-reject-import-policy
            leaf source-active-reject-import-policy {
                type yang:counter32;
                description "The number of SA messages that rejected due to import policy";
            }

            ///nokia-state/state/router/msdp/group
            list group {
                key "name";
                description "Enter the group list instance";

                ///nokia-state/state/router/msdp/group/name
                leaf name {
                    type types-sros:named-item;
                    description "MSDP group name";
                }

                ///nokia-state/state/router/msdp/group/source-active-reject-export-policy
                leaf source-active-reject-export-policy {
                    type yang:counter32;
                    description "The number of SA messages that were not sent due to export policy";
                }

                ///nokia-state/state/router/msdp/group/source-active-reject-import-policy
                leaf source-active-reject-import-policy {
                    type yang:counter32;
                    description "The number of SA messages that rejected due to import policy";
                }

                ///nokia-state/state/router/msdp/group/active-source-limit-exceeded
                leaf active-source-limit-exceeded {
                    type yang:counter32;
                    description
                        "The number of source active messages received from this
                         group that exceeded the established maximum number";
                }

                ///nokia-state/state/router/msdp/group/peer
                list peer {
                    key "ip-address";
                    description "Enter the peer list instance";

                    ///nokia-state/state/router/msdp/group/peer/ip-address
                    leaf ip-address {
                        type types-sros:ipv4-unicast-address;
                        description "IP address of the remote MSDP router for peering";
                    }

                    ///nokia-state/state/router/msdp/group/peer/peer-state
                    leaf peer-state {
                        type enumeration {
                            enum listen                         { value 0; }
                            enum established                    { value 1; }
                            enum inactive                       { value 2; }
                            enum disabled                       { value 3; }
                            enum connecting                     { value 4; }
                        }
                        description "The status of connection";
                    }

                    ///nokia-state/state/router/msdp/group/peer/last-peer-state-change
                    leaf last-peer-state-change {
                        type types-sros:date-time;
                        description "The time at which the most recent peer-state change occurred";
                    }

                    ///nokia-state/state/router/msdp/group/peer/connection-retry
                    leaf connection-retry {
                        type yang:counter32;
                        description "The number of peer connection retry attempts";
                    }

                    ///nokia-state/state/router/msdp/group/peer/state-timer
                    leaf state-timer {
                        type uint32;
                        units "seconds";
                        description "The number of seconds to wait before another message is sent to a peer";
                    }

                    ///nokia-state/state/router/msdp/group/peer/timeout
                    leaf timeout {
                        type uint32;
                        units "seconds";
                        description
                            "The number of seconds to wait for a response from the peer before declaring
                             the peer unavailable";
                    }

                    ///nokia-state/state/router/msdp/group/peer/active-source-accepted
                    leaf active-source-accepted {
                        type yang:counter32;
                        description "The number of source-active cache accepted from the peer";
                    }

                    ///nokia-state/state/router/msdp/group/peer/active-source-received
                    leaf active-source-received {
                        type yang:counter32;
                        description "The number of source-active cache received by the peer";
                    }

                    ///nokia-state/state/router/msdp/group/peer/last-active-source-limit
                    leaf last-active-source-limit {
                        type types-sros:date-time;
                        description "The time that the last active source limit was triggered";
                    }

                    ///nokia-state/state/router/msdp/group/peer/operational-local-address
                    leaf operational-local-address {
                        type types-sros:ipv4-unicast-address;
                        description "The operational value of the local address";
                    }

                    ///nokia-state/state/router/msdp/group/peer/statistics
                    container statistics {
                        description "Enter the statistics context";

                        ///nokia-state/state/router/msdp/group/peer/statistics/active-source-limit-exceeded
                        leaf active-source-limit-exceeded {
                            type yang:counter32;
                            description
                                "The number of source active messages received from this
                                 group that exceeded the established maximum number";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/last-state-change
                        leaf last-state-change {
                            type types-sros:time-interval;
                            description "This indicates how long ago the peer state changed";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/rpf-failures
                        leaf rpf-failures {
                            type yang:counter32;
                            description "The number of reverse path forwarding (RPF) failures";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/remote-closes
                        leaf remote-closes {
                            type yang:counter32;
                            description "The number of times the remote peer closed";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/peer-timeouts
                        leaf peer-timeouts {
                            type yang:counter32;
                            description "The number of peer timeouts";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/source-active-messages-tx
                        leaf source-active-messages-tx {
                            type yang:counter32;
                            description "The number of source-active messages sent";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/source-active-messages-rx
                        leaf source-active-messages-rx {
                            type yang:counter32;
                            description "The number of source-active messages received";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/source-active-requests-messages-tx
                        leaf source-active-requests-messages-tx {
                            type yang:counter32;
                            description "The number of source-active request messages sent";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/source-active-requests-messages-rx
                        leaf source-active-requests-messages-rx {
                            type yang:counter32;
                            description "The number of source-active request messages received";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/source-active-response-messages-tx
                        leaf source-active-response-messages-tx {
                            type yang:counter32;
                            description "The number of source-active response messages sent";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/source-active-response-messages-rx
                        leaf source-active-response-messages-rx {
                            type yang:counter32;
                            description "The number of source-active response messages received";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/keepalive-messages-tx
                        leaf keepalive-messages-tx {
                            type yang:counter32;
                            description "The number of keep-alive messages sent";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/keepalive-messages-rx
                        leaf keepalive-messages-rx {
                            type yang:counter32;
                            description "The number of keep-alive messages received";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/unknown-messages-rx
                        leaf unknown-messages-rx {
                            type yang:counter32;
                            description "The number of unknown messages received";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/error-messages-rx
                        leaf error-messages-rx {
                            type yang:counter32;
                            description "The number of error messages received";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/source-active-learnt
                        leaf source-active-learnt {
                            type yang:gauge32;
                            description
                                "The number of unique source active entries in the cache
                                 learned from the peer";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/source-active-reject-export-policy
                        leaf source-active-reject-export-policy {
                            type yang:counter32;
                            description
                                "The number of source active messages from the peer that
                                 were not sent due to export policy";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/source-active-reject-import-policy
                        leaf source-active-reject-import-policy {
                            type yang:counter32;
                            description
                                "The number of source active messages from the peer that
                                 were not sent due to import policy";
                        }

                        ///nokia-state/state/router/msdp/group/peer/statistics/reserved-messages-rx
                        leaf reserved-messages-rx {
                            type yang:counter32;
                            description "The number of MSDP messages received with type 'Reserved'";
                        }

                    } // container statistics

                } // list peer

            } // list group

            ///nokia-state/state/router/msdp/peer
            list peer {
                key "ip-address";
                description "Enter the peer list instance";

                ///nokia-state/state/router/msdp/peer/ip-address
                leaf ip-address {
                    type types-sros:ipv4-unicast-address;
                    description "IP address of the remote MSDP router for peering";
                }

                ///nokia-state/state/router/msdp/peer/peer-state
                leaf peer-state {
                    type enumeration {
                        enum listen                         { value 0; }
                        enum established                    { value 1; }
                        enum inactive                       { value 2; }
                        enum disabled                       { value 3; }
                        enum connecting                     { value 4; }
                    }
                    description "The status of connection";
                }

                ///nokia-state/state/router/msdp/peer/last-peer-state-change
                leaf last-peer-state-change {
                    type types-sros:date-time;
                    description "The time at which the most recent peer-state change occurred";
                }

                ///nokia-state/state/router/msdp/peer/connection-retry
                leaf connection-retry {
                    type yang:counter32;
                    description "The number of peer connection retry attempts";
                }

                ///nokia-state/state/router/msdp/peer/state-timer
                leaf state-timer {
                    type uint32;
                    units "seconds";
                    description "The number of seconds to wait before another message is sent to a peer";
                }

                ///nokia-state/state/router/msdp/peer/timeout
                leaf timeout {
                    type uint32;
                    units "seconds";
                    description
                        "The number of seconds to wait for a response from the peer before declaring
                         the peer unavailable";
                }

                ///nokia-state/state/router/msdp/peer/active-source-accepted
                leaf active-source-accepted {
                    type yang:counter32;
                    description "The number of source-active cache accepted from the peer";
                }

                ///nokia-state/state/router/msdp/peer/active-source-received
                leaf active-source-received {
                    type yang:counter32;
                    description "The number of source-active cache received by the peer";
                }

                ///nokia-state/state/router/msdp/peer/last-active-source-limit
                leaf last-active-source-limit {
                    type types-sros:date-time;
                    description "The time that the last active source limit was triggered";
                }

                ///nokia-state/state/router/msdp/peer/operational-local-address
                leaf operational-local-address {
                    type types-sros:ipv4-unicast-address;
                    description "The operational value of the local address";
                }

                ///nokia-state/state/router/msdp/peer/statistics
                container statistics {
                    description "Enter the statistics context";

                    ///nokia-state/state/router/msdp/peer/statistics/active-source-limit-exceeded
                    leaf active-source-limit-exceeded {
                        type yang:counter32;
                        description
                            "The number of source active messages received from this
                             group that exceeded the established maximum number";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/last-state-change
                    leaf last-state-change {
                        type types-sros:time-interval;
                        description "This indicates how long ago the peer state changed";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/rpf-failures
                    leaf rpf-failures {
                        type yang:counter32;
                        description "The number of reverse path forwarding (RPF) failures";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/remote-closes
                    leaf remote-closes {
                        type yang:counter32;
                        description "The number of times the remote peer closed";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/peer-timeouts
                    leaf peer-timeouts {
                        type yang:counter32;
                        description "The number of peer timeouts";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/source-active-messages-tx
                    leaf source-active-messages-tx {
                        type yang:counter32;
                        description "The number of source-active messages sent";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/source-active-messages-rx
                    leaf source-active-messages-rx {
                        type yang:counter32;
                        description "The number of source-active messages received";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/source-active-requests-messages-tx
                    leaf source-active-requests-messages-tx {
                        type yang:counter32;
                        description "The number of source-active request messages sent";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/source-active-requests-messages-rx
                    leaf source-active-requests-messages-rx {
                        type yang:counter32;
                        description "The number of source-active request messages received";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/source-active-response-messages-tx
                    leaf source-active-response-messages-tx {
                        type yang:counter32;
                        description "The number of source-active response messages sent";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/source-active-response-messages-rx
                    leaf source-active-response-messages-rx {
                        type yang:counter32;
                        description "The number of source-active response messages received";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/keepalive-messages-tx
                    leaf keepalive-messages-tx {
                        type yang:counter32;
                        description "The number of keep-alive messages sent";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/keepalive-messages-rx
                    leaf keepalive-messages-rx {
                        type yang:counter32;
                        description "The number of keep-alive messages received";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/unknown-messages-rx
                    leaf unknown-messages-rx {
                        type yang:counter32;
                        description "The number of unknown messages received";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/error-messages-rx
                    leaf error-messages-rx {
                        type yang:counter32;
                        description "The number of error messages received";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/source-active-learnt
                    leaf source-active-learnt {
                        type yang:gauge32;
                        description
                            "The number of unique source active entries in the cache
                             learned from the peer";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/source-active-reject-export-policy
                    leaf source-active-reject-export-policy {
                        type yang:counter32;
                        description
                            "The number of source active messages from the peer that
                             were not sent due to export policy";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/source-active-reject-import-policy
                    leaf source-active-reject-import-policy {
                        type yang:counter32;
                        description
                            "The number of source active messages from the peer that
                             were not sent due to import policy";
                    }

                    ///nokia-state/state/router/msdp/peer/statistics/reserved-messages-rx
                    leaf reserved-messages-rx {
                        type yang:counter32;
                        description "The number of MSDP messages received with type 'Reserved'";
                    }

                } // container statistics

            } // list peer

            ///nokia-state/state/router/msdp/source
            list source {
                key "ip-prefix";
                description "Enter the source list instance";

                ///nokia-state/state/router/msdp/source/ip-prefix
                leaf ip-prefix {
                    type types-sros:ipv4-unicast-prefix;
                    description "Source IP address for accepted active source messages";
                }

                ///nokia-state/state/router/msdp/source/discovery-method
                leaf discovery-method {
                    type enumeration {
                        enum dynamic                        { value 0; }
                        enum configured                     { value 1; }
                    }
                    description "The discovery method for this multicast source";
                }

                ///nokia-state/state/router/msdp/source/active-messages-exceed-max
                leaf active-messages-exceed-max {
                    type yang:counter32;
                    description
                        "The number of source active messages received from
                         the source that exceeded the established maximum number";
                }

                ///nokia-state/state/router/msdp/source/last-exceeded-event-time
                leaf last-exceeded-event-time {
                    type types-sros:date-time;
                    description "The time the last exceed event was triggered";
                }

            } // list source

            ///nokia-state/state/router/msdp/status
            container status {
                description "Enter the status context";

                ///nokia-state/state/router/msdp/status/peer-count
                leaf peer-count {
                    type yang:gauge32;
                    description "The number of configured peers";
                }

                ///nokia-state/state/router/msdp/status/peers-established
                leaf peers-established {
                    type yang:gauge32;
                    description "The number of peers in established state";
                }

                ///nokia-state/state/router/msdp/status/source-active-messages-count
                leaf source-active-messages-count {
                    type yang:gauge32;
                    description "The number of source active configured in cache";
                }

                ///nokia-state/state/router/msdp/status/last-time-up
                leaf last-time-up {
                    type types-sros:date-time;
                    description "The last time the protocol was enabled";
                }

                ///nokia-state/state/router/msdp/status/active-source-limit-exceeded
                leaf active-source-limit-exceeded {
                    type yang:counter32;
                    description
                        "The number of time the global active source
                         limit has been exceeded";
                }

            } // container status

            ///nokia-state/state/router/msdp/source-active
            list source-active {
                key "group source origin-rendez-vous-point";
                description "Enter the source-active list instance";

                ///nokia-state/state/router/msdp/source-active/group
                leaf group {
                    type types-sros:ipv4-multicast-address;
                    description "The group address of the source active";
                }

                ///nokia-state/state/router/msdp/source-active/source
                leaf source {
                    type types-sros:ipv4-unicast-address;
                    description "The source address of the source active";
                }

                ///nokia-state/state/router/msdp/source-active/origin-rendez-vous-point
                leaf origin-rendez-vous-point {
                    type types-sros:ipv4-address;
                    description "The origin rendez vous point address of the source active";
                }

                ///nokia-state/state/router/msdp/source-active/peer-learnt-from
                leaf peer-learnt-from {
                    type types-sros:ipv4-address;
                    description "The peer from which the source active was last accepted";
                }

                ///nokia-state/state/router/msdp/source-active/reverse-path-forwarding-peer
                leaf reverse-path-forwarding-peer {
                    type types-sros:ipv4-address;
                    description
                        "The peer from which an source active message corresponding
                         to this cache would be accepted";
                }

                ///nokia-state/state/router/msdp/source-active/up-time
                leaf up-time {
                    type types-sros:time-duration;
                    description "The time since this was first placed in the source active cache";
                }

                ///nokia-state/state/router/msdp/source-active/expiry-time
                leaf expiry-time {
                    type types-sros:time-interval;
                    description
                        "The time remaining before this will expire from the
                         source active cache";
                }

            } // list source-active

            ///nokia-state/state/router/msdp/source-active-rejected
            list source-active-rejected {
                key "group-name group source origin-rendez-vous-point peer";
                description "Enter the source-active-rejected list instance";

                ///nokia-state/state/router/msdp/source-active-rejected/group-name
                leaf group-name {
                    type types-sros:named-item-or-empty;
                    description "The peer group name of the source active that is rejected";
                }

                ///nokia-state/state/router/msdp/source-active-rejected/group
                leaf group {
                    type types-sros:ipv4-address;
                    description "The group address of the source active that is rejected";
                }

                ///nokia-state/state/router/msdp/source-active-rejected/source
                leaf source {
                    type types-sros:ipv4-address;
                    description "The source address of the source active that is rejected";
                }

                ///nokia-state/state/router/msdp/source-active-rejected/origin-rendez-vous-point
                leaf origin-rendez-vous-point {
                    type types-sros:ipv4-address;
                    description "The origin rendez vous point address of the source active that is rejected";
                }

                ///nokia-state/state/router/msdp/source-active-rejected/peer
                leaf peer {
                    type types-sros:ipv4-address;
                    description "The peer from which this rejected source active was last received";
                }

                ///nokia-state/state/router/msdp/source-active-rejected/failure-reason
                leaf failure-reason {
                    type enumeration {
                        enum import-policy-failure          { value 1; }
                        enum export-policy-failure          { value 2; }
                        enum reverse-path-forwarding-failure { value 3; }
                        enum active-source-limit-exceeded   { value 4; }
                        enum source-active-source-limit-exceeded { value 5; }
                        enum peer-active-source-limit-exceeded { value 6; }
                        enum group-active-source-limit-exceeded { value 7; }
                        enum group-peer-active-source-limit-exceeded { value 8; }
                    }
                    description "The reason why this source active is rejected";
                }

            } // list source-active-rejected

        } // container msdp

    } // grouping state-router-msdp

}
