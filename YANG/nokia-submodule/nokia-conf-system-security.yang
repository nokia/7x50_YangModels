                    key "policy-name";
                    description "Enter the radius-policy list instance";

                    ///nokia-conf/configure/system/security/dot1x/radius-policy/policy-name
                    leaf policy-name {
                        type types-sros:named-item;
                        description "RADIUS server policy name for 802.1X authentication";
                    }

                    ///nokia-conf/configure/system/security/dot1x/radius-policy/source-address
                    leaf source-address {
                        type types-sros:ipv4-unicast-address;
                        description "Source address of the RADIUS packet";
