submodule nokia-conf-system-security {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-security           { prefix "types-security"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-system             { prefix "types-system"; }

    include nokia-conf-groups;
    include nokia-conf-qos;
    include nokia-conf-system-security-aaa;
    include nokia-conf-system-security-cli-script;
    include nokia-conf-system-security-cpm;
    include nokia-conf-system-security-management-access-filter;
    include nokia-conf-system-security-pki;
    include nokia-conf-system-security-python-script;
    include nokia-conf-system-security-snmp;
    include nokia-conf-system-security-ssh;
    include nokia-conf-system-security-tls;
    include nokia-conf-system-security-user-parameters;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure system security parameters. on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-system-security YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2022-12-12";

    grouping conf-system-security {
        ///nokia-conf/configure/system/security
        container security {
            description "Enable the security context";

            ///nokia-conf/configure/system/security/telnet-server
            leaf telnet-server {
                type boolean;
                default "false";
                description "Enable Telnet servers running on the system";
            }

            ///nokia-conf/configure/system/security/telnet6-server
            leaf telnet6-server {
                type boolean;
                default "false";
                description "Enable Telnet IPv6 servers running on the system";
            }

            ///nokia-conf/configure/system/security/ftp-server
            leaf ftp-server {
                type boolean;
                default "false";
                description "Enable FTP servers running on the system";
            }

            ///nokia-conf/configure/system/security/per-peer-queuing
            leaf per-peer-queuing {
                type boolean;
                description "Allow CPM hardware queuing per peer";
            }

            ///nokia-conf/configure/system/security/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/system/security/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/system/security/vprn-network-exceptions
            container vprn-network-exceptions {
                presence "Enable the vprn-network-exceptions context";
                description "Enable the vprn-network-exceptions context";

                ///nokia-conf/configure/system/security/vprn-network-exceptions/count
                leaf count {
                    type uint32 {
                        range "10..1000";
                    }
                    default "100";
                    description "Limit of exception messages received";
                }

                ///nokia-conf/configure/system/security/vprn-network-exceptions/window
                leaf window {
                    type uint32 {
                        range "1..60";
                    }
                    units "seconds";
                    default "10";
                    description "Time interval to measure exception messages";
                }

            } // container vprn-network-exceptions

            ///nokia-conf/configure/system/security/cpu-protection
            container cpu-protection {
                description "Enable the cpu-protection context";

                ///nokia-conf/configure/system/security/cpu-protection/link-specific-rate
                leaf link-specific-rate {
                    type types-security:cpm-packet-rate-limit;
                    units "packets per second";
                    description "Packet arrival rate limit for link level protocols";
                }

                ///nokia-conf/configure/system/security/cpu-protection/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/system/security/cpu-protection/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/system/security/cpu-protection/port-overall-rate
                container port-overall-rate {
                    description "Enter the port-overall-rate context";

                    ///nokia-conf/configure/system/security/cpu-protection/port-overall-rate/pir
                    leaf pir {
                        type types-security:cpm-packet-rate-limit;
                        units "packets per second";
                        description "Per-port packet arrival rate limit";
                    }

                    ///nokia-conf/configure/system/security/cpu-protection/port-overall-rate/action-low-priority
                    leaf action-low-priority {
                        type boolean;
                        default "false";
                        description "Mark packets that exceed the rate as low-priority";
                    }

                } // container port-overall-rate

                ///nokia-conf/configure/system/security/cpu-protection/protocol-protection
                container protocol-protection {
                    presence "Enable the protocol-protection context";
                    description "Enable the protocol-protection context";

                    ///nokia-conf/configure/system/security/cpu-protection/protocol-protection/allow-sham-links
                    leaf allow-sham-links {
                        type boolean;
                        default "false";
                        description "Allow OSPF sham link traffic";
                    }

                    ///nokia-conf/configure/system/security/cpu-protection/protocol-protection/block-pim-tunneled
                    leaf block-pim-tunneled {
                        type boolean;
                        default "false";
                        description "Block extraction and processing of PIM packets";
                    }

                } // container protocol-protection

                ///nokia-conf/configure/system/security/cpu-protection/ip-src-monitoring
                container ip-src-monitoring {
                    description "Enter the ip-src-monitoring context";

                    ///nokia-conf/configure/system/security/cpu-protection/ip-src-monitoring/included-protocols
                    container included-protocols {
                        description "Enter the included-protocols context";

                        ///nokia-conf/configure/system/security/cpu-protection/ip-src-monitoring/included-protocols/dhcp
                        leaf dhcp {
                            type boolean;
                            default "true";
                            description "Include extracted DHCP packets for IP source monitoring";
                        }

                        ///nokia-conf/configure/system/security/cpu-protection/ip-src-monitoring/included-protocols/gtp
                        leaf gtp {
                            type boolean;
                            default "false";
                            description "Include extracted GTP packets for IP source monitoring";
                        }

                        ///nokia-conf/configure/system/security/cpu-protection/ip-src-monitoring/included-protocols/icmp
                        leaf icmp {
                            type boolean;
                            default "false";
                            description "Include extracted ICMP packets for IP source monitoring";
                        }

                        ///nokia-conf/configure/system/security/cpu-protection/ip-src-monitoring/included-protocols/igmp
                        leaf igmp {
                            type boolean;
                            default "false";
                            description "Include extracted IGMP packets for IP source monitoring";
                        }

                    } // container included-protocols

                } // container ip-src-monitoring

                ///nokia-conf/configure/system/security/cpu-protection/policy
                list policy {
                    key "policy-id";
                    description "Enter the policy list instance";

                    ///nokia-conf/configure/system/security/cpu-protection/policy/policy-id
                    leaf policy-id {
                        type types-services:cpm-prot-policy-id;
                        description "Policy ID";
                    }

                    ///nokia-conf/configure/system/security/cpu-protection/policy/alarm
                    leaf alarm {
                        type boolean;
                        default "true";
                        description "Generate an event when the rate is exceeded";
                    }

                    ///nokia-conf/configure/system/security/cpu-protection/policy/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/system/security/cpu-protection/policy/overall-rate
                    leaf overall-rate {
                        type types-security:cpm-policy-packet-rate-limit;
                        units "packets per second";
                        description "Packet arrival rate limit for all packets";
                    }

                    ///nokia-conf/configure/system/security/cpu-protection/policy/per-source-rate
                    leaf per-source-rate {
                        type types-security:cpm-policy-packet-rate-limit;
                        units "packets per second";
                        default "max";
                        description "Per-source packet arrival rate limit";
                    }

                    ///nokia-conf/configure/system/security/cpu-protection/policy/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/security/cpu-protection/policy/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/security/cpu-protection/policy/out-profile-rate
                    container out-profile-rate {
                        description "Enter the out-profile-rate context";

                        ///nokia-conf/configure/system/security/cpu-protection/policy/out-profile-rate/pir
                        leaf pir {
                            type types-security:cpm-policy-packet-rate-limit;
                            units "packets per second";
                            description "Packet arrival rate limit";
                        }

                        ///nokia-conf/configure/system/security/cpu-protection/policy/out-profile-rate/log-events
                        leaf log-events {
                            type boolean;
                            default "false";
                            description "Generate a log event when the packet rate is exceeded";
                        }

                    } // container out-profile-rate

                    ///nokia-conf/configure/system/security/cpu-protection/policy/per-source-parameters
                    container per-source-parameters {
                        description "Enter the per-source-parameters context";

                        ///nokia-conf/configure/system/security/cpu-protection/policy/per-source-parameters/ip-src-monitoring
                        container ip-src-monitoring {
                            description "Enter the ip-src-monitoring context";

                            ///nokia-conf/configure/system/security/cpu-protection/policy/per-source-parameters/ip-src-monitoring/limit-dhcp-ci-addr-zero
                            leaf limit-dhcp-ci-addr-zero {
                                type boolean;
                                default "false";
                                description "Apply rate limiting to packets with client IP address 0";
                            }

                        } // container ip-src-monitoring

                    } // container per-source-parameters

                    ///nokia-conf/configure/system/security/cpu-protection/policy/eth-cfm
                    container eth-cfm {
                        description "Enter the eth-cfm context";

                        ///nokia-conf/configure/system/security/cpu-protection/policy/eth-cfm/entry
                        list entry {
                            key "id";
                            max-elements 10;
                            description "Enter the entry list instance";

                            ///nokia-conf/configure/system/security/cpu-protection/policy/eth-cfm/entry/id
                            leaf id {
                                type uint32 {
                                    range "1..100";
                                }
                                description "Entry ID";
                            }

                            ///nokia-conf/configure/system/security/cpu-protection/policy/eth-cfm/entry/pir
                            leaf pir {
                                type union {
                                    type int32 {
                                        range "0..65534";
                                    }
                                    type enumeration {
                                        enum max                            { value -1; }
                                    }
                                }
                                units "packets per second";
                                default "max";
                                description "Packet arrival rate limit";
                            }

                            ///nokia-conf/configure/system/security/cpu-protection/policy/eth-cfm/entry/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/system/security/cpu-protection/policy/eth-cfm/entry/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/system/security/cpu-protection/policy/eth-cfm/entry/level
                            list level {
                                key "start end";
                                min-elements 1;
                                description "Add a list entry for level";

                                ///nokia-conf/configure/system/security/cpu-protection/policy/eth-cfm/entry/level/start
                                leaf start {
                                    type uint32 {
                                        range "0..7";
                                    }
                                    description "Lower bound of the level range";
                                }

                                ///nokia-conf/configure/system/security/cpu-protection/policy/eth-cfm/entry/level/end
                                leaf end {
                                    type uint32 {
                                        range "0..7";
                                    }
                                    description "Upper bound of the level range";
                                }

                            } // list level

                            ///nokia-conf/configure/system/security/cpu-protection/policy/eth-cfm/entry/opcode
                            list opcode {
                                key "start end";
                                min-elements 1;
                                description "Add a list entry for opcode";

                                ///nokia-conf/configure/system/security/cpu-protection/policy/eth-cfm/entry/opcode/start
                                leaf start {
                                    type uint32 {
                                        range "0..255";
                                    }
                                    description "Lower bound of the OpCode range";
                                }

                                ///nokia-conf/configure/system/security/cpu-protection/policy/eth-cfm/entry/opcode/end
                                leaf end {
                                    type uint32 {
                                        range "0..255";
                                    }
                                    description "Upper bound of the OpCode range";
                                }

                            } // list opcode

                        } // list entry

                    } // container eth-cfm

                } // list policy

            } // container cpu-protection

            ///nokia-conf/configure/system/security/dist-cpu-protection
            container dist-cpu-protection {
                description "Enable the dist-cpu-protection context";

                ///nokia-conf/configure/system/security/dist-cpu-protection/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/system/security/dist-cpu-protection/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/system/security/dist-cpu-protection/policy
                list policy {
                    key "policy-name";
                    max-elements 130;
                    description "Enter the policy list instance";

                    ///nokia-conf/configure/system/security/dist-cpu-protection/policy/policy-name
                    leaf policy-name {
                        type types-sros:named-item;
                        description "Policy name";
                    }

                    ///nokia-conf/configure/system/security/dist-cpu-protection/policy/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/system/security/dist-cpu-protection/policy/type
                    leaf type {
                        type types-system:dcpu-policy-type;
                        sros-ext:immutable;
                        description "Policy type";
                    }

                    ///nokia-conf/configure/system/security/dist-cpu-protection/policy/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/security/dist-cpu-protection/policy/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol
                    list protocol {
                        key "protocol-name";
                        description "Enter the protocol list instance";

                        ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/protocol-name
                        leaf protocol-name {
                            type types-system:dist-cpu-protocol-id;
                            description "Protocol name";
                        }

                        ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/enforcement
                        container enforcement {
                            description "Enter the enforcement context";

                            ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/enforcement/type
                            choice type {
                                default "dynamic-local-mon-bypass";
                                ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/enforcement/type/static
                                case static {

                                    ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/enforcement/type/static/static
                                    container static {
                                        description "Enter the static context";

                                        ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/enforcement/type/static/static/policer-name
                                        leaf policer-name {
                                            type leafref {
                                                path "../../../../static-policer/policer-name";
                                            }
                                            description "Static policer enforced by the protocol";
                                        }

                                    } // container static

                                }
                                ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/enforcement/type/dynamic
                                case dynamic {

                                    ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/enforcement/type/dynamic/dynamic
                                    container dynamic {
                                        description "Enter the dynamic context";

                                        ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/enforcement/type/dynamic/dynamic/mon-policer-name
                                        leaf mon-policer-name {
                                            type leafref {
                                                path "../../../../local-monitoring-policer/policer-name";
                                            }
                                            description "Dynamic enforcement policer for the protocol";
                                        }

                                    } // container dynamic

                                }
                                ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/enforcement/type/dynamic-local-mon-bypass
                                case dynamic-local-mon-bypass {

                                    ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/enforcement/type/dynamic-local-mon-bypass/dynamic-local-mon-bypass
                                    leaf dynamic-local-mon-bypass {
                                        type empty;
                                        description "Do not include packets in the local monitoring function";
                                    }

                                }
                                ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/enforcement/type/shared
                                case shared {

                                    ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/enforcement/type/shared/shared
                                    container shared {
                                        description "Enter the shared context";

                                        ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/enforcement/type/shared/shared/shared-policer-name
                                        leaf shared-policer-name {
                                            type leafref {
                                                path "../../../../../../../../qos/shared-policers/policer/shared-policer-name";
                                            }
                                            description "Shared enforcement policer name";
                                        }

                                    } // container shared

                                }
                            }
                        } // container enforcement

                        ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters
                        container dynamic-parameters {
                            description "Enter the dynamic-parameters context";

                            ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/detection-time
                            leaf detection-time {
                                type uint32 {
                                    range "1..128000";
                                }
                                units "seconds";
                                default "30";
                                description "Minimum time the dynamic policer remains allocated";
                            }

                            ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/log-events
                            leaf log-events {
                                type enumeration {
                                    enum false                          { value 0; }
                                    enum true                           { value 1; }
                                    enum verbose                        { value 2; }
                                }
                                default "true";
                                description "Control of log events creation for status and activity";
                            }

                            ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/exceed-action
                            container exceed-action {
                                description "Enter the exceed-action context";

                                ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/exceed-action/action
                                leaf action {
                                    type types-system:dcp-exceed-action;
                                    description "Action taken on control packets when rates are exceeded";
                                }

                                ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/exceed-action/hold-down
                                leaf hold-down {
                                    type union {
                                        type enumeration {
                                            enum indefinite                     { value -1; }
                                            enum none                           { value 0; }
                                        }
                                        type uint32 {
                                            range "1..10080";
                                        }
                                    }
                                    units "seconds";
                                    default "none";
                                    description "Hold down behavior";
                                }

                            } // container exceed-action

                            ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/rate
                            container rate {
                                description "Enter the rate context";

                                ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/rate/type
                                choice type {
                                    default "packets";
                                    ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/rate/type/kbps
                                    case kbps {

                                        ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/rate/type/kbps/kbps
                                        container kbps {
                                            description "Enter the kbps context";

                                            ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/rate/type/kbps/kbps/limit
                                            leaf limit {
                                                type union {
                                                    type enumeration {
                                                        enum max                            { value -1; }
                                                    }
                                                    type uint32 {
                                                        range "1..20000000";
                                                    }
                                                }
                                                units "kilobps";
                                                default "max";
                                                description "Rate limit";
                                            }

                                            ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/rate/type/kbps/kbps/mbs
                                            leaf mbs {
                                                type int32 {
                                                    range "0..4194304";
                                                }
                                                units "bytes";
                                                description "Tolerance for the rate";
                                            }

                                        } // container kbps

                                    }
                                    ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/rate/type/packets
                                    case packets {

                                        ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/rate/type/packets/packets
                                        container packets {
                                            description "Enter the packets context";

                                            ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/rate/type/packets/packets/limit
                                            leaf limit {
                                                type union {
                                                    type enumeration {
                                                        enum max                            { value -1; }
                                                    }
                                                    type uint32 {
                                                        range "0..8000";
                                                    }
                                                }
                                                units "packets per interval";
                                                default "max";
                                                description "Packets per interval limit";
                                            }

                                            ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/rate/type/packets/packets/within
                                            leaf within {
                                                type uint32 {
                                                    range "1..32767";
                                                }
                                                units "seconds";
                                                default "1";
                                                description "Measurement interval for packets rate";
                                            }

                                            ///nokia-conf/configure/system/security/dist-cpu-protection/policy/protocol/dynamic-parameters/rate/type/packets/packets/initial-delay
                                            leaf initial-delay {
                                                type uint32 {
                                                    range "0..255";
                                                }
                                                units "packets";
                                                default "0";
                                                description "Additional packets allowed in an initial burst";
                                            }

                                        } // container packets

                                    }
                                }
                            } // container rate

                        } // container dynamic-parameters

                    } // list protocol

                    ///nokia-conf/configure/system/security/dist-cpu-protection/policy/local-monitoring-policer
                    list local-monitoring-policer {
                        key "policer-name";
                        max-elements 1;
                        description "Enter the local-monitoring-policer list instance";

                        ///nokia-conf/configure/system/security/dist-cpu-protection/policy/local-monitoring-policer/policer-name
                        leaf policer-name {
                            type types-sros:named-item;
                            description "Local monitoring policer name";
                        }

                        ///nokia-conf/configure/system/security/dist-cpu-protection/policy/local-monitoring-policer/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

