                    ///nokia-conf/configure/system/security/dot1x/radius-policy/timeout
                    leaf timeout {
                        type uint32 {
                            range "1..90";
                        }
                        units "seconds";
                        default "5";
                        description "Time to wait for a response from the RADIUS server";
