                        ///nokia-conf/configure/system/security/dot1x/radius-policy/server/secret
                        leaf secret {
                            type types-sros:encrypted-leaf {
                                length "1..54";
                            }
                            mandatory true;
                            description "Secret key associated with the RADIUS server";
