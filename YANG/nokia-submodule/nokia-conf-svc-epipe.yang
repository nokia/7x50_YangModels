                }

                ///nokia-conf/configure/service/epipe/bgp-mh-site/min-down-timer
                leaf min-down-timer {
                    type types-services:site-min-down-timer;
                    units "seconds";
                    description "Minimum down time when site goes operationally down";
                }

                ///nokia-conf/configure/service/epipe/bgp-mh-site/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
