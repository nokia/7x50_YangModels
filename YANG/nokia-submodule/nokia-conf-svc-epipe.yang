                    ///nokia-conf/configure/service/epipe/spoke-sdp/endpoint/icb
                    leaf icb {
                        type boolean;
                        sros-ext:auto-restart-to-modify;
                        default "false";
                        description "Bind SDP as type Inter-Chassis Backup (ICB)";
                    }

                } // container endpoint

                ///nokia-conf/configure/service/epipe/spoke-sdp/cpu-protection
                container cpu-protection {
                    description "Enter the cpu-protection context";

                    ///nokia-conf/configure/service/epipe/spoke-sdp/cpu-protection/policy-id
                    leaf policy-id {
                        type leafref {
                            path "../../../../../system/security/cpu-protection/policy/policy-id";
                        }
                        description "CPM protection policy";
                    }

                    ///nokia-conf/configure/service/epipe/spoke-sdp/cpu-protection/monitoring
                    choice monitoring {
                        ///nokia-conf/configure/service/epipe/spoke-sdp/cpu-protection/monitoring/mac
                        case mac {

                            ///nokia-conf/configure/service/epipe/spoke-sdp/cpu-protection/monitoring/mac/mac-monitoring
                            leaf mac-monitoring {
                                type empty;
                                description "Monitor MAC for CPU protection";
                            }

                        }
                        ///nokia-conf/configure/service/epipe/spoke-sdp/cpu-protection/monitoring/cfm
                        case cfm {

                            ///nokia-conf/configure/service/epipe/spoke-sdp/cpu-protection/monitoring/cfm/eth-cfm-monitoring
                            container eth-cfm-monitoring {
                                presence "Enable the eth-cfm-monitoring context";
                                description "Enable the eth-cfm-monitoring context";

                                ///nokia-conf/configure/service/epipe/spoke-sdp/cpu-protection/monitoring/cfm/eth-cfm-monitoring/aggregate
                                leaf aggregate {
                                    type empty;
                                    description "Apply rate limit to the sum of the per peer packet rates";
                                }

                                ///nokia-conf/configure/service/epipe/spoke-sdp/cpu-protection/monitoring/cfm/eth-cfm-monitoring/car
                                leaf car {
                                    type empty;
                                    description "Ignore Ethernet CFM packets when enforcing overall rate";
                                }

                            } // container eth-cfm-monitoring

                        }
                    }
                } // container cpu-protection

                ///nokia-conf/configure/service/epipe/spoke-sdp/pw-status
                container pw-status {
                    description "Enter the pw-status context";

                    ///nokia-conf/configure/service/epipe/spoke-sdp/pw-status/signaling
                    leaf signaling {
                        type boolean;
                        default "true";
                        description "Allow SDP binding to support pseudowire status signaling";
                    }

                    ///nokia-conf/configure/service/epipe/spoke-sdp/pw-status/sdp-bind-pw-status
                    choice sdp-bind-pw-status {
                        ///nokia-conf/configure/service/epipe/spoke-sdp/pw-status/sdp-bind-pw-status/block-on-peer-fault
                        case block-on-peer-fault {

                            ///nokia-conf/configure/service/epipe/spoke-sdp/pw-status/sdp-bind-pw-status/block-on-peer-fault/block-on-peer-fault
                            leaf block-on-peer-fault {
                                type boolean;
                                default "false";
                                description "Block transmit direction of PW based on status code";
                            }

                        }
                        ///nokia-conf/configure/service/epipe/spoke-sdp/pw-status/sdp-bind-pw-status/standby-signaling-slave
                        case standby-signaling-slave {

                            ///nokia-conf/configure/service/epipe/spoke-sdp/pw-status/sdp-bind-pw-status/standby-signaling-slave/standby-signaling-slave
                            leaf standby-signaling-slave {
                                type boolean;
                                default "false";
                                description "Block spoke transmission based on PW standby status";
                            }

                        }
                    }
                } // container pw-status

                ///nokia-conf/configure/service/epipe/spoke-sdp/aarp
                container aarp {
                    presence "Enable the aarp context";
                    description "Enable the aarp context";

                    ///nokia-conf/configure/service/epipe/spoke-sdp/aarp/id
                    leaf id {
                        type leafref {
                            path "../../../../../application-assurance/aarp/aarp-id";
                        }
                        description "AARP instance ID";
                    }

                    ///nokia-conf/configure/service/epipe/spoke-sdp/aarp/type
                    leaf type {
                        type types-services:dual-homing-bsx-aarp-service-ref-type;
                        description "Role referenced by the AARP";
                    }

                } // container aarp

                ///nokia-conf/configure/service/epipe/spoke-sdp/source-bmac
                container source-bmac {
                    description "Enter the source-bmac context";

                    ///nokia-conf/configure/service/epipe/spoke-sdp/source-bmac/use-sdp-bmac-lsb
                    leaf use-sdp-bmac-lsb {
                        type boolean;
                        default "false";
                        description "Allow the spoke SDP to be part of a redundant pseudo-wire within PBB Epipe service";
                    }

                } // container source-bmac

                ///nokia-conf/configure/service/epipe/spoke-sdp/bfd-liveness
                container bfd-liveness {
                    status obsolete;
                    presence "Enable the bfd-liveness context";
                    description "Enable the bfd-liveness context";

                    ///nokia-conf/configure/service/epipe/spoke-sdp/bfd-liveness/encap
                    leaf encap {
                        status obsolete;
                        type types-services:bfd-encap;
                        default "ipv4";
                        description "BFD encapsulation used on the SDP binding";
                    }

                } // container bfd-liveness

                ///nokia-conf/configure/service/epipe/spoke-sdp/bfd
                container bfd {
                    description "Enter the bfd context";

                    ///nokia-conf/configure/service/epipe/spoke-sdp/bfd/bfd-template
                    leaf bfd-template {
                        type leafref {
                            path "../../../../../bfd/bfd-template/name";
                        }
                        description "BFD template associated with the SDP binding";
                    }

                    ///nokia-conf/configure/service/epipe/spoke-sdp/bfd/failure-action
                    leaf failure-action {
                        type types-services:sbind-bfd-fail-action;
                        default "none";
                        description "VCCV BFD action taken on the SDP binding";
                    }

                    ///nokia-conf/configure/service/epipe/spoke-sdp/bfd/wait-for-up-timer
                    leaf wait-for-up-timer {
                        type uint32 {
                            range "1..60";
                        }
                        units "seconds";
                        description "Time waited for BFD up status";
                    }

                    ///nokia-conf/configure/service/epipe/spoke-sdp/bfd/bfd-liveness
                    container bfd-liveness {
                        presence "Enable the bfd-liveness context";
                        description "Enable the bfd-liveness context";

                        ///nokia-conf/configure/service/epipe/spoke-sdp/bfd/bfd-liveness/encap
                        leaf encap {
                            type types-services:bfd-encap;
                            default "ipv4";
                            description "BFD encapsulation used on the SDP binding";
                        }

                    } // container bfd-liveness

                } // container bfd

                ///nokia-conf/configure/service/epipe/spoke-sdp/transit-policy
                container transit-policy {
                    presence "Enable the transit-policy context";
                    description "Enable the transit-policy context";

                    ///nokia-conf/configure/service/epipe/spoke-sdp/transit-policy/transit-policy
                    choice transit-policy {
                        mandatory true;
                        ///nokia-conf/configure/service/epipe/spoke-sdp/transit-policy/transit-policy/prefix
                        case prefix {

                            ///nokia-conf/configure/service/epipe/spoke-sdp/transit-policy/transit-policy/prefix/prefix
                            leaf prefix {
                                type leafref {
                                    path "../../../../../application-assurance/group/partition/transit-prefix-policy/prefix-policy-id";
                                }
                                sros-ext:immutable;
                                description "IP prefix policy ID";
                            }

                        }
                        ///nokia-conf/configure/service/epipe/spoke-sdp/transit-policy/transit-policy/ip
                        case ip {

                            ///nokia-conf/configure/service/epipe/spoke-sdp/transit-policy/transit-policy/ip/ip
                            leaf ip {
                                type leafref {
                                    path "../../../../../application-assurance/group/partition/transit-ip-policy/ip-policy-id";
                                }
                                sros-ext:immutable;
                                description "IP transit policy ID";
                            }

                        }
                    }
                } // container transit-policy

            } // list spoke-sdp


            ///nokia-conf/configure/service/epipe/bgp-mh-site
            list bgp-mh-site {
                key "site-name";
                max-elements 1;
                description "Enter the bgp-mh-site list instance";

                ///nokia-conf/configure/service/epipe/bgp-mh-site/site-name
                leaf site-name {
                    type types-sros:named-item;
                    description "Name for the specific site";
                }

                ///nokia-conf/configure/service/epipe/bgp-mh-site/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of the site";
                }

                ///nokia-conf/configure/service/epipe/bgp-mh-site/id
                leaf id {
                    type types-services:site-id;
                    sros-ext:auto-restart-to-modify;
                    description "Site ID for the service";
                }

                ///nokia-conf/configure/service/epipe/bgp-mh-site/sap
                leaf sap {
                    type types-sros:sap;
                    sros-ext:auto-restart-to-modify;
                    description "SAP to be associated with this site";
                }

                ///nokia-conf/configure/service/epipe/bgp-mh-site/boot-timer
                leaf boot-timer {
                    type types-services:site-boot-timer;
                    units "seconds";
                    description "Wait time after reboot to run the DF election algorithm";
