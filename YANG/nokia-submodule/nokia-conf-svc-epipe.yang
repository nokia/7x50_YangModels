                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/send-tunnel-encap
                    container send-tunnel-encap {
                        description "Enter the send-tunnel-encap context";

                        ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/send-tunnel-encap/mpls
                        leaf mpls {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "true";
                            description "Enable MPLS encapsulation";
                        }

                        ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/send-tunnel-encap/mpls-over-udp
                        leaf mpls-over-udp {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "false";
                            description "Enable MPLS over UDP encapsulation";
                        }

                    } // container send-tunnel-encap

                    ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel
                    container auto-bind-tunnel {
                        description "Enter the auto-bind-tunnel context";

                        ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/resolution
                        leaf resolution {
                            type types-services:resolve-status;
                            default "none";
                            description "Resolution method for tunnel selection";
                        }

                        ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/enforce-strict-tunnel-tagging
                        leaf enforce-strict-tunnel-tagging {
                            type boolean;
                            default "false";
                            description "Enable/disable enforcement of strict tunnel tagging";
                        }

                        ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/ecmp
                        leaf ecmp {
                            type types-router:mpls-ecmp;
                            default "1";
                            description "Maximum ECMP routes information";
                        }

                        ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/allow-flex-algo-fallback
                        leaf allow-flex-algo-fallback {
                            type boolean;
                            default "false";
                            description "Enable flexible algorithm fallback";
                        }

                        ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/weighted-ecmp
                        leaf weighted-ecmp {
                            type boolean;
                            default "false";
                            description "Allow weighted load balancing";
                        }

                        ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/resolution-filter
                        container resolution-filter {
                            description "Enter the resolution-filter context";

                            ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/resolution-filter/bgp
                            leaf bgp {
                                type boolean;
                                default "false";
                                description "Use BGP tunneling for next-hop resolution";
                            }

                            ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/resolution-filter/ldp
                            leaf ldp {
                                type boolean;
                                default "false";
                                description "Use LDP tunneling for next-hop resolution";
                            }

                            ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/resolution-filter/rsvp
                            leaf rsvp {
                                type boolean;
                                default "false";
                                description "Use RSVP tunneling for next-hop resolution";
                            }

                            ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/resolution-filter/sr-isis
                            leaf sr-isis {
                                type boolean;
                                default "false";
                                description "Use IS-IS SR tunneling for next-hop resolution";
                            }

                            ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/resolution-filter/sr-ospf
                            leaf sr-ospf {
                                type boolean;
                                default "false";
                                description "Use OSPF SR tunneling for next-hop resolution";
                            }

                            ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/resolution-filter/sr-te
                            leaf sr-te {
                                type boolean;
                                default "false";
                                description "Use SR-TE tunneling for next-hop resolution";
                            }

                            ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/resolution-filter/udp
                            leaf udp {
                                type boolean;
                                default "false";
                                description "Use MPLS over UDP tunneling for next-hop resolution";
                            }

                            ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/resolution-filter/mpls-fwd-policy
                            leaf mpls-fwd-policy {
                                type boolean;
                                default "false";
                                description "Use MPLS forwarding policy for next-hop resolution";
                            }

                            ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/resolution-filter/sr-policy
                            leaf sr-policy {
                                type boolean;
                                default "false";
                                description "Use SR policies for next-hop resolution";
                            }

                            ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/resolution-filter/rib-api
                            leaf rib-api {
                                type boolean;
                                default "false";
                                description "Use RIB API gRPC service for next-hop resolution";
                            }

                            ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/auto-bind-tunnel/resolution-filter/sr-ospf3
                            leaf sr-ospf3 {
                                type boolean;
                                default "false";
                                description "Use OSPFv3 SR tunneling for next-hop resolution";
                            }

                        } // container resolution-filter

                    } // container auto-bind-tunnel

                    ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/route-next-hop
                    container route-next-hop {
                        description "Enter the route-next-hop context";

                        ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/route-next-hop/tag
                        choice tag {
                            default "ipv4";
                            ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/route-next-hop/tag/ipv4
                            case ipv4 {

                                ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/route-next-hop/tag/ipv4/system-ipv4
                                leaf system-ipv4 {
                                    type empty;
                                    sros-ext:auto-restart-to-modify;
                                    description "System IPv4 address for service EVPN route next hop";
                                }

                            }
                            ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/route-next-hop/tag/ipv6
                            case ipv6 {

                                ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/route-next-hop/tag/ipv6/system-ipv6
                                leaf system-ipv6 {
                                    type empty;
                                    sros-ext:auto-restart-to-modify;
                                    description "System IPv6 address for service EVPN route next hop";
                                }

                            }
                            ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/route-next-hop/tag/explicit
                            case explicit {

                                ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/route-next-hop/tag/explicit/ip-address
                                leaf ip-address {
                                    type types-sros:ip-address;
                                    sros-ext:auto-restart-to-modify;
                                    description "IP address of the next-hop for the service EVPN route";
                                }

                            }
                        }
                    } // container route-next-hop

                    ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/endpoint
                    container endpoint {
                        status obsolete;
                        description "Enter the endpoint context";

                        ///nokia-conf/configure/service/epipe/bgp-evpn/mpls/endpoint/name
                        leaf name {
                            status obsolete;
                            type leafref {
                                path "../../../../endpoint/name";
                            }
                            sros-ext:immutable;
                            description "Name of endpoint the bgp-evpn mpls instance is associated with";
                        }

                    } // container endpoint

                } // list mpls

                ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6
                list segment-routing-v6 {
                    key "bgp-instance";
                    description "Enter the segment-routing-v6 list instance";

                    ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/bgp-instance
                    leaf bgp-instance {
                        type int32 {
                            range "1";
                        }
                        description "BGP instance";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of segment routing over IPv6";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/default-route-tag
                    leaf default-route-tag {
                        type types-services:one-byte-value;
                        sros-ext:auto-restart-to-modify;
                        description "Default route tag";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/source-address
                    leaf source-address {
                        type types-sros:ipv6-unicast-address;
                        sros-ext:auto-restart-to-modify;
                        description "Source IPv6 address";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/oper-group
                    leaf oper-group {
                        type leafref {
                            path "../../../../oper-group/name";
                        }
                        description "Operational group";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/ecmp
                    leaf ecmp {
                        type types-services:ecmp-value;
                        default "1";
                        description "Maximum ECMP value configured on the service";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/force-vc-forwarding
                    leaf force-vc-forwarding {
                        type enumeration {
                            enum vlan                           { value 1; }
                            enum qinq-c-tag-c-tag               { value 2; }
                            enum qinq-s-tag-c-tag               { value 3; }
                        }
                        description "Datapath forwarding to force vlan-vc-type";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/evi-three-byte-auto-rt
                    leaf evi-three-byte-auto-rt {
                        type boolean;
                        sros-ext:auto-restart-to-modify;
                        default "false";
                        description "Auto-derive the BGP EVPN route target";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/resolution
                    leaf resolution {
                        type types-sros:srv6-route-resolution;
                        default "route-table";
                        description "Resolution options for routes";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/srv6
                    container srv6 {
                        description "Enter the srv6 context";

                        ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/srv6/instance
                        leaf instance {
                            type leafref {
                                path "../../../../segment-routing-v6/instance";
                            }
                            sros-ext:immutable;
                            description "Segment Routing SRv6 instance";
                        }

                        ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/srv6/default-locator
                        leaf default-locator {
                            type string;
                            sros-ext:immutable;
                            description "Default route locator";
                        }

                    } // container srv6

                    ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/route-next-hop
                    container route-next-hop {
                        description "Enter the route-next-hop context";

                        ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/route-next-hop/tag
                        choice tag {
                            default "ipv4";
                            ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/route-next-hop/tag/ipv4
                            case ipv4 {

                                ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/route-next-hop/tag/ipv4/system-ipv4
                                leaf system-ipv4 {
                                    type empty;
                                    sros-ext:auto-restart-to-modify;
                                    description "System IPv4 address for service EVPN route next hop";
                                }

                            }
                            ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/route-next-hop/tag/ipv6
                            case ipv6 {

                                ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/route-next-hop/tag/ipv6/system-ipv6
                                leaf system-ipv6 {
                                    type empty;
                                    sros-ext:auto-restart-to-modify;
                                    description "System IPv6 address for service EVPN route next hop";
                                }

                            }
                            ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/route-next-hop/tag/explicit
                            case explicit {

                                ///nokia-conf/configure/service/epipe/bgp-evpn/segment-routing-v6/route-next-hop/tag/explicit/ip-address
                                leaf ip-address {
                                    type types-sros:ip-address;
                                    sros-ext:auto-restart-to-modify;
                                    description "IP address of the next-hop for the service EVPN route";
                                }

                            }
                        }
                    } // container route-next-hop

                } // list segment-routing-v6

            } // container bgp-evpn

            ///nokia-conf/configure/service/epipe/network
            container network {
                description "Enter the network context";

                ///nokia-conf/configure/service/epipe/network/ingress
                container ingress {
                    description "Enter the ingress context";

                    ///nokia-conf/configure/service/epipe/network/ingress/classification-override
                    leaf classification-override {
                        type leafref {
                            path "../../../../../qos/ingress-classification-policy/ingress-classification-policy-name";
                        }
                        description "Short-pipe mode for CoS propagation";
                    }

                } // container ingress

            } // container network

        } // list epipe

    } // grouping conf-svc-epipe

}
