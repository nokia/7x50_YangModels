                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/epipe/bgp-mh-site/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // list bgp-mh-site

            ///nokia-conf/configure/service/epipe/sap
            list sap {
                key "sap-id";
                max-elements 2;
                description "Enter the sap list instance";

                ///nokia-conf/configure/service/epipe/sap/sap-id
                leaf sap-id {
                    type types-sros:sap;
                    description "SAP ID";
                }

                ///nokia-conf/configure/service/epipe/sap/description
                leaf description {
                    type types-sros:long-description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/epipe/sap/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "enable";
                    description "Administrative state of the SAP";
                }

                ///nokia-conf/configure/service/epipe/sap/accounting-policy
                leaf accounting-policy {
                    type leafref {
                        path "../../../../log/accounting-policy/policy-id";
                    }
                    description "Accounting policy";
                }

                ///nokia-conf/configure/service/epipe/sap/app-profile
                leaf app-profile {
                    type leafref {
                        path "../../../../application-assurance/group/partition/policy/app-profile/app-profile-name";
                    }
                    description "Application profile name";
                }

                ///nokia-conf/configure/service/epipe/sap/bandwidth
                leaf bandwidth {
                    type types-services:sap-bandwidth;
                    units "kilobps";
                    description "SAP bandwidth";
                }

