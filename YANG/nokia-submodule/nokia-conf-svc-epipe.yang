                                            type types-qos:bytes {
                                                range "0..1073741824";
                                            }
                                            type enumeration {
                                                enum auto                           { value -1; }
                                            }
                                        }
                                        units "bytes";
                                        description "MBS";
                                    }

                                    ///nokia-conf/configure/service/epipe/sap/ingress/qos/sap-ingress/overrides/queue/monitor-depth
                                    leaf monitor-depth {
