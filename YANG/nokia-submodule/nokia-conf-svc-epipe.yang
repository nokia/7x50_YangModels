submodule nokia-conf-svc-epipe {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-eth-cfm            { prefix "types-eth-cfm"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-router             { prefix "types-router"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-aa-group;
    include nokia-conf-application-assurance;
    include nokia-conf-bfd;
    include nokia-conf-eth-cfm;
    include nokia-conf-filter;
    include nokia-conf-groups;
    include nokia-conf-log;
    include nokia-conf-policy-options;
    include nokia-conf-port;
    include nokia-conf-port-eth-access;
    include nokia-conf-port-ethernet;
    include nokia-conf-qos;
    include nokia-conf-router;
    include nokia-conf-router-segment-routing;
    include nokia-conf-service;
    include nokia-conf-svc-vpls;
    include nokia-conf-system;
    include nokia-conf-system-security;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure epipe services on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-svc-epipe YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-05-12";

    grouping conf-svc-epipe {
        ///nokia-conf/configure/service/epipe
        list epipe {
            key "service-name";
            description "Enter the epipe list instance";

            ///nokia-conf/configure/service/epipe/service-name
            leaf service-name {
                type types-services:service-name;
                description "Administrative service name";
            }

            ///nokia-conf/configure/service/epipe/service-id
            leaf service-id {
                type types-services:external-service-id;
                sros-ext:immutable;
                description "Service ID";
            }

            ///nokia-conf/configure/service/epipe/description
            leaf description {
                type types-sros:description;
                description "Text description";
            }

            ///nokia-conf/configure/service/epipe/customer
            leaf customer {
                type leafref {
                    path "../../customer/customer-name";
                }
                sros-ext:immutable;
                mandatory true;
                description "Service customer ID";
            }

            ///nokia-conf/configure/service/epipe/admin-state
            leaf admin-state {
                type types-sros:admin-state;
                default "disable";
                description "Administrative state of the service";
            }

            ///nokia-conf/configure/service/epipe/vpn-id
            leaf vpn-id {
                type types-services:vpn-id;
                sros-ext:immutable;
                description "VPN identifier for the service";
            }

            ///nokia-conf/configure/service/epipe/vc-switching
            leaf vc-switching {
                type boolean;
                sros-ext:immutable;
                default "false";
                description "Use PW switching signaling for spoke SDPs in service";
            }

            ///nokia-conf/configure/service/epipe/test
            leaf test {
                type boolean;
                sros-ext:immutable;
                default "false";
                description "Designate as a test service";
            }

            ///nokia-conf/configure/service/epipe/service-mtu
            leaf service-mtu {
                type int32 {
                    range "1..9782";
                }
                description "MTU size";
            }

            ///nokia-conf/configure/service/epipe/oper-group
            leaf oper-group {
                type leafref {
                    path "../../oper-group/name";
                }
                description "Oper-group name";
            }

            ///nokia-conf/configure/service/epipe/ignore-l2vpn-mtu-mismatch
            leaf ignore-l2vpn-mtu-mismatch {
                type boolean;
                default "false";
                description "Ignore the L2 VPN MTU mismatch with local service MTU";
            }

            ///nokia-conf/configure/service/epipe/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/service/epipe/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/service/epipe/segment-routing-v6
            list segment-routing-v6 {
                key "instance";
                description "Enter the segment-routing-v6 list instance";

                ///nokia-conf/configure/service/epipe/segment-routing-v6/instance
                leaf instance {
                    type types-services:epipe-srv6-instance;
                    description "Segment routing v6 instance";
                }

                ///nokia-conf/configure/service/epipe/segment-routing-v6/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/epipe/segment-routing-v6/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/epipe/segment-routing-v6/locator
                list locator {
                    key "locator-name";
                    max-elements 1;
                    description "Enter the locator list instance";

                    ///nokia-conf/configure/service/epipe/segment-routing-v6/locator/locator-name
                    leaf locator-name {
                        type leafref {
                            path "../../../../../router/segment-routing/segment-routing-v6/locator/locator-name";
                        }
                        description "SRv6 locator name";
                    }

                    ///nokia-conf/configure/service/epipe/segment-routing-v6/locator/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/epipe/segment-routing-v6/locator/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/epipe/segment-routing-v6/locator/function
                    container function {
                        description "Enter the function context";

                        ///nokia-conf/configure/service/epipe/segment-routing-v6/locator/function/end-dx2
                        container end-dx2 {
                            presence "Enable the end-dx2 context";
                            description "Enable the end-dx2 context";

                            ///nokia-conf/configure/service/epipe/segment-routing-v6/locator/function/end-dx2/value
                            leaf value {
                                type uint32 {
                                    range "1..1048575";
                                }
                                description "SRv6 function value";
                            }

                        } // container end-dx2

                    } // container function

                } // list locator

                ///nokia-conf/configure/service/epipe/segment-routing-v6/micro-segment-locator
                list micro-segment-locator {
                    key "locator-name";
                    max-elements 1;
                    description "Enter the micro-segment-locator list instance";

                    ///nokia-conf/configure/service/epipe/segment-routing-v6/micro-segment-locator/locator-name
                    leaf locator-name {
                        type leafref {
                            path "../../../../../router/segment-routing/segment-routing-v6/micro-segment-locator/locator-name";
                        }
                        description "Micro-segment SRv6 locator name";
                    }

                    ///nokia-conf/configure/service/epipe/segment-routing-v6/micro-segment-locator/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/epipe/segment-routing-v6/micro-segment-locator/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/epipe/segment-routing-v6/micro-segment-locator/function
                    container function {
                        description "Enter the function context";

                        ///nokia-conf/configure/service/epipe/segment-routing-v6/micro-segment-locator/function/udx2
                        container udx2 {
                            presence "Enable the udx2 context";
                            description "Enable the udx2 context";

                            ///nokia-conf/configure/service/epipe/segment-routing-v6/micro-segment-locator/function/udx2/value
                            leaf value {
                                type uint32 {
                                    range "1..1048575";
                                }
                                description "SRv6 function value";
                            }

                        } // container udx2

                    } // container function

                } // list micro-segment-locator

            } // list segment-routing-v6

            ///nokia-conf/configure/service/epipe/bgp
            list bgp {
                key "bgp-instance";
                description "Enter the bgp list instance";

                ///nokia-conf/configure/service/epipe/bgp/bgp-instance
                leaf bgp-instance {
                    type int32 {
                        range "1";
                    }
                    description "BGP instance";
                }

                ///nokia-conf/configure/service/epipe/bgp/route-distinguisher
                leaf route-distinguisher {
                    type types-services:vpn-route-distinguisher-or-auto-rd;
                    description "RD component for NLRI for L2VPN and EVPN families";
                }

                ///nokia-conf/configure/service/epipe/bgp/vsi-import
                leaf-list vsi-import {
                    type leafref {
                        path "../../../../policy-options/policy-statement/name";
                    }
                    max-elements 5;
                    ordered-by user;
                    description "VSI import policies";
                }

                ///nokia-conf/configure/service/epipe/bgp/vsi-export
                leaf-list vsi-export {
                    type leafref {
                        path "../../../../policy-options/policy-statement/name";
                    }
                    max-elements 5;
                    ordered-by user;
                    description "VSI export policies";
                }

                ///nokia-conf/configure/service/epipe/bgp/adv-service-mtu
                leaf adv-service-mtu {
                    type int32 {
                        range "0..9782";
                    }
                    description "Advertised service MTU value";
                }

                ///nokia-conf/configure/service/epipe/bgp/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/epipe/bgp/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/epipe/bgp/route-target
                container route-target {
                    description "Enter the route-target context";

                    ///nokia-conf/configure/service/epipe/bgp/route-target/export
                    leaf export {
                        type types-services:route-target;
                        description "Extended community name for default import policy";
                    }

                    ///nokia-conf/configure/service/epipe/bgp/route-target/import
                    leaf import {
                        type types-services:route-target;
                        description "Extended community name for default import policy";
                    }

                } // container route-target

                ///nokia-conf/configure/service/epipe/bgp/pw-template-binding
                list pw-template-binding {
                    key "pw-template-name";
                    max-elements 100;
                    description "Enter the pw-template-binding list instance";

                    ///nokia-conf/configure/service/epipe/bgp/pw-template-binding/pw-template-name
                    leaf pw-template-name {
                        type leafref {
                            path "../../../../pw-template/pw-template-name";
                        }
                        description "Policy name";
                    }

                    ///nokia-conf/configure/service/epipe/bgp/pw-template-binding/bfd-template
                    leaf bfd-template {
                        type leafref {
                            path "../../../../../bfd/bfd-template/name";
                        }
                        description "BFD template name for PW-Template binding";
                    }

                    ///nokia-conf/configure/service/epipe/bgp/pw-template-binding/bfd-liveness
                    leaf bfd-liveness {
                        type boolean;
                        default "false";
                        description "Enable BFD";
                    }

                    ///nokia-conf/configure/service/epipe/bgp/pw-template-binding/import-rt
                    leaf-list import-rt {
                        type types-services:route-target;
                        max-elements 5;
                        description "Import route-target communities";
                    }

                    ///nokia-conf/configure/service/epipe/bgp/pw-template-binding/endpoint
                    leaf endpoint {
                        type leafref {
                            path "../../../endpoint/name";
                        }
                        sros-ext:immutable;
                        description "Endpoint name associated with the BGP PW template";
                    }

                    ///nokia-conf/configure/service/epipe/bgp/pw-template-binding/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/epipe/bgp/pw-template-binding/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list pw-template-binding

            } // list bgp

            ///nokia-conf/configure/service/epipe/bgp-vpws
            container bgp-vpws {
                presence "Enable the bgp-vpws context";
                description "Enable the bgp-vpws context";

                ///nokia-conf/configure/service/epipe/bgp-vpws/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of the VPWS edge instance";
                }

                ///nokia-conf/configure/service/epipe/bgp-vpws/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/epipe/bgp-vpws/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/epipe/bgp-vpws/local-ve
                container local-ve {
                    description "Enter the local-ve context";

                    ///nokia-conf/configure/service/epipe/bgp-vpws/local-ve/name
                    leaf name {
                        type types-sros:named-item;
                        sros-ext:auto-restart-to-modify;
                        description "Local VPWS instance name";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-vpws/local-ve/id
                    leaf id {
                        type types-services:site-id;
                        sros-ext:auto-restart-to-modify;
                        description "Local VPWS edge ID";
                    }

                } // container local-ve

                ///nokia-conf/configure/service/epipe/bgp-vpws/remote-ve
                list remote-ve {
                    key "name";
                    max-elements 2;
                    description "Enter the remote-ve list instance";

                    ///nokia-conf/configure/service/epipe/bgp-vpws/remote-ve/name
                    leaf name {
                        type types-sros:named-item;
                        description "Remote PE name to which a PW is to be signaled";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-vpws/remote-ve/id
                    leaf id {
                        type types-services:site-id;
                        description "Remote VPWS edge ID";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-vpws/remote-ve/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/epipe/bgp-vpws/remote-ve/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list remote-ve

            } // container bgp-vpws

            ///nokia-conf/configure/service/epipe/endpoint
            list endpoint {
                key "name";
                max-elements 2;
                description "Enter the endpoint list instance";

                ///nokia-conf/configure/service/epipe/endpoint/name
                leaf name {
                    type types-sros:named-item;
                    description "Service endpoint name";
                }

                ///nokia-conf/configure/service/epipe/endpoint/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/epipe/endpoint/revert-time
                leaf revert-time {
                    type types-services:revert-time;
                    default "immediate";
                    description "Time to wait before reverting to primary spoke SDP";
                }

                ///nokia-conf/configure/service/epipe/endpoint/hold-time-active
                leaf hold-time-active {
                    type types-services:hold-time-active;
                    description "Time before entering standby when MC-LAG SAP goes down";
                }

                ///nokia-conf/configure/service/epipe/endpoint/standby-signaling
                leaf standby-signaling {
                    type enumeration {
                        enum master                         { value 1; }
                        enum slave                          { value 2; }
                    }
                    description "Endpoint behavior to handle the PW standby bit";
                }

                ///nokia-conf/configure/service/epipe/endpoint/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/epipe/endpoint/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // list endpoint

            ///nokia-conf/configure/service/epipe/eth-cfm
            container eth-cfm {
                description "Enter the eth-cfm context";

                ///nokia-conf/configure/service/epipe/eth-cfm/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/epipe/eth-cfm/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container eth-cfm

            ///nokia-conf/configure/service/epipe/load-balancing
            container load-balancing {
                description "Enter the load-balancing context";

                ///nokia-conf/configure/service/epipe/load-balancing/per-service-hashing
                leaf per-service-hashing {
                    type boolean;
                    default "false";
                    description "Enable per-service hashing for Ethernet services";
                }

                ///nokia-conf/configure/service/epipe/load-balancing/lbl-eth-or-ip-l4-teid
                leaf lbl-eth-or-ip-l4-teid {
                    type boolean;
                    default "false";
                    description "Enable hashing of MPLS ethernet and IP packets on SAPs";
                }

            } // container load-balancing

            ///nokia-conf/configure/service/epipe/nat-outside
            list nat-outside {
                key "nat-group";
                description "Enter the nat-outside list instance";

                ///nokia-conf/configure/service/epipe/nat-outside/nat-group
                leaf nat-group {
                    type uint32;
                    description "NAT group";
                }

                ///nokia-conf/configure/service/epipe/nat-outside/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of Epipe as a NAT outside service";
                }

                ///nokia-conf/configure/service/epipe/nat-outside/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/epipe/nat-outside/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // list nat-outside

            ///nokia-conf/configure/service/epipe/pbb
            container pbb {
                description "Enter the pbb context";

                ///nokia-conf/configure/service/epipe/pbb/force-qtag-forwarding
                leaf force-qtag-forwarding {
                    type boolean;
                    default "false";
                    description "Add an IEEE 802.1q tag after CMAC addresses PBB header";
                }

                ///nokia-conf/configure/service/epipe/pbb/local-switch-service-state
                leaf local-switch-service-state {
                    type enumeration {
                        enum sap                            { value 0; }
                        enum pbb-tunnel                     { value 1; }
                    }
                    default "sap";
                    description "Endpoint state used to determine PBB-Epipe state";
                }

                ///nokia-conf/configure/service/epipe/pbb/tunnel
                container tunnel {
                    presence "Enable the tunnel context";
                    description "Enable the tunnel context";

                    ///nokia-conf/configure/service/epipe/pbb/tunnel/backbone-vpls-service-name
                    leaf backbone-vpls-service-name {
                        type leafref {
                            path "../../../../vpls/service-name";
                        }
                        sros-ext:immutable;
                        mandatory true;
                        description "Backbone VPLS service";
                    }

                    ///nokia-conf/configure/service/epipe/pbb/tunnel/isid
                    leaf isid {
                        type types-services:isid;
                        sros-ext:immutable;
                        mandatory true;
                        description "Service instance ID";
                    }

                    ///nokia-conf/configure/service/epipe/pbb/tunnel/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/epipe/pbb/tunnel/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/epipe/pbb/tunnel/backbone-mac
                    choice backbone-mac {
                        mandatory true;
                        ///nokia-conf/configure/service/epipe/pbb/tunnel/backbone-mac/backbone-dest-mac
                        case backbone-dest-mac {

                            ///nokia-conf/configure/service/epipe/pbb/tunnel/backbone-mac/backbone-dest-mac/backbone-dest-mac
                            leaf backbone-dest-mac {
                                type types-sros:mac-unicast-address-no-zero;
                                description "Backbone Destination MAC address";
                            }

                        }
                        ///nokia-conf/configure/service/epipe/pbb/tunnel/backbone-mac/backbone-dest-mac-name
                        case backbone-dest-mac-name {

                            ///nokia-conf/configure/service/epipe/pbb/tunnel/backbone-mac/backbone-dest-mac-name/backbone-dest-mac-name
                            leaf backbone-dest-mac-name {
                                type leafref {
                                    path "../../../../pbb/mac/name";
                                }
                                description "Backbone destination MAC name for PBB packets";
                            }

                        }
                    }
                } // container tunnel

            } // container pbb

            ///nokia-conf/configure/service/epipe/spoke-sdp
            list spoke-sdp {
                key "sdp-bind-id";
                description "Enter the spoke-sdp list instance";

                ///nokia-conf/configure/service/epipe/spoke-sdp/sdp-bind-id
                leaf sdp-bind-id {
                    type types-services:sdp-bind-id;
                    description "SDP binding ID";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "enable";
                    description "Administrative state of the SDP binding to the service";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/adv-service-mtu
                leaf adv-service-mtu {
                    type int32 {
                        range "0..9782";
                    }
                    description "Service MTU used in signaling";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/vlan-vc-tag
                leaf vlan-vc-tag {
                    type types-services:vlan-vc-tag;
                    description "SDP bind VC tag";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/collect-stats
                leaf collect-stats {
                    type boolean;
                    default "false";
                    description "Allow agent to collect accounting statistics";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/accounting-policy
                leaf accounting-policy {
                    type leafref {
                        path "../../../../log/accounting-policy/policy-id";
                    }
                    description "Policy to collect accounting statistics";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/control-word
                leaf control-word {
                    type boolean;
                    default "false";
                    description "Use the control word as preferred";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/force-vc-forwarding
                leaf force-vc-forwarding {
                    type enumeration {
                        enum vlan                           { value 1; }
                        enum qinq-c-tag-c-tag               { value 2; }
                        enum qinq-s-tag-c-tag               { value 3; }
                    }
                    description "VC forwarding action";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/bandwidth
                leaf bandwidth {
                    type types-services:sdp-bind-bandwidth;
                    units "kilobps";
                    default "0";
                    description "Bandwidth that is reserved for this SDP binding";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/app-profile
                leaf app-profile {
                    type leafref {
                        path "../../../../application-assurance/group/partition/policy/app-profile/app-profile-name";
                    }
                    description "Application profile name for this SDP";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/bfd-template
                leaf bfd-template {
                    status obsolete;
                    type leafref {
                        path "../../../../bfd/bfd-template/name";
                    }
                    description "BFD template associated with SDP binding";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/vc-type
                leaf vc-type {
                    type types-services:epipe-sdp-bind-vc-type;
                    sros-ext:auto-restart-to-modify;
                    default "ether";
                    description "Virtual circuit type associated with the SDP binding";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/epipe/spoke-sdp/label
                choice label {
                    ///nokia-conf/configure/service/epipe/spoke-sdp/label/entropy
                    case entropy {

                        ///nokia-conf/configure/service/epipe/spoke-sdp/label/entropy/entropy-label
                        leaf entropy-label {
                            type empty;
                            description "Enable the use of entropy labels for spoke SDPs";
                        }

                    }
                    ///nokia-conf/configure/service/epipe/spoke-sdp/label/hash
                    case hash {

                        ///nokia-conf/configure/service/epipe/spoke-sdp/label/hash/hash-label
                        container hash-label {
                            presence "Enable the hash-label context";
                            description "Enable the hash-label context";

                            ///nokia-conf/configure/service/epipe/spoke-sdp/label/hash/hash-label/signal-capability
                            leaf signal-capability {
                                type empty;
                                description "Signal hash label capability to the remote PE";
                            }

                        } // container hash-label

                    }
                }
                ///nokia-conf/configure/service/epipe/spoke-sdp/oper-group-association
                choice oper-group-association {
                    ///nokia-conf/configure/service/epipe/spoke-sdp/oper-group-association/oper-group
                    case oper-group {

                        ///nokia-conf/configure/service/epipe/spoke-sdp/oper-group-association/oper-group/oper-group
                        leaf oper-group {
                            type leafref {
                                path "../../../oper-group/name";
                            }
                            description "Operational group identifier";
                        }

                    }
                    ///nokia-conf/configure/service/epipe/spoke-sdp/oper-group-association/monitor-oper-group
                    case monitor-oper-group {

                        ///nokia-conf/configure/service/epipe/spoke-sdp/oper-group-association/monitor-oper-group/monitor-oper-group
                        leaf monitor-oper-group {
                            type leafref {
                                path "../../../oper-group/name";
                            }
                            description "Operational group that affects state of the SDP bind";
                        }

                    }
                }
                ///nokia-conf/configure/service/epipe/spoke-sdp/ingress
                container ingress {
                    description "Enter the ingress context";

                    ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/vc-label
                    leaf vc-label {
                        type types-services:ingress-vc-label;
                        sros-ext:auto-restart-to-modify;
                        description "Ingress MPLS VC label to send packets to the far end";
                    }

                    ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/filter
                    container filter {
                        description "Enter the filter context";

                        ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/filter/mac
                        leaf mac {
                            type leafref {
                                path "../../../../../../filter/mac-filter/filter-name";
                            }
                            description "MAC filter policy name";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/filter/ip
                        leaf ip {
                            type leafref {
                                path "../../../../../../filter/ip-filter/filter-name";
                            }
                            description "IPv4 filter policy name";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/filter/ipv6
                        leaf ipv6 {
                            type leafref {
                                path "../../../../../../filter/ipv6-filter/filter-name";
                            }
                            description "IPv6 filter policy name";
                        }

                    } // container filter

                    ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/qos/network
                        container network {
                            description "Enter the network context";

                            ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/qos/network/policy-name
                            leaf policy-name {
                                type leafref {
                                    path "../../../../../../../qos/network/network-policy-name";
                                }
                                description "Network policy ID";
                            }

                            ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/qos/network/fp-redirect-group
                            container fp-redirect-group {
                                description "Enter the fp-redirect-group context";

                                ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/qos/network/fp-redirect-group/group-name
                                leaf group-name {
                                    type leafref {
                                        path "../../../../../../../../qos/queue-group-templates/ingress/queue-group/ingress-queue-group-name";
                                    }
                                    description "Name of the forwarding plane queue group template";
                                }

                                ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/qos/network/fp-redirect-group/instance
                                leaf instance {
                                    type types-services:qos-qgrp-instance-id;
                                    description "Instance of FP ingress queue group for the SDP binding";
                                }

                            } // container fp-redirect-group

                        } // container network

                    } // container qos

                    ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/l2tpv3
                    container l2tpv3 {
                        description "Enter the l2tpv3 context";

                        ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/l2tpv3/cookie
                        container cookie {
                            description "Enter the cookie context";

                            ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/l2tpv3/cookie/cookie1
                            leaf cookie1 {
                                type types-services:cookie-value;
                                description "Value of cookie-1 for the tunnel";
                            }

                            ///nokia-conf/configure/service/epipe/spoke-sdp/ingress/l2tpv3/cookie/cookie2
                            leaf cookie2 {
                                type types-services:cookie-value;
                                description "Value of cookie-1 for the tunnel";
                            }

                        } // container cookie

                    } // container l2tpv3

                } // container ingress

                ///nokia-conf/configure/service/epipe/spoke-sdp/egress
                container egress {
                    description "Enter the egress context";

                    ///nokia-conf/configure/service/epipe/spoke-sdp/egress/vc-label
                    leaf vc-label {
                        type types-services:egress-vc-label;
                        sros-ext:auto-restart-to-modify;
                        description "Egress MPLS VC label to send packets to the far end";
                    }

                    ///nokia-conf/configure/service/epipe/spoke-sdp/egress/filter
                    container filter {
                        description "Enter the filter context";

                        ///nokia-conf/configure/service/epipe/spoke-sdp/egress/filter/mac
                        leaf mac {
                            type leafref {
                                path "../../../../../../filter/mac-filter/filter-name";
                            }
                            description "MAC filter policy name";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/egress/filter/ip
                        leaf ip {
                            type leafref {
                                path "../../../../../../filter/ip-filter/filter-name";
                            }
                            description "IPv4 filter policy name";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/egress/filter/ipv6
                        leaf ipv6 {
                            type leafref {
                                path "../../../../../../filter/ipv6-filter/filter-name";
                            }
                            description "IPv6 filter policy name";
                        }

                    } // container filter

                    ///nokia-conf/configure/service/epipe/spoke-sdp/egress/qos
                    container qos {
                        description "Enter the qos context";

                        ///nokia-conf/configure/service/epipe/spoke-sdp/egress/qos/network
                        container network {
                            description "Enter the network context";

                            ///nokia-conf/configure/service/epipe/spoke-sdp/egress/qos/network/policy-name
                            leaf policy-name {
                                type leafref {
                                    path "../../../../../../../qos/network/network-policy-name";
                                }
                                description "Network policy ID";
                            }

                            ///nokia-conf/configure/service/epipe/spoke-sdp/egress/qos/network/port-redirect-group
                            container port-redirect-group {
                                description "Enter the port-redirect-group context";

                                ///nokia-conf/configure/service/epipe/spoke-sdp/egress/qos/network/port-redirect-group/group-name
                                leaf group-name {
                                    type leafref {
                                        path "../../../../../../../../qos/queue-group-templates/egress/queue-group/egress-queue-group-name";
                                    }
                                    description "Name of the egress port queue group";
                                }

                                ///nokia-conf/configure/service/epipe/spoke-sdp/egress/qos/network/port-redirect-group/instance
                                leaf instance {
                                    type types-services:qos-qgrp-instance-id;
                                    description "Queue-group instance ID";
                                }

                            } // container port-redirect-group

                        } // container network

                    } // container qos

                    ///nokia-conf/configure/service/epipe/spoke-sdp/egress/l2tpv3
                    container l2tpv3 {
                        description "Enter the l2tpv3 context";

                        ///nokia-conf/configure/service/epipe/spoke-sdp/egress/l2tpv3/cookie
                        leaf cookie {
                            type types-services:cookie-value;
                            description "Cookie parameters";
                        }

                    } // container l2tpv3

                } // container egress

                ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm
                container eth-cfm {
                    description "Enter the eth-cfm context";

                    ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/collect-lmm-stats
                    leaf collect-lmm-stats {
                        type boolean;
                        default "false";
                        description "Collect statistics for loss measurement message tests";
                    }

                    ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/squelch-ingress-levels
                    leaf-list squelch-ingress-levels {
                        type uint32 {
                            range "0..7";
                        }
                        max-elements 8;
                        description "Levels for which ETH-CFM packets are silently discarded";
                    }

                    ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/squelch-ingress-ctag-levels
                    leaf-list squelch-ingress-ctag-levels {
                        type uint32 {
                            range "0..7";
                        }
                        max-elements 8;
                        description "Squelch levels using additional VLAN C-Tag space";
                    }

                    ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/collect-lmm-fc-stats
                    container collect-lmm-fc-stats {
                        description "Enter the collect-lmm-fc-stats context";

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/collect-lmm-fc-stats/fc
                        leaf-list fc {
                            type types-eth-cfm:fc-type;
                            max-elements 8;
                            description "Forwarding class name for profile-unaware counter";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/collect-lmm-fc-stats/fc-in-profile
                        leaf-list fc-in-profile {
                            type types-eth-cfm:fc-type;
                            max-elements 8;
                            description "Forwarding class name for profile-aware counter";
                        }

                    } // container collect-lmm-fc-stats

                    ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep
                    list mep {
                        key "md-admin-name ma-admin-name mep-id";
                        description "Enter the mep list instance";

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/md-admin-name
                        leaf md-admin-name {
                            type leafref {
                                path "../../../../../../eth-cfm/domain/md-admin-name";
                            }
                            description "Maintenance Domain (MD) name";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/ma-admin-name
                        leaf ma-admin-name {
                            type leafref {
                                path "../../../../../../eth-cfm/domain[md-admin-name=current()/../md-admin-name]/association/ma-admin-name";
                            }
                            description "Maintenance Association (MA) name";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/mep-id
                        leaf mep-id {
                            type types-eth-cfm:mep-id-type;
                            description "Maintenance Endpoint (MEP) ID";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/primary-vlan
                        leaf primary-vlan {
                            type boolean;
                            sros-ext:immutable;
                            default "false";
                            description "MEP provisioned using MA primary VLAN ID";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/direction
                        leaf direction {
                            type types-eth-cfm:mp-direction;
                            sros-ext:immutable;
                            description "Direction the MEP faces";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the MEP";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/mac-address
                        leaf mac-address {
                            type types-sros:mac-unicast-address-no-zero;
                            description "MAC address of the MEP";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/install-mep
                        leaf install-mep {
                            type boolean;
                            default "false";
                            description "Install MEP in the forwarding plane";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/cfm-vlan-tag
                        leaf cfm-vlan-tag {
                            type types-sros:one-or-two-vlan-tags;
                            description "VLAN tags to apply to CFM PDUs for egress processing";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/low-priority-defect
                        leaf low-priority-defect {
                            type types-eth-cfm:lowest-alarm-priority;
                            default "mac-rem-err-xcon";
                            description "Lowest priority defect allowed to generate fault alarm";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/fault-propagation
                        leaf fault-propagation {
                            type types-eth-cfm:fault-propagation-type;
                            description "Fault propagation for the MEP";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/ccm
                        leaf ccm {
                            type boolean;
                            default "false";
                            description "Generate CCM messages";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/ccm-ltm-priority
                        leaf ccm-ltm-priority {
                            type types-eth-cfm:frame-priority;
                            default "7";
                            description "Priority of CCM and LTM messages transmitted by the MEP";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/ccm-padding-size
                        leaf ccm-padding-size {
                            type uint32 {
                                range "3..1500";
                            }
                            description "Number of octets of padding to insert in CCM packets";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/lbm-svc-act-responder
                        leaf lbm-svc-act-responder {
                            type boolean;
                            description "Process service activation streams in ETH-CFM LBM";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/one-way-delay-threshold
                        leaf one-way-delay-threshold {
                            type uint32 {
                                range "0..600";
                            }
                            units "seconds";
                            default "3";
                            description "Threshold time limit for the one-way delay test";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/alarm-notification
                        container alarm-notification {
                            description "Enter the alarm-notification context";

                            ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/alarm-notification/fng-alarm-time
                            leaf fng-alarm-time {
                                type int32 {
                                    range "250|500|1000";
                                }
                                units "centiseconds";
                                description "Time that must expire before an FNG alarm is raised";
                            }

                            ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/alarm-notification/fng-reset-time
                            leaf fng-reset-time {
                                type int32 {
                                    range "250|500|1000";
                                }
                                units "centiseconds";
                                description "Time that must expire before an FNG alarm is reset";
                            }

                        } // container alarm-notification

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/ais
                        container ais {
                            presence "Enable the ais context";
                            description "Enable the ais context";

                            ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/ais/client-meg-level
                            leaf-list client-meg-level {
                                type uint32 {
                                    range "1..7";
                                }
                                max-elements 7;
                                description "Client MEG level for AIS message generation";
                            }

                            ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/ais/interface-support
                            leaf interface-support {
                                type boolean;
                                default "false";
                                description "Enable generation of AIS PDUs based on endpoint state";
                            }

                            ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/ais/interval
                            leaf interval {
                                type uint32 {
                                    range "1|60";
                                }
                                units "seconds";
                                default "1";
                                description "Transmission interval for AIS messages";
                            }

                            ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/ais/low-priority-defect
                            leaf low-priority-defect {
                                type enumeration {
                                    enum all-def                        { value 1; }
                                    enum mac-rem-err-xcon               { value 2; }
                                }
                                default "all-def";
                                description "Lowest priority defect allowed to generate fault alarm";
                            }

                            ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/ais/priority
                            leaf priority {
                                type types-eth-cfm:frame-priority;
                                default "7";
                                description "Priority of the AIS messages generated by the node";
                            }

                        } // container ais

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/csf
                        container csf {
                            presence "Enable the csf context";
                            description "Enable the csf context";

                            ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/csf/multiplier
                            leaf multiplier {
                                type decimal64 {
                                    range "0.0|2.0..30.0";
                                    fraction-digits 1;
                                }
                                default "3.5";
                                description "Multiplication factor used to clear the CSF condition";
                            }

                        } // container csf

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/eth-test
                        container eth-test {
                            presence "Enable the eth-test context";
                            description "Enable the eth-test context";

                            ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/eth-test/bit-error-threshold
                            leaf bit-error-threshold {
                                type uint32 {
                                    range "0..11840";
                                }
                                units "bit errors";
                                default "1";
                                description "Lowest priority defect allowed to generate fault alarm";
                            }

                            ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/eth-test/test-pattern
                            container test-pattern {
                                description "Enter the test-pattern context";

                                ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/eth-test/test-pattern/pattern
                                leaf pattern {
                                    type enumeration {
                                        enum all-zeros                      { value 0; }
                                        enum all-ones                       { value 1; }
                                    }
                                    default "all-zeros";
                                    description "Test pattern for Ethernet Test frames";
                                }

                                ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/eth-test/test-pattern/crc-tlv
                                leaf crc-tlv {
                                    type boolean;
                                    default "false";
                                    description "Generate a CRC checksum";
                                }

                            } // container test-pattern

                        } // container eth-test

                        ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/grace
                        container grace {
                            description "Enter the grace context";

                            ///nokia-conf/configure/service/epipe/spoke-sdp/eth-cfm/mep/grace/eth-ed
                            container eth-ed {
                                description "Enter the eth-ed context";

