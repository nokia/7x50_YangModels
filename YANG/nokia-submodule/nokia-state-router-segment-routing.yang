submodule nokia-state-router-segment-routing {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-igp                { prefix "types-igp"; }
    import nokia-types-sr-mpls            { prefix "types-sr-mpls"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-state-router;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for segment routing management on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-router-segment-routing YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-06-02";

    grouping state-router-segment-routing {
        ///nokia-state/state/router/segment-routing
        container segment-routing {
            description "Enable the segment-routing context";

            ///nokia-state/state/router/segment-routing/sr-mpls
            container sr-mpls {
                description "Enable the sr-mpls context";

                ///nokia-state/state/router/segment-routing/sr-mpls/prefix-sids
                list prefix-sids {
                    key "interface-name";
                    description "Enter the prefix-sids list instance";

                    ///nokia-state/state/router/segment-routing/sr-mpls/prefix-sids/interface-name
                    leaf interface-name {
                        type leafref {
                            path "../../../../interface/interface-name";
                        }
                        description "Loopback interface name that owns prefix to advertise";
                    }

                    ///nokia-state/state/router/segment-routing/sr-mpls/prefix-sids/ipv4-sid
                    container ipv4-sid {
                        description "Enable the ipv4-sid context";

                        ///nokia-state/state/router/segment-routing/sr-mpls/prefix-sids/ipv4-sid/oper-state
                        leaf oper-state {
                            type types-sr-mpls:sid-oper-state;
                        }

                    } // container ipv4-sid

                    ///nokia-state/state/router/segment-routing/sr-mpls/prefix-sids/ipv6-sid
                    container ipv6-sid {
                        description "Enable the ipv6-sid context";

                        ///nokia-state/state/router/segment-routing/sr-mpls/prefix-sids/ipv6-sid/oper-state
                        leaf oper-state {
                            type types-sr-mpls:sid-oper-state;
                        }

                    } // container ipv6-sid

                } // list prefix-sids

            } // container sr-mpls

            ///nokia-state/state/router/segment-routing/sr-policies
            container sr-policies {
                description "Enter the sr-policies context";

                ///nokia-state/state/router/segment-routing/sr-policies/ttm-preferences
                leaf ttm-preferences {
                    type uint32;
                    description "Specifies the TTM preferences used for sr-policy tunnels.";
                }

                ///nokia-state/state/router/segment-routing/sr-policies/binding-sids-allocated
                leaf binding-sids-allocated {
                    type uint32;
                    description "Number of allocated SR-MPLS binding SIDs.";
                }

                ///nokia-state/state/router/segment-routing/sr-policies/srv6-binding-sids-allocated
                leaf srv6-binding-sids-allocated {
                    type uint32;
                    description "Number of allocated SRv6 binding SIDs.";
                }

                ///nokia-state/state/router/segment-routing/sr-policies/static-local-policies
                leaf static-local-policies {
                    type uint32;
                    description "Specifies the static local policies.";
                }

                ///nokia-state/state/router/segment-routing/sr-policies/active-static-local-policies
                leaf active-static-local-policies {
                    type uint32;
                    description "Specifies the active static local policies.";
                }

                ///nokia-state/state/router/segment-routing/sr-policies/static-non-local-policies
                leaf static-non-local-policies {
                    type uint32;
                    description "Specifies the static non-local policies.";
                }

                ///nokia-state/state/router/segment-routing/sr-policies/bgp-policies
                leaf bgp-policies {
                    type uint32;
                    description "Specifies the bgp policies.";
                }

                ///nokia-state/state/router/segment-routing/sr-policies/active-bgp-policies
                leaf active-bgp-policies {
                    type uint32;
                    description "Specifies the active bgp policies.";
                }

                ///nokia-state/state/router/segment-routing/sr-policies/static-policy
                list static-policy {
                    key "name";
                    description "Enter the static-policy list instance";

                    ///nokia-state/state/router/segment-routing/sr-policies/static-policy/name
                    leaf name {
                        type types-sros:named-item-64;
                        description "Name for the segment routing static policy";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/static-policy/segment-routing-v6
                    container segment-routing-v6 {
                        description "Enable the segment-routing-v6 context";

                        ///nokia-state/state/router/segment-routing/sr-policies/static-policy/segment-routing-v6/binding-sid
                        list binding-sid {
                            key "index";
                            description "Enter the binding-sid list instance";

                            ///nokia-state/state/router/segment-routing/sr-policies/static-policy/segment-routing-v6/binding-sid/index
                            leaf index {
                                type uint32 {
                                    range "1";
                                }
                                description "Binding SID index";
                            }

                            ///nokia-state/state/router/segment-routing/sr-policies/static-policy/segment-routing-v6/binding-sid/binding-sid-type
                            choice binding-sid-type {
                                ///nokia-state/state/router/segment-routing/sr-policies/static-policy/segment-routing-v6/binding-sid/binding-sid-type/local-binding-sid
                                case local-binding-sid {

                                    ///nokia-state/state/router/segment-routing/sr-policies/static-policy/segment-routing-v6/binding-sid/binding-sid-type/local-binding-sid/locator
                                    container locator {
                                        description "Enable the locator context";

                                        ///nokia-state/state/router/segment-routing/sr-policies/static-policy/segment-routing-v6/binding-sid/binding-sid-type/local-binding-sid/locator/function-oper-value
                                        leaf function-oper-value {
                                            type uint32;
                                            description "SRv6 operational function value.";
                                        }

                                        ///nokia-state/state/router/segment-routing/sr-policies/static-policy/segment-routing-v6/binding-sid/binding-sid-type/local-binding-sid/locator/function-status
                                        leaf function-status {
                                            type types-sros:srv6-error-status;
                                            description "State of the function.";
                                        }

                                    } // container locator

                                }
                            }
                        } // list binding-sid

                    } // container segment-routing-v6

                } // list static-policy

                ///nokia-state/state/router/segment-routing/sr-policies/sr-path
                list sr-path {
                    key "head-end color endpoint owner preference distinguisher";
                    description "Enter the sr-path list instance";

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/head-end
                    leaf head-end {
                        type types-sros:ip-address;
                        description "Head end address of the segment route path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/color
                    leaf color {
                        type uint32;
                        description "Determines the set of traffic flows that would be steered by this policy.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/endpoint
                    leaf endpoint {
                        type types-sros:ip-address;
                        description "Destination of the source-routed path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/owner
                    leaf owner {
                        type enumeration {
                            enum none                           { value 0; }
                            enum bgp                            { value 1; }
                            enum static                         { value 2; }
                            enum nsp                            { value 3; }
                        }
                        description "Owner of the segment routing policy instance.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/preference
                    leaf preference {
                        type uint32;
                        description "Indicates the preference value of this static policy.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/distinguisher
                    leaf distinguisher {
                        type uint32;
                        description "Is required if the head-end of the policy is not local.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/tunnel-id
                    leaf tunnel-id {
                        type uint32;
                        description "Specifies the tunnel-id used by the segment routing path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/active
                    leaf active {
                        type boolean;
                        description "Indicates whether segment routing path is active.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/path-age
                    leaf path-age {
                        type uint32;
                        description "Specifies the age of the segment routing path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/re-evaluations
                    leaf re-evaluations {
                        type uint32;
                        description "Specifies the number of re-evaluations for segment routing path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/active-path-changes
                    leaf active-path-changes {
                        type uint32;
                        description "Specifies the number of active path changes for segment routing path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/re-evaluation-reason
                    leaf re-evaluation-reason {
                        type enumeration {
                            enum none                           { value 0; }
                            enum route-add                      { value 1; }
                            enum route-mod                      { value 2; }
                            enum route-del                      { value 3; }
                            enum tunnel-down                    { value 4; }
                            enum sr-failure                     { value 5; }
                            enum route-audit                    { value 6; }
                            enum admin-no-shut                  { value 7; }
                            enum bsid-failure                   { value 8; }
                        }
                        description "Specifies the reason for the last re-evaluation of the segment routing path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/binding-sid
                    leaf binding-sid {
                        type types-sros:mpls-label-or-zero;
                        description "Specifies the number of active path changes for segment routing path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/origin-address
                    leaf origin-address {
                        type types-sros:ip-address;
                        description "Address of the originating bgp router-id.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/origin-asn
                    leaf origin-asn {
                        type uint32;
                        description
                            "Specifies the ASN of the node/controller signalling the candidate path
                             when it has a BGP session to the head-end.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/maintenance-policy
                    leaf maintenance-policy {
                        type types-sros:named-item-or-empty;
                        description "Specifies the maintenance-policy for the sr-path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/type
                    leaf type {
                        type enumeration {
                            enum sr-mpls                        { value 1; }
                            enum srv6                           { value 2; }
                        }
                        description "Path type.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/active-sbfd-sessions
                    leaf active-sbfd-sessions {
                        type uint32;
                        description "Specifies number of active BFD sessions for the sr-path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/srv6-binding-sid
                    list srv6-binding-sid {
                        key "index";
                        description "Enter the srv6-binding-sid list instance";

                        ///nokia-state/state/router/segment-routing/sr-policies/sr-path/srv6-binding-sid/index
                        leaf index {
                            type uint32;
                            description "Binding SID index.";
                        }

                        ///nokia-state/state/router/segment-routing/sr-policies/sr-path/srv6-binding-sid/srv6-oper-binding-sid
                        leaf srv6-oper-binding-sid {
                            type types-sros:ipv6-address;
                            description "Operational SRv6 Binding SID IP address";
                        }

                    } // list srv6-binding-sid

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path/sr-path-seg-list
                    list sr-path-seg-list {
                        key "seg-list-index";
                        description "Enter the sr-path-seg-list list instance";

                        ///nokia-state/state/router/segment-routing/sr-policies/sr-path/sr-path-seg-list/seg-list-index
                        leaf seg-list-index {
                            type uint32 {
                                range "1..32";
                            }
                            description "Index for identifying a particular segment list";
                        }

                        ///nokia-state/state/router/segment-routing/sr-policies/sr-path/sr-path-seg-list/weight
                        leaf weight {
                            type uint32;
                            description "Specifies the relative weight of the segment list associated with a static policy.";
                        }

                        ///nokia-state/state/router/segment-routing/sr-policies/sr-path/sr-path-seg-list/segments
                        leaf segments {
                            type uint32;
                            description "Specifies the number of segments in the routing path.";
                        }

                        ///nokia-state/state/router/segment-routing/sr-policies/sr-path/sr-path-seg-list/sbfd-state
                        leaf sbfd-state {
                            type enumeration {
                                enum down                           { value 0; }
                                enum up                             { value 1; }
                            }
                            description "Specifies the s-bfd state for the routing path.";
                        }

                        ///nokia-state/state/router/segment-routing/sr-policies/sr-path/sr-path-seg-list/sbfd-transitions
                        leaf sbfd-transitions {
                            type uint32;
                            description "Specifies the s-bfd transitions for the routing path.";
                        }

                        ///nokia-state/state/router/segment-routing/sr-policies/sr-path/sr-path-seg-list/segment
                        list segment {
                            key "segment-index";
                            description "Enter the segment list instance";

                            ///nokia-state/state/router/segment-routing/sr-policies/sr-path/sr-path-seg-list/segment/segment-index
                            leaf segment-index {
                                type uint32 {
                                    range "1..11";
                                }
                                description "Index for identifying a particular segment list";
                            }

                            ///nokia-state/state/router/segment-routing/sr-policies/sr-path/sr-path-seg-list/segment/label
                            leaf label {
                                type uint32;
                                description "Specifies the label used by the segment in the segment routing path.";
                            }

                            ///nokia-state/state/router/segment-routing/sr-policies/sr-path/sr-path-seg-list/segment/srv6-sid
                            leaf srv6-sid {
                                type types-sros:ipv6-address;
                                description "SRv6 SID used by the segment in the segment routing path.";
                            }

                            ///nokia-state/state/router/segment-routing/sr-policies/sr-path/sr-path-seg-list/segment/segment-state
                            leaf segment-state {
                                type enumeration {
                                    enum not-applicable                 { value 0; }
                                    enum resolved-up                    { value 1; }
                                    enum resolved-down                  { value 2; }
                                }
                                description "Specifies the state of the segment.";
                            }

                        } // list segment

                    } // list sr-path-seg-list

                } // list sr-path

                ///nokia-state/state/router/segment-routing/sr-policies/sr-path-bsids
                list sr-path-bsids {
                    key "binding-sid";
                    description "Enter the sr-path-bsids list instance";

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path-bsids/binding-sid
                    leaf binding-sid {
                        type types-sros:mpls-label-or-zero;
                        description "Specifies the binding SID";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-path-bsids/active
                    leaf active {
                        type boolean;
                        description "Indicates whether segment routing path is active.";
                    }

                } // list sr-path-bsids

                ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-ingress-statistics
                list sr-policy-ingress-statistics {
                    key "color endpoint";
                    description "Enter the sr-policy-ingress-statistics list instance";

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-ingress-statistics/color
                    leaf color {
                        type uint32;
                        description "Determines the set of traffic flows that would be steered by this policy.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-ingress-statistics/endpoint
                    leaf endpoint {
                        type types-sros:ip-address;
                        description "Destination of the source-routed path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-ingress-statistics/tunnel-id
                    leaf tunnel-id {
                        type uint32;
                        description "Specifies the tunnel-id used by the segment routing path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-ingress-statistics/binding-sid
                    leaf binding-sid {
                        type types-sros:mpls-label-or-zero;
                        description
                            "The binding SID (segment ID) value that opaquely represents the SR policy to
                             upstream routers.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-ingress-statistics/packet-count
                    leaf packet-count {
                        type uint64;
                        description "Specifies the packets received.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-ingress-statistics/byte-count
                    leaf byte-count {
                        type uint64;
                        description "Specifies the bytes received.";
                    }

                } // list sr-policy-ingress-statistics

                ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-egress-statistics
                list sr-policy-egress-statistics {
                    key "color endpoint segment-index";
                    description "Enter the sr-policy-egress-statistics list instance";

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-egress-statistics/color
                    leaf color {
                        type uint32;
                        description "Determines the set of traffic flows that would be steered by this policy.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-egress-statistics/endpoint
                    leaf endpoint {
                        type types-sros:ip-address;
                        description "Destination of the source-routed path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-egress-statistics/segment-index
                    leaf segment-index {
                        type uint32 {
                            range "1..32";
                        }
                        description "Segment-list index.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-egress-statistics/tunnel-id
                    leaf tunnel-id {
                        type uint32;
                        description "Specifies the tunnel-id used by the segment routing path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-egress-statistics/binding-sid
                    leaf binding-sid {
                        type types-sros:mpls-label-or-zero;
                        description "Specifies the number of active path changes for segment routing path.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-egress-statistics/packet-count
                    leaf packet-count {
                        type uint64;
                        description "Specifies the packets received.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-egress-statistics/byte-count
                    leaf byte-count {
                        type uint64;
                        description "Specifies the bytes received.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-egress-statistics/packet-rate
                    leaf packet-rate {
                        type yang:counter32;
                        units "packets per second";
                        description
                            "The value of packet-rate indicates the average rate in packets per second for the
                             collection interval that ended most recently.";
                    }

                    ///nokia-state/state/router/segment-routing/sr-policies/sr-policy-egress-statistics/bits-rate
                    leaf bits-rate {
                        type yang:counter32;
                        units "megabps";
                        description
                            "The value of bits-rate indicates the average rate in megabits per second (Mbps)
                             for the collection interval that ended most recently.";
                    }

                } // list sr-policy-egress-statistics

            } // container sr-policies

            ///nokia-state/state/router/segment-routing/segment-routing-v6
            container segment-routing-v6 {
                description "Enable the segment-routing-v6 context";

                ///nokia-state/state/router/segment-routing/segment-routing-v6/locator
                list locator {
                    key "locator-name";
                    description "Enter the locator list instance";

                    ///nokia-state/state/router/segment-routing/segment-routing-v6/locator/locator-name
                    leaf locator-name {
                        type types-sros:named-item-64;
                        description "SRv6 locator name";
                    }

                    ///nokia-state/state/router/segment-routing/segment-routing-v6/locator/oper-state
                    leaf oper-state {
                        type types-sros:oper-state;
                        description "Locator operational state.";
                    }

                    ///nokia-state/state/router/segment-routing/segment-routing-v6/locator/down-reason
                    leaf down-reason {
                        type types-sros:srv6-error-status;
                        description "Locator down reason.";
                    }

                } // list locator

                ///nokia-state/state/router/segment-routing/segment-routing-v6/micro-segment
                container micro-segment {
                    description "Enable the micro-segment context";

                    ///nokia-state/state/router/segment-routing/segment-routing-v6/micro-segment/block
                    list block {
                        key "block-name";
                        description "Enter the block list instance";

                        ///nokia-state/state/router/segment-routing/segment-routing-v6/micro-segment/block/block-name
                        leaf block-name {
                            type types-sros:named-item-64;
                            description "Micro-SID block name";
                        }

                    } // list block

                } // container micro-segment

                ///nokia-state/state/router/segment-routing/segment-routing-v6/micro-segment-locator
                list micro-segment-locator {
                    key "locator-name";
                    description "Enter the micro-segment-locator list instance";

                    ///nokia-state/state/router/segment-routing/segment-routing-v6/micro-segment-locator/locator-name
                    leaf locator-name {
                        type types-sros:named-item-64;
                        description "SRv6 locator name";
                    }

                    ///nokia-state/state/router/segment-routing/segment-routing-v6/micro-segment-locator/oper-state
                    leaf oper-state {
                        type types-sros:oper-state;
                        description "Locator operational state.";
                    }

                    ///nokia-state/state/router/segment-routing/segment-routing-v6/micro-segment-locator/down-reason
                    leaf down-reason {
                        type types-sros:srv6-error-status;
                        description "Locator down reason.";
                    }

                } // list micro-segment-locator

                ///nokia-state/state/router/segment-routing/segment-routing-v6/base-routing-instance
                container base-routing-instance {
                    description "Enable the base-routing-instance context";

                    ///nokia-state/state/router/segment-routing/segment-routing-v6/base-routing-instance/locator
                    list locator {
                        key "locator-name";
                        description "Enter the locator list instance";

                        ///nokia-state/state/router/segment-routing/segment-routing-v6/base-routing-instance/locator/locator-name
                        leaf locator-name {
                            type leafref {
                                path "../../../locator/locator-name";
                            }
                            description "SRv6 locator name";
                        }

                        ///nokia-state/state/router/segment-routing/segment-routing-v6/base-routing-instance/locator/function
                        container function {
                            description "Enter the function context";

                            ///nokia-state/state/router/segment-routing/segment-routing-v6/base-routing-instance/locator/function/end
                            list end {
                                key "value";
                                description "Enter the end list instance";

                                ///nokia-state/state/router/segment-routing/segment-routing-v6/base-routing-instance/locator/function/end/value
                                leaf value {
                                    type uint32 {
                                        range "1..1048575";
                                    }
                                    description "SRv6 SID function value";
                                }

                                ///nokia-state/state/router/segment-routing/segment-routing-v6/base-routing-instance/locator/function/end/oper-value
                                leaf oper-value {
                                    type uint32;
                                    description "SRv6 function value.";
                                }

                                ///nokia-state/state/router/segment-routing/segment-routing-v6/base-routing-instance/locator/function/end/status
                                leaf status {
                                    type types-sros:srv6-error-status;
                                    description "specifies the current state of this function.";
                                }

