submodule nokia-state-users {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-system             { prefix "types-system"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for users lists on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-users YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-04-17";

    grouping state-users {
        ///nokia-state/state/users
        container users {
            description "Enter the users context";

            ///nokia-state/state/users/session
            list session {
                key "session-id";
                description "Enter the session list instance";

                ///nokia-state/state/users/session/session-id
                leaf session-id {
                    type uint32;
                    description "The unique identifier of session";
                }

                ///nokia-state/state/users/session/user
                leaf user {
                    type types-sros:named-item;
                    description "Name of the user";
                }

                ///nokia-state/state/users/session/router-instance
                leaf router-instance {
                    type types-sros:named-item-64-or-empty;
                    description "The value indicates the routing context of a connection";
                }

                ///nokia-state/state/users/session/connection-ip
                leaf connection-ip {
                    type types-sros:ip-address-with-zone;
                    description "Connection ip address";
                }

                ///nokia-state/state/users/session/connection-type
                leaf connection-type {
                    type types-system:session-type;
                    description "The type of the connection used for the cli session";
                }

                ///nokia-state/state/users/session/login-time
                leaf login-time {
                    type yang:date-and-time;
                    description "The time the user logged in and created the session";
                }

                ///nokia-state/state/users/session/idle-time
                leaf idle-time {
                    type types-sros:time-duration;
                    units "seconds";
                    description "Indicates the session's idle time";
                }

                ///nokia-state/state/users/session/current-active-session
                leaf current-active-session {
                    type boolean;
                    description "Indicates the current active session";
                }

                ///nokia-state/state/users/session/connection-id
                leaf connection-id {
                    type uint32;
                    description "The unique identifier of connection";
                }

                ///nokia-state/state/users/session/session-create-time
                leaf session-create-time {
                    type yang:date-and-time;
                    description "Time of the session is created";
                }

            } // list session

        } // container users

    } // grouping state-users

}
