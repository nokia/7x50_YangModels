submodule nokia-conf-svc-ies-grp-if-wlangw {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-application-assurance { prefix "types-aa"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-subscriber-mgmt    { prefix "types-submgt"; }
    import nokia-types-wlangw             { prefix "types-wlangw"; }

    include nokia-conf-aa-group;
    include nokia-conf-aaa;
    include nokia-conf-application-assurance;
    include nokia-conf-group-encryption;
    include nokia-conf-groups;
    include nokia-conf-isa;
    include nokia-conf-qos;
    include nokia-conf-redundancy;
    include nokia-conf-service;
    include nokia-conf-submgt-isa-filter;
    include nokia-conf-submgt-isa-policer;
    include nokia-conf-submgt-vrgw;
    include nokia-conf-subscriber-mgmt;
    include nokia-conf-svc-nat;
    include nokia-conf-svc-vpls;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure ies service group interface WLAN Gateway on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-svc-ies-grp-if-wlangw YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-05-23";

    grouping conf-svc-ies-grp-if-wlangw-wlan-gw {
        ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw
        container wlan-gw {
            when "../type = 'wlan-gw'" {
                description "only available in group-interfaces of type 'wlan-gw'";
            }
            description "Enable the wlan-gw context";

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/admin-state
            leaf admin-state {
                type types-sros:admin-state;
                default "disable";
                description "Administrative state of WLAN Gateway";
            }

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/oper-down-on-group-degrade
            leaf oper-down-on-group-degrade {
                type boolean;
                default "false";
                description "Bring interface down when the ISA WLAN Gateway degraded";
            }

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/tcp-mss-adjust
            leaf tcp-mss-adjust {
                type uint32 {
                    range "160..10240";
                }
                units "bytes";
                description "TCP Maximum Segment Size (MSS) adjustment for gateway";
            }

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/wlan-gw-group
            leaf wlan-gw-group {
                type leafref {
                    path "../../../../../../isa/wlan-gw-group/id";
                }
                sros-ext:auto-restart-to-modify;
                description "ID of WLAN Gateway ISA group that gateway binds to";
            }

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/gateway-router
            leaf gateway-router {
                type string;
                sros-ext:auto-restart-to-modify;
                description "Routing instance the WLAN-GW endpoint resides in";
            }

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/lanext
            container lanext {
                presence "Enable the lanext context";
                sros-ext:auto-restart-to-modify;
                description "Enable the lanext context";

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/lanext/max-bd
                leaf max-bd {
                    type uint32 {
                        range "1..131071";
                    }
                    sros-ext:auto-restart-to-modify;
                    default "131071";
                    description "Maximum number of bridge domains";
                }

            } // container lanext

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/learn-ap-mac
            container learn-ap-mac {
                presence "Enable the learn-ap-mac context";
                description "Enable the learn-ap-mac context";

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/learn-ap-mac/delay-auth
                leaf delay-auth {
                    type boolean;
                    default "false";
                    description "Delay AP-MAC until after ARP/ND authentication response";
                }

            } // container learn-ap-mac

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/mobility
            container mobility {
                description "Enter the mobility context";

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/mobility/hold-time
                leaf hold-time {
                    type types-sros:time-duration {
                        range "0..255";
                    }
                    units "seconds";
                    default "5";
                    description "Minimum time between two mobility events for single UE";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/mobility/inter-tunnel-type
                leaf inter-tunnel-type {
                    type boolean;
                    sros-ext:auto-restart-to-modify;
                    default "false";
                    description "Enable mobility between terminating tunnel types";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/mobility/inter-vlan
                leaf inter-vlan {
                    type boolean;
                    default "false";
                    description "Allow mobility within different VLANs of the same range";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/mobility/trigger
                container trigger {
                    description "Enter the trigger context";

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/mobility/trigger/data
                    leaf data {
                        type boolean;
                        default "false";
                        description "Use data traffic as mobility trigger";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/mobility/trigger/iapp
                    leaf iapp {
                        type boolean;
                        default "false";
                        description "Use IAPP messages as a mobility trigger";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/mobility/trigger/control
                    leaf control {
                        type boolean;
                        default "false";
                        description "Use control traffic as a mobility trigger";
                    }

                } // container trigger

            } // container mobility

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/tunnel-egress-qos
            container tunnel-egress-qos {
                description "Enter the tunnel-egress-qos context";

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/tunnel-egress-qos/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    sros-ext:auto-restart-to-modify;
                    default "disable";
                    description "Administrative state of egress QoS for WLAN-GW tunnels";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/tunnel-egress-qos/agg-rate-limit
                leaf agg-rate-limit {
                    type types-qos:pir-rate-override;
                    default "max";
                    description "HQoS aggregate rate limit";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/tunnel-egress-qos/granularity
                leaf granularity {
                    type types-wlangw:shaping-granularity;
                    sros-ext:auto-restart-to-modify;
                    default "per-tunnel";
                    description "Granularity of the egress shaping for WLAN Gateway";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/tunnel-egress-qos/hold-time
                leaf hold-time {
                    type union {
                        type types-sros:time-duration {
                            range "1..86400";
                        }
                        type enumeration {
                            enum infinite                       { value -1; }
                        }
                    }
                    units "seconds";
                    description "Minimum time to hold egress shaping resources";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/tunnel-egress-qos/multi-client-only
                leaf multi-client-only {
                    type boolean;
                    default "false";
                    description "Allow shaping for tunnel traffic for multiple UEs";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/tunnel-egress-qos/qos
                leaf qos {
                    type leafref {
                        path "../../../../../../../qos/sap-egress/sap-egress-policy-name";
                    }
                    sros-ext:auto-restart-to-modify;
                    description "Egress QoS policy linked with each interface tunnel";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/tunnel-egress-qos/scheduler-policy
                leaf scheduler-policy {
                    type leafref {
                        path "../../../../../../../qos/scheduler-policy/scheduler-policy-name";
                    }
                    sros-ext:auto-restart-to-modify;
                    description "Egress scheduler policy linked with each tunnel";
                }

            } // container tunnel-egress-qos

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/tunnel-encaps
            container tunnel-encaps {
                description "Enter the tunnel-encaps context";

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/tunnel-encaps/learn-l2tp-cookie
                leaf learn-l2tp-cookie {
                    type union {
                        type enumeration {
                            enum never {
                                value 2;
                                description "Never interpret the cookie";
                            }
                            enum always {
                                value 3;
                                description "Interpret cookie regardless of first two octets value";
                            }
                        }
                        type types-sros:hex-string {
                            length "6";
                        }
                    }
                    default "never";
                    description "System that learns the cookie from L2TP tunnels terminating on this interface";
                }

            } // container tunnel-encaps

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/group-encryption
            container group-encryption {
                description "Enter the group-encryption context";

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/group-encryption/encryption-keygroup-inbound
                leaf encryption-keygroup-inbound {
                    type leafref {
                        path "../../../../../../../group-encryption/encryption-keygroup/id";
                    }
                    description "Encryption keygroup for inbound traffic";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/group-encryption/encryption-keygroup-outbound
                leaf encryption-keygroup-outbound {
                    type leafref {
                        path "../../../../../../../group-encryption/encryption-keygroup/id";
                    }
                    description "Encryption keygroup for inbound traffic";
                }

            } // container group-encryption


            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/l2-ap
            container l2-ap {
                description "Enter the l2-ap context";

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/l2-ap/auto-sub-id-fmt
                leaf auto-sub-id-fmt {
                    type enumeration {
                        enum include-ap-tags                { value 0; }
                        enum sap-only                       { value 1; }
                    }
                    default "include-ap-tags";
                    description "Format of the auto-generated subscriber ID";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/l2-ap/default-encap-type
                leaf default-encap-type {
                    type enumeration {
                        enum null                           { value 1; }
                        enum dot1q                          { value 2; }
                        enum qinq                           { value 10; }
                    }
                    sros-ext:auto-restart-to-modify;
                    default "null";
                    description "Default encapsulation type for Layer 2 access points";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/l2-ap/access-point
                list access-point {
                    key "sap-id";
                    description "Enter the access-point list instance";

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/l2-ap/access-point/sap-id
                    leaf sap-id {
                        type types-sros:sap;
                        description "SAP ID for the Layer 2 access point";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/l2-ap/access-point/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the L2 access points";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/l2-ap/access-point/encap-type
                    leaf encap-type {
                        type enumeration {
                            enum null                           { value 1; }
                            enum dot1q                          { value 2; }
                            enum qinq                           { value 10; }
                        }
                        sros-ext:auto-restart-to-modify;
                        description "Service encapsulation type of this access point";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/l2-ap/access-point/epipe-sap-template
                    leaf epipe-sap-template {
                        type leafref {
                            path "../../../../../../../template/epipe-sap-template/name";
                        }
                        sros-ext:auto-restart-to-modify;
                        description "Parameters template for the L2 access point SAP";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/l2-ap/access-point/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/l2-ap/access-point/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list access-point

            } // container l2-ap

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/gateway-address
            list gateway-address {
                key "address";
                max-elements 10;
                description "Enter the gateway-address list instance";

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/gateway-address/address
                leaf address {
                    type types-sros:ip-unicast-without-local-address;
                    description "Gateway endpoint address of the WLAN Gateway tunnel";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/gateway-address/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/gateway-address/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/gateway-address/purpose
                container purpose {
                    description "Enter the purpose context";

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/gateway-address/purpose/xconnect
                    leaf xconnect {
                        type boolean;
                        default "false";
                        description "Use tunnel IP address for crosse-connect traffic";
                    }

                } // container purpose

            } // list gateway-address

            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range
            list vlan-range {
                key "range";
                description "Enter the vlan-range list instance";

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/range
                leaf range {
                    type string {
                        pattern "((([0-9]|[1-9][0-9]{1,2}|[1-3][0-9]{3}|40[0-8][0-9]|409[0-5]))(\\.\\.(([0-9]|[1-9][0-9]{1,2}|[1-3][0-9]{3}|40[0-8][0-9]|409[0-5])))?)|(unmatched)";
                    }
                    description "IEEE 802.1q VLAN tag range";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/dynamic-service
                leaf dynamic-service {
                    type boolean;
                    default "false";
                    description "Dynamically assign WLAN-GW subscriber to a VPLS service";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/http-redirect-policy
                leaf http-redirect-policy {
                    type leafref {
                        path "../../../../../../../subscriber-mgmt/http-redirect-policy/name";
                    }
                    description "Default HTTP redirect policy for portal authentication";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/idle-timeout-action
                leaf idle-timeout-action {
                    type types-wlangw:idle-timeout-action;
                    default "remove";
                    description "Action to perform when the idle timeout expires";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/nat-policy
                leaf nat-policy {
                    type leafref {
                        path "../../../../../../nat/nat-policy/name";
                    }
                    description "NAT policy for DSM and ISA portal authentication";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/retail-service
                leaf retail-service {
                    type types-services:service-name;
                    description "Default retail service for new UEs in this range";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/authentication
                container authentication {
                    description "Enter the authentication context";

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/authentication/hold-time
                    leaf hold-time {
                        type types-sros:time-duration {
                            range "0..3600";
                        }
                        units "seconds";
                        default "5";
                        description "Minimum time UE held down after failed authentication";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/authentication/on-control-plane
                    leaf on-control-plane {
                        type boolean;
                        default "false";
                        description "Allow authentication on first control plane packet";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/authentication/policy
                    leaf policy {
                        type leafref {
                            path "../../../../../../../../aaa/radius/isa-policy/name";
                        }
                        description "RADIUS policy for authentication";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/authentication/vlan-mismatch-timeout
                    leaf vlan-mismatch-timeout {
                        type types-sros:time-duration {
                            range "5..60";
                        }
                        units "seconds";
                        description "Timeout if packet received with a non-matching VLAN tag";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/authentication/local
                    container local {
                        presence "Enable the local context";
                        description "Enable the local context";

                        ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/authentication/local/coa-policy
                        leaf coa-policy {
                            type leafref {
                                path "../../../../../../../../../aaa/radius/isa-policy/name";
                            }
                            description "RADIUS ISA policy applied to CoA or disconnect messages";
                        }

                        ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/authentication/local/default-ue-state
                        leaf default-ue-state {
                            type enumeration {
                                enum portal {
                                    value 1;
                                    description "Portal redirect UE using the http-redirect-policy";
                                }
                                enum dsm {
                                    value 2;
                                    description "DSM UE using DSM parameters";
                                }
                            }
                            mandatory true;
                            description "UE state accepted for immediate authentication";
                        }

                    } // container local

                } // container authentication

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/data-triggered-ue-creation
                container data-triggered-ue-creation {
                    description "Enter the data-triggered-ue-creation context";

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/data-triggered-ue-creation/arp
                    leaf arp {
                        type boolean;
                        default "false";
                        description "Authenticate ARP packets received from an unknown UE";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/data-triggered-ue-creation/ospf
                    leaf ospf {
                        type boolean;
                        default "false";
                        description "Authenticate OSPF packets received from an unknown UE";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/data-triggered-ue-creation/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of data-triggered UE creation";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/data-triggered-ue-creation/create-proxy-cache-entry
                    container create-proxy-cache-entry {
                        description "Enter the create-proxy-cache-entry context";

                        ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/data-triggered-ue-creation/create-proxy-cache-entry/mac-format
                        leaf mac-format {
                            type types-submgt:mac-specification;
                            default "aa:";
                            description "MAC address format";
                        }

                        ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/data-triggered-ue-creation/create-proxy-cache-entry/proxy-server
                        container proxy-server {
                            presence "Enable the proxy-server context";
                            description "Enable the proxy-server context";

                            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/data-triggered-ue-creation/create-proxy-cache-entry/proxy-server/router-instance
                            leaf router-instance {
                                type string;
                                mandatory true;
                                description "Router instance of the RADIUS proxy server";
                            }

                            ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/data-triggered-ue-creation/create-proxy-cache-entry/proxy-server/name
                            leaf name {
                                type types-sros:named-item;
                                mandatory true;
                                description "RADIUS Proxy server name";
                            }

                        } // container proxy-server

                    } // container create-proxy-cache-entry

                } // container data-triggered-ue-creation

                ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/l2-service
                container l2-service {
                    presence "Enable the l2-service context";
                    description "Enable the l2-service context";

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/l2-service/admin-state
                    leaf admin-state {
