                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/xconnect/accounting
                    container accounting {
                        description "Enter the accounting context";

                        ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/xconnect/accounting/policy
                        leaf policy {
                            type leafref {
                                path "../../../../../../../../../aaa/radius/isa-policy/name";
                            }
                            sros-ext:auto-restart-to-modify;
                            description "ISA RADIUS accounting policy for cross-connected UEs";
                        }

                        ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/xconnect/accounting/update-interval
                        leaf update-interval {
                            type types-sros:time-duration {
                                range "5..259200";
                            }
                            units "minutes";
                            description "Time between successive interim accounting updates";
                        }

                        ///nokia-conf/configure/service/ies/subscriber-interface/group-interface/wlan-gw/vlan-range/xconnect/accounting/mobility-updates
                        leaf mobility-updates {
                            type boolean;
                            default "false";
                            description "Accounting updates triggered by mobility";
                        }

                    } // container accounting

                } // container xconnect

            } // list vlan-range

        } // container wlan-gw

    } // grouping conf-svc-ies-grp-if-wlangw-wlan-gw

