                            case add {

                                ///nokia-conf/configure/policy-options/policy-statement/default-action/aigp-metric/action/add/add
                                leaf add {
                                    type types-policy:value-param-name-metric;
                                    description "AIGP metric to add";
                                }

                            }
                            ///nokia-conf/configure/policy-options/policy-statement/default-action/aigp-metric/action/set
                            case set {

                                ///nokia-conf/configure/policy-options/policy-statement/default-action/aigp-metric/action/set/set
                                leaf set {
                                    type types-policy:value-param-name-metric-igp;
                                    description "AIGP metric";
                                }

                            }
                        }
                    } // container aigp-metric

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/as-path
                    container as-path {
