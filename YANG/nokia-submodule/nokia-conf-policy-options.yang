submodule nokia-conf-policy-options {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-bgp                { prefix "types-bgp"; }
    import nokia-types-policy             { prefix "types-policy"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-groups;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure policy-options on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-policy-options YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-03-30";

    grouping conf-policy-options {
        ///nokia-conf/configure/policy-options
        container policy-options {
            description "Enter the policy-options context";

            ///nokia-conf/configure/policy-options/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/policy-options/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/policy-options/as-path
            list as-path {
                key "name";
                description "Enter the as-path list instance";

                ///nokia-conf/configure/policy-options/as-path/name
                leaf name {
                    type types-sros:named-item;
                    description "AS path name";
                }

                ///nokia-conf/configure/policy-options/as-path/expression
                leaf expression {
                    type types-sros:policy-as-path-regexp;
                    description "Regular expression string";
                }

                ///nokia-conf/configure/policy-options/as-path/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/policy-options/as-path/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // list as-path

            ///nokia-conf/configure/policy-options/as-path-group
            list as-path-group {
                key "name";
                sros-ext:shared-model-management {
                    sros-ext:openconfig false;
                }
                description "Enter the as-path-group list instance";

                ///nokia-conf/configure/policy-options/as-path-group/name
                leaf name {
                    type types-sros:named-item;
                    description "AS path group name";
                }

                ///nokia-conf/configure/policy-options/as-path-group/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/policy-options/as-path-group/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/policy-options/as-path-group/entry
                list entry {
                    key "entry";
                    description "Enter the entry list instance";

                    ///nokia-conf/configure/policy-options/as-path-group/entry/entry
                    leaf entry {
                        type uint32 {
                            range "1..128";
                        }
                        description "AS path entry ID";
                    }

                    ///nokia-conf/configure/policy-options/as-path-group/entry/expression
                    leaf expression {
                        type types-sros:policy-as-path-regexp;
                        mandatory true;
                        description "Regular expression string";
                    }

                    ///nokia-conf/configure/policy-options/as-path-group/entry/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/policy-options/as-path-group/entry/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // list entry

            } // list as-path-group

            ///nokia-conf/configure/policy-options/damping
            list damping {
                key "name";
                description "Enter the damping list instance";

                ///nokia-conf/configure/policy-options/damping/name
                leaf name {
                    type types-sros:named-item;
                    description "Damping profile name";
                }

                ///nokia-conf/configure/policy-options/damping/half-life
                leaf half-life {
                    type uint32 {
                        range "1..45";
                    }
                    units "minutes";
                    description "Time after the FOM value is halved";
                }

                ///nokia-conf/configure/policy-options/damping/max-suppress
                leaf max-suppress {
                    type uint32 {
                        range "1..720";
                    }
                    units "minutes";
                    description "Maximum time for a route to remain suppressed";
                }

                ///nokia-conf/configure/policy-options/damping/reuse
                leaf reuse {
                    type uint32 {
                        range "1..20000";
                    }
                    description "Value below which a suppressed route can be used again";
                }

                ///nokia-conf/configure/policy-options/damping/suppress
                leaf suppress {
                    type uint32 {
                        range "1..20000";
                    }
                    description "Value above which a route is suppressed";
                }

                ///nokia-conf/configure/policy-options/damping/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/policy-options/damping/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // list damping

            ///nokia-conf/configure/policy-options/global-variables
            container global-variables {
                description "Enter the global-variables context";

                ///nokia-conf/configure/policy-options/global-variables/name
                list name {
                    key "variable-name";
                    max-elements 1000;
                    description "Enter the name list instance";

                    ///nokia-conf/configure/policy-options/global-variables/name/variable-name
                    leaf variable-name {
                        type types-policy:policy-var-name;
                        description "Global variable name used to reference policy functions";
                    }

                    ///nokia-conf/configure/policy-options/global-variables/name/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/policy-options/global-variables/name/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/policy-options/global-variables/name/variable-type
                    choice variable-type {
                        mandatory true;
                        ///nokia-conf/configure/policy-options/global-variables/name/variable-type/value
                        case value {

                            ///nokia-conf/configure/policy-options/global-variables/name/variable-type/value/value
                            leaf value {
                                type types-sros:named-item;
                                description "Policy variable value";
                            }

                        }
                        ///nokia-conf/configure/policy-options/global-variables/name/variable-type/number
                        case number {

                            ///nokia-conf/configure/policy-options/global-variables/name/variable-type/number/number
                            leaf number {
                                type uint64 {
                                    range "0..4294967295";
                                }
                                description "Numerical value of the policy variable";
                            }

                        }
                        ///nokia-conf/configure/policy-options/global-variables/name/variable-type/address
                        case address {

                            ///nokia-conf/configure/policy-options/global-variables/name/variable-type/address/address
                            leaf address {
                                type types-sros:ip-address;
                                description "IP address of the policy variable";
                            }

                        }
                        ///nokia-conf/configure/policy-options/global-variables/name/variable-type/decimal
                        case decimal {

                            ///nokia-conf/configure/policy-options/global-variables/name/variable-type/decimal/decimal
                            leaf decimal {
                                type decimal64 {
                                    range "0.000..4294967295.000";
                                    fraction-digits 3;
                                }
                                description "Attribute decimal to which variable name is resolved";
                            }

                        }
                        ///nokia-conf/configure/policy-options/global-variables/name/variable-type/prefix
                        case prefix {

                            ///nokia-conf/configure/policy-options/global-variables/name/variable-type/prefix/prefix
                            leaf prefix {
                                type types-sros:ip-prefix;
                                description "Prefix value associated with the variable name";
                            }

                        }
                    }
                } // list name

            } // container global-variables

            ///nokia-conf/configure/policy-options/community
            list community {
                key "name";
                description "Enter the community list instance";

                ///nokia-conf/configure/policy-options/community/name
                leaf name {
                    type types-sros:policy-community-name;
                    description "Community name";
                }

                ///nokia-conf/configure/policy-options/community/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/policy-options/community/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/policy-options/community/member
                list member {
                    key "member";
                    description "Add a list entry for member";

                    ///nokia-conf/configure/policy-options/community/member/member
                    leaf member {
                        type types-sros:member-community;
                        description "Community member value";
                    }

                } // list member

                ///nokia-conf/configure/policy-options/community/expression
                container expression {
                    presence "Enable the expression context";
                    description "Enable the expression context";

                    ///nokia-conf/configure/policy-options/community/expression/expr
                    leaf expr {
                        type types-sros:expression-community;
                        mandatory true;
                        description "Community expression value";
                    }

                    ///nokia-conf/configure/policy-options/community/expression/exact
                    leaf exact {
                        type boolean;
                        default "false";
                        description "Match exactly for the specified expression";
                    }

                    ///nokia-conf/configure/policy-options/community/expression/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/policy-options/community/expression/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container expression

            } // list community

            ///nokia-conf/configure/policy-options/prefix-list
            list prefix-list {
                key "name";
                description "Enter the prefix-list list instance";

                ///nokia-conf/configure/policy-options/prefix-list/name
                leaf name {
                    type types-sros:named-item-64;
                    description "Prefix list name";
                }

                ///nokia-conf/configure/policy-options/prefix-list/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/policy-options/prefix-list/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/policy-options/prefix-list/prefix
                list prefix {
                    key "ip-prefix type";
                    description "Enter the prefix list instance";

                    ///nokia-conf/configure/policy-options/prefix-list/prefix/ip-prefix
                    leaf ip-prefix {
                        type types-sros:ip-prefix;
                        description "IP prefix associated with the prefix length";
                    }

                    ///nokia-conf/configure/policy-options/prefix-list/prefix/type
                    leaf type {
                        type enumeration {
                            enum exact                          { value 1; }
                            enum longer                         { value 2; }
                            enum through                        { value 3; }
                            enum range                          { value 4; }
                            enum to                             { value 5; }
                            enum address-mask                   { value 6; }
                        }
                        description "Prefix list match type";
                    }

                    ///nokia-conf/configure/policy-options/prefix-list/prefix/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/policy-options/prefix-list/prefix/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/policy-options/prefix-list/prefix/length
                    choice length {
                        ///nokia-conf/configure/policy-options/prefix-list/prefix/length/range
                        case range {

                            ///nokia-conf/configure/policy-options/prefix-list/prefix/length/range/start-length
                            leaf start-length {
                                type uint32 {
                                    range "0..128";
                                }
                                description "Start in the prefix range length";
                            }

                            ///nokia-conf/configure/policy-options/prefix-list/prefix/length/range/end-length
                            leaf end-length {
                                type uint32 {
                                    range "0..128";
                                }
                                description "Prefix range end length";
                            }

                        }
                        ///nokia-conf/configure/policy-options/prefix-list/prefix/length/through
                        case through {

                            ///nokia-conf/configure/policy-options/prefix-list/prefix/length/through/through-length
                            leaf through-length {
                                type uint32 {
                                    range "0..128";
                                }
                                description "Prefix through length";
                            }

                        }
                        ///nokia-conf/configure/policy-options/prefix-list/prefix/length/to
                        case to {

                            ///nokia-conf/configure/policy-options/prefix-list/prefix/length/to/to-prefix
                            list to-prefix {
                                when "../type = 'to'" {
                                    description "Active only when 'type' is to";
                                }
                                key "ip-prefix";
                                description "Add a list entry for to-prefix";

                                ///nokia-conf/configure/policy-options/prefix-list/prefix/length/to/to-prefix/ip-prefix
                                leaf ip-prefix {
                                    type types-sros:ip-prefix;
                                    description "IP prefix for \"to\" match type";
                                }

                            } // list to-prefix

                        }
                        ///nokia-conf/configure/policy-options/prefix-list/prefix/length/address-mask
                        case address-mask {

                            ///nokia-conf/configure/policy-options/prefix-list/prefix/length/address-mask/mask-pattern
                            list mask-pattern {
                                when "../type = 'address-mask'" {
                                    description "Active only when 'type' is address-mask";
                                }
                                key "address";
                                description "Add a list entry for mask-pattern";

                                ///nokia-conf/configure/policy-options/prefix-list/prefix/length/address-mask/mask-pattern/address
                                leaf address {
                                    type types-sros:ip-address;
                                    description "Address mask for matching routes to the prefix entry";
                                }

                            } // list mask-pattern

                        }
                    }
                } // list prefix

            } // list prefix-list

            ///nokia-conf/configure/policy-options/route-distinguisher-list
            list route-distinguisher-list {
                key "name";
                description "Enter the route-distinguisher-list list instance";

                ///nokia-conf/configure/policy-options/route-distinguisher-list/name
                leaf name {
                    type types-sros:named-item-64 {
                        pattern "[^@]*" {
                            error-message "name cannot contain @";
                        }
                    }
                    description "Route distinguisher list name";
                }

                ///nokia-conf/configure/policy-options/route-distinguisher-list/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/policy-options/route-distinguisher-list/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/policy-options/route-distinguisher-list/rd-entry
                list rd-entry {
                    key "rd";
                    max-elements 128;
                    description "Add a list entry for rd-entry";

                    ///nokia-conf/configure/policy-options/route-distinguisher-list/rd-entry/rd
                    leaf rd {
                        type string {
                            length "3..28";
                            pattern "(((((((([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5]))))/((\\d|[0-2]\\d|3[0-2])))|([1-9]|[1-9][0-9]{1,8}|[1-3][0-9]{9}|4[0-1][0-9]{8}|42[0-8][0-9]{7}|429[0-3][0-9]{6}|4294[0-8][0-9]{5}|42949[0-5][0-9]{4}|429496[0-6][0-9]{3}|4294967[0-1][0-9]{2}|42949672[0-8][0-9]|429496729[0-5])):(([0-9]|[1-9][0-9]{1,3}|[1-5][0-9]{1,4}|6[0-4][0-9]{1,3}|65[0-4][0-9][0-9]|655[0-2][0-9]|6553[0-5])|\\*))|((([1-9]|[1-9][0-9]{1,3}|[1-5][0-9]{1,4}|6[0-4][0-9]{1,3}|65[0-4][0-9][0-9]|655[0-2][0-9]|6553[0-5])):(([0-9]|[1-9][0-9]{1,8}|[1-3][0-9]{9}|4[0-1][0-9]{8}|42[0-8][0-9]{7}|429[0-3][0-9]{6}|4294[0-8][0-9]{5}|42949[0-5][0-9]{4}|429496[0-6][0-9]{3}|4294967[0-1][0-9]{2}|42949672[0-8][0-9]|429496729[0-5])|\\*))" {
                                error-message "RD not valid";
                            }
                        }
                        description "Route distinguisher";
                    }

                } // list rd-entry

            } // list route-distinguisher-list

            ///nokia-conf/configure/policy-options/policy-statement
            list policy-statement {
                key "name";
                max-elements 65535;
                sros-ext:shared-model-management {
                    sros-ext:openconfig false;
                }
                description "Enter the policy-statement list instance";

                ///nokia-conf/configure/policy-options/policy-statement/name
                leaf name {
                    type types-sros:named-item-64;
                    description "Route policy statement name";
                }

                ///nokia-conf/configure/policy-options/policy-statement/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/policy-options/policy-statement/entry-type
                leaf entry-type {
                    type enumeration {
                        enum numbered                       { value 1; }
                        enum named                          { value 2; }
                    }
                    default "numbered";
                    description "The entry type for a route policy entry";
                }

                ///nokia-conf/configure/policy-options/policy-statement/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/policy-options/policy-statement/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/policy-options/policy-statement/entry
                list entry {
                    when "../entry-type = 'numbered'" {
                        description "Active only when 'entry-type' is numbered";
                    }
                    key "entry-id";
                    description "Enter the entry list instance";

                    ///nokia-conf/configure/policy-options/policy-statement/entry/entry-id
                    leaf entry-id {
                        type uint32 {
                            range "1..4294967295";
                        }
                        description "Entry ID for a route policy entry";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/entry/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/entry/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/entry/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/entry/conditional-expression
                    container conditional-expression {
                        presence "Enable the conditional-expression context";
                        description "Enable the conditional-expression context";

                        ///nokia-conf/configure/policy-options/policy-statement/entry/conditional-expression/route-exists
                        leaf route-exists {
                            type string {
                                length "1..255";
                                pattern "\\s*\\[.+\\]\\s*(all|none)?" {
                                    error-message " enter valid prefix between [ ] and optionally add all or none";
                                }
                            }
                            description "Conditional expression to test route existence";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/conditional-expression/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/conditional-expression/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container conditional-expression

                    ///nokia-conf/configure/policy-options/policy-statement/entry/from
                    container from {
                        presence "Enable the from context";
                        description "Enable the from context";

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/level
                        leaf level {
                            type int32 {
                                range "1..2";
                            }
                            description "IS-IS route level as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/prefix-list
                        leaf-list prefix-list {
                            type union {
                                type types-policy:param-midstring-64;
                                type string;
                            }
                            max-elements 28;
                            ordered-by user;
                            description "Prefix list as match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/area
                        leaf area {
                            type types-sros:ipv4-address;
                            description "OSPF area as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/aggregate-contributor
                        leaf aggregate-contributor {
                            type boolean;
                            default "false";
                            description "Enable aggregate route match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/evpn-type
                        leaf evpn-type {
                            type enumeration {
                                enum ethernet-auto-discovery        { value 1; }
                                enum mac-ip-advertisement           { value 2; }
                                enum inclusive-multicast            { value 3; }
                                enum ethernet-segment               { value 4; }
                                enum ip-prefix                      { value 5; }
                                enum selective-multicast            { value 6; }
                                enum multicast-join-sync            { value 7; }
                                enum multicast-leave-sync           { value 8; }
                                enum selective-pmsi-auto-discovery  { value 10; }
                            }
                            description "EVPN type as a match criterion for the entry";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/external
                        leaf external {
                            type boolean;
                            default "false";
                            description "Specify the external IS-IS route as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/family
                        leaf-list family {
                            type types-bgp:ip-family-identifier;
                            max-elements 20;
                            description "Address family as the match condition";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/group-address
                        leaf group-address {
                            type union {
                                type types-policy:param-midstring;
                                type string;
                            }
                            description "Prefix list of multicast group addresses for mathcing";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/host-ip
                        leaf host-ip {
                            type union {
                                type types-policy:param-midstring;
                                type string;
                            }
                            description "Prefix list of IGMP host IP addresses for matching";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/interface
                        leaf-list interface {
                            type union {
                                type types-sros:named-item;
                                type types-sros:interface-name;
                                type types-sros:interface-name;
                                type types-sros:interface-name;
                            }
                            max-elements 1;
                            ordered-by user;
                            description "Interface name as match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/mvpn-type
                        leaf mvpn-type {
                            type enumeration {
                                enum intra-as-ipmsi-auto-discovery  { value 1; }
                                enum inter-as-ipmsi-auto-discovery  { value 2; }
                                enum s-pmsi-auto-discovery          { value 3; }
                                enum intra-as-segment-leaf-auto-discovery { value 4; }
                                enum source-active-auto-discovery   { value 5; }
                                enum shared-tree-join               { value 6; }
                                enum source-tree-join               { value 7; }
                            }
                            description "MVPN type as a match criterion for the entry";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/origin
                        leaf origin {
                            type enumeration {
                                enum igp                            { value 2; }
                                enum egp                            { value 3; }
                                enum incomplete                     { value 4; }
                                enum any                            { value 5; }
                                enum aaa                            { value 6; }
                                enum dynamic                        { value 7; }
                                enum static                         { value 8; }
                                enum bonding                        { value 9; }
                                enum pfcp                           { value 10; }
                            }
                            description "Route origin match criteria";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/origin-validation-state
                        leaf origin-validation-state {
                            type enumeration {
                                enum valid                          { value 1; }
                                enum not-found                      { value 2; }
                                enum invalid                        { value 3; }
                            }
                            description "Origin validation state used for match criteria";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/ospf-type
                        leaf ospf-type {
                            type int32 {
                                range "1..2";
                            }
                            description "OSPF type metric applied to unmatching route entries";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/path-type
                        leaf path-type {
                            type enumeration {
                                enum ibgp                           { value 2; }
                                enum ebgp                           { value 3; }
                            }
                            description "Path type as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy
                        leaf policy {
                            type union {
                                type string {
                                    length "1..255";
                                    pattern ".*[\\[].*" {
                                        error-message "expression not valid.";
                                    }
                                }
                                type string;
                            }
                            description "Policy statement as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/route-distinguisher-list
                        leaf route-distinguisher-list {
                            type union {
                                type types-policy:param-midstring-64;
                                type string;
                            }
                            description "Route distinguisher list as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/state
                        leaf state {
                            type enumeration {
                                enum srrp-master                    { value 2; }
                                enum srrp-non-master                { value 3; }
                                enum ipsec-master-with-peer         { value 6; }
                                enum ipsec-master-without-peer      { value 7; }
                                enum ipsec-non-master               { value 8; }
                                enum fsg-active                     { value 10; }
                                enum fsg-standby                    { value 11; }
                            }
                            description "State used as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/tag
                        leaf tag {
                            type union {
                                type int64 {
                                    range "1..4294967295";
                                }
                                type enumeration {
                                    enum no-tag                         { value 0; }
                                }
                            }
                            description "Route tag used as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/color
                        leaf color {
                            type int64 {
                                range "0..4294967295";
                            }
                            description "Color ID as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/distinguisher
                        leaf distinguisher {
                            type int64 {
                                range "0..4294967295";
                            }
                            description "SR policy distinguisher as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/endpoint
                        leaf endpoint {
                            type types-sros:ip-address;
                            description "SR policy endpoint address as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/srv6-sid-prefix
                        leaf srv6-sid-prefix {
                            type union {
                                type types-policy:policy-var-name;
                                type types-sros:ipv6-unicast-prefix;
                            }
                            description "SRv6 SID or uSID value as match criterion for BGP route";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/srv6-tlv
                        leaf srv6-tlv {
                            type enumeration {
                                enum present                        { value 1; }
                                enum not-present                    { value 2; }
                            }
                            description "SRv6 TLV presence as match criterion for BGP route";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/neighbor
                        container neighbor {
                            description "Enter the neighbor context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/neighbor/neighbor
                            choice neighbor {
                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/neighbor/neighbor/ip-address
                                case ip-address {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/neighbor/neighbor/ip-address/ip-address
                                    leaf ip-address {
                                        type union {
                                            type types-sros:ip-address;
                                            type types-sros:ip-address-with-zone;
                                        }
                                        description "IP address to match the neighbor";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/neighbor/neighbor/prefix-list
                                case prefix-list {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/neighbor/neighbor/prefix-list/prefix-list
                                    leaf prefix-list {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "Name to match the neighbor prefix list";
                                    }

                                }
                            }
                        } // container neighbor

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/interface-subnets
                        container interface-subnets {
                            description "Enter the interface-subnets context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/interface-subnets/service
                            leaf service {
                                type string;
                                default "Base";
                                description "Service ID of the interface subnets";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/interface-subnets/ip-int-name
                            leaf-list ip-int-name {
                                type types-sros:interface-name;
                                max-elements 10;
                                ordered-by user;
                                description "Interface name as the match criterion";
                            }

                        } // container interface-subnets

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/as-path
                        container as-path {
                            description "Enter the as-path context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/as-path/as-path
                            choice as-path {
                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/as-path/as-path/name
                                case name {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/as-path/as-path/name/name
                                    leaf name {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "AS path name to match";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/as-path/as-path/group
                                case group {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/as-path/as-path/group/group
                                    leaf group {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "AS path group as a match criterion";
                                    }

                                }
                            }
                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/as-path/length
                            container length {
                                description "Enter the length context";

                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/as-path/length/value
                                leaf value {
                                    type types-policy:value-255-param-name;
                                    description "AS numbers in the AS path that match on the BGP route";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/as-path/length/qualifier
                                leaf qualifier {
                                    type types-policy:policy-qualifier;
                                    default "equal";
                                    description "Higher or lower values to be accepted as match criteria";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/as-path/length/unique
                                leaf unique {
                                    type boolean;
                                    default "false";
                                    description "Use unique AS numbers as matching criteria";
                                }

                            } // container length

                        } // container as-path

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/cluster-id
                        container cluster-id {
                            description "Enter the cluster-id context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/cluster-id/cluster-id
                            choice cluster-id {
                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/cluster-id/cluster-id/ip-address
                                case ip-address {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/cluster-id/cluster-id/ip-address/ip-address
                                    leaf-list ip-address {
                                        type types-sros:ipv4-prefix-with-host-bits;
                                        max-elements 5;
                                        ordered-by user;
                                        description "Cluster list attribute IP addresses for route matching";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/cluster-id/cluster-id/none-cluster-list
                                case none-cluster-list {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/cluster-id/cluster-id/none-cluster-list/none-cluster-list
                                    leaf none-cluster-list {
                                        type boolean;
                                        default "false";
                                        description "Specify matching BGP routes without a cluster ID";
                                    }

                                }
                            }
                        } // container cluster-id

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/community
                        container community {
                            description "Enter the community context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/community/community
                            choice community {
                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/community/community/name
                                case name {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/community/community/name/name
                                    leaf name {
                                        type union {
                                            type types-policy:param-midstring-64 {
                                                pattern "[^\\]\\[]+" {
                                                    error-message "String must not contain square brackets ([]).";
                                                }
                                            }
                                            type string;
                                        }
                                        description "Community list name";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/community/community/expression
                                case expression {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/community/community/expression/expression
                                    leaf expression {
                                        type string {
                                            length "1..900";
                                        }
                                        description "Community expression name as a match criterion";
                                    }

                                }
                            }
                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/community/count
                            container count {
                                description "Enter the count context";

                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/community/count/value
                                leaf value {
                                    type types-policy:value-param-1024-name;
                                    description "Number of BGP communities to match the BGP route";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/community/count/qualifier
                                leaf qualifier {
                                    type types-policy:policy-qualifier;
                                    default "equal";
                                    description "Higher or lower values to be accepted as match criteria";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/community/count/type
                                leaf type {
                                    type enumeration {
                                        enum standard                       { value 2; }
                                        enum extended                       { value 3; }
                                        enum large                          { value 4; }
                                    }
                                    description "Community count to match the community count criteria";
                                }

                            } // container count

                        } // container community

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/flowspec
                        container flowspec {
                            description "Enter the flowspec context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/flowspec/dest
                            leaf dest {
                                type union {
                                    type types-policy:param-midstring;
                                    type string;
                                }
                                description "BGP FlowSpec route matches per destination IP prefix";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/flowspec/source
                            leaf source {
                                type union {
                                    type types-policy:param-midstring;
                                    type string;
                                }
                                description "BGP FlowSpec route matches per the source IP prefix";
                            }

                        } // container flowspec

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/local-preference
                        container local-preference {
                            description "Enter the local-preference context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/local-preference/value
                            leaf value {
                                type types-policy:value-param-name-metric;
                                description "BGP routes per local preference value or variable name";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/local-preference/qualifier
                            leaf qualifier {
                                type types-policy:policy-qualifier;
                                default "equal";
                                description "Higher or lower values to be accepted as match criteria";
                            }

                        } // container local-preference

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/metric
                        container metric {
                            description "Enter the metric context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/metric/value
                            leaf value {
                                type types-policy:value-param-name-metric;
                                description "Local preference value or variable name";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/metric/qualifier
                            leaf qualifier {
                                type types-policy:policy-qualifier;
                                default "equal";
                                description "Higher or lower values to be accepted as match criteria";
                            }

                        } // container metric

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/next-hop
                        container next-hop {
                            description "Enter the next-hop context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/next-hop/next-hop
                            choice next-hop {
                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/next-hop/next-hop/ip-address
                                case ip-address {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/next-hop/next-hop/ip-address/ip-address
                                    leaf ip-address {
                                        type types-sros:ip-unicast-address;
                                        description "IP address of the next hop to match";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/next-hop/next-hop/prefix-list
                                case prefix-list {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/next-hop/next-hop/prefix-list/prefix-list
                                    leaf prefix-list {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "Name of a next hop prefix list to match";
                                    }

                                }
                            }
                        } // container next-hop

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/protocol
                        container protocol {
                            description "Enter the protocol context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/protocol/name
                            leaf-list name {
                                type enumeration {
                                    enum direct                         { value 2; }
                                    enum static                         { value 3; }
                                    enum bgp                            { value 4; }
                                    enum isis                           { value 5; }
                                    enum ospf                           { value 6; }
                                    enum rip                            { value 7; }
                                    enum aggregate                      { value 8; }
                                    enum bgp-vpn                        { value 9; }
                                    enum igmp                           { value 10; }
                                    enum pim                            { value 11; }
                                    enum ospf3                          { value 12; }
                                    enum ldp                            { value 13; }
                                    enum sub-mgmt                       { value 14; }
                                    enum mld                            { value 15; }
                                    enum managed                        { value 16; }
                                    enum vpn-leak                       { value 18; }
                                    enum nat                            { value 21; }
                                    enum periodic                       { value 22; }
                                    enum ipsec                          { value 23; }
                                    enum dhcpv6-pd                      { value 25; }
                                    enum dhcpv6-na                      { value 26; }
                                    enum dhcpv6-ta                      { value 27; }
                                    enum dhcpv6-pd-excl                 { value 28; }
                                    enum ripng                          { value 29; }
                                    enum bgp-label                      { value 31; }
                                    enum direct-interface               { value 32; }
                                    enum arp-nd                         { value 33; }
                                    enum rib-api                        { value 34; }
                                    enum dhcp-client                    { value 36; }
                                    enum evpn-ifl                       { value 37; }
                                    enum srv6                           { value 39; }
                                    enum video                          { value 40; }
                                }
                                max-elements 5;
                                description "List of protocol names as the match criterion";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/protocol/instance
                            leaf instance {
                                type union {
                                    type enumeration {
                                        enum all                            { value -1; }
                                    }
                                    type uint32 {
                                        range "0..127";
                                    }
                                }
                                default "0";
                                description "Instance for protocol IS-IS, OSPF, or OSPFv3 to match";
                            }

                        } // container protocol

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/source-address
                        container source-address {
                            description "Enter the source-address context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/source-address/source-address
                            choice source-address {
                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/source-address/source-address/ip-address
                                case ip-address {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/source-address/source-address/ip-address/ip-address
                                    leaf ip-address {
                                        type types-sros:ip-address;
                                        description "Source IP address to match";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/source-address/source-address/prefix-list
                                case prefix-list {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/source-address/source-address/prefix-list/prefix-list
                                    leaf prefix-list {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "Name of a source address prefix list to match";
                                    }

                                }
                            }
                        } // container source-address

                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables
                        container policy-variables {
                            description "Enter the policy-variables context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name
                            list name {
                                key "variable-name";
                                max-elements 10;
                                description "Enter the name list instance";

                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name/variable-name
                                leaf variable-name {
                                    type types-policy:policy-var-name;
                                    description "Global variable name used to reference policy functions";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name/variable-type
                                choice variable-type {
                                    mandatory true;
                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name/variable-type/value
                                    case value {

                                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name/variable-type/value/value
                                        leaf value {
                                            type types-sros:named-item;
                                            description "Policy variable value";
                                        }

                                    }
                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name/variable-type/number
                                    case number {

                                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name/variable-type/number/number
                                        leaf number {
                                            type uint64 {
                                                range "0..4294967295";
                                            }
                                            description "Numerical value of the policy variable";
                                        }

                                    }
                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name/variable-type/address
                                    case address {

                                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name/variable-type/address/address
                                        leaf address {
                                            type types-sros:ip-address;
                                            description "IP address of the policy variable";
                                        }

                                    }
                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name/variable-type/decimal
                                    case decimal {

                                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name/variable-type/decimal/decimal
                                        leaf decimal {
                                            type decimal64 {
                                                range "0.000..4294967295.000";
                                                fraction-digits 3;
                                            }
                                            description "Attribute decimal to which variable name is resolved";
                                        }

                                    }
                                    ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name/variable-type/prefix
                                    case prefix {

                                        ///nokia-conf/configure/policy-options/policy-statement/entry/from/policy-variables/name/variable-type/prefix/prefix
                                        leaf prefix {
                                            type types-sros:ip-prefix;
                                            description "Prefix value associated with the variable name";
                                        }

                                    }
                                }
                            } // list name

                        } // container policy-variables

                    } // container from

                    ///nokia-conf/configure/policy-options/policy-statement/entry/to
                    container to {
                        presence "Enable the to context";
                        description "Enable the to context";

                        ///nokia-conf/configure/policy-options/policy-statement/entry/to/level
                        leaf level {
                            type int32 {
                                range "1..2";
                            }
                            description "IS-IS route level as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/to/prefix-list
                        leaf-list prefix-list {
                            type union {
                                type types-policy:param-midstring-64;
                                type string;
                            }
                            max-elements 28;
                            ordered-by user;
                            description "Prefix list as match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/to/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/to/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/to/neighbor
                        container neighbor {
                            description "Enter the neighbor context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/to/neighbor/neighbor
                            choice neighbor {
                                ///nokia-conf/configure/policy-options/policy-statement/entry/to/neighbor/neighbor/ip-address
                                case ip-address {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/to/neighbor/neighbor/ip-address/ip-address
                                    leaf ip-address {
                                        type union {
                                            type types-sros:ip-address;
                                            type types-sros:ip-address-with-zone;
                                        }
                                        description "IP address to match the neighbor";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/entry/to/neighbor/neighbor/prefix-list
                                case prefix-list {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/to/neighbor/neighbor/prefix-list/prefix-list
                                    leaf prefix-list {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "Name to match the neighbor prefix list";
                                    }

                                }
                            }
                        } // container neighbor

                        ///nokia-conf/configure/policy-options/policy-statement/entry/to/protocol
                        container protocol {
                            description "Enter the protocol context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/to/protocol/name
                            leaf-list name {
                                type enumeration {
                                    enum bgp                            { value 4; }
                                    enum isis                           { value 5; }
                                    enum ospf                           { value 6; }
                                    enum rip                            { value 7; }
                                    enum bgp-vpn                        { value 9; }
                                    enum ospf3                          { value 12; }
                                    enum ldp                            { value 13; }
                                    enum vpn-leak                       { value 18; }
                                    enum ripng                          { value 29; }
                                    enum bgp-label                      { value 31; }
                                    enum evpn-ifl                       { value 37; }
                                }
                                max-elements 2;
                                description "Protocol name to match";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/entry/to/protocol/instance
                            leaf instance {
                                type union {
                                    type enumeration {
                                        enum all                            { value -1; }
                                    }
                                    type uint32 {
                                        range "0..127";
                                    }
                                }
                                default "0";
                                description "Instance for the IS-IS, OSPF, or OSPF3 protocol";
                            }

                        } // container protocol

                    } // container to

                    ///nokia-conf/configure/policy-options/policy-statement/entry/action
                    container action {
                        presence "Enable the action context";
                        description "Enable the action context";

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/action-type
                        leaf action-type {
                            type enumeration {
                                enum accept                         { value 1; }
                                enum reject                         { value 2; }
                                enum next-entry                     { value 3; }
                                enum next-policy                    { value 4; }
                            }
                            mandatory true;
                            description "Action type for routes matching the route policy entry";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/add-paths-send-limit
                        leaf add-paths-send-limit {
                            type union {
                                type int32 {
                                    range "1..16";
                                }
                                type enumeration {
                                    enum multipaths                     { value -1; }
                                }
                            }
                            description "BGP Add-Paths send limit applied for routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/admin-tag-policy
                        leaf admin-tag-policy {
                            type union {
                                type types-policy:param-midstring-64;
                                type string;
                            }
                            description "Administrative tag policy name";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/advertise-label
                        leaf advertise-label {
                            type enumeration {
                                enum per-prefix                     { value 1; }
                                enum pop                            { value 2; }
                                enum pop-and-forward                { value 3; }
                            }
                            description "Label allocation for matched BGP routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/bgp-high-priority
                        leaf bgp-high-priority {
                            type boolean;
                            default "false";
                            description "Tag routes as high priority for fast table updates";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/bgp-leak
                        leaf bgp-leak {
                            type boolean;
                            default "false";
                            description "Allow leaking for BGP routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/create-mpls-tunnel
                        leaf create-mpls-tunnel {
                            type boolean;
                            default "false";
                            description "Create an MPLS tunnel";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/create-udp-tunnel
                        leaf create-udp-tunnel {
                            type boolean;
                            default "false";
                            description "Create an MPLS-over-UDP tunnel";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/damping
                        leaf damping {
                            type union {
                                type enumeration {
                                    enum none                           { value 0; }
                                }
                                type types-policy:param-midstring;
                                type string;
                            }
                            description "Damping profile used for routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/dest-class
                        leaf dest-class {
                            type types-sros:class-index;
                            description "Default destination class for the policy statement";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/egress-statistics
                        leaf egress-statistics {
                            type boolean;
                            default "false";
                            description "Enable egress statistics for BGP-LU routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/ingress-statistics
                        leaf ingress-statistics {
                            type boolean;
                            default "false";
                            description "Enable ingress statistics for BGP-LU routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/flex-algo
                        leaf flex-algo {
                            type types-policy:value-param-name-flex-algo;
                            description "Flexible algorithm for BGP next-hop autobind operation";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/install-backup-path
                        leaf install-backup-path {
                            type boolean;
                            default "false";
                            description "Install a preprogrammed backup path for the prefix";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/local-preference
                        leaf local-preference {
                            type types-policy:value-param-name-metric;
                            description "BGP local preference for routes not matching any entry";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/nat-policy
                        leaf nat-policy {
                            type union {
                                type types-policy:param-midstring;
                                type string;
                            }
                            description "BGP-VPN imported route and a NAT policy association";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/next-hop
                        leaf next-hop {
                            type union {
                                type enumeration {
                                    enum peer-address                   { value -2; }
                                    enum self                           { value -1; }
                                }
                                type types-sros:ip-address;
                                type string {
                                    length "1..32";
                                    pattern "@.+@" {
                                        error-message "String must start and end with an at-sign (@)";
                                    }
                                }
                            }
                            description "Next-hop IP address applied to routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/origin
                        leaf origin {
                            type union {
                                type enumeration {
                                    enum igp                            { value 2; }
                                    enum egp                            { value 3; }
                                    enum incomplete                     { value 4; }
                                }
                                type string {
                                    length "1..32";
                                    pattern "@.+@" {
                                        error-message "String must start and end with an at-sign (@)";
                                    }
                                }
                            }
                            description "BGP origin for routes that are exported into BGP";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/origin-validation-state
                        leaf origin-validation-state {
                            type union {
                                type enumeration {
                                    enum valid                          { value 1; }
                                    enum not-found                      { value 2; }
                                    enum invalid                        { value 3; }
                                }
                                type string {
                                    length "1..32";
                                    pattern "@.+@" {
                                        error-message "String must start and end with an at-sign (@)";
                                    }
                                }
                            }
                            description "Origin validation state for routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/prefer-transport-frr
                        leaf prefer-transport-frr {
                            type boolean;
                            default "false";
                            description "Enable transport FRR preference";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/preference
                        leaf preference {
                            type union {
                                type uint32 {
                                    range "1..255";
                                }
                                type string {
                                    length "1..32";
                                    pattern "@.+@" {
                                        error-message "String must start and end with an at-sign (@)";
                                    }
                                }
                            }
                            description "Route preference applied to routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/resolve-static
                        leaf resolve-static {
                            type boolean;
                            default "false";
                            description "Resolve next hop of a static route for the BGP next hop";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/source-class
                        leaf source-class {
                            type types-sros:class-index;
                            description "Default source class for the policy statement";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/sticky-ecmp
                        leaf sticky-ecmp {
                            type boolean;
                            default "false";
                            description "Specify the sticky ECMP flag for BGP ECMP routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/tag
                        leaf tag {
                            type union {
                                type uint32 {
                                    range "1..4294967295";
                                }
                                type string {
                                    length "1..32";
                                    pattern "@.+@" {
                                        error-message "String must start and end with an at-sign (@)";
                                    }
                                }
                            }
                            description "OSPF RIP or IS-IS tag applied to routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/type
                        leaf type {
                            type union {
                                type int32 {
                                    range "1..2";
                                }
                                type string {
                                    length "1..32";
                                    pattern "@.+@" {
                                        error-message "String must start and end with an at-sign (@)";
                                    }
                                }
                            }
                            description "OSPF metric type applied to routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/route-table-install
                        leaf route-table-install {
                            type boolean;
                            default "true";
                            description "Allow BGP route installation in the route table";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/sr-maintenance-policy
                        leaf sr-maintenance-policy {
                            type union {
                                type types-policy:param-midstring;
                                type string;
                            }
                            description "SR maintenance policy as an action";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/srv6-locator
                        leaf srv6-locator {
                            type union {
                                type types-policy:param-midstring-64;
                                type string;
                            }
                            description "Locator for SRv6 TLV to use";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/srv6-micro-segment-locator
                        leaf srv6-micro-segment-locator {
                            type union {
                                type types-policy:param-midstring-64;
                                type string;
                            }
                            description "Micro-segment locator for SRv6 TLV to use";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/srv6-return-path-bfd-sid
                        leaf srv6-return-path-bfd-sid {
                            when "../sr-maintenance-policy" {
                                description "sr-maintenance-policy must be set to access this leaf";
                            }
                            type union {
                                type types-policy:policy-var-name;
                                type types-sros:ipv6-address;
                            }
                            description "SRv6 return path BFD SID applied to SRv6 policy routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/aigp-metric
                        container aigp-metric {
                            description "Enter the aigp-metric context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/aigp-metric/action
                            choice action {
                                ///nokia-conf/configure/policy-options/policy-statement/entry/action/aigp-metric/action/add
                                case add {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/action/aigp-metric/action/add/add
                                    leaf add {
                                        type types-policy:value-param-name-metric;
                                        description "AIGP metric to add";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/entry/action/aigp-metric/action/set
                                case set {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/action/aigp-metric/action/set/set
                                    leaf set {
                                        type types-policy:value-param-name-metric-igp;
                                        description "AIGP metric";
                                    }

                                }
                            }
                        } // container aigp-metric

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/as-path
                        container as-path {
                            description "Enter the as-path context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/as-path/action
                            choice action {
                                ///nokia-conf/configure/policy-options/policy-statement/entry/action/as-path/action/add
                                case add {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/action/as-path/action/add/add
                                    leaf add {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "AS path to add";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/entry/action/as-path/action/replace
                                case replace {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/action/as-path/action/replace/replace
                                    leaf replace {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "AS path to replace";
                                    }

                                }
                            }
                        } // container as-path

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/as-path-prepend
                        container as-path-prepend {
                            description "Enter the as-path-prepend context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/as-path-prepend/as-path
                            leaf as-path {
                                type types-policy:value-param-name-range;
                                description "AS number to prepend to the AS path attribute";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/as-path-prepend/repeat
                            leaf repeat {
                                type types-policy:repeat-value-param-name;
                                default "1";
                                description "Number of times to prepend the specified AS number";
                            }

                        } // container as-path-prepend

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/bgp-tunnel-metric
                        container bgp-tunnel-metric {
                            description "Enter the bgp-tunnel-metric context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/bgp-tunnel-metric/value
                            leaf value {
                                type types-policy:value-param-name;
                                description "BGP tunnel table metric value";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/bgp-tunnel-metric/prefer-aigp
                            leaf prefer-aigp {
                                type boolean;
                                default "false";
                                description "Use AIGP attribute as tunnel metric when present";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/bgp-tunnel-metric/prefer-med
                            leaf prefer-med {
                                type boolean;
                                default "false";
                                description "Use MED attribute as tunnel metric when present";
                            }

                        } // container bgp-tunnel-metric

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/metric
                        container metric {
                            description "Enter the metric context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/metric/action
                            choice action {
                                ///nokia-conf/configure/policy-options/policy-statement/entry/action/metric/action/add
                                case add {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/action/metric/action/add/add
                                    leaf add {
                                        type types-policy:value-param-name-metric;
                                        description "Metric to add";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/entry/action/metric/action/subtract
                                case subtract {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/action/metric/action/subtract/subtract
                                    leaf subtract {
                                        type types-policy:value-param-name-metric;
                                        description "Metric to subtract";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/entry/action/metric/action/set
                                case set {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/action/metric/action/set/set
                                    leaf set {
                                        type union {
                                            type enumeration {
                                                enum igp                            { status obsolete; }
                                            }
                                            type int64 {
                                                range "0..4294967295";
                                            }
                                            type string {
                                                length "1..32";
                                                pattern "@.+@" {
                                                    error-message "String must start and end with an at-sign (@)";
                                                }
                                            }
                                        }
                                        description "Metric to assign";
                                    }

                                }
                            }
                        } // container metric

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/multicast-redirection
                        container multicast-redirection {
                            description "Enter the multicast-redirection context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/multicast-redirection/fwd-service
                            leaf fwd-service {
                                type string;
                                default "Base";
                                description "Service ID for multicast redirection";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/multicast-redirection/ip-int-name
                            leaf ip-int-name {
                                type types-sros:interface-name;
                                description "Alternate interface where IGMP messages are redirected";
                            }

                        } // container multicast-redirection

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/sr-label-index
                        container sr-label-index {
                            description "Enter the sr-label-index context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/sr-label-index/value
                            leaf value {
                                type types-policy:value-524287-param-name;
                                description "BGP SR label index associated with routes";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/sr-label-index/prefer-igp
                            leaf prefer-igp {
                                type boolean;
                                default "false";
                                description "Use the SR label index from the IGP route when present";
                            }

                        } // container sr-label-index

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/bgp-med
                        container bgp-med {
                            description "Enter the bgp-med context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/bgp-med/action
                            choice action {
                                ///nokia-conf/configure/policy-options/policy-statement/entry/action/bgp-med/action/set
                                case set {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/action/bgp-med/action/set/set
                                    leaf set {
                                        type types-policy:value-param-name-bgp-med-igp;
                                        description "BGP-MED assignment";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/entry/action/bgp-med/action/adjust
                                case adjust {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/action/bgp-med/action/adjust/adjust
                                    leaf adjust {
                                        type string {
                                            length "1..64";
                                            pattern "(.*@.+@.*)|(.*_med_.*)|(.*_igp_.*)" {
                                                error-message "Expression must contain variable _igp_ or _med_ or @parameter@";
                                            }
                                            pattern ".*[+\\-*].*" {
                                                error-message "Expression must contain one valid operator. Supported operators are +, - and *";
                                            }
                                        }
                                        description "BGP-MED adjustment";
                                    }

                                }
                            }
                        } // container bgp-med

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/forwarding-class
                        container forwarding-class {
                            description "Enter the forwarding-class context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/forwarding-class/fc
                            leaf fc {
                                type enumeration {
                                    enum be                             { value 0; }
                                    enum l2                             { value 1; }
                                    enum af                             { value 2; }
                                    enum l1                             { value 3; }
                                    enum h2                             { value 4; }
                                    enum ef                             { value 5; }
                                    enum h1                             { value 6; }
                                    enum nc                             { value 7; }
                                }
                                description "Forwarding class associated with the route";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/forwarding-class/priority
                            leaf priority {
                                type enumeration {
                                    enum low                            { value 1; }
                                    enum high                           { value 2; }
                                }
                                description "Route priority";
                            }

                        } // container forwarding-class

                        ///nokia-conf/configure/policy-options/policy-statement/entry/action/community
                        container community {
                            description "Enable the community context";

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/community/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/community/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/entry/action/community/action
                            choice action {
                                ///nokia-conf/configure/policy-options/policy-statement/entry/action/community/action/action1
                                case action1 {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/action/community/action/action1/add
                                    leaf-list add {
                                        type union {
                                            type types-policy:param-midstring-64;
                                            type string;
                                        }
                                        max-elements 28;
                                        ordered-by user;
                                        description "List of community names to add";
                                    }

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/action/community/action/action1/remove
                                    leaf-list remove {
                                        type union {
                                            type types-policy:param-midstring-64;
                                            type string;
                                        }
                                        max-elements 28;
                                        ordered-by user;
                                        description "List of community names to remove";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/entry/action/community/action/action2
                                case action2 {

                                    ///nokia-conf/configure/policy-options/policy-statement/entry/action/community/action/action2/replace
                                    leaf-list replace {
                                        type union {
                                            type types-policy:param-midstring-64;
                                            type string;
                                        }
                                        max-elements 28;
                                        ordered-by user;
                                        description "List of community names to replace";
                                    }

                                }
                            }
                        } // container community

                    } // container action

                } // list entry

                ///nokia-conf/configure/policy-options/policy-statement/named-entry
                list named-entry {
                    when "../entry-type = 'named'" {
                        description "Active only when 'entry-type' is named";
                    }
                    key "entry-name";
                    max-elements 16384;
                    ordered-by user;
                    description "Enter the named-entry list instance";

                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/entry-name
                    leaf entry-name {
                        type types-sros:named-item-255;
                        description "Route policy entry name";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/conditional-expression
                    container conditional-expression {
                        presence "Enable the conditional-expression context";
                        description "Enable the conditional-expression context";

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/conditional-expression/route-exists
                        leaf route-exists {
                            type string {
                                length "1..255";
                                pattern "\\s*\\[.+\\]\\s*(all|none)?" {
                                    error-message " enter valid prefix between [ ] and optionally add all or none";
                                }
                            }
                            description "Conditional expression to test route existence";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/conditional-expression/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/conditional-expression/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container conditional-expression

                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from
                    container from {
                        presence "Enable the from context";
                        description "Enable the from context";

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/level
                        leaf level {
                            type int32 {
                                range "1..2";
                            }
                            description "IS-IS route level as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/prefix-list
                        leaf-list prefix-list {
                            type union {
                                type types-policy:param-midstring-64;
                                type string;
                            }
                            max-elements 28;
                            ordered-by user;
                            description "Prefix list as match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/area
                        leaf area {
                            type types-sros:ipv4-address;
                            description "OSPF area as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/aggregate-contributor
                        leaf aggregate-contributor {
                            type boolean;
                            default "false";
                            description "Enable aggregate route match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/evpn-type
                        leaf evpn-type {
                            type enumeration {
                                enum ethernet-auto-discovery        { value 1; }
                                enum mac-ip-advertisement           { value 2; }
                                enum inclusive-multicast            { value 3; }
                                enum ethernet-segment               { value 4; }
                                enum ip-prefix                      { value 5; }
                                enum selective-multicast            { value 6; }
                                enum multicast-join-sync            { value 7; }
                                enum multicast-leave-sync           { value 8; }
                                enum selective-pmsi-auto-discovery  { value 10; }
                            }
                            description "EVPN type as a match criterion for the entry";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/external
                        leaf external {
                            type boolean;
                            default "false";
                            description "Specify the external IS-IS route as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/family
                        leaf-list family {
                            type types-bgp:ip-family-identifier;
                            max-elements 20;
                            description "Address family as the match condition";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/group-address
                        leaf group-address {
                            type union {
                                type types-policy:param-midstring;
                                type string;
                            }
                            description "Prefix list of multicast group addresses for mathcing";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/host-ip
                        leaf host-ip {
                            type union {
                                type types-policy:param-midstring;
                                type string;
                            }
                            description "Prefix list of IGMP host IP addresses for matching";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/interface
                        leaf-list interface {
                            type union {
                                type types-sros:named-item;
                                type types-sros:interface-name;
                                type types-sros:interface-name;
                                type types-sros:interface-name;
                            }
                            max-elements 1;
                            ordered-by user;
                            description "Interface name as match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/mvpn-type
                        leaf mvpn-type {
                            type enumeration {
                                enum intra-as-ipmsi-auto-discovery  { value 1; }
                                enum inter-as-ipmsi-auto-discovery  { value 2; }
                                enum s-pmsi-auto-discovery          { value 3; }
                                enum intra-as-segment-leaf-auto-discovery { value 4; }
                                enum source-active-auto-discovery   { value 5; }
                                enum shared-tree-join               { value 6; }
                                enum source-tree-join               { value 7; }
                            }
                            description "MVPN type as a match criterion for the entry";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/origin
                        leaf origin {
                            type enumeration {
                                enum igp                            { value 2; }
                                enum egp                            { value 3; }
                                enum incomplete                     { value 4; }
                                enum any                            { value 5; }
                                enum aaa                            { value 6; }
                                enum dynamic                        { value 7; }
                                enum static                         { value 8; }
                                enum bonding                        { value 9; }
                                enum pfcp                           { value 10; }
                            }
                            description "Route origin match criteria";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/origin-validation-state
                        leaf origin-validation-state {
                            type enumeration {
                                enum valid                          { value 1; }
                                enum not-found                      { value 2; }
                                enum invalid                        { value 3; }
                            }
                            description "Origin validation state used for match criteria";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/ospf-type
                        leaf ospf-type {
                            type int32 {
                                range "1..2";
                            }
                            description "OSPF type metric applied to unmatching route entries";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/path-type
                        leaf path-type {
                            type enumeration {
                                enum ibgp                           { value 2; }
                                enum ebgp                           { value 3; }
                            }
                            description "Path type as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy
                        leaf policy {
                            type union {
                                type string {
                                    length "1..255";
                                    pattern ".*[\\[].*" {
                                        error-message "expression not valid.";
                                    }
                                }
                                type string;
                            }
                            description "Policy statement as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/route-distinguisher-list
                        leaf route-distinguisher-list {
                            type union {
                                type types-policy:param-midstring-64;
                                type string;
                            }
                            description "Route distinguisher list as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/state
                        leaf state {
                            type enumeration {
                                enum srrp-master                    { value 2; }
                                enum srrp-non-master                { value 3; }
                                enum ipsec-master-with-peer         { value 6; }
                                enum ipsec-master-without-peer      { value 7; }
                                enum ipsec-non-master               { value 8; }
                                enum fsg-active                     { value 10; }
                                enum fsg-standby                    { value 11; }
                            }
                            description "State used as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/tag
                        leaf tag {
                            type union {
                                type int64 {
                                    range "1..4294967295";
                                }
                                type enumeration {
                                    enum no-tag                         { value 0; }
                                }
                            }
                            description "Route tag used as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/color
                        leaf color {
                            type int64 {
                                range "0..4294967295";
                            }
                            description "Color ID as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/distinguisher
                        leaf distinguisher {
                            type int64 {
                                range "0..4294967295";
                            }
                            description "SR policy distinguisher as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/endpoint
                        leaf endpoint {
                            type types-sros:ip-address;
                            description "SR policy endpoint address as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/srv6-sid-prefix
                        leaf srv6-sid-prefix {
                            type union {
                                type types-policy:policy-var-name;
                                type types-sros:ipv6-unicast-prefix;
                            }
                            description "SRv6 SID or uSID value as match criterion for BGP route";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/srv6-tlv
                        leaf srv6-tlv {
                            type enumeration {
                                enum present                        { value 1; }
                                enum not-present                    { value 2; }
                            }
                            description "SRv6 TLV presence as match criterion for BGP route";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/neighbor
                        container neighbor {
                            description "Enter the neighbor context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/neighbor/neighbor
                            choice neighbor {
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/neighbor/neighbor/ip-address
                                case ip-address {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/neighbor/neighbor/ip-address/ip-address
                                    leaf ip-address {
                                        type union {
                                            type types-sros:ip-address;
                                            type types-sros:ip-address-with-zone;
                                        }
                                        description "IP address to match the neighbor";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/neighbor/neighbor/prefix-list
                                case prefix-list {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/neighbor/neighbor/prefix-list/prefix-list
                                    leaf prefix-list {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "Name to match the neighbor prefix list";
                                    }

                                }
                            }
                        } // container neighbor

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/interface-subnets
                        container interface-subnets {
                            description "Enter the interface-subnets context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/interface-subnets/service
                            leaf service {
                                type string;
                                default "Base";
                                description "Service ID of the interface subnets";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/interface-subnets/ip-int-name
                            leaf-list ip-int-name {
                                type types-sros:interface-name;
                                max-elements 10;
                                ordered-by user;
                                description "Interface name as the match criterion";
                            }

                        } // container interface-subnets

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/as-path
                        container as-path {
                            description "Enter the as-path context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/as-path/as-path
                            choice as-path {
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/as-path/as-path/name
                                case name {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/as-path/as-path/name/name
                                    leaf name {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "AS path name to match";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/as-path/as-path/group
                                case group {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/as-path/as-path/group/group
                                    leaf group {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "AS path group as a match criterion";
                                    }

                                }
                            }
                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/as-path/length
                            container length {
                                description "Enter the length context";

                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/as-path/length/value
                                leaf value {
                                    type types-policy:value-255-param-name;
                                    description "AS numbers in the AS path that match on the BGP route";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/as-path/length/qualifier
                                leaf qualifier {
                                    type types-policy:policy-qualifier;
                                    default "equal";
                                    description "Higher or lower values to be accepted as match criteria";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/as-path/length/unique
                                leaf unique {
                                    type boolean;
                                    default "false";
                                    description "Use unique AS numbers as matching criteria";
                                }

                            } // container length

                        } // container as-path

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/cluster-id
                        container cluster-id {
                            description "Enter the cluster-id context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/cluster-id/cluster-id
                            choice cluster-id {
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/cluster-id/cluster-id/ip-address
                                case ip-address {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/cluster-id/cluster-id/ip-address/ip-address
                                    leaf-list ip-address {
                                        type types-sros:ipv4-prefix-with-host-bits;
                                        max-elements 5;
                                        ordered-by user;
                                        description "Cluster list attribute IP addresses for route matching";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/cluster-id/cluster-id/none-cluster-list
                                case none-cluster-list {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/cluster-id/cluster-id/none-cluster-list/none-cluster-list
                                    leaf none-cluster-list {
                                        type boolean;
                                        default "false";
                                        description "Specify matching BGP routes without a cluster ID";
                                    }

                                }
                            }
                        } // container cluster-id

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/community
                        container community {
                            description "Enter the community context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/community/community
                            choice community {
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/community/community/name
                                case name {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/community/community/name/name
                                    leaf name {
                                        type union {
                                            type types-policy:param-midstring-64 {
                                                pattern "[^\\]\\[]+" {
                                                    error-message "String must not contain square brackets ([]).";
                                                }
                                            }
                                            type string;
                                        }
                                        description "Community list name";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/community/community/expression
                                case expression {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/community/community/expression/expression
                                    leaf expression {
                                        type string {
                                            length "1..900";
                                        }
                                        description "Community expression name as a match criterion";
                                    }

                                }
                            }
                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/community/count
                            container count {
                                description "Enter the count context";

                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/community/count/value
                                leaf value {
                                    type types-policy:value-param-1024-name;
                                    description "Number of BGP communities to match the BGP route";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/community/count/qualifier
                                leaf qualifier {
                                    type types-policy:policy-qualifier;
                                    default "equal";
                                    description "Higher or lower values to be accepted as match criteria";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/community/count/type
                                leaf type {
                                    type enumeration {
                                        enum standard                       { value 2; }
                                        enum extended                       { value 3; }
                                        enum large                          { value 4; }
                                    }
                                    description "Community count to match the community count criteria";
                                }

                            } // container count

                        } // container community

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/flowspec
                        container flowspec {
                            description "Enter the flowspec context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/flowspec/dest
                            leaf dest {
                                type union {
                                    type types-policy:param-midstring;
                                    type string;
                                }
                                description "BGP FlowSpec route matches per destination IP prefix";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/flowspec/source
                            leaf source {
                                type union {
                                    type types-policy:param-midstring;
                                    type string;
                                }
                                description "BGP FlowSpec route matches per the source IP prefix";
                            }

                        } // container flowspec

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/local-preference
                        container local-preference {
                            description "Enter the local-preference context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/local-preference/value
                            leaf value {
                                type types-policy:value-param-name-metric;
                                description "BGP routes per local preference value or variable name";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/local-preference/qualifier
                            leaf qualifier {
                                type types-policy:policy-qualifier;
                                default "equal";
                                description "Higher or lower values to be accepted as match criteria";
                            }

                        } // container local-preference

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/metric
                        container metric {
                            description "Enter the metric context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/metric/value
                            leaf value {
                                type types-policy:value-param-name-metric;
                                description "Local preference value or variable name";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/metric/qualifier
                            leaf qualifier {
                                type types-policy:policy-qualifier;
                                default "equal";
                                description "Higher or lower values to be accepted as match criteria";
                            }

                        } // container metric

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/next-hop
                        container next-hop {
                            description "Enter the next-hop context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/next-hop/next-hop
                            choice next-hop {
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/next-hop/next-hop/ip-address
                                case ip-address {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/next-hop/next-hop/ip-address/ip-address
                                    leaf ip-address {
                                        type types-sros:ip-unicast-address;
                                        description "IP address of the next hop to match";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/next-hop/next-hop/prefix-list
                                case prefix-list {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/next-hop/next-hop/prefix-list/prefix-list
                                    leaf prefix-list {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "Name of a next hop prefix list to match";
                                    }

                                }
                            }
                        } // container next-hop

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/protocol
                        container protocol {
                            description "Enter the protocol context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/protocol/name
                            leaf-list name {
                                type enumeration {
                                    enum direct                         { value 2; }
                                    enum static                         { value 3; }
                                    enum bgp                            { value 4; }
                                    enum isis                           { value 5; }
                                    enum ospf                           { value 6; }
                                    enum rip                            { value 7; }
                                    enum aggregate                      { value 8; }
                                    enum bgp-vpn                        { value 9; }
                                    enum igmp                           { value 10; }
                                    enum pim                            { value 11; }
                                    enum ospf3                          { value 12; }
                                    enum ldp                            { value 13; }
                                    enum sub-mgmt                       { value 14; }
                                    enum mld                            { value 15; }
                                    enum managed                        { value 16; }
                                    enum vpn-leak                       { value 18; }
                                    enum nat                            { value 21; }
                                    enum periodic                       { value 22; }
                                    enum ipsec                          { value 23; }
                                    enum dhcpv6-pd                      { value 25; }
                                    enum dhcpv6-na                      { value 26; }
                                    enum dhcpv6-ta                      { value 27; }
                                    enum dhcpv6-pd-excl                 { value 28; }
                                    enum ripng                          { value 29; }
                                    enum bgp-label                      { value 31; }
                                    enum direct-interface               { value 32; }
                                    enum arp-nd                         { value 33; }
                                    enum rib-api                        { value 34; }
                                    enum dhcp-client                    { value 36; }
                                    enum evpn-ifl                       { value 37; }
                                    enum srv6                           { value 39; }
                                    enum video                          { value 40; }
                                }
                                max-elements 5;
                                description "List of protocol names as the match criterion";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/protocol/instance
                            leaf instance {
                                type union {
                                    type enumeration {
                                        enum all                            { value -1; }
                                    }
                                    type uint32 {
                                        range "0..127";
                                    }
                                }
                                default "0";
                                description "Instance for protocol IS-IS, OSPF, or OSPFv3 to match";
                            }

                        } // container protocol

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/source-address
                        container source-address {
                            description "Enter the source-address context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/source-address/source-address
                            choice source-address {
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/source-address/source-address/ip-address
                                case ip-address {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/source-address/source-address/ip-address/ip-address
                                    leaf ip-address {
                                        type types-sros:ip-address;
                                        description "Source IP address to match";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/source-address/source-address/prefix-list
                                case prefix-list {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/source-address/source-address/prefix-list/prefix-list
                                    leaf prefix-list {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "Name of a source address prefix list to match";
                                    }

                                }
                            }
                        } // container source-address

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables
                        container policy-variables {
                            description "Enter the policy-variables context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name
                            list name {
                                key "variable-name";
                                max-elements 10;
                                description "Enter the name list instance";

                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name/variable-name
                                leaf variable-name {
                                    type types-policy:policy-var-name;
                                    description "Global variable name used to reference policy functions";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name/variable-type
                                choice variable-type {
                                    mandatory true;
                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name/variable-type/value
                                    case value {

                                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name/variable-type/value/value
                                        leaf value {
                                            type types-sros:named-item;
                                            description "Policy variable value";
                                        }

                                    }
                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name/variable-type/number
                                    case number {

                                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name/variable-type/number/number
                                        leaf number {
                                            type uint64 {
                                                range "0..4294967295";
                                            }
                                            description "Numerical value of the policy variable";
                                        }

                                    }
                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name/variable-type/address
                                    case address {

                                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name/variable-type/address/address
                                        leaf address {
                                            type types-sros:ip-address;
                                            description "IP address of the policy variable";
                                        }

                                    }
                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name/variable-type/decimal
                                    case decimal {

                                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name/variable-type/decimal/decimal
                                        leaf decimal {
                                            type decimal64 {
                                                range "0.000..4294967295.000";
                                                fraction-digits 3;
                                            }
                                            description "Attribute decimal to which variable name is resolved";
                                        }

                                    }
                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name/variable-type/prefix
                                    case prefix {

                                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/from/policy-variables/name/variable-type/prefix/prefix
                                        leaf prefix {
                                            type types-sros:ip-prefix;
                                            description "Prefix value associated with the variable name";
                                        }

                                    }
                                }
                            } // list name

                        } // container policy-variables

                    } // container from

                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/to
                    container to {
                        presence "Enable the to context";
                        description "Enable the to context";

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/to/level
                        leaf level {
                            type int32 {
                                range "1..2";
                            }
                            description "IS-IS route level as a match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/to/prefix-list
                        leaf-list prefix-list {
                            type union {
                                type types-policy:param-midstring-64;
                                type string;
                            }
                            max-elements 28;
                            ordered-by user;
                            description "Prefix list as match criterion";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/to/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/to/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/to/neighbor
                        container neighbor {
                            description "Enter the neighbor context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/to/neighbor/neighbor
                            choice neighbor {
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/to/neighbor/neighbor/ip-address
                                case ip-address {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/to/neighbor/neighbor/ip-address/ip-address
                                    leaf ip-address {
                                        type union {
                                            type types-sros:ip-address;
                                            type types-sros:ip-address-with-zone;
                                        }
                                        description "IP address to match the neighbor";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/to/neighbor/neighbor/prefix-list
                                case prefix-list {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/to/neighbor/neighbor/prefix-list/prefix-list
                                    leaf prefix-list {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "Name to match the neighbor prefix list";
                                    }

                                }
                            }
                        } // container neighbor

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/to/protocol
                        container protocol {
                            description "Enter the protocol context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/to/protocol/name
                            leaf-list name {
                                type enumeration {
                                    enum bgp                            { value 4; }
                                    enum isis                           { value 5; }
                                    enum ospf                           { value 6; }
                                    enum rip                            { value 7; }
                                    enum bgp-vpn                        { value 9; }
                                    enum ospf3                          { value 12; }
                                    enum ldp                            { value 13; }
                                    enum vpn-leak                       { value 18; }
                                    enum ripng                          { value 29; }
                                    enum bgp-label                      { value 31; }
                                    enum evpn-ifl                       { value 37; }
                                }
                                max-elements 2;
                                description "Protocol name to match";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/to/protocol/instance
                            leaf instance {
                                type union {
                                    type enumeration {
                                        enum all                            { value -1; }
                                    }
                                    type uint32 {
                                        range "0..127";
                                    }
                                }
                                default "0";
                                description "Instance for the IS-IS, OSPF, or OSPF3 protocol";
                            }

                        } // container protocol

                    } // container to

                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/action
                    container action {
                        presence "Enable the action context";
                        description "Enable the action context";

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/action-type
                        leaf action-type {
                            type enumeration {
                                enum accept                         { value 1; }
                                enum reject                         { value 2; }
                                enum next-entry                     { value 3; }
                                enum next-policy                    { value 4; }
                            }
                            mandatory true;
                            description "Action type for routes matching the route policy entry";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/add-paths-send-limit
                        leaf add-paths-send-limit {
                            type union {
                                type int32 {
                                    range "1..16";
                                }
                                type enumeration {
                                    enum multipaths                     { value -1; }
                                }
                            }
                            description "BGP Add-Paths send limit applied for routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/admin-tag-policy
                        leaf admin-tag-policy {
                            type union {
                                type types-policy:param-midstring-64;
                                type string;
                            }
                            description "Administrative tag policy name";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/advertise-label
                        leaf advertise-label {
                            type enumeration {
                                enum per-prefix                     { value 1; }
                                enum pop                            { value 2; }
                                enum pop-and-forward                { value 3; }
                            }
                            description "Label allocation for matched BGP routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/bgp-high-priority
                        leaf bgp-high-priority {
                            type boolean;
                            default "false";
                            description "Tag routes as high priority for fast table updates";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/bgp-leak
                        leaf bgp-leak {
                            type boolean;
                            default "false";
                            description "Allow leaking for BGP routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/create-mpls-tunnel
                        leaf create-mpls-tunnel {
                            type boolean;
                            default "false";
                            description "Create an MPLS tunnel";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/create-udp-tunnel
                        leaf create-udp-tunnel {
                            type boolean;
                            default "false";
                            description "Create an MPLS-over-UDP tunnel";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/damping
                        leaf damping {
                            type union {
                                type enumeration {
                                    enum none                           { value 0; }
                                }
                                type types-policy:param-midstring;
                                type string;
                            }
                            description "Damping profile used for routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/dest-class
                        leaf dest-class {
                            type types-sros:class-index;
                            description "Default destination class for the policy statement";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/egress-statistics
                        leaf egress-statistics {
                            type boolean;
                            default "false";
                            description "Enable egress statistics for BGP-LU routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/ingress-statistics
                        leaf ingress-statistics {
                            type boolean;
                            default "false";
                            description "Enable ingress statistics for BGP-LU routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/flex-algo
                        leaf flex-algo {
                            type types-policy:value-param-name-flex-algo;
                            description "Flexible algorithm for BGP next-hop autobind operation";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/install-backup-path
                        leaf install-backup-path {
                            type boolean;
                            default "false";
                            description "Install a preprogrammed backup path for the prefix";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/local-preference
                        leaf local-preference {
                            type types-policy:value-param-name-metric;
                            description "BGP local preference for routes not matching any entry";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/nat-policy
                        leaf nat-policy {
                            type union {
                                type types-policy:param-midstring;
                                type string;
                            }
                            description "BGP-VPN imported route and a NAT policy association";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/next-hop
                        leaf next-hop {
                            type union {
                                type enumeration {
                                    enum peer-address                   { value -2; }
                                    enum self                           { value -1; }
                                }
                                type types-sros:ip-address;
                                type string {
                                    length "1..32";
                                    pattern "@.+@" {
                                        error-message "String must start and end with an at-sign (@)";
                                    }
                                }
                            }
                            description "Next-hop IP address applied to routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/origin
                        leaf origin {
                            type union {
                                type enumeration {
                                    enum igp                            { value 2; }
                                    enum egp                            { value 3; }
                                    enum incomplete                     { value 4; }
                                }
                                type string {
                                    length "1..32";
                                    pattern "@.+@" {
                                        error-message "String must start and end with an at-sign (@)";
                                    }
                                }
                            }
                            description "BGP origin for routes that are exported into BGP";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/origin-validation-state
                        leaf origin-validation-state {
                            type union {
                                type enumeration {
                                    enum valid                          { value 1; }
                                    enum not-found                      { value 2; }
                                    enum invalid                        { value 3; }
                                }
                                type string {
                                    length "1..32";
                                    pattern "@.+@" {
                                        error-message "String must start and end with an at-sign (@)";
                                    }
                                }
                            }
                            description "Origin validation state for routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/prefer-transport-frr
                        leaf prefer-transport-frr {
                            type boolean;
                            default "false";
                            description "Enable transport FRR preference";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/preference
                        leaf preference {
                            type union {
                                type uint32 {
                                    range "1..255";
                                }
                                type string {
                                    length "1..32";
                                    pattern "@.+@" {
                                        error-message "String must start and end with an at-sign (@)";
                                    }
                                }
                            }
                            description "Route preference applied to routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/resolve-static
                        leaf resolve-static {
                            type boolean;
                            default "false";
                            description "Resolve next hop of a static route for the BGP next hop";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/source-class
                        leaf source-class {
                            type types-sros:class-index;
                            description "Default source class for the policy statement";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/sticky-ecmp
                        leaf sticky-ecmp {
                            type boolean;
                            default "false";
                            description "Specify the sticky ECMP flag for BGP ECMP routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/tag
                        leaf tag {
                            type union {
                                type uint32 {
                                    range "1..4294967295";
                                }
                                type string {
                                    length "1..32";
                                    pattern "@.+@" {
                                        error-message "String must start and end with an at-sign (@)";
                                    }
                                }
                            }
                            description "OSPF RIP or IS-IS tag applied to routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/type
                        leaf type {
                            type union {
                                type int32 {
                                    range "1..2";
                                }
                                type string {
                                    length "1..32";
                                    pattern "@.+@" {
                                        error-message "String must start and end with an at-sign (@)";
                                    }
                                }
                            }
                            description "OSPF metric type applied to routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/route-table-install
                        leaf route-table-install {
                            type boolean;
                            default "true";
                            description "Allow BGP route installation in the route table";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/sr-maintenance-policy
                        leaf sr-maintenance-policy {
                            type union {
                                type types-policy:param-midstring;
                                type string;
                            }
                            description "SR maintenance policy as an action";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/srv6-locator
                        leaf srv6-locator {
                            type union {
                                type types-policy:param-midstring-64;
                                type string;
                            }
                            description "Locator for SRv6 TLV to use";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/srv6-micro-segment-locator
                        leaf srv6-micro-segment-locator {
                            type union {
                                type types-policy:param-midstring-64;
                                type string;
                            }
                            description "Micro-segment locator for SRv6 TLV to use";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/srv6-return-path-bfd-sid
                        leaf srv6-return-path-bfd-sid {
                            when "../sr-maintenance-policy" {
                                description "sr-maintenance-policy must be set to access this leaf";
                            }
                            type union {
                                type types-policy:policy-var-name;
                                type types-sros:ipv6-address;
                            }
                            description "SRv6 return path BFD SID applied to SRv6 policy routes";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/aigp-metric
                        container aigp-metric {
                            description "Enter the aigp-metric context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/aigp-metric/action
                            choice action {
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/aigp-metric/action/add
                                case add {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/aigp-metric/action/add/add
                                    leaf add {
                                        type types-policy:value-param-name-metric;
                                        description "AIGP metric to add";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/aigp-metric/action/set
                                case set {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/aigp-metric/action/set/set
                                    leaf set {
                                        type types-policy:value-param-name-metric-igp;
                                        description "AIGP metric";
                                    }

                                }
                            }
                        } // container aigp-metric

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/as-path
                        container as-path {
                            description "Enter the as-path context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/as-path/action
                            choice action {
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/as-path/action/add
                                case add {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/as-path/action/add/add
                                    leaf add {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "AS path to add";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/as-path/action/replace
                                case replace {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/as-path/action/replace/replace
                                    leaf replace {
                                        type union {
                                            type types-policy:param-midstring;
                                            type string;
                                        }
                                        description "AS path to replace";
                                    }

                                }
                            }
                        } // container as-path

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/as-path-prepend
                        container as-path-prepend {
                            description "Enter the as-path-prepend context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/as-path-prepend/as-path
                            leaf as-path {
                                type types-policy:value-param-name-range;
                                description "AS number to prepend to the AS path attribute";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/as-path-prepend/repeat
                            leaf repeat {
                                type types-policy:repeat-value-param-name;
                                default "1";
                                description "Number of times to prepend the specified AS number";
                            }

                        } // container as-path-prepend

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/bgp-tunnel-metric
                        container bgp-tunnel-metric {
                            description "Enter the bgp-tunnel-metric context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/bgp-tunnel-metric/value
                            leaf value {
                                type types-policy:value-param-name;
                                description "BGP tunnel table metric value";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/bgp-tunnel-metric/prefer-aigp
                            leaf prefer-aigp {
                                type boolean;
                                default "false";
                                description "Use AIGP attribute as tunnel metric when present";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/bgp-tunnel-metric/prefer-med
                            leaf prefer-med {
                                type boolean;
                                default "false";
                                description "Use MED attribute as tunnel metric when present";
                            }

                        } // container bgp-tunnel-metric

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/metric
                        container metric {
                            description "Enter the metric context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/metric/action
                            choice action {
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/metric/action/add
                                case add {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/metric/action/add/add
                                    leaf add {
                                        type types-policy:value-param-name-metric;
                                        description "Metric to add";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/metric/action/subtract
                                case subtract {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/metric/action/subtract/subtract
                                    leaf subtract {
                                        type types-policy:value-param-name-metric;
                                        description "Metric to subtract";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/metric/action/set
                                case set {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/metric/action/set/set
                                    leaf set {
                                        type union {
                                            type enumeration {
                                                enum igp                            { status obsolete; }
                                            }
                                            type int64 {
                                                range "0..4294967295";
                                            }
                                            type string {
                                                length "1..32";
                                                pattern "@.+@" {
                                                    error-message "String must start and end with an at-sign (@)";
                                                }
                                            }
                                        }
                                        description "Metric to assign";
                                    }

                                }
                            }
                        } // container metric

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/multicast-redirection
                        container multicast-redirection {
                            description "Enter the multicast-redirection context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/multicast-redirection/fwd-service
                            leaf fwd-service {
                                type string;
                                default "Base";
                                description "Service ID for multicast redirection";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/multicast-redirection/ip-int-name
                            leaf ip-int-name {
                                type types-sros:interface-name;
                                description "Alternate interface where IGMP messages are redirected";
                            }

                        } // container multicast-redirection

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/sr-label-index
                        container sr-label-index {
                            description "Enter the sr-label-index context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/sr-label-index/value
                            leaf value {
                                type types-policy:value-524287-param-name;
                                description "BGP SR label index associated with routes";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/sr-label-index/prefer-igp
                            leaf prefer-igp {
                                type boolean;
                                default "false";
                                description "Use the SR label index from the IGP route when present";
                            }

                        } // container sr-label-index

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/bgp-med
                        container bgp-med {
                            description "Enter the bgp-med context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/bgp-med/action
                            choice action {
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/bgp-med/action/set
                                case set {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/bgp-med/action/set/set
                                    leaf set {
                                        type types-policy:value-param-name-bgp-med-igp;
                                        description "BGP-MED assignment";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/bgp-med/action/adjust
                                case adjust {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/bgp-med/action/adjust/adjust
                                    leaf adjust {
                                        type string {
                                            length "1..64";
                                            pattern "(.*@.+@.*)|(.*_med_.*)|(.*_igp_.*)" {
                                                error-message "Expression must contain variable _igp_ or _med_ or @parameter@";
                                            }
                                            pattern ".*[+\\-*].*" {
                                                error-message "Expression must contain one valid operator. Supported operators are +, - and *";
                                            }
                                        }
                                        description "BGP-MED adjustment";
                                    }

                                }
                            }
                        } // container bgp-med

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/forwarding-class
                        container forwarding-class {
                            description "Enter the forwarding-class context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/forwarding-class/fc
                            leaf fc {
                                type enumeration {
                                    enum be                             { value 0; }
                                    enum l2                             { value 1; }
                                    enum af                             { value 2; }
                                    enum l1                             { value 3; }
                                    enum h2                             { value 4; }
                                    enum ef                             { value 5; }
                                    enum h1                             { value 6; }
                                    enum nc                             { value 7; }
                                }
                                description "Forwarding class associated with the route";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/forwarding-class/priority
                            leaf priority {
                                type enumeration {
                                    enum low                            { value 1; }
                                    enum high                           { value 2; }
                                }
                                description "Route priority";
                            }

                        } // container forwarding-class

                        ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/community
                        container community {
                            description "Enter the community context";

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/community/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/community/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/community/action
                            choice action {
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/community/action/action1
