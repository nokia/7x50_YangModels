                                leaf add {
                                    type union {
                                        type types-policy:param-midstring;
                                        type string;
                                    }
                                    description "AS path to add";
                                }

                            }
                            ///nokia-conf/configure/policy-options/policy-statement/default-action/as-path/action/replace
                            case replace {

                                ///nokia-conf/configure/policy-options/policy-statement/default-action/as-path/action/replace/replace
                                leaf replace {
