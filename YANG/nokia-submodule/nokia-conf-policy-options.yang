                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/default-action/community/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/policy-options/policy-statement/default-action/community/action
                        choice action {
                            ///nokia-conf/configure/policy-options/policy-statement/default-action/community/action/action1
                            case action1 {

                                ///nokia-conf/configure/policy-options/policy-statement/default-action/community/action/action1/add
                                leaf-list add {
                                    type union {
                                        type types-policy:param-midstring-64;
                                        type string;
                                    }
                                    max-elements 28;
                                    ordered-by user;
                                    description "List of community names to add";
                                }

                                ///nokia-conf/configure/policy-options/policy-statement/default-action/community/action/action1/remove
                                leaf-list remove {
                                    type union {
                                        type types-policy:param-midstring-64;
                                        type string;
                                    }
                                    max-elements 28;
                                    ordered-by user;
                                    description "List of community names to remove";
                                }

                            }
                            ///nokia-conf/configure/policy-options/policy-statement/default-action/community/action/action2
                            case action2 {

                                ///nokia-conf/configure/policy-options/policy-statement/default-action/community/action/action2/replace
                                leaf-list replace {
                                    type union {
                                        type types-policy:param-midstring-64;
                                        type string;
                                    }
                                    max-elements 28;
                                    ordered-by user;
                                    description "List of community names to replace";
                                }

                            }
                        }
                    } // container community

                } // container default-action

            } // list policy-statement

        } // container policy-options

    } // grouping conf-policy-options

}
