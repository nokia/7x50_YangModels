                        ///nokia-conf/configure/policy-options/policy-statement/default-action/bgp-med/action
                        choice action {
                            ///nokia-conf/configure/policy-options/policy-statement/default-action/bgp-med/action/set
                            case set {

                                ///nokia-conf/configure/policy-options/policy-statement/default-action/bgp-med/action/set/set
                                leaf set {
                                    type types-policy:value-param-name-bgp-med-igp;
                                    description "BGP-MED assignment";
                                }

