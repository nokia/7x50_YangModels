                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/community/action/action1/add
                                    leaf-list add {
                                        type union {
                                            type types-policy:param-midstring-64;
                                            type string;
                                        }
                                        max-elements 28;
                                        ordered-by user;
                                        description "List of community names to add";
                                    }

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/community/action/action1/remove
                                    leaf-list remove {
                                        type union {
                                            type types-policy:param-midstring-64;
                                            type string;
                                        }
                                        max-elements 28;
                                        ordered-by user;
                                        description "List of community names to remove";
                                    }

                                }
                                ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/community/action/action2
                                case action2 {

                                    ///nokia-conf/configure/policy-options/policy-statement/named-entry/action/community/action/action2/replace
                                    leaf-list replace {
                                        type union {
                                            type types-policy:param-midstring-64;
                                            type string;
                                        }
                                        max-elements 28;
                                        ordered-by user;
                                        description "List of community names to replace";
                                    }

                                }
                            }
                        } // container community

                    } // container action

                } // list named-entry

                ///nokia-conf/configure/policy-options/policy-statement/default-action
                container default-action {
                    presence "Enable the default-action context";
                    description "Enable the default-action context";

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/action-type
                    leaf action-type {
                        type enumeration {
                            enum accept                         { value 1; }
                            enum reject                         { value 2; }
                            enum next-entry                     { value 3; }
                            enum next-policy                    { value 4; }
                        }
                        mandatory true;
                        description "Action type for routes matching the route policy entry";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/add-paths-send-limit
                    leaf add-paths-send-limit {
                        type union {
                            type int32 {
                                range "1..16";
                            }
                            type enumeration {
                                enum multipaths                     { value -1; }
                            }
                        }
                        description "BGP Add-Paths send limit applied for routes";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/admin-tag-policy
                    leaf admin-tag-policy {
                        type union {
                            type types-policy:param-midstring-64;
                            type string;
                        }
                        description "Administrative tag policy name";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/advertise-label
                    leaf advertise-label {
                        type enumeration {
                            enum per-prefix                     { value 1; }
                            enum pop                            { value 2; }
                            enum pop-and-forward                { value 3; }
                        }
                        description "Label allocation for matched BGP routes";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/bgp-high-priority
                    leaf bgp-high-priority {
                        type boolean;
                        default "false";
                        description "Tag routes as high priority for fast table updates";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/bgp-leak
                    leaf bgp-leak {
                        type boolean;
                        default "false";
                        description "Allow leaking for BGP routes";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/create-mpls-tunnel
                    leaf create-mpls-tunnel {
                        type boolean;
                        default "false";
                        description "Create an MPLS tunnel";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/create-udp-tunnel
                    leaf create-udp-tunnel {
                        type boolean;
                        default "false";
                        description "Create an MPLS-over-UDP tunnel";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/damping
                    leaf damping {
                        type union {
                            type enumeration {
                                enum none                           { value 0; }
                            }
                            type types-policy:param-midstring;
                            type string;
                        }
                        description "Damping profile used for routes";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/dest-class
                    leaf dest-class {
                        type types-sros:class-index;
                        description "Default destination class for the policy statement";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/egress-statistics
                    leaf egress-statistics {
                        type boolean;
                        default "false";
                        description "Enable egress statistics for BGP-LU routes";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/ingress-statistics
                    leaf ingress-statistics {
                        type boolean;
                        default "false";
                        description "Enable ingress statistics for BGP-LU routes";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/flex-algo
                    leaf flex-algo {
                        type types-policy:value-param-name-flex-algo;
                        description "Flexible algorithm for BGP next-hop autobind operation";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/install-backup-path
                    leaf install-backup-path {
                        type boolean;
                        default "false";
                        description "Install a preprogrammed backup path for the prefix";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/local-preference
                    leaf local-preference {
                        type types-policy:value-param-name-metric;
                        description "BGP local preference for routes not matching any entry";
                    }

                    ///nokia-conf/configure/policy-options/policy-statement/default-action/nat-policy
