submodule nokia-state-submgt-subscriber {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-card               { prefix "types-card"; }
    import nokia-types-cellular           { prefix "types-cellular"; }
    import nokia-types-dhcp               { prefix "types-dhcp"; }
    import nokia-types-l2tp               { prefix "types-l2tp"; }
    import nokia-types-nat                { prefix "types-nat"; }
    import nokia-types-pcc-rule           { prefix "types-pcc-rule"; }
    import nokia-types-ppp                { prefix "types-ppp"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-subscriber-mgmt    { prefix "types-submgt"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for subscriber management subscriber on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-submgt-subscriber YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-02-02";

    grouping state-submgt-subscriber {
        ///nokia-state/state/subscriber-mgmt/subscriber
        list subscriber {
            key "subscriber-id";
            description "Enter the subscriber list instance";

            ///nokia-state/state/subscriber-mgmt/subscriber/subscriber-id
            leaf subscriber-id {
                type types-submgt:subscriber-id;
                description "Subscriber Identification string";
            }

            ///nokia-state/state/subscriber-mgmt/subscriber/port
            leaf port {
                type types-sros:port;
                description "Port.";
            }

            ///nokia-state/state/subscriber-mgmt/subscriber/sub-profile
            leaf sub-profile {
                type types-sros:named-item;
                description "The subscriber profile of this subscriber.";
            }

            ///nokia-state/state/subscriber-mgmt/subscriber/sub-profile-changes
            leaf sub-profile-changes {
                type uint32;
                description "The number of times the sub-profile of this subscriber has changed.";
            }

            ///nokia-state/state/subscriber-mgmt/subscriber/ancp-string
            leaf ancp-string {
                type types-sros:display-string {
                    length "1..64";
                }
                description "The ancp string of this subscriber.";
            }

            ///nokia-state/state/subscriber-mgmt/subscriber/int-dest-id
            leaf int-dest-id {
                type types-submgt:int-dest-id;
                description "The Int-dest-id corresponding to this subscriber.";
            }

            ///nokia-state/state/subscriber-mgmt/subscriber/application-profile
            leaf application-profile {
                type types-sros:named-item;
                description "The application profile used by this subscriber.";
            }

            ///nokia-state/state/subscriber-mgmt/subscriber/mcac
            container mcac {
                description "Enter the mcac context";

                ///nokia-state/state/subscriber-mgmt/subscriber/mcac/sub-mcac-policy
                leaf sub-mcac-policy {
                    type types-sros:named-item;
                    description "The sub-mcac-policy (based on the sub-profile)";
                }

                ///nokia-state/state/subscriber-mgmt/subscriber/mcac/in-use-mandatory-bandwidth
                leaf in-use-mandatory-bandwidth {
                    type uint32;
                    units "kilobps";
                    description
                        "The in-use bandwidth for the mandatory channels for this
                         subscriber.";
                }

                ///nokia-state/state/subscriber-mgmt/subscriber/mcac/in-use-optional-bandwidth
                leaf in-use-optional-bandwidth {
                    type uint32;
                    units "kilobps";
                    description
                        "The in-use bandwidth for the optional channels for this
                         subscriber.";
                }

                ///nokia-state/state/subscriber-mgmt/subscriber/mcac/available-mandatory-bandwidth
                leaf available-mandatory-bandwidth {
                    type uint32;
                    units "kilobps";
                    description
                        "The pre-reserved bandwidth for the mandatory channels for
                         this subscriber.";
                }

                ///nokia-state/state/subscriber-mgmt/subscriber/mcac/available-optional-bandwidth
                leaf available-optional-bandwidth {
                    type uint32;
                    units "kilobps";
                    description "The available bandwidth for this subscriber.";
                }

            } // container mcac

            ///nokia-state/state/subscriber-mgmt/subscriber/igmp-host-tracking
            container igmp-host-tracking {
                description "Enter the igmp-host-tracking context";

                ///nokia-state/state/subscriber-mgmt/subscriber/igmp-host-tracking/host-tracking-policy
                leaf host-tracking-policy {
                    type types-sros:named-item;
                    description "The host-tracking-policy (based on the sub-profile)";
                }

                ///nokia-state/state/subscriber-mgmt/subscriber/igmp-host-tracking/egress-rate-reduction
                leaf egress-rate-reduction {
                    type yang:gauge32;
                    description
                        "The egress rate reduction to be applied for this subscriber
                         in order to account for its multicast membership.";
                }

            } // container igmp-host-tracking

            ///nokia-state/state/subscriber-mgmt/subscriber/ingress
            container ingress {
                description "Enter the ingress context";

                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos
                container qos {
                    description "Enter the qos context";

                    ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/scheduler-policy
                    container scheduler-policy {
                        description "Enter the scheduler-policy context";

                        ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/scheduler-policy/scheduler-policy-instance-name
                        leaf scheduler-policy-instance-name {
                            type types-sros:named-item;
                            description "The name of scheduler policy.";
                        }

                        ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/scheduler-policy/scheduler
                        list scheduler {
                            key "scheduler-name";
                            description "Enter the scheduler list instance";

                            ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/scheduler-policy/scheduler/scheduler-name
                            leaf scheduler-name {
                                type types-sros:named-item;
                                description "Scheduler name";
                            }

                            ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/scheduler-policy/scheduler/statistics
                            container statistics {
                                description "Enter the statistics context";

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/scheduler-policy/scheduler/statistics/forwarded-packets
                                leaf forwarded-packets {
                                    type yang:counter64;
                                    description "The number of forwarded packets.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/scheduler-policy/scheduler/statistics/forwarded-octets
                                leaf forwarded-octets {
                                    type yang:counter64;
                                    description "The number of forwarded octets.";
                                }

                            } // container statistics

                            ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/scheduler-policy/scheduler/overrides
                            container overrides {
                                description "Enter the overrides context";

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/scheduler-policy/scheduler/overrides/pir
                                leaf pir {
                                    type union {
                                        type uint32 {
                                            range "1..3200000000";
                                        }
                                        type enumeration {
                                            enum max                            { value -1; }
                                        }
                                    }
                                    units "kilobps";
                                    description "PIR rate";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/scheduler-policy/scheduler/overrides/cir
                                leaf cir {
                                    type union {
                                        type uint32 {
                                            range "0..3200000000";
                                        }
                                        type enumeration {
                                            enum sum                            { value -3; }
                                            enum max                            { value -1; }
                                        }
                                    }
                                    units "kilobps";
                                    description "CIR rate";
                                }

                            } // container overrides

                        } // list scheduler

                    } // container scheduler-policy

                    ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/arbiter
                    list arbiter {
                        key "arbiter-name";
                        description "Enter the arbiter list instance";

                        ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/arbiter/arbiter-name
                        leaf arbiter-name {
                            type types-sros:named-item;
                            description "Arbiter name";
                        }

                        ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/arbiter/statistics
                        container statistics {
                            description "Enter the statistics context";

                            ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/arbiter/statistics/forwarded-packets
                            leaf forwarded-packets {
                                type yang:counter64;
                                description "The number of forwarded packets.";
                            }

                            ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/arbiter/statistics/forwarded-octets
                            leaf forwarded-octets {
                                type yang:counter64;
                                description "The number of forwarded octets.";
                            }

                        } // container statistics

                        ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/arbiter/overrides
                        container overrides {
                            description "Enter the overrides context";

                            ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/arbiter/overrides/pir
                            leaf pir {
                                type types-qos:policer-pir-rate-override;
                                units "kilobps";
                                description "PIR rate";
                            }

                        } // container overrides

                    } // list arbiter

                    ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics
                    container accumulated-statistics {
                        description "Enter the accumulated-statistics context";

                        ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue
                        list queue {
                            key "id";
                            description "Enter the queue list instance";

                            ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue/id
                            leaf id {
                                type types-qos:ingress-queue-id;
                                description "The id identifies the queue.";
                            }

                            ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue/statistics
                            container statistics {
                                description "Enter the statistics context";

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue/statistics/in-profile-forwarded-packets
                                leaf in-profile-forwarded-packets {
                                    type yang:counter64;
                                    description
                                        "The total number of in-profile packets
                                         (rate below CIR) forwarded by the ingress packet processor.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue/statistics/in-profile-forwarded-octets
                                leaf in-profile-forwarded-octets {
                                    type yang:counter64;
                                    description
                                        "The total number of in-profile octets
                                         (rate below CIR) forwarded by the ingress packet processor.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue/statistics/out-profile-forwarded-packets
                                leaf out-profile-forwarded-packets {
                                    type yang:counter64;
                                    description
                                        "The total number of out-profile packets
                                         (rate above CIR) forwarded by the ingress packet processor.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue/statistics/out-profile-forwarded-octets
                                leaf out-profile-forwarded-octets {
                                    type yang:counter64;
                                    description
                                        "The total number of out-profile octets
                                         (rate above CIR) forwarded by the ingress packet processor.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue/statistics/high-priority-dropped-packets
                                leaf high-priority-dropped-packets {
                                    type yang:counter64;
                                    description
                                        "The total number of high-priority packets dropped by the traffic manager
                                         for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue/statistics/high-priority-dropped-octets
                                leaf high-priority-dropped-octets {
                                    type yang:counter64;
                                    description
                                        "The total number of high-priority octets, as determined by the
                                         SLA profile ingress QoS policy, dropped by the packet processor for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue/statistics/low-priority-dropped-packets
                                leaf low-priority-dropped-packets {
                                    type yang:counter64;
                                    description
                                        "The total number of low-priority packets  dropped by the
                                         packet processor for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue/statistics/low-priority-dropped-octets
                                leaf low-priority-dropped-octets {
                                    type yang:counter64;
                                    description
                                        "The total number of low-priority octets as determined by the SLA profile
                                         ingress QoS policy, dropped by the packet processor to the traffic manager for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue/statistics/high-priority-offered-packets
                                leaf high-priority-offered-packets {
                                    type yang:counter64;
                                    description
                                        "The total number of high-priority packets offered by the
                                         packet processor to the traffic manager for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue/statistics/high-priority-offered-octets
                                leaf high-priority-offered-octets {
                                    type yang:counter64;
                                    description
                                        "The total number of high-priority octets offered by the
                                         packet processor to the traffic manager for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue/statistics/low-priority-offered-packets
                                leaf low-priority-offered-packets {
                                    type yang:counter64;
                                    description
                                        "The total number of low-priority packets offered by the
                                         packet processor to the traffic manager for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/queue/statistics/low-priority-offered-octets
                                leaf low-priority-offered-octets {
                                    type yang:counter64;
                                    description
                                        "The total number of low-priority octets offered by the
                                         packet processor to the traffic manager for this subscriber.";
                                }

                            } // container statistics

                        } // list queue

                        ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer
                        list policer {
                            key "id";
                            description "Enter the policer list instance";

                            ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/id
                            leaf id {
                                type types-qos:ingress-policer-id;
                                description "The id identifies the policer.";
                            }

                            ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/statistics
                            container statistics {
                                description "Enter the statistics context";

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/statistics/in-profile-forwarded-packets
                                leaf in-profile-forwarded-packets {
                                    type yang:counter64;
                                    description
                                        "The total number of in-profile packets
                                         (rate below CIR) forwarded by the ingress packet processor.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/statistics/in-profile-forwarded-octets
                                leaf in-profile-forwarded-octets {
                                    type yang:counter64;
                                    description
                                        "The total number of in-profile octets
                                         (rate below CIR) forwarded by the ingress packet processor.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/statistics/out-profile-forwarded-packets
                                leaf out-profile-forwarded-packets {
                                    type yang:counter64;
                                    description
                                        "The total number of out-profile packets
                                         (rate above CIR) forwarded by the ingress packet processor.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/statistics/out-profile-forwarded-octets
                                leaf out-profile-forwarded-octets {
                                    type yang:counter64;
                                    description
                                        "The total number of out-profile octets
                                         (rate above CIR) forwarded by the ingress packet processor.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/statistics/high-priority-dropped-packets
                                leaf high-priority-dropped-packets {
                                    type yang:counter64;
                                    description
                                        "The total number of high-priority packets dropped by the traffic manager
                                         for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/statistics/high-priority-dropped-octets
                                leaf high-priority-dropped-octets {
                                    type yang:counter64;
                                    description
                                        "The total number of high-priority octets, as determined by the
                                         SLA profile ingress QoS policy, dropped by the packet processor for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/statistics/low-priority-dropped-packets
                                leaf low-priority-dropped-packets {
                                    type yang:counter64;
                                    description
                                        "The total number of low-priority packets  dropped by the
                                         packet processor for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/statistics/low-priority-dropped-octets
                                leaf low-priority-dropped-octets {
                                    type yang:counter64;
                                    description
                                        "The total number of low-priority octets as determined by the SLA profile
                                         ingress QoS policy, dropped by the packet processor to the traffic manager for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/statistics/high-priority-offered-packets
                                leaf high-priority-offered-packets {
                                    type yang:counter64;
                                    description
                                        "The total number of high-priority packets offered by the
                                         packet processor to the traffic manager for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/statistics/high-priority-offered-octets
                                leaf high-priority-offered-octets {
                                    type yang:counter64;
                                    description
                                        "The total number of high-priority octets offered by the
                                         packet processor to the traffic manager for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/statistics/low-priority-offered-packets
                                leaf low-priority-offered-packets {
                                    type yang:counter64;
                                    description
                                        "The total number of low-priority packets offered by the
                                         packet processor to the traffic manager for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/statistics/low-priority-offered-octets
                                leaf low-priority-offered-octets {
                                    type yang:counter64;
                                    description
                                        "The total number of low-priority octets offered by the
                                         packet processor to the traffic manager for this subscriber.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/ingress/qos/accumulated-statistics/policer/statistics/stat-mode
                                leaf stat-mode {
                                    type types-submgt:subscriber-ingress-policer-stat-mode-type;
                                    description "The stat mode format of this policer.";
                                }

                            } // container statistics

                        } // list policer

                    } // container accumulated-statistics

                } // container qos

            } // container ingress

            ///nokia-state/state/subscriber-mgmt/subscriber/egress
            container egress {
                description "Enter the egress context";

                ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos
                container qos {
                    description "Enter the qos context";

                    ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/hardware-type
                    leaf hardware-type {
                        type enumeration {
                            enum unknown                        { value 0; }
                            enum fp                             { value 1; }
                            enum hs                             { value 4; }
                            enum fp-hw-agg-shaper               { value 5; }
                        }
                        description "The Quality of Service model (based on the MDA and/or IOM)";
                    }

                    ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/queue-group
                    leaf queue-group {
                        type string;
                        description "Queue group for policer output";
                    }

                    ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/virtual-port
                    leaf virtual-port {
                        type string;
                        description "Virtual port";
                    }

                    ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/secondary-shaper
                    leaf secondary-shaper {
                        type enumeration {
                            enum unused                         { value 0; }
                            enum default                        { value 1; }
                            enum int-dest-id                    { value 2; }
                        }
                        description
                            "'unused'      no secondary shaper is used.
                             'default'     the default secondary shaper is used.
                             'int-dest-id' the value of int-dest-id is used as secondary shaper.";
                    }

                    ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/aggregate-rate-limit-override
                    leaf aggregate-rate-limit-override {
                        type types-qos:rate-limit;
                        description "The egress aggregate rate limit override received by RADIUS/AAA/... .";
                    }

                    ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/aggregate-rate-limit
                    leaf aggregate-rate-limit {
                        type types-qos:rate-limit;
                        description "The intended egress aggregate rate limit, regardless of its origin.";
                    }

                    ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/aggregate-rate-limit-below-min
                    leaf aggregate-rate-limit-below-min {
                        type boolean;
                        description "True if the egress aggregate rate limit was below min-resv-bw and has been set to min-resv-bw.";
                    }

                    ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/scheduler-policy
                    container scheduler-policy {
                        description "Enter the scheduler-policy context";

                        ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/scheduler-policy/scheduler-policy-instance-name
                        leaf scheduler-policy-instance-name {
                            type types-sros:named-item;
                            description "The name of scheduler policy.";
                        }

                        ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/scheduler-policy/scheduler
                        list scheduler {
                            key "scheduler-name";
                            description "Enter the scheduler list instance";

                            ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/scheduler-policy/scheduler/scheduler-name
                            leaf scheduler-name {
                                type types-sros:named-item;
                                description "Scheduler name";
                            }

                            ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/scheduler-policy/scheduler/statistics
                            container statistics {
                                description "Enter the statistics context";

                                ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/scheduler-policy/scheduler/statistics/forwarded-packets
                                leaf forwarded-packets {
                                    type yang:counter64;
                                    description "The number of forwarded packets.";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/scheduler-policy/scheduler/statistics/forwarded-octets
                                leaf forwarded-octets {
                                    type yang:counter64;
                                    description "The number of forwarded octets.";
                                }

                            } // container statistics

                            ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/scheduler-policy/scheduler/overrides
                            container overrides {
                                description "Enter the overrides context";

                                ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/scheduler-policy/scheduler/overrides/pir
                                leaf pir {
                                    type union {
                                        type uint32 {
                                            range "1..3200000000";
                                        }
                                        type enumeration {
                                            enum max                            { value -1; }
                                        }
                                    }
                                    units "kilobps";
                                    description "PIR rate";
                                }

                                ///nokia-state/state/subscriber-mgmt/subscriber/egress/qos/scheduler-policy/scheduler/overrides/cir
                                leaf cir {
                                    type union {
                                        type uint32 {
                                            range "0..3200000000";
                                        }
                                        type enumeration {
                                            enum sum                            { value -3; }
                                            enum max                            { value -1; }
                                        }
                                    }
                                    units "kilobps";
                                    description "CIR rate";
                                }

