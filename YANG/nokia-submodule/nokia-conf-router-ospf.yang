submodule nokia-conf-router-ospf {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-igp                { prefix "types-igp"; }
    import nokia-types-ospf               { prefix "types-ospf"; }
    import nokia-types-router             { prefix "types-router"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-groups;
    include nokia-conf-policy-options;
    include nokia-conf-router;
    include nokia-conf-router-bier;
    include nokia-conf-routing-options;
    include nokia-conf-system;
    include nokia-conf-system-security;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure router ospf protocol attributes on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-router-ospf YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2022-12-07";

    grouping conf-router-ospf {
        ///nokia-conf/configure/router/ospf
        list ospf {
            key "ospf-instance";
            max-elements 32;
            description "Enter the ospf list instance";

            ///nokia-conf/configure/router/ospf/ospf-instance
            leaf ospf-instance {
                type uint32 {
                    range "0..31";
                }
                description "Value for the integrated OSPF instance";
            }

            ///nokia-conf/configure/router/ospf/admin-state
            leaf admin-state {
                type types-sros:admin-state;
                default "disable";
                description "Administrative state of the OSPF instance";
            }

            ///nokia-conf/configure/router/ospf/router-id
            leaf router-id {
                type types-igp:router-id;
                sros-ext:auto-restart-to-modify;
                description "Unique router ID for the OSPF instance";
            }

            ///nokia-conf/configure/router/ospf/advertise-router-capability
            leaf advertise-router-capability {
                type types-ospf:advertise-router-capability-type;
                default "false";
                description "Allow router advertisement capabilities";
            }

            ///nokia-conf/configure/router/ospf/advertise-tunnel-link
            leaf advertise-tunnel-link {
                type boolean;
                default "false";
                description "Allow router advertisement capabilities";
            }

            ///nokia-conf/configure/router/ospf/external-preference
            leaf external-preference {
                type int32 {
                    range "1..255";
                }
                default "150";
                description "Preference for OSPF external routes";
            }

            ///nokia-conf/configure/router/ospf/export-policy
            leaf-list export-policy {
                type leafref {
                    path "../../../policy-options/policy-statement/name";
                }
                max-elements 5;
                ordered-by user;
                description "Export policies that determine exported routes";
            }

            ///nokia-conf/configure/router/ospf/import-policy
            leaf-list import-policy {
                type leafref {
                    path "../../../policy-options/policy-statement/name";
                }
                max-elements 5;
                ordered-by user;
                description "Import policy names for routes from IGP to route table";
            }

            ///nokia-conf/configure/router/ospf/ldp-over-rsvp
            leaf ldp-over-rsvp {
                type boolean;
                default "false";
                description "Allow LSP over RSVP in this OSPF instance";
            }

            ///nokia-conf/configure/router/ospf/ldp-sync
            leaf ldp-sync {
                type boolean;
                default "true";
                description "Configure IGP-LDP synchronization for interfaces";
            }

            ///nokia-conf/configure/router/ospf/multicast-import
            leaf multicast-import {
                type boolean;
                default "false";
                description "Submit routes into the multicast Route Table Manager";
            }

            ///nokia-conf/configure/router/ospf/overload-include-ext-1
            leaf overload-include-ext-1 {
                type boolean;
                default "false";
                description "Advertise routes with maximum metric value for overload";
            }

            ///nokia-conf/configure/router/ospf/overload-include-ext-2
            leaf overload-include-ext-2 {
                type boolean;
                default "false";
                description "Advertise routes with maximum metric value for overload";
            }

            ///nokia-conf/configure/router/ospf/overload-include-stub
            leaf overload-include-stub {
                type boolean;
                default "false";
                description "Advertise all stub interfaces with max metric value";
            }

            ///nokia-conf/configure/router/ospf/overload
            leaf overload {
                type boolean;
                default "false";
                description "Change local router state to appear overloaded";
            }

            ///nokia-conf/configure/router/ospf/preference
            leaf preference {
                type int32 {
                    range "1..255";
                }
                default "10";
                description "Preference for OSPF internal routes";
            }

            ///nokia-conf/configure/router/ospf/reference-bandwidth
            leaf reference-bandwidth {
                type types-sros:reference-bandwidth-ext;
                units "kilobps";
                default "100000000";
                description "Bandwidth to reference default costing of interfaces";
            }

            ///nokia-conf/configure/router/ospf/traffic-engineering
            leaf traffic-engineering {
                type boolean;
                default "false";
                description "Calculate traffic engineering route";
            }

            ///nokia-conf/configure/router/ospf/unicast-import
            leaf unicast-import {
                type boolean;
                default "true";
                description "Submit routes into the unicast Route Table Manager";
            }

            ///nokia-conf/configure/router/ospf/compatible-rfc1583
            leaf compatible-rfc1583 {
                type boolean;
                default "true";
                description "OSPF summary and external route calculations";
            }

            ///nokia-conf/configure/router/ospf/multi-instance
            leaf multi-instance {
                type boolean;
                default "false";
                description "Enable OSPF Multi Instance Extensions";
            }

            ///nokia-conf/configure/router/ospf/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/router/ospf/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/router/ospf/external-db-overflow
            container external-db-overflow {
                presence "Enable the external-db-overflow context";
                description "Enable the external-db-overflow context";

                ///nokia-conf/configure/router/ospf/external-db-overflow/limit
                leaf limit {
                    type uint32 {
                        range "0..2147483647";
                    }
                    default "0";
                    description "Number of external LSA at which overload is triggered";
                }

                ///nokia-conf/configure/router/ospf/external-db-overflow/interval
                leaf interval {
                    type uint32 {
                        range "0..2147483647";
                    }
                    units "seconds";
                    default "0";
                    description "Time during which the router operates in overload";
                }

            } // container external-db-overflow

            ///nokia-conf/configure/router/ospf/export-limit
            container export-limit {
                presence "Enable the export-limit context";
                description "Enable the export-limit context";

                ///nokia-conf/configure/router/ospf/export-limit/number
                leaf number {
                    type uint32 {
                        range "1..max";
                    }
                    mandatory true;
                    description "Maximum routes or prefixes exported from route table";
                }

                ///nokia-conf/configure/router/ospf/export-limit/log-percent
                leaf log-percent {
                    type uint32 {
                        range "1..100";
                    }
                    description "Export limit before warning and SNMP notification sent";
                }

            } // container export-limit

            ///nokia-conf/configure/router/ospf/overload-on-boot
            container overload-on-boot {
                presence "Enable the overload-on-boot context";
                description "Enable the overload-on-boot context";

                ///nokia-conf/configure/router/ospf/overload-on-boot/timeout
                leaf timeout {
                    type uint32 {
                        range "1..1800";
                    }
                    units "seconds";
                    description "Time during which the router operates in overload state";
                }

            } // container overload-on-boot

            ///nokia-conf/configure/router/ospf/rib-priority
            container rib-priority {
                description "Enter the rib-priority context";

                ///nokia-conf/configure/router/ospf/rib-priority/high
                container high {
                    description "Enter the high context";

                    ///nokia-conf/configure/router/ospf/rib-priority/high/prefix-list
                    leaf prefix-list {
                        type leafref {
                            path "../../../../../policy-options/prefix-list/name";
                        }
                        description "Higher priority list used during OSPF route calculation";
                    }

                } // container high

            } // container rib-priority

            ///nokia-conf/configure/router/ospf/rtr-adv-lsa-limit
            container rtr-adv-lsa-limit {
                presence "Enable the rtr-adv-lsa-limit context";
                description "Enable the rtr-adv-lsa-limit context";

                ///nokia-conf/configure/router/ospf/rtr-adv-lsa-limit/max-lsa-count
                leaf max-lsa-count {
                    type uint32 {
                        range "1..max";
                    }
                    mandatory true;
                    description "Max number of LSAs one router can advertise";
                }

                ///nokia-conf/configure/router/ospf/rtr-adv-lsa-limit/log-only
                leaf log-only {
                    type boolean;
                    default "false";
                    description "Log the event without triggering overload";
                }

                ///nokia-conf/configure/router/ospf/rtr-adv-lsa-limit/warning-threshold
                leaf warning-threshold {
                    type uint32 {
                        range "0..100";
                    }
                    units "percent";
                    default "0";
                    description "Percentage of the max LSA count that causes a warning";
                }

                ///nokia-conf/configure/router/ospf/rtr-adv-lsa-limit/overload-timeout
                leaf overload-timeout {
                    type types-igp:overload-timeout;
                    default "forever";
                    description "Maximum time in overload after LSA limit is reached";
                }

            } // container rtr-adv-lsa-limit

            ///nokia-conf/configure/router/ospf/graceful-restart
            container graceful-restart {
                presence "Enable the graceful-restart context";
                description "Enable the graceful-restart context";

                ///nokia-conf/configure/router/ospf/graceful-restart/helper-mode
                leaf helper-mode {
                    type boolean;
                    default "true";
                    description "Enable graceful restart helper for OSPF";
                }

                ///nokia-conf/configure/router/ospf/graceful-restart/strict-lsa-checking
                leaf strict-lsa-checking {
                    type boolean;
                    default "true";
                    description "Perform strict LSA checking during graceful restart";
                }

            } // container graceful-restart

            ///nokia-conf/configure/router/ospf/traffic-engineering-options
            container traffic-engineering-options {
                description "Enter the traffic-engineering-options context";

                ///nokia-conf/configure/router/ospf/traffic-engineering-options/sr-te
                leaf sr-te {
                    type enumeration {
                        enum false                          { value 0; }
                        enum legacy                         { value 1; }
                        enum application-specific-link-attributes { value 2; }
                    }
                    default "false";
                    description "Advertisement of link attributes for SR-TE";
                }

                ///nokia-conf/configure/router/ospf/traffic-engineering-options/advertise-delay
                leaf advertise-delay {
                    type boolean;
                    default "false";
                    description "Enable the advertisement of link delay for TE";
                }

            } // container traffic-engineering-options

            ///nokia-conf/configure/router/ospf/timers
            container timers {
                description "Enter the timers context";

                ///nokia-conf/configure/router/ospf/timers/incremental-spf-wait
                leaf incremental-spf-wait {
                    type uint32 {
                        range "0..1000";
                    }
                    units "milliseconds";
                    default "1000";
                    description "Delay time before an incremental SPF calculation starts";
                }

                ///nokia-conf/configure/router/ospf/timers/lsa-accumulate
                leaf lsa-accumulate {
                    type uint32 {
                        range "0..1000";
                    }
                    units "milliseconds";
                    default "1000";
                    description "Delay to gather LSAs before advertising to neighbors";
                }

                ///nokia-conf/configure/router/ospf/timers/lsa-arrival
                leaf lsa-arrival {
                    type uint32 {
                        range "0..600000";
                    }
                    units "milliseconds";
                    default "1000";
                    description "Min delay between receipt of same LSAs from neighbors";
                }

                ///nokia-conf/configure/router/ospf/timers/redistribute-delay
                leaf redistribute-delay {
                    type uint32 {
                        range "0..1000";
                    }
                    units "milliseconds";
                    default "1000";
                    description "Hold down timer for external routes into OSPF";
                }

                ///nokia-conf/configure/router/ospf/timers/lsa-generate
                container lsa-generate {
                    description "Enter the lsa-generate context";

                    ///nokia-conf/configure/router/ospf/timers/lsa-generate/max-lsa-wait
                    leaf max-lsa-wait {
                        type uint32 {
                            range "10..600000";
                        }
                        units "milliseconds";
                        default "5000";
                        description "Max time between two LSAs being generated";
                    }

                    ///nokia-conf/configure/router/ospf/timers/lsa-generate/lsa-initial-wait
                    leaf lsa-initial-wait {
                        type uint32 {
                            range "10..600000";
                        }
                        units "milliseconds";
                        default "5000";
                        description "First wait period between OSPF LSA generation";
                    }

                    ///nokia-conf/configure/router/ospf/timers/lsa-generate/lsa-second-wait
                    leaf lsa-second-wait {
                        type uint32 {
                            range "10..600000";
                        }
                        units "milliseconds";
                        default "5000";
                        description "Hold time between the first and second LSA generation";
                    }

                } // container lsa-generate

                ///nokia-conf/configure/router/ospf/timers/spf-wait
                container spf-wait {
                    description "Enter the spf-wait context";

                    ///nokia-conf/configure/router/ospf/timers/spf-wait/spf-max-wait
                    leaf spf-max-wait {
                        type uint32 {
                            range "10..120000";
                        }
                        units "milliseconds";
                        default "10000";
                        description "Max interval between two consecutive SPF calculations";
                    }

                    ///nokia-conf/configure/router/ospf/timers/spf-wait/spf-initial-wait
                    leaf spf-initial-wait {
                        type uint32 {
                            range "10..100000";
                        }
                        units "milliseconds";
                        default "1000";
                        description "Initial SPF calculation delay after a topology change";
                    }

                    ///nokia-conf/configure/router/ospf/timers/spf-wait/spf-second-wait
                    leaf spf-second-wait {
                        type uint32 {
                            range "10..100000";
                        }
                        units "milliseconds";
                        default "1000";
                        description "Hold time between the first and second SPF calculation";
                    }

                } // container spf-wait

            } // container timers

            ///nokia-conf/configure/router/ospf/loopfree-alternate
            container loopfree-alternate {
                presence "Enable the loopfree-alternate context";
                description "Enable the loopfree-alternate context";

                ///nokia-conf/configure/router/ospf/loopfree-alternate/augment-route-table
                leaf augment-route-table {
                    type boolean;
                    default "false";
                    description "Attach remote LFA information to RTM entries";
                }

                ///nokia-conf/configure/router/ospf/loopfree-alternate/exclude
                container exclude {
                    description "Enter the exclude context";

                    ///nokia-conf/configure/router/ospf/loopfree-alternate/exclude/prefix-policy
                    leaf-list prefix-policy {
                        type leafref {
                            path "../../../../../policy-options/policy-statement/name";
                        }
                        max-elements 5;
                        ordered-by user;
                        description "Policy to exclude prefixes from LFA SPF calculation";
                    }

                } // container exclude

                ///nokia-conf/configure/router/ospf/loopfree-alternate/multi-homed-prefix
                container multi-homed-prefix {
                    presence "Enable the multi-homed-prefix context";
                    description "Enable the multi-homed-prefix context";

                    ///nokia-conf/configure/router/ospf/loopfree-alternate/multi-homed-prefix/preference
                    leaf preference {
                        type enumeration {
                            enum none                           { value 0; }
                            enum all                            { value 1; }
                        }
                        default "none";
                        description "Multi-homed prefix LFA backup path preference";
                    }

                } // container multi-homed-prefix

                ///nokia-conf/configure/router/ospf/loopfree-alternate/remote-lfa
                container remote-lfa {
                    presence "Enable the remote-lfa context";
                    description "Enable the remote-lfa context";

                    ///nokia-conf/configure/router/ospf/loopfree-alternate/remote-lfa/max-pq-cost
                    leaf max-pq-cost {
                        type uint32;
                        default "65535";
                        description "Destination max cost for reverse SPF calculation";
                    }

                    ///nokia-conf/configure/router/ospf/loopfree-alternate/remote-lfa/node-protect
                    container node-protect {
                        presence "Enable the node-protect context";
                        description "Enable the node-protect context";

                        ///nokia-conf/configure/router/ospf/loopfree-alternate/remote-lfa/node-protect/max-pq-nodes
                        leaf max-pq-nodes {
                            type uint32 {
                                range "1..32";
                            }
                            default "16";
                            description "Maximum number of PQ nodes found in the LFA SPFs";
                        }

                    } // container node-protect

                } // container remote-lfa

                ///nokia-conf/configure/router/ospf/loopfree-alternate/ti-lfa
                container ti-lfa {
                    presence "Enable the ti-lfa context";
                    description "Enable the ti-lfa context";

                    ///nokia-conf/configure/router/ospf/loopfree-alternate/ti-lfa/max-sr-frr-labels
                    leaf max-sr-frr-labels {
                        type uint32 {
                            range "0..3";
                        }
                        default "2";
                        description "Maximum number of labels the TI-LFA backup path can use";
                    }

                    ///nokia-conf/configure/router/ospf/loopfree-alternate/ti-lfa/node-protect
                    container node-protect {
                        presence "Enable the node-protect context";
                        description "Enable the node-protect context";

                    } // container node-protect

                } // container ti-lfa

            } // container loopfree-alternate

            ///nokia-conf/configure/router/ospf/database-export
            container database-export {
                presence "Enable the database-export context";
                description "Enable the database-export context";

                ///nokia-conf/configure/router/ospf/database-export/igp-identifier
                leaf igp-identifier {
                    type uint64;
                    description "IGP instance in the BGP-LS NLRI";
                }

                ///nokia-conf/configure/router/ospf/database-export/bgp-ls-identifier
                container bgp-ls-identifier {
                    presence "Enable the bgp-ls-identifier context";
                    description "Enable the bgp-ls-identifier context";

                    ///nokia-conf/configure/router/ospf/database-export/bgp-ls-identifier/value
                    leaf value {
                        type uint32;
                        default "0";
                        description "BGP-LS identifier sent in the BGP-LS NLRI";
                    }

                } // container bgp-ls-identifier

            } // container database-export

            ///nokia-conf/configure/router/ospf/asbr
            container asbr {
                presence "Enable the asbr context";
                description "Enable the asbr context";

                ///nokia-conf/configure/router/ospf/asbr/trace-path
                leaf trace-path {
                    type union {
                        type int32 {
                            range "0..31";
                        }
                        type enumeration {
                            enum none                           { value -1; }
                        }
                    }
                    default "none";
                    description "Domain identity";
                }

            } // container asbr

            ///nokia-conf/configure/router/ospf/entropy-label
            container entropy-label {
                description "Enter the entropy-label context";

                ///nokia-conf/configure/router/ospf/entropy-label/override-tunnel-elc
                leaf override-tunnel-elc {
                    type boolean;
                    default "false";
                    description "Enable override of received ELC advertisements";
                }

            } // container entropy-label

            ///nokia-conf/configure/router/ospf/segment-routing
            container segment-routing {
                description "Enter the segment-routing context";

                ///nokia-conf/configure/router/ospf/segment-routing/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of segment routing";
                }

                ///nokia-conf/configure/router/ospf/segment-routing/adj-sid-hold
                leaf adj-sid-hold {
                    type types-ospf:adj-sid-hold-type;
                    units "seconds";
                    default "15";
                    description "Adjacency SID hold time";
                }

                ///nokia-conf/configure/router/ospf/segment-routing/entropy-label
                leaf entropy-label {
                    type boolean;
                    description "Enable processing of received ELC signaled in the IGP";
                }

                ///nokia-conf/configure/router/ospf/segment-routing/export-tunnel-table
                leaf export-tunnel-table {
                    type types-igp:export-tunnel-table-type;
                    description "Export tunnel table";
                }

                ///nokia-conf/configure/router/ospf/segment-routing/srlb
                leaf srlb {
                    type leafref {
                        path "../../../mpls-labels/reserved-label-block/label-block-name";
                    }
                    description "Segment routing local block";
                }

                ///nokia-conf/configure/router/ospf/segment-routing/tunnel-mtu
                leaf tunnel-mtu {
                    type types-router:ip-mtu-ext;
                    description "Tunnel MTU size";
                }

                ///nokia-conf/configure/router/ospf/segment-routing/tunnel-table-pref
                leaf tunnel-table-pref {
                    type uint32 {
                        range "1..255";
                    }
                    default "10";
                    description "Preference of SR tunnels created by the IGP instance";
                }

                ///nokia-conf/configure/router/ospf/segment-routing/class-forwarding
                leaf class-forwarding {
                    type boolean;
                    default "false";
                    description "Allow CBF with ECMP over RSVP-TE LSPs as IGP shortcuts";
                }

                ///nokia-conf/configure/router/ospf/segment-routing/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/router/ospf/segment-routing/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }


                ///nokia-conf/configure/router/ospf/segment-routing/prefix-sid-range
                container prefix-sid-range {
                    presence "Enable the prefix-sid-range context";
                    description "Enable the prefix-sid-range context";

                    ///nokia-conf/configure/router/ospf/segment-routing/prefix-sid-range/type
                    choice type {
                        ///nokia-conf/configure/router/ospf/segment-routing/prefix-sid-range/type/global
                        case global {

                            ///nokia-conf/configure/router/ospf/segment-routing/prefix-sid-range/type/global/global
                            leaf global {
                                type empty;
                                description "BGP global SR range allocation";
                            }

                        }
                        ///nokia-conf/configure/router/ospf/segment-routing/prefix-sid-range/type/local
                        case local {

                            ///nokia-conf/configure/router/ospf/segment-routing/prefix-sid-range/type/local/start-label
                            leaf start-label {
                                type uint32 {
                                    range "0..524287";
                                }
                                sros-ext:auto-restart-to-modify;
                                default "0";
                                description "Lower bound value for the local label offset";
                            }

                            ///nokia-conf/configure/router/ospf/segment-routing/prefix-sid-range/type/local/max-index
                            leaf max-index {
                                type uint32 {
                                    range "0..524287";
                                }
                                sros-ext:auto-restart-to-modify;
                                default "1";
                                description "Upper bound value for the local SID index";
                            }

                        }
                    }
                } // container prefix-sid-range

                ///nokia-conf/configure/router/ospf/segment-routing/adjacency-sid
                container adjacency-sid {
                    description "Enter the adjacency-sid context";

                    ///nokia-conf/configure/router/ospf/segment-routing/adjacency-sid/allocate-dual-sids
                    leaf allocate-dual-sids {
                        type boolean;
                        default "false";
                        description "Allocate dual adjacency SIDs per interface";
                    }

                } // container adjacency-sid

                ///nokia-conf/configure/router/ospf/segment-routing/backup-node-sid
                container backup-node-sid {
                    presence "Enable the backup-node-sid context";
                    description "Enable the backup-node-sid context";

                    ///nokia-conf/configure/router/ospf/segment-routing/backup-node-sid/ip-prefix
                    leaf ip-prefix {
                        type types-sros:ipv4-unicast-prefix;
                        description "IP prefix and prefix length for the backup node SID";
                    }

                    ///nokia-conf/configure/router/ospf/segment-routing/backup-node-sid/type
                    choice type {
                        ///nokia-conf/configure/router/ospf/segment-routing/backup-node-sid/type/label
                        case label {

                            ///nokia-conf/configure/router/ospf/segment-routing/backup-node-sid/type/label/label
