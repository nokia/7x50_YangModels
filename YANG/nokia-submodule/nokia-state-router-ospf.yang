                            leaf destination-router-id {
                                type types-igp:router-id;
                                description "Identifies the destination node associated with this route.";
                            }

                            ///nokia-state/state/router/ospf/flexible-algorithms/flex-algo/area/paths/if-index
                            leaf if-index {
                                type int32 {
                                    range "1..2147483647";
