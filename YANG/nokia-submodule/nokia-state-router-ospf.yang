                    list area {
                        key "area-id";
                        description "Enter the area list instance";

                        ///nokia-state/state/router/ospf/flexible-algorithms/flex-algo/area/area-id
                        leaf area-id {
                            type types-sros:ipv4-address;
                            description "Area-ID attribute";
