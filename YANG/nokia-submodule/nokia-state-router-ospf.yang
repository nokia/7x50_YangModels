                }

                ///nokia-state/state/router/ospf/lfa-coverage/nodes-covered
                leaf nodes-covered {
                    type yang:gauge32;
                    description "The value of nodes-covered indicates in absolute numbers the number of nodes for which an LFA is found.";
                }

                ///nokia-state/state/router/ospf/lfa-coverage/nodes-total
                leaf nodes-total {
                    type yang:gauge32;
                    description "The value of total-nodes indicates in absolute numbers the number of nodes eligible for LFA calculation.";
                }

                ///nokia-state/state/router/ospf/lfa-coverage/nodes-coverage
                leaf nodes-coverage {
                    type yang:gauge32 {
                        range "0..100";
                    }
                    units "percent";
                    description "The value of coverage indicates how much LFA coverage is being obtained for the set of available LFA eligible nodes.";
                }

                ///nokia-state/state/router/ospf/lfa-coverage/pfx-covered
                leaf pfx-covered {
                    type yang:gauge32;
                    description "The value of pfx-covered indicates in absolute numbers the number of prefixes for which an LFA is found.";
                }

                ///nokia-state/state/router/ospf/lfa-coverage/pfx-total
                leaf pfx-total {
                    type yang:gauge32;
                    description "The value of total-pfx indicates in absolute numbers the number of prefixes eligible for LFA calculation.";
                }

                ///nokia-state/state/router/ospf/lfa-coverage/pfx-coverage
                leaf pfx-coverage {
                    type yang:gauge32 {
                        range "0..100";
                    }
                    units "percent";
                    description "The value of pfx-coverage indicates how much LFA prefix coverage is being obtained for the set of available LFA eligible prefixes.";
                }

            } // list lfa-coverage

            ///nokia-state/state/router/ospf/sr-lfa-coverage
            list sr-lfa-coverage {
                key "area-id sid-type";
                description "Enter the sr-lfa-coverage list instance";

                ///nokia-state/state/router/ospf/sr-lfa-coverage/area-id
                leaf area-id {
                    type types-sros:ipv4-address;
                    description "Area-ID attribute";
                }

                ///nokia-state/state/router/ospf/sr-lfa-coverage/sid-type
                leaf sid-type {
                    type enumeration {
                        enum node-sid                       { value 0; }
                        enum adj-sid                        { value 1; }
                    }
                    description
                        "The value specifies the SID type for which SR LFA coverage
                         has been computed.";
                }

                ///nokia-state/state/router/ospf/sr-lfa-coverage/lfa-covered
                leaf lfa-covered {
                    type uint32;
                    description "The value indicates the number of SIDs for which an LFA is found.";
                }

                ///nokia-state/state/router/ospf/sr-lfa-coverage/lfa-covered-percent
                leaf lfa-covered-percent {
                    type types-sros:percent;
                    description "The value indicates the portion of SIDs for which an LFA is found.";
                }

                ///nokia-state/state/router/ospf/sr-lfa-coverage/remote-lfa-covered
                leaf remote-lfa-covered {
                    type uint32;
                    description "The value indicates the number of SIDs for which an Remote LFA is found.";
                }

                ///nokia-state/state/router/ospf/sr-lfa-coverage/remote-lfa-covered-percent
                leaf remote-lfa-covered-percent {
                    type types-sros:percent;
                    description "The value indicates the portion of SIDs for which an Remote LFA is found.";
                }

                ///nokia-state/state/router/ospf/sr-lfa-coverage/ti-lfa-covered
                leaf ti-lfa-covered {
                    type uint32;
                    description "The value indicates the number of SIDs for which an Topology Independent LFA is found.";
                }

                ///nokia-state/state/router/ospf/sr-lfa-coverage/ti-lfa-covered-percent
                leaf ti-lfa-covered-percent {
                    type types-sros:percent;
                    description "The value indicates the portion of SIDs for which an LFA is found.";
                }

                ///nokia-state/state/router/ospf/sr-lfa-coverage/total-sid
                leaf total-sid {
                    type uint32;
                    description "The value indicates the number of SIDs eligible for SR LFA calculation";
                }

                ///nokia-state/state/router/ospf/sr-lfa-coverage/backup-total-sid
                leaf backup-total-sid {
                    type uint32;
                    description "The value indicates the number of SIDs for which an LFA, Remote LFA or Topology Independent LFA is found";
                }

                ///nokia-state/state/router/ospf/sr-lfa-coverage/backup-total-sid-percent
                leaf backup-total-sid-percent {
                    type uint32;
                    description "The value indicates the portion of SIDs for which an LFA, Remote LFA or Topology Independent LFA is found";
                }

            } // list sr-lfa-coverage

            ///nokia-state/state/router/ospf/as-linkstate-database
            list as-linkstate-database {
                key "ls-type adv-rtr-id ls-id";
                description "Enter the as-linkstate-database list instance";

                ///nokia-state/state/router/ospf/as-linkstate-database/ls-type
                leaf ls-type {
                    type uint32;
                    description "Link state type.";
                }

                ///nokia-state/state/router/ospf/as-linkstate-database/adv-rtr-id
                leaf adv-rtr-id {
                    type types-sros:ipv4-address;
                    description "Router that originated the link state advertisement.";
                }

                ///nokia-state/state/router/ospf/as-linkstate-database/ls-id
                leaf ls-id {
                    type types-sros:ipv4-address;
                    description "Link state advertisement identity.";
                }

                ///nokia-state/state/router/ospf/as-linkstate-database/sequence-nr
                leaf sequence-nr {
                    type uint32;
                }

                ///nokia-state/state/router/ospf/as-linkstate-database/age
                leaf age {
                    type int32;
                }

                ///nokia-state/state/router/ospf/as-linkstate-database/checksum
                leaf checksum {
                    type uint32;
                }

                ///nokia-state/state/router/ospf/as-linkstate-database/known-type
                leaf known-type {
                    type boolean;
                }

                ///nokia-state/state/router/ospf/as-linkstate-database/advertisement
                leaf advertisement {
                    type string {
                        length "1..9778";
                    }
                    description "Contents of the link state advertisement.";
                }

            } // list as-linkstate-database

            ///nokia-state/state/router/ospf/as-capabilities
            list as-capabilities {
                key "ls-type adv-rtr-id ls-id";
                description "Enter the as-capabilities list instance";

                ///nokia-state/state/router/ospf/as-capabilities/ls-type
                leaf ls-type {
                    type uint32;
                    description "Link state type.";
                }

                ///nokia-state/state/router/ospf/as-capabilities/adv-rtr-id
                leaf adv-rtr-id {
                    type types-sros:ipv4-address;
                    description "Router that originated the link state advertisement.";
                }

                ///nokia-state/state/router/ospf/as-capabilities/ls-id
                leaf ls-id {
                    type types-sros:ipv4-address;
                    description "Link state advertisement identity.";
                }

                ///nokia-state/state/router/ospf/as-capabilities/sequence-nr
                leaf sequence-nr {
                    type uint32;
                }

                ///nokia-state/state/router/ospf/as-capabilities/age
                leaf age {
                    type int32;
                }

                ///nokia-state/state/router/ospf/as-capabilities/checksum
                leaf checksum {
                    type uint32;
                }

                ///nokia-state/state/router/ospf/as-capabilities/advertisement
                leaf advertisement {
                    type string {
                        length "1..9778";
                    }
                    description "Contents of the link state advertisement.";
                }

            } // list as-capabilities

            ///nokia-state/state/router/ospf/route
            list route {
                key "route-type destination area";
                description "Enter the route list instance";

                ///nokia-state/state/router/ospf/route/route-type
                leaf route-type {
                    type types-ospf:route-type;
                    description "The route type os this route, network or router.";
                }

                ///nokia-state/state/router/ospf/route/destination
                leaf destination {
                    type types-sros:ip-prefix;
                    description "The value specifies the IP address and mask of the route destination.";
                }

                ///nokia-state/state/router/ospf/route/area
                leaf area {
                    type types-sros:ipv4-address;
                    description "Area through which the destination is reachable.";
                }

                ///nokia-state/state/router/ospf/route/path-type
                leaf path-type {
                    type types-ospf:route-path-type;
                    description "Select path type for the destination.";
                }

                ///nokia-state/state/router/ospf/route/dest-type
                leaf dest-type {
                    type types-ospf:route-dest-type;
                }

                ///nokia-state/state/router/ospf/route/rtm-status
                leaf rtm-status {
                    type types-ospf:rtm-status;
                    description "Indicates status in RTM.";
                }

                ///nokia-state/state/router/ospf/route/cost
                leaf cost {
                    type uint32;
                    description "The cost of this route, calculated during SPF.";
                }

                ///nokia-state/state/router/ospf/route/type2-cost
                leaf type2-cost {
                    type uint32;
                    description "The external type2 cost of this route, calculated during SPF.";
                }

                ///nokia-state/state/router/ospf/route/sid
                leaf sid {
                    type uint32;
                    description "The sid that corresponds to this destination";
                }

                ///nokia-state/state/router/ospf/route/sid-flags
                leaf sid-flags {
                    type bits {
                        bit bit-n                          { position 0; }
                        bit bit-p                          { position 1; }
                        bit bit-m                          { position 2; }
                        bit bit-e                          { position 3; }
                        bit bit-v                          { position 4; }
                        bit bit-l                          { position 5; }
                        bit bit-ia                         { position 6; }
                        bit bit-at                         { position 7; }
                        bit bit-b                          { position 8; }
                    }
                    description "The sid flags that corresponds to this destination";
                }

                ///nokia-state/state/router/ospf/route/next-hop
                list next-hop {
                    key "ecmp-index";
                    description "Enter the next-hop list instance";

                    ///nokia-state/state/router/ospf/route/next-hop/ecmp-index
                    leaf ecmp-index {
                        type types-router:ip-ecmp;
                        description "ECMP index of this route next-hop.";
                    }

                    ///nokia-state/state/router/ospf/route/next-hop/type
                    leaf type {
                        type enumeration {
                            enum unknown                        { value 0; }
                            enum direct                         { value 1; }
                            enum rsvp                           { value 2; }
                            enum srte                           { value 3; }
                            enum shortcut                       { value 4; }
                            enum blackhole                      { value 5; }
                            enum unnumbered                     { value 6; }
                            enum ip                             { value 7; }
                        }
                        description "Indicates the next hop type";
                    }

                    ///nokia-state/state/router/ospf/route/next-hop/ip-address
                    leaf ip-address {
                        type types-sros:ip-address;
                        description "The value specifies the IP address of the next hop for this route";
                    }

                    ///nokia-state/state/router/ospf/route/next-hop/interface
                    leaf interface {
                        type types-sros:interface-name;
                        description "The value specifies the interface of the next hop for this route";
                    }

                    ///nokia-state/state/router/ospf/route/next-hop/tunnel-id
                    leaf tunnel-id {
                        type uint32;
                        description "The tunnel-id given to this tunnelled next hop";
                    }

                    ///nokia-state/state/router/ospf/route/next-hop/area
                    leaf area {
                        type types-sros:ipv4-address;
                        description "The value specifies the area the shortest path was found in";
                    }

                    ///nokia-state/state/router/ospf/route/next-hop/rtm-status
                    leaf rtm-status {
                        type enumeration {
                            enum success                        { value 0; }
                            enum discarded                      { value 1; }
                        }
                        description "Indicates the next hop status in RTM";
                    }

                    ///nokia-state/state/router/ospf/route/next-hop/weight
                    container weight {
                        description "Enter the weight context";

                        ///nokia-state/state/router/ospf/route/next-hop/weight/configured
                        leaf configured {
                            type uint32;
                            description "The weight given to this next hop";
                        }

                        ///nokia-state/state/router/ospf/route/next-hop/weight/normalised
                        leaf normalised {
                            type uint32;
                            description "The normalised weight of this next hop for this route";
                        }

                    } // container weight

                } // list next-hop

            } // list route

            ///nokia-state/state/router/ospf/hostname
            list hostname {
                key "router-id";
                description "Enter the hostname list instance";

                ///nokia-state/state/router/ospf/hostname/router-id
                leaf router-id {
                    type types-igp:router-id;
                    description "Specifies the router-id for which we map the hostname";
                }

                ///nokia-state/state/router/ospf/hostname/name
                leaf name {
                    type types-sros:display-string;
                    description "The value indicates the mapping hostname for the corresponding router-id";
                }

            } // list hostname

        } // list ospf

    } // grouping state-router-ospf

}
