                    description "The number of interfaces configured in this area.";
                }

                ///nokia-state/state/router/ospf/area/virtual-link-count
                leaf virtual-link-count {
                    type uint32;
                    description "The total number of virtual links configured through this area.";
                }

                ///nokia-state/state/router/ospf/area/neighbor-count
                leaf neighbor-count {
                    type yang:gauge32;
                    description "The number of active neighbors in this area.";
                }

                ///nokia-state/state/router/ospf/area/last-spf-run-time
                leaf last-spf-run-time {
                    type yang:date-and-time;
                    description "The sys-up-time when intra-area SPF was last run on this area.";
                }

                ///nokia-state/state/router/ospf/area/spf-runs
                leaf spf-runs {
                    type yang:counter32;
                    description "The number of times that the intra-area route table has been calculated using this area's link-state database.";
                }

                ///nokia-state/state/router/ospf/area/nssa-translator-state
                leaf nssa-translator-state {
                    type enumeration {
                        enum enabled                        { value 1; }
                        enum elected                        { value 2; }
                        enum disabled                       { value 3; }
                    }
                    description "The current NSSA translator state";
                }

                ///nokia-state/state/router/ospf/area/nssa-translator-events
                leaf nssa-translator-events {
                    type yang:counter32;
                    description "The number of translator state changes that have occurred since the last boot-up.";
                }

                ///nokia-state/state/router/ospf/area/lsa-totals
                container lsa-totals {
                    description "Enter the lsa-totals context";

                    ///nokia-state/state/router/ospf/area/lsa-totals/router-lsa
                    leaf router-lsa {
                        type yang:gauge32;
                        description "The number of router LSAs in this area's link-state database.";
                    }

                    ///nokia-state/state/router/ospf/area/lsa-totals/network-lsa
                    leaf network-lsa {
                        type yang:gauge32;
                        description "The number of network LSAs in this area's link-state database.";
                    }

                    ///nokia-state/state/router/ospf/area/lsa-totals/network-summary-lsa
                    leaf network-summary-lsa {
                        type yang:gauge32;
                        description "The number of network summary LSAs in this area's link-state database.";
                    }

                    ///nokia-state/state/router/ospf/area/lsa-totals/asbr-summary-lsa
                    leaf asbr-summary-lsa {
                        type yang:gauge32;
                        description "The number of ASBR summary LSAs in this area's link-state database.";
                    }

                    ///nokia-state/state/router/ospf/area/lsa-totals/nssa-lsa
                    leaf nssa-lsa {
                        type yang:gauge32;
                        description "The number of NSSA LSAs in this area's link-state database.";
                    }

                    ///nokia-state/state/router/ospf/area/lsa-totals/area-opaque-lsa
                    leaf area-opaque-lsa {
                        type yang:gauge32;
                        description "The number of NSSA LSAs in this area's link-state database.";
                    }

                    ///nokia-state/state/router/ospf/area/lsa-totals/unknown-lsa
                    leaf unknown-lsa {
                        type yang:gauge32;
                        description "The number of unknown LSA advertisements in this area's link-state database.";
                    }

                    ///nokia-state/state/router/ospf/area/lsa-totals/total
                    leaf total {
                        type yang:gauge32;
                        description "The number of area scope LSAs within this area.";
                    }

                    ///nokia-state/state/router/ospf/area/lsa-totals/total-lsa-cksum-sum
                    leaf total-lsa-cksum-sum {
                        type int32;
                        description "The 32-bit unsigned sum of the area scope LSA checksums contained in this area's link-state database. The sum can be used to determine if there has been a change in a router's link-state database, and to compare the link-state database of two routers.";
                    }

                } // container lsa-totals

                ///nokia-state/state/router/ospf/area/lsa-filter-totals
                container lsa-filter-totals {
                    description "Enter the lsa-filter-totals context";

                    ///nokia-state/state/router/ospf/area/lsa-filter-totals/export-filtered
                    leaf export-filtered {
                        type yang:gauge32;
                        description "The number of LSAs not sent due to area export policy.";
                    }

                    ///nokia-state/state/router/ospf/area/lsa-filter-totals/import-filtered
                    leaf import-filtered {
                        type yang:gauge32;
                        description "The number of LSAs not sent due to area import policy.";
                    }

                } // container lsa-filter-totals

                ///nokia-state/state/router/ospf/area/interface
                list interface {
                    key "interface-name";
                    description "Enter the interface list instance";

                    ///nokia-state/state/router/ospf/area/interface/interface-name
                    leaf interface-name {
                        type types-sros:interface-name;
                        description "Router interface name";
                    }

                    ///nokia-state/state/router/ospf/area/interface/type
                    leaf type {
                        type enumeration {
                            enum broadcast                      { value 1; }
                            enum non-broadcast                  { value 2; }
                            enum point-to-point                 { value 3; }
                            enum secondary                      { value 6; }
                            enum p2mp-nbma                      { value 9; }
                        }
                        description "the value of type indicates the operational OSPF interface type.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/oper-state
                    leaf oper-state {
                        type enumeration {
                            enum down                           { value 1; }
                            enum loopback                       { value 2; }
                            enum waiting                        { value 3; }
                            enum point-to-point                 { value 4; }
                            enum designated-router              { value 5; }
                            enum backup-designated-router       { value 6; }
                            enum other-designated-router        { value 7; }
                        }
                        description "the OSPF interface state.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/last-enabled-time
                    leaf last-enabled-time {
                        type yang:date-and-time;
                        description "the value of last-enabled-time indicates the sys-up-time value when ospf-if-admin-stat was last set to enabled (1) to run the ospf on this interface.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/oper-mtu
                    leaf oper-mtu {
                        type int32;
                        description "the value of oper-MTU indicates the operational size of the largest packet which can be sent/received on this OSPF interface, specified in octets. this size DOES include the underlying IP header length, but not the underlying layer headers/trailers.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/oper-metric
                    leaf oper-metric {
                        type int32 {
                            range "0..65535";
                        }
                        description "the value of metric-value indicates the metric value this interface is using. the default value of the metric is (reference bandwidth / if-speed). the value of the reference bandwidth is configured by the reference-bandwidth object.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/neighbor-count
                    leaf neighbor-count {
                        type yang:gauge32;
                        description "The total number of OSPF neighbors adjacent on this interface, in a state of INIT or greater, since admin-state was last set to 'enabled'.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/dr-id
                    leaf dr-id {
                        type types-sros:ipv4-address;
                        description "the value of DR-id indicates the router ID of the designated router.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/dr-ip-address
                    leaf dr-ip-address {
                        type types-sros:ip-address-with-zone;
                        description "the IP address of the designated router.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/bdr-id
                    leaf bdr-id {
                        type types-sros:ipv4-address;
                        description "the value of BDR-id indicates the router ID of the backup designated router.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/bdr-ip-address
                    leaf bdr-ip-address {
                        type types-sros:ip-address-with-zone;
                        description "the IP address of the backup designated router.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/link-lsa-count
                    leaf link-lsa-count {
                        type yang:gauge32;
                        description "the value of link-lsa-count indicates the total number of link-scope link-state advertisements in this link's link-state database.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/link-lsa-cksum-sum
                    leaf link-lsa-cksum-sum {
                        type int32;
                        description "the value of link-lsa-cksum-sum indicates the 32-bit unsigned sum of the link-scope link-state advertisements' LS checksums contained in this link's link-state database. the sum can be used to determine if there has been a change in a router's link-state database, and to compare the link state database of two routers.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/md5transmit-key-id
                    leaf md5transmit-key-id {
                        type int32 {
                            range "0..255";
                        }
                        description "the value of md5transmit-key-id indicates the index value used to access the row entry in the md5key-table that specifies the authentication type and key transmitted on this OSPF interface. this object is valid only when version has the value 'version2'. if md5transmit-key-id has a value of 0, it means that no keys are configured.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/local-ip-address
                    leaf local-ip-address {
                        type types-sros:ip-address-with-zone;
                        description "the value of local-ip-address indicates the IP address of this OSPF interface.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/te-metric
                    leaf te-metric {
                        type uint32 {
                            range "0..16777215";
                        }
                        description "the value of te-metric indicates the MPLS TE metric configured for this interface. this metric is flooded out in the TE metric sub-tlv in the OSPF TE LS-as. depending on the configuration, either the TE metric value or the native OSPF metric value is used in cspf computations.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/te-state
                    leaf te-state {
                        type types-sros:oper-state;
                        description "the value of te-state indicates the MPLS interface TE status from OSPF standpoint.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/admin-group
                    leaf admin-group {
                        type uint32;
                        description "the value of admin-group indicates the bit-map inherited from MPLS interface that identifies the admin groups to which this interface belongs. if bit 'n' is set, then the interface belongs to the admin group with value 'n'.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/events
                    leaf events {
                        type yang:counter32;
                        description "the value of events indicates the number of times this OSPF interface has changed its state, or an error has occurred.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/last-event-time
                    leaf last-event-time {
                        type yang:date-and-time;
                        description "the value of last-event-time indicates the value of sys-up-time when an event was last associated with this OSPF interface.";
                    }

                    ///nokia-state/state/router/ospf/area/interface/ldp-sync
                    container ldp-sync {
                        description "Enter the ldp-sync context";

                        ///nokia-state/state/router/ospf/area/interface/ldp-sync/ldp-sync-state
                        leaf ldp-sync-state {
                            type types-sros:oper-state;
                            description "the value of ldp-sync-state indicates if IGP-LDP synchronization is enabled on this interface. ldp-sync-state is derived from ldp-sync-admin-state and also whether it was enabled at the IP layer from OSPF standpoint.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/ldp-sync/ldp-sync-max-metric
                        leaf ldp-sync-max-metric {
                            type boolean;
                            description "the value of ldp-sync-max-metric indicates if OSPF advertised maximum metric value for the interface, depending on whether ldp-sync-state is enabled on this interface.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/ldp-sync/ldp-sync-timer-state
                        leaf ldp-sync-timer-state {
                            type enumeration {
                                enum wait-for-ldp-adj               { value 1; }
                                enum timer-active                   { value 2; }
                                enum ldp-exchg-done                 { value 3; }
                                enum timer-expired                  { value 4; }
                                enum manual-exit                    { value 5; }
                                enum disabled                       { value 6; }
                            }
                            description "the value of ldp-sync-timer-state indicates the state of the ldp-sync-time-left on this OSPF interface.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/ldp-sync/ldp-sync-time-left
                        leaf ldp-sync-time-left {
                            type uint32 {
                                range "0|1..1800";
                            }
                            units "seconds";
                            description "the value of ldp-sync-time-left indicates the time left before OSPF reverts back to advertising normal metric for this interface. this is applicable only when ldp-sync-max-metric is 'true'.";
                        }

                    } // container ldp-sync

                    ///nokia-state/state/router/ospf/area/interface/shared-sid
                    container shared-sid {
                        description "Enter the shared-sid context";

                        ///nokia-state/state/router/ospf/area/interface/shared-sid/type
                        choice type {
                            ///nokia-state/state/router/ospf/area/interface/shared-sid/type/label
                            case label {

                                ///nokia-state/state/router/ospf/area/interface/shared-sid/type/label/label
                                leaf label {
                                    type int64 {
                                        range "1..4294967295";
                                    }
                                    description "Specifies IPvX node SID label for this interface.";
                                }

                            }
                            ///nokia-state/state/router/ospf/area/interface/shared-sid/type/index
                            case index {

                                ///nokia-state/state/router/ospf/area/interface/shared-sid/type/index/index
                                leaf index {
                                    type int64 {
                                        range "0..4294967295";
                                    }
                                    description "Specifies IPvX node SID index for this interface.";
                                }

                            }
                        }
                    } // container shared-sid

                    ///nokia-state/state/router/ospf/area/interface/packets
                    container packets {
                        description "Enter the packets context";

                        ///nokia-state/state/router/ospf/area/interface/packets/tx-total
                        leaf tx-total {
                            type yang:counter32;
                            description "The total number of OSPF packets transmitted since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/tx-hello
                        leaf tx-hello {
                            type yang:counter32;
                            description "The total number of OSPF hello packets transmitted since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/tx-db-description
                        leaf tx-db-description {
                            type yang:counter32;
                            description "The total number of OSPF database description packets transmitted since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/tx-ls-request
                        leaf tx-ls-request {
                            type yang:counter32;
                            description "The total number of OSPF link state requests (LS-rs)transmitted since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/tx-ls-update
                        leaf tx-ls-update {
                            type yang:counter32;
                            description "The total number of OSPF link state updates (LS-us) transmitted since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/tx-ls-ack
                        leaf tx-ls-ack {
                            type yang:counter32;
                            description "The total number of OSPF link state acknowledgements transmitted since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/rx-total
                        leaf rx-total {
                            type yang:counter32;
                            description "The total number of OSPF packets received since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/rx-hello
                        leaf rx-hello {
                            type yang:counter32;
                            description "The total number of OSPF hello packets received since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/rx-db-description
                        leaf rx-db-description {
                            type yang:counter32;
                            description "The total number of OSPF database description packets received since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/rx-ls-request
                        leaf rx-ls-request {
                            type yang:counter32;
                            description "The total number of link state requests (LS-rs) received since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/rx-ls-update
                        leaf rx-ls-update {
                            type yang:counter32;
                            description "The total number of link state updates (LS-us) received since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/rx-ls-ack
                        leaf rx-ls-ack {
                            type yang:counter32;
                            description "The total number of link state acknowledgements received since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/discarded
                        leaf discarded {
                            type yang:counter32;
                            description "The total number of OSPF packets discarded since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/retransmits
                        leaf retransmits {
                            type yang:counter32;
                            description "The total number of OSPF retransmits since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/tx-failures
                        leaf tx-failures {
                            type yang:counter32;
                            description "The total number of OSPF transmit failures since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/bad-version
                        leaf bad-version {
                            type yang:counter32;
                            description "The total number of OSPF packets received with bad OSPF version numbers since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/bad-network
                        leaf bad-network {
                            type yang:counter32;
                            description "The total number of OSPF packets received with invalid network or mask since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/bad-area
                        leaf bad-area {
                            type yang:counter32;
                            description "The total number of OSPF packets received with an area mismatch since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/bad-dest-address
                        leaf bad-dest-address {
                            type yang:counter32;
                            description "The total number of OSPF packets received with the incorrect IP destination address since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/bad-neighbors
                        leaf bad-neighbors {
                            type yang:counter32;
                            description "The total number of OSPF packets received where the neighbor information does not match the information this router has for the neighbor since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/bad-packet-type
                        leaf bad-packet-type {
                            type yang:counter32;
                            description "The total number of OSPF packets received with an invalid OSPF packet type since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/bad-length
                        leaf bad-length {
                            type yang:counter32;
                            description "The total number of OSPF packets received with a total length not equal to the length given in the packet itself since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/bad-hello-interval
                        leaf bad-hello-interval {
                            type yang:counter32;
                            description "the value of bad-hello-intervals indicates the total number of OSPF packets received where the hello interval given in packet was not equal to that configured on this interface since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/bad-dead-interval
                        leaf bad-dead-interval {
                            type yang:counter32;
                            description "The total number of OSPF packets received where the dead interval given in the packet was not equal to that configured on this interface since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/bad-options
                        leaf bad-options {
                            type yang:counter32;
                            description "The total number of OSPF packets received with an option that does not match those configured for this interface or area since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/rx-bad-checksum
                        leaf rx-bad-checksum {
                            type yang:counter32;
                            description "The count of LS-as received with bad checksums.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/bad-auth-type
                        leaf bad-auth-type {
                            type yang:counter32;
                            description "The total number of OSPF packets received with an invalid authorization type since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/auth-failures
                        leaf auth-failures {
                            type yang:counter32;
                            description "The total number of OSPF packets received with an invalid authorization key since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/packets/bad-virtual-link
                        leaf bad-virtual-link {
                            type yang:counter32;
                            description "The total number of OSPF packets received that are destined to a virtual link that does not exist since admin-state was last set to 'enabled'.";
                        }

                    } // container packets

                    ///nokia-state/state/router/ospf/area/interface/last-packet-drop
                    container last-packet-drop {
                        description "Enter the last-packet-drop context";

                        ///nokia-state/state/router/ospf/area/interface/last-packet-drop/reason
                        leaf reason {
                            type types-ospf:packet-drop-reason;
                            description "The reason for dropping the packet.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/last-packet-drop/packet-type
                        leaf packet-type {
                            type uint8;
                            description "the OSPF packet type of the dropped packet from the packet header.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/last-packet-drop/router-id
                        leaf router-id {
                            type types-sros:ipv4-address;
                            description "the OSPF router ID of the dropped packet from the packet header.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/last-packet-drop/area-id
                        leaf area-id {
                            type types-sros:ipv4-address;
                            description "the OSPF area ID of the dropped packet from the packet header.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/last-packet-drop/checksum
                        leaf checksum {
                            type uint32;
                            description "the OSPF checksum of the dropped packet from the packet header.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/last-packet-drop/drop-time
                        leaf drop-time {
                            type yang:date-and-time;
                            description "the time when the drop occured.";
                        }

                    } // container last-packet-drop

                    ///nokia-state/state/router/ospf/area/interface/neighbor
                    list neighbor {
                        key "address";
                        description "Add a list entry for neighbor";

                        ///nokia-state/state/router/ospf/area/interface/neighbor/address
                        leaf address {
                            type types-sros:ipv4-unicast-address;
                            description "IPv4 address of the OSPFv2 neighbor";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor/priority
                        leaf priority {
                            type int32 {
                                range "0..255";
                            }
                            description
                                "Specifies the priority of this neighbor in the designated router election algorithm.

                                 The value 0 signifies that the neighbor is not eligible to become
                                 the designated router on this particular network.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor/router-id
                        leaf router-id {
                            type types-sros:ipv4-address;
                            description
                                "Indicates a 32-bit integer uniquely identifying the neighboring router in the Autonomous System.

                                 A value of 0 is returned until a Hello is received from the configured neighbor.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor/oper-state
                        leaf oper-state {
                            type types-ospf:neighbor-oper-state-type;
                            description "Indicates the State of the relationship with this Neighbor.";
                        }

                    } // list neighbor

                    ///nokia-state/state/router/ospf/area/interface/neighbor-state
                    list neighbor-state {
                        key "router-id";
                        description "Enter the neighbor-state list instance";

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/router-id
                        leaf router-id {
                            type types-sros:ipv4-address;
                            description "Router ID of the neighbor.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/address
                        leaf address {
                            type types-sros:ip-address-with-zone;
                            description "the value of address indicates the IP address of the neighbor associated with the local link.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/priority
                        leaf priority {
                            type int32 {
                                range "0..255";
                            }
                            description
                                "Specifies the priority of this neighbor in the designated router election algorithm.

                                 The value 0 signifies that the neighbor is not eligible to become
                                 the designated router on this particular network.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/bfd-tracking
                        leaf bfd-tracking {
                            type boolean;
                            description "the value of bfd-tracking indicates whether a BFD session is being maintained to track the forwarding path to this neighbor or not.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/dr-id
                        leaf dr-id {
                            type types-sros:ipv4-address;
                            description "the value of dr-id indicates the DR value advertised in the HELLO packets received from the OSPF neighbor. if it is not a broadcast interface dr-id will be zero.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/bdr-id
                        leaf bdr-id {
                            type types-sros:ipv4-address;
                            description "the value of bdr-id indicates the BDR value advertised in the HELLO packets received from the OSPF neighbor. if it is not a broadcast interface bdr-id will be zero.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/bfd-strict-state
                        leaf bfd-strict-state {
                            type enumeration {
                                enum disabled                       { value 0; }
                                enum up                             { value 1; }
                                enum blocked                        { value 2; }
                                enum held                           { value 3; }
                            }
                            description "the value of bfd-strict-state indicates the state of the strict-bfd session";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/group-if-index
                        leaf group-if-index {
                            type int32 {
                                range "1..2147483647";
                            }
                            description "The value of group-if-index indicates the interface ID of the group interface over which the neighbor can be reached.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/group-if-oper-mtu
                        leaf group-if-oper-mtu {
                            type int32;
                            description "The value of group-if-oper-mtu indicates the operational size of the largest packet which can be sent/received on the neighbor's group interface, specified in octets. this size DOES include the underlying IP header length, but not the underlying layer headers/trailers.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/oper-state
                        leaf oper-state {
                            type enumeration {
                                enum down                           { value 1; }
                                enum attempt                        { value 2; }
                                enum init                           { value 3; }
                                enum two-way                        { value 4; }
                                enum exchange-start                 { value 5; }
                                enum exchange                       { value 6; }
                                enum loading                        { value 7; }
                                enum full                           { value 8; }
                            }
                            description "Indicates the State of the relationship with this Neighbor.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/options
                        leaf options {
                            type int32;
                            description "the value of options indicates a bit mask corresponding to the neighbor's options field.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/hello-suppressed
                        leaf hello-suppressed {
                            type boolean;
                            description "the value of hello-suppressed indicates whether hellos are being suppressed to the neighbor.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/restart-helper-status
                        leaf restart-helper-status {
                            type enumeration {
                                enum not-helping                    { value 1; }
                                enum helping                        { value 2; }
                            }
                            description "the value of restart-helper-status indicates whether the router is acting as a graceful restart helper for the neighbor.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/restart-helper-age
                        leaf restart-helper-age {
                            type int32 {
                                range "0..1800";
                            }
                            units "seconds";
                            description "the value of restart-helper-age indicates the remaining time in the current OSPF graceful restart interval, if the router is acting as a restart helper for the neighbor.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/restart-helper-exit-rc
                        leaf restart-helper-exit-rc {
                            type enumeration {
                                enum none                           { value 1; }
                                enum in-progress                    { value 2; }
                                enum completed                      { value 3; }
                                enum timed-out                      { value 4; }
                                enum topology-changed               { value 5; }
                                enum bfd-down                       { value 6; }
                            }
                            description "the value of restart-helper-exit-rc indicates the outcome of the last attempt at acting as a graceful restart helper for the neighbor. none no restart has yet been attempted. in-progress A restart attempt is currently underway. completed the last restart completed successfully. timed-out the last restart timed out. topology-changed the last restart was aborted due to a topology change.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/last-restart-time
                        leaf last-restart-time {
                            type yang:date-and-time;
                            description "the value of last-restart-time indicates the last time the neighbor attempted restart.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/restart-reason
                        leaf restart-reason {
                            type types-ospf:restart-reason-type;
                            description "the value of restart-reason indicates the OSPF neighbor's graceful restart reason.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/up-time
                        leaf up-time {
                            type int32 {
                                range "0..2147483647";
                            }
                            units "centiseconds";
                            description "the value of up-time indicates the uninterrupted time, in hundredths of seconds, the adjacency to this neighbour has been up.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/last-event
                        leaf last-event {
                            type types-ospf:neighbor-event;
                            description "Indicates the last event as defined in RFC causing an oper-state change for this Neighbor.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/last-event-time
                        leaf last-event-time {
                            type yang:date-and-time;
                            description "the value of last-event-time indicates the value of sys-up-time when the last event occurred that affected the adjacency to the neighbour.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/dead-time-outstanding
                        leaf dead-time-outstanding {
                            type uint32 {
                                range "0..2147483647";
                            }
                            units "seconds";
                            description "the value of dead-time-outstanding indicates the amount of time in seconds until the dead-router interval expires. normally, this value should not be significantly smaller than the configured dead router interval minus one hello interval.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/statistics
                        container statistics {
                            description "Enter the statistics context";

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/statistics/events
                            leaf events {
                                type yang:counter32;
                                description "the value of events indicates the number of times this neighbor relationship has changed state, or an error has occurred.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/statistics/ls-retrans-q-len
                            leaf ls-retrans-q-len {
                                type yang:gauge32;
                                description "the value of ls-retrans-Q-len indicates the current length of the retransmission queue.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/statistics/bad-nbr-states
                            leaf bad-nbr-states {
                                type yang:counter32;
                                description "the value of bad-nbr-states indicates the total number of OSPF packets received when the neighbor state was not expecting to receive this packet type since admin-state was last set to 'enabled'.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/statistics/lsa-install-failed
                            leaf lsa-install-failed {
                                type yang:counter32;
                                description "the value of lsa-install-failed indicates the total number of times an LSA could not be installed into the LSDB due to a resource allocation issue since admin-state was last set to 'enabled'.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/statistics/bad-seq-nums
                            leaf bad-seq-nums {
                                type yang:counter32;
                                description "the value of bad-seq-nums indicates the total number of times when a database description packet was received with a sequence number mismatch since admin-state was last set to 'enabled'.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/statistics/bad-mtu
                            leaf bad-mtu {
                                type yang:counter32;
                                description "the value of bad-MT-us indicates the total number of times when the MTU in a received database description packet was larger than the MTU of the receiving interface since admin-state was last set to 'enabled'.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/statistics/bad-packets
                            leaf bad-packets {
                                type yang:counter32;
                                description "the value of bad-packets indicates the total number of times when an LS update was received with an illegal LS type or an option mismatch since admin-state was last set to 'enabled'.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/statistics/lsa-not-in-lsdbs
                            leaf lsa-not-in-lsdbs {
                                type yang:counter32;
                                description "the value of lsa-not-in-lsdbs indicates the total number of times when an LS request was received for an LSA not installed in the LSDB of this router since admin-state was last set to 'enabled'.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/statistics/option-mismatches
                            leaf option-mismatches {
                                type yang:counter32;
                                description "the value of option-mismatches indicates the total number of times when a LS update was received with an option mismatch since admin-state was last set to 'enabled'.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/statistics/duplicates
                            leaf duplicates {
                                type yang:counter32;
                                description "the value of duplicates indicates the total number of times when a duplicate database description packet was received during the exchange state since admin-state was last set to 'enabled'.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/statistics/num-restarts
                            leaf num-restarts {
                                type yang:counter32;
                                description "the value of num-restarts indicates the number of times the neighbor has attempted restart.";
                            }

                        } // container statistics

                        ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency
                        container sr-adjacency {
                            description "Enter the sr-adjacency context";

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/sid-type
                            leaf sid-type {
                                type enumeration {
                                    enum none                           { value 0; }
                                    enum index                          { value 1; }
                                    enum mpls-label                     { value 2; }
                                }
                                description "the value of sid-type indicates how the value of sid-value shall be interpreted.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/sid-value
                            leaf sid-value {
                                type uint32;
                                description "the value of the object sid-value indicates the IPv4 SID label or index for this adjacency.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/sid-protected
                            leaf sid-protected {
                                type boolean;
                                description "The value of the object sid-protected indicates the if the SID for this adjacency is eligible for protection.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/secondary-sid-type
                            leaf secondary-sid-type {
                                type enumeration {
                                    enum none                           { value 0; }
                                    enum index                          { value 1; }
                                    enum mpls-label                     { value 2; }
                                }
                                description "the value of secondary-sid-type indicates how the value of secondary-sid-value shall be interpreted.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/secondary-sid-value
                            leaf secondary-sid-value {
                                type uint32;
                                description "the value of the object sid-value indicates the secondary SID label or index for this adjacency.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/secondary-sid-protected
                            leaf secondary-sid-protected {
                                type boolean;
                                description "The value of the object secondary-sid-protected indicates the if the secondary SID for this adjacency is eligible for protection.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/secondary-sid-mtu
                            leaf secondary-sid-mtu {
                                type uint32;
                                description "The MTU of the link used by this adjacency using the secondary sid.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/sid-hold-timer
                            leaf sid-hold-timer {
                                type int32 {
                                    range "0..65535";
                                }
                                units "seconds";
                                description "The remaining holding time for this adjacency-sid.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/pgid
                            leaf pgid {
                                type uint32;
                                description "The value indicates the PGID for this adjacency.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/mtu
                            leaf mtu {
                                type uint32;
                                description "The MTU of the link used by this adjacency.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/lfa-nexthop
                            leaf lfa-nexthop {
                                type types-sros:ip-address;
                                description "The IP address associated with the LFA nexthop.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/lfa-interface
                            leaf lfa-interface {
                                type types-sros:interface-name;
                                description "The value specifies the interface index of the outgoing interface for the adjacency backup.";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/lfa-tunnel-type
                            leaf lfa-tunnel-type {
                                type enumeration {
                                    enum none                           { value 0; }
                                    enum rsvp                           { value 1; }
                                    enum ldp                            { value 2; }
                                }
                                description "Type of tunnel used for LFA";
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/lfa-tunnel-id
                            leaf lfa-tunnel-id {
                                type types-sros:tunnel-id;
                            }

                            ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/lfa-label-stack
                            list lfa-label-stack {
                                key "label-index";
                                description "Enter the lfa-label-stack list instance";

                                ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/lfa-label-stack/label-index
                                leaf label-index {
                                    type uint32;
                                    description
                                        "An adjacency can have a backup label stack.
                                         Label-index 0 indicates the first label which can be a destination
                                         node-sid , a node-sid towards a P or PQ node or an adj-sid to a
                                         Q node. The last label always indicates the destination label
                                         unless PHP is enabled.";
                                }

                                ///nokia-state/state/router/ospf/area/interface/neighbor-state/sr-adjacency/lfa-label-stack/label
                                leaf label {
                                    type uint32;
                                    description
                                        "A label from an adjacency backup. The label can be a destination
                                         node-sid, an adj-sid or a node-sid towards a P or PQ node.";
                                }

                            } // list lfa-label-stack

                        } // container sr-adjacency

                    } // list neighbor-state

                    ///nokia-state/state/router/ospf/area/interface/linkstate-database
                    list linkstate-database {
                        key "ls-type adv-rtr-id ls-id";
                        description "Enter the linkstate-database list instance";

                        ///nokia-state/state/router/ospf/area/interface/linkstate-database/ls-type
                        leaf ls-type {
                            type uint32;
                            description "Link state type.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/linkstate-database/adv-rtr-id
                        leaf adv-rtr-id {
                            type types-sros:ipv4-address;
                            description "Router that originated the link state advertisement.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/linkstate-database/ls-id
                        leaf ls-id {
                            type types-sros:ipv4-address;
                            description "Link state advertisement identity.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/linkstate-database/sequence-nr
                        leaf sequence-nr {
                            type uint32;
                        }

                        ///nokia-state/state/router/ospf/area/interface/linkstate-database/age
                        leaf age {
                            type int32;
                        }

                        ///nokia-state/state/router/ospf/area/interface/linkstate-database/checksum
                        leaf checksum {
                            type uint32;
                        }

                        ///nokia-state/state/router/ospf/area/interface/linkstate-database/known-type
                        leaf known-type {
                            type boolean;
                        }

                        ///nokia-state/state/router/ospf/area/interface/linkstate-database/advertisement
                        leaf advertisement {
                            type string {
                                length "1..9778";
                            }
                            description "Contents of the link state advertisement.";
                        }

                    } // list linkstate-database

                    ///nokia-state/state/router/ospf/area/interface/capabilities
                    list capabilities {
                        key "ls-type adv-rtr-id ls-id";
                        description "Enter the capabilities list instance";

                        ///nokia-state/state/router/ospf/area/interface/capabilities/ls-type
                        leaf ls-type {
                            type uint32;
                            description "Link state type.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/capabilities/adv-rtr-id
                        leaf adv-rtr-id {
                            type types-sros:ipv4-address;
                            description "Router that originated the link state advertisement.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/capabilities/ls-id
                        leaf ls-id {
                            type types-sros:ipv4-address;
                            description "Link state advertisement identity.";
                        }

                        ///nokia-state/state/router/ospf/area/interface/capabilities/sequence-nr
                        leaf sequence-nr {
                            type uint32;
                        }

                        ///nokia-state/state/router/ospf/area/interface/capabilities/age
                        leaf age {
                            type int32;
                        }

                        ///nokia-state/state/router/ospf/area/interface/capabilities/checksum
                        leaf checksum {
                            type uint32;
                        }

                        ///nokia-state/state/router/ospf/area/interface/capabilities/advertisement
                        leaf advertisement {
                            type string {
                                length "1..9778";
                            }
                            description "Contents of the link state advertisement.";
                        }

                    } // list capabilities

                } // list interface

                ///nokia-state/state/router/ospf/area/virtual-link
                list virtual-link {
                    key "router-id transit-area";
                    description "Enter the virtual-link list instance";

                    ///nokia-state/state/router/ospf/area/virtual-link/router-id
                    leaf router-id {
                        type types-sros:ipv4-address;
                        description "Router identity of the virtual link neighbor";
                    }

                    ///nokia-state/state/router/ospf/area/virtual-link/transit-area
                    leaf transit-area {
                        type leafref {
                            path "../../../area/area-id";
                        }
                        description "Transit area linking the backbone to not-connected area";
                    }

                    ///nokia-state/state/router/ospf/area/virtual-link/oper-state
                    leaf oper-state {
                        type enumeration {
                            enum down                           { value 1; }
                            enum point-to-point                 { value 4; }
                        }
                        description "the value of state indicates the operational state of this virtual interface.";
                    }

                    ///nokia-state/state/router/ospf/area/virtual-link/last-enabled-time
                    leaf last-enabled-time {
                        type yang:date-and-time;
                        description "the value of last-enabled-time indicates the sys-up-time value when this entry row entry was enabled.";
                    }

                    ///nokia-state/state/router/ospf/area/virtual-link/cost
                    leaf cost {
                        type int32 {
                            range "0..65535";
                        }
                        description "the value of cost indicates the OSPF virtual interface cost represents the cost to traverse the transit area to the adjacent router. if multiple routes are available the lowest cost link will be used to maintain the adjacency and route traffic across the transit area.";
                    }

                    ///nokia-state/state/router/ospf/area/virtual-link/link-lsa-count
                    leaf link-lsa-count {
                        type yang:gauge32;
                        description "the value of link-lsa-count indicates the total number of link-scope link-state advertisements in this virtual link's link-state database.";
                    }

                    ///nokia-state/state/router/ospf/area/virtual-link/link-lsa-cksum-sum
                    leaf link-lsa-cksum-sum {
                        type int32;
                        description "the value of link-lsa-cksum-sum indicates the 32-bit unsigned sum of the link-scope link-state advertisements' LS checksums contained in this virtual link's link-state database. the sum can be used to determine if there has been a change in a router's link-state database, and to compare the link-state database of two routers.";
                    }

                    ///nokia-state/state/router/ospf/area/virtual-link/link-unk-lsa-count
                    leaf link-unk-lsa-count {
                        type yang:gauge32;
                        description "the value of link-unk-lsa-count indicates the total number of unknown link-scope link-state advertisements in this virtual link's link-state database.";
                    }

                    ///nokia-state/state/router/ospf/area/virtual-link/link-unk-lsa-cksum-sum
                    leaf link-unk-lsa-cksum-sum {
                        type int32;
                        description "the value of link-unk-lsa-cksum-sum indicates the 32-bit unsigned sum of the unknown link-scope link-state advertisements' LS checksums contained in this virtual link's link-state database. the sum can be used to determine if there has been a change in a router's link state database, and to compare the link-state database of two routers.";
                    }

                    ///nokia-state/state/router/ospf/area/virtual-link/md5transmit-key-id
                    leaf md5transmit-key-id {
                        type int32 {
                            range "0..255";
                        }
                        description "the value of md5transmit-key-id indicates the index value used to access the row entry in the md5key-table that specifies the authentication type and key transmitted on this OSPF virtual interface. this object is valid only when version has the value 'version2'. if md5transmit-key-id has a value of 0, it means that no keys are configured.";
                    }

                    ///nokia-state/state/router/ospf/area/virtual-link/local-ip-address
                    leaf local-ip-address {
                        type types-sros:ip-address;
                        description "the value of local-ip-address indicates the IP address of the local egress interface that is used to maintain the adjacency to reach this virtual neighbor.";
                    }

                    ///nokia-state/state/router/ospf/area/virtual-link/events
                    leaf events {
                        type yang:counter32;
                        description "the value of events indicates the number of times this OSPF interface has changed its state, or an error has occurred.";
                    }

                    ///nokia-state/state/router/ospf/area/virtual-link/last-event-time
                    leaf last-event-time {
                        type yang:date-and-time;
                        description "the value of last-event-time indicates the value of sys-up-time when an event was last associated with this OSPF interface.";
                    }

                    ///nokia-state/state/router/ospf/area/virtual-link/packets
                    container packets {
                        description "Enter the packets context";

                        ///nokia-state/state/router/ospf/area/virtual-link/packets/tx-total
                        leaf tx-total {
                            type yang:counter32;
                            description "The total number of OSPF packets transmitted since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/virtual-link/packets/tx-hello
                        leaf tx-hello {
                            type yang:counter32;
                            description "The total number of OSPF hello packets transmitted since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/virtual-link/packets/tx-db-description
                        leaf tx-db-description {
                            type yang:counter32;
                            description "The total number of OSPF database description packets transmitted since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/virtual-link/packets/tx-ls-request
                        leaf tx-ls-request {
                            type yang:counter32;
                            description "The total number of OSPF link state requests (LS-rs)transmitted since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/virtual-link/packets/tx-ls-update
                        leaf tx-ls-update {
                            type yang:counter32;
                            description "The total number of OSPF link state updates (LS-us) transmitted since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/virtual-link/packets/tx-ls-ack
                        leaf tx-ls-ack {
                            type yang:counter32;
                            description "The total number of OSPF link state acknowledgements transmitted since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/virtual-link/packets/rx-total
                        leaf rx-total {
                            type yang:counter32;
                            description "The total number of OSPF packets received since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/virtual-link/packets/rx-hello
                        leaf rx-hello {
                            type yang:counter32;
                            description "The total number of OSPF hello packets received since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/virtual-link/packets/rx-db-description
                        leaf rx-db-description {
                            type yang:counter32;
                            description "The total number of OSPF database description packets received since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/virtual-link/packets/rx-ls-request
                        leaf rx-ls-request {
                            type yang:counter32;
                            description "The total number of link state requests (LS-rs) received since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/virtual-link/packets/rx-ls-update
                        leaf rx-ls-update {
                            type yang:counter32;
                            description "The total number of link state updates (LS-us) received since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/virtual-link/packets/rx-ls-ack
                        leaf rx-ls-ack {
                            type yang:counter32;
                            description "The total number of link state acknowledgements received since admin-state was last set to 'enabled'.";
                        }

                        ///nokia-state/state/router/ospf/area/virtual-link/packets/discarded
                        leaf discarded {
