                                        ///nokia-state/state/service/ies/interface/sap/ipsec-gateway/dynamic-tunnel/ipsec-sa/isa/statistics/packets-processed
                                        leaf packets-processed {
                                            type yang:counter64;
                                            description "The the number of packets successfully processed for this SA";
