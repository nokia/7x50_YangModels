                                    ///nokia-state/state/service/ies/interface/sap/ipsec-gateway/dynamic-tunnel/ipsec-sa/ts/to-address
                                    leaf to-address {
                                        type types-sros:ip-address;
                                        description "The end address of the range for this traffic selector entry.";
                                    }

