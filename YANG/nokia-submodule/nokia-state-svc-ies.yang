                        ///nokia-state/state/service/ies/interface/ipv6/vrrp/master
                        list master {
                            key "primary-ip-address";
                            description "Enter the master list instance";

                            ///nokia-state/state/service/ies/interface/ipv6/vrrp/master/primary-ip-address
