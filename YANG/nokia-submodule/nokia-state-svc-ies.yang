                            }

                            ///nokia-state/state/service/ies/interface/ipv6/vrrp/statistics/zero-priority-packets-sent
                            leaf zero-priority-packets-sent {
                                type yang:counter32;
                                description
                                    "The total number of VRRP packets sent by the virtual router
