submodule nokia-conf-card-fp {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-card               { prefix "types-card"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-groups;
    include nokia-conf-log;
    include nokia-conf-mcast-mgmt;
    include nokia-conf-qos;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure card forwarding plane on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-card-fp YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-05-30";

    grouping conf-card-fp {
        ///nokia-conf/configure/card/fp
        list fp {
            key "fp-number";
            description "Enter the fp list instance";

            ///nokia-conf/configure/card/fp/fp-number
            leaf fp-number {
                type types-card:fp-id;
                description "Forwarding plane within the card";
            }

            ///nokia-conf/configure/card/fp/ingress-buffer-allocation
            leaf ingress-buffer-allocation {
                type decimal64 {
                    range "20.00..80.00";
                    fraction-digits 2;
                }
                units "percent";
                default "50.00";
                description "Ingress buffer pool allocation for the forwarding plane";
            }

            ///nokia-conf/configure/card/fp/init-extract-prio-mode
            leaf init-extract-prio-mode {
                type enumeration {
                    enum uniform                        { value 1; }
                    enum l3-classify                    { value 2; }
                }
                default "uniform";
                description "Initial drop priority mode of extracted traffic";
            }

            ///nokia-conf/configure/card/fp/policy-accounting
            leaf policy-accounting {
                status obsolete;
                type uint32 {
                    range "1000..128000";
                }
                description "Number of stats resources for policy accounting for the forwarding plane";
            }

            ///nokia-conf/configure/card/fp/stable-pool-sizing
            leaf stable-pool-sizing {
                type boolean;
                default "false";
                description "Enable stable pool sizing for the forwarding plane";
            }

            ///nokia-conf/configure/card/fp/fp-resource-policy
            leaf fp-resource-policy {
                type leafref {
                    path "../../../qos/fp-resource-policy/fp-resource-policy-name";
                }
                sros-ext:card-auto-reset-on-modify;
                description "Resource policy to manage resources on the FP";
            }

            ///nokia-conf/configure/card/fp/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/card/fp/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/card/fp/dist-cpu-protection
            container dist-cpu-protection {
                status obsolete;
                description "Enter the dist-cpu-protection context";

                ///nokia-conf/configure/card/fp/dist-cpu-protection/dynamic-enforcement-policer-pool
                leaf dynamic-enforcement-policer-pool {
                    status obsolete;
                    type uint32 {
                        range "1000..32000";
                    }
                    description "Number of policers reserved as enforcement policers use";
                }

            } // container dist-cpu-protection

            ///nokia-conf/configure/card/fp/egress
            container egress {
                description "Enter the egress context";

                ///nokia-conf/configure/card/fp/egress/hs-fixed-high-thresh-delta
                leaf hs-fixed-high-thresh-delta {
                    type int32 {
                        range "0..65536";
                    }
                    description "High threshold delta on the forwarding plane";
                }

                ///nokia-conf/configure/card/fp/egress/hs-pool-policy
                leaf hs-pool-policy {
                    type leafref {
                        path "../../../../qos/hs-pool-policy/name";
                    }
                    description "HS pool policy";
                }

                ///nokia-conf/configure/card/fp/egress/wred-queue-control
                container wred-queue-control {
                    description "Enter the wred-queue-control context";

                    ///nokia-conf/configure/card/fp/egress/wred-queue-control/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the WRED queue control function";
                    }

                    ///nokia-conf/configure/card/fp/egress/wred-queue-control/buffer-allocation
                    leaf buffer-allocation {
                        type decimal64 {
                            range "0.01..99.99";
                            fraction-digits 2;
                        }
                        units "percent";
                        default "25.00";
                        description "Buffer allocation for WRED queue buffer pools";
                    }

                    ///nokia-conf/configure/card/fp/egress/wred-queue-control/reserved-cbs
                    leaf reserved-cbs {
                        type decimal64 {
                            range "0.01..99.99";
                            fraction-digits 2;
                        }
                        units "percent";
                        default "25.00";
                        description "Buffers for queues operating within CBS thresholds";
                    }

                    ///nokia-conf/configure/card/fp/egress/wred-queue-control/slope-policy
                    leaf slope-policy {
                        type leafref {
                            path "../../../../../qos/slope-policy/slope-policy-name";
                        }
                        description "Slope policy for the forwarding plane";
                    }

                } // container wred-queue-control

            } // container egress

            ///nokia-conf/configure/card/fp/hi-bw-mcast-src
            container hi-bw-mcast-src {
                presence "Enable the hi-bw-mcast-src context";
                description "Enable the hi-bw-mcast-src context";

                ///nokia-conf/configure/card/fp/hi-bw-mcast-src/alarm
                leaf alarm {
                    type boolean;
                    default "false";
                    description "Raise an alarm when multiple traffic taps share a plane";
                }

                ///nokia-conf/configure/card/fp/hi-bw-mcast-src/group
                leaf group {
                    type uint32 {
                        range "0..32";
                    }
                    default "0";
                    description "Logical MSFP group for the MDA";
                }

                ///nokia-conf/configure/card/fp/hi-bw-mcast-src/default-paths-only
                leaf default-paths-only {
                    type boolean;
                    default "false";
                    description "Allocate the default paths to dedicated MSFPs";
                }

            } // container hi-bw-mcast-src

            ///nokia-conf/configure/card/fp/ingress
            container ingress {
                description "Enter the ingress context";

                ///nokia-conf/configure/card/fp/ingress/access
                container access {
                    description "Enter the access context";

                    ///nokia-conf/configure/card/fp/ingress/access/queue-group
                    list queue-group {
                        key "queue-group-name instance-id";
                        description "Enter the queue-group list instance";

                        ///nokia-conf/configure/card/fp/ingress/access/queue-group/queue-group-name
                        leaf queue-group-name {
                            type leafref {
                                path "../../../../../../qos/queue-group-templates/ingress/queue-group/ingress-queue-group-name";
                            }
                            description "Queue group name";
                        }

                        ///nokia-conf/configure/card/fp/ingress/access/queue-group/instance-id
                        leaf instance-id {
                            type uint16 {
                                range "1..65535";
                            }
                            description "Instance ID";
                        }

                        ///nokia-conf/configure/card/fp/ingress/access/queue-group/accounting-policy
                        leaf accounting-policy {
                            type leafref {
                                path "../../../../../../log/accounting-policy/policy-id";
                            }
                            description "Accounting policy for the FP ingress queue group";
                        }

                        ///nokia-conf/configure/card/fp/ingress/access/queue-group/collect-stats
                        leaf collect-stats {
                            type boolean;
                            default "false";
                            description "Collect statistics on FP ingress queue group";
                        }

                        ///nokia-conf/configure/card/fp/ingress/access/queue-group/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/card/fp/ingress/access/queue-group/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/card/fp/ingress/access/queue-group/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-control-policy
                        container policer-control-policy {
                            description "Enter the policer-control-policy context";

                            ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-control-policy/policy-name
                            leaf policy-name {
                                type leafref {
                                    path "../../../../../../../qos/policer-control-policy/policer-control-policy-name";
                                }
                                description "Policer control policy name";
                            }

                            ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-control-policy/overrides
                            container overrides {
                                presence "Enable the overrides context";
                                description "Enable the overrides context";

                                ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-control-policy/overrides/max-rate
                                leaf max-rate {
                                    type types-qos:queue-pir-rate-override;
                                    units "kilobps";
                                    description "Maximum rate override";
                                }

                                ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-control-policy/overrides/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-control-policy/overrides/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-control-policy/overrides/priority-mbs-thresholds
                                container priority-mbs-thresholds {
                                    description "Enter the priority-mbs-thresholds context";

                                    ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-control-policy/overrides/priority-mbs-thresholds/min-threshold-separation
                                    leaf min-threshold-separation {
                                        type types-qos:policer-burst-size-override;
                                        description "Minimum separation between any two active thresholds";
                                    }

                                    ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-control-policy/overrides/priority-mbs-thresholds/priority
                                    list priority {
                                        key "level";
                                        description "Enter the priority list instance";

                                        ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-control-policy/overrides/priority-mbs-thresholds/priority/level
                                        leaf level {
                                            type types-qos:hierarchy-level;
                                            description "Priority level, higher number is stricter";
                                        }

                                        ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-control-policy/overrides/priority-mbs-thresholds/priority/mbs-contribution
                                        leaf mbs-contribution {
                                            type types-qos:policer-burst-size-override;
                                            description "MBS contribution size override";
                                        }

                                        ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-control-policy/overrides/priority-mbs-thresholds/priority/apply-groups
                                        leaf-list apply-groups {
                                            type leafref {
                                                path "../../../../../../../../../../groups/group/name";
                                            }
                                            max-elements 8;
                                            ordered-by user;
                                            description "Apply a configuration group at this level";
                                        }

                                        ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-control-policy/overrides/priority-mbs-thresholds/priority/apply-groups-exclude
                                        leaf-list apply-groups-exclude {
                                            type leafref {
                                                path "../../../../../../../../../../groups/group/name";
                                            }
                                            max-elements 8;
                                            ordered-by user;
                                            description "Exclude a configuration group at this level";
                                        }

                                    } // list priority

                                } // container priority-mbs-thresholds

                            } // container overrides

                        } // container policer-control-policy

                        ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-overrides
                        container policer-overrides {
                            description "Enter the policer-overrides context";

                            ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-overrides/policer
                            list policer {
                                key "policer-id";
                                description "Enter the policer list instance";

                                ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-overrides/policer/policer-id
                                leaf policer-id {
                                    type leafref {
                                        path "../../../../../../../../qos/queue-group-templates/ingress/queue-group[ingress-queue-group-name=current()/../../../queue-group-name]/policer/policer-id";
                                    }
                                    description "Policer ID";
                                }

                                ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-overrides/policer/cbs
                                leaf cbs {
                                    type types-qos:policer-burst-size-override-max;
                                    description "CBS tolerance allowed by the policer";
                                }

                                ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-overrides/policer/mbs
                                leaf mbs {
                                    type types-qos:policer-burst-size-override-max;
                                    description "Maximum burst tolerance allowed by the policer";
                                }

                                ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-overrides/policer/packet-byte-offset
                                leaf packet-byte-offset {
                                    type types-qos:ingress-per-packet-offset-override;
                                    description "Packet size adjustment";
                                }

                                ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-overrides/policer/stat-mode
                                leaf stat-mode {
                                    type types-qos:ingress-policer-stat-mode;
                                    description "Stat mode for the policer";
                                }

                                ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-overrides/policer/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-overrides/policer/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-overrides/policer/rate
                                container rate {
                                    description "Enter the rate context";

                                    ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-overrides/policer/rate/cir
                                    leaf cir {
                                        type types-qos:queue-cir-rate-override;
                                        units "kilobps";
                                        description "CIR for the policer";
                                    }

                                    ///nokia-conf/configure/card/fp/ingress/access/queue-group/policer-overrides/policer/rate/pir
                                    leaf pir {
                                        type types-qos:queue-pir-rate-override;
                                        units "kilobps";
                                        description "PIR for the policer";
                                    }

                                } // container rate

                            } // list policer

                        } // container policer-overrides

                    } // list queue-group

                } // container access

                ///nokia-conf/configure/card/fp/ingress/mcast-path-management
                container mcast-path-management {
                    description "Enter the mcast-path-management context";

                    ///nokia-conf/configure/card/fp/ingress/mcast-path-management/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of multicast path management";
                    }

                    ///nokia-conf/configure/card/fp/ingress/mcast-path-management/bandwidth-policy
                    leaf bandwidth-policy {
                        type leafref {
                            path "../../../../../multicast-management/bandwidth-policy/policy-name";
                        }
                        description "Bandwidth policy for ingress multicast path management";
                    }

                } // container mcast-path-management

                ///nokia-conf/configure/card/fp/ingress/network
                container network {
                    description "Enter the network context";

                    ///nokia-conf/configure/card/fp/ingress/network/queue-policy
                    leaf queue-policy {
                        type leafref {
                            path "../../../../../qos/network-queue/network-queue-policy";
                        }
                        description "Network queue policy";
                    }

                    ///nokia-conf/configure/card/fp/ingress/network/queue-group
                    list queue-group {
                        key "queue-group-name instance-id";
                        description "Enter the queue-group list instance";

                        ///nokia-conf/configure/card/fp/ingress/network/queue-group/queue-group-name
                        leaf queue-group-name {
                            type leafref {
                                path "../../../../../../qos/queue-group-templates/ingress/queue-group/ingress-queue-group-name";
                            }
                            description "Queue group name";
                        }

                        ///nokia-conf/configure/card/fp/ingress/network/queue-group/instance-id
                        leaf instance-id {
                            type uint16 {
                                range "1..65535";
                            }
                            description "Instance ID";
                        }

                        ///nokia-conf/configure/card/fp/ingress/network/queue-group/accounting-policy
                        leaf accounting-policy {
                            type leafref {
                                path "../../../../../../log/accounting-policy/policy-id";
                            }
                            description "Accounting policy for the FP ingress queue group";
                        }

                        ///nokia-conf/configure/card/fp/ingress/network/queue-group/collect-stats
                        leaf collect-stats {
                            type boolean;
                            default "false";
                            description "Collect statistics on FP ingress queue group";
                        }

                        ///nokia-conf/configure/card/fp/ingress/network/queue-group/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/card/fp/ingress/network/queue-group/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/card/fp/ingress/network/queue-group/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-control-policy
                        container policer-control-policy {
                            description "Enter the policer-control-policy context";

                            ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-control-policy/policy-name
                            leaf policy-name {
                                type leafref {
                                    path "../../../../../../../qos/policer-control-policy/policer-control-policy-name";
                                }
                                description "Policer control policy name";
                            }

                            ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-control-policy/overrides
                            container overrides {
                                presence "Enable the overrides context";
                                description "Enable the overrides context";

                                ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-control-policy/overrides/max-rate
                                leaf max-rate {
                                    type types-qos:queue-pir-rate-override;
                                    units "kilobps";
                                    description "Maximum rate override";
                                }

                                ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-control-policy/overrides/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-control-policy/overrides/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-control-policy/overrides/priority-mbs-thresholds
                                container priority-mbs-thresholds {
                                    description "Enter the priority-mbs-thresholds context";

                                    ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-control-policy/overrides/priority-mbs-thresholds/min-threshold-separation
                                    leaf min-threshold-separation {
                                        type types-qos:policer-burst-size-override;
                                        description "Minimum separation between any two active thresholds";
                                    }

                                    ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-control-policy/overrides/priority-mbs-thresholds/priority
                                    list priority {
                                        key "level";
                                        description "Enter the priority list instance";

                                        ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-control-policy/overrides/priority-mbs-thresholds/priority/level
                                        leaf level {
                                            type types-qos:hierarchy-level;
                                            description "Priority level, higher number is stricter";
                                        }

                                        ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-control-policy/overrides/priority-mbs-thresholds/priority/mbs-contribution
                                        leaf mbs-contribution {
                                            type types-qos:policer-burst-size-override;
                                            description "MBS contribution size override";
                                        }

                                        ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-control-policy/overrides/priority-mbs-thresholds/priority/apply-groups
                                        leaf-list apply-groups {
                                            type leafref {
                                                path "../../../../../../../../../../groups/group/name";
                                            }
                                            max-elements 8;
                                            ordered-by user;
                                            description "Apply a configuration group at this level";
                                        }

                                        ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-control-policy/overrides/priority-mbs-thresholds/priority/apply-groups-exclude
                                        leaf-list apply-groups-exclude {
                                            type leafref {
                                                path "../../../../../../../../../../groups/group/name";
                                            }
                                            max-elements 8;
                                            ordered-by user;
                                            description "Exclude a configuration group at this level";
                                        }

                                    } // list priority

                                } // container priority-mbs-thresholds

                            } // container overrides

                        } // container policer-control-policy

                        ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-overrides
                        container policer-overrides {
                            description "Enter the policer-overrides context";

                            ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-overrides/policer
                            list policer {
                                key "policer-id";
                                description "Enter the policer list instance";

                                ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-overrides/policer/policer-id
                                leaf policer-id {
                                    type leafref {
                                        path "../../../../../../../../qos/queue-group-templates/ingress/queue-group[ingress-queue-group-name=current()/../../../queue-group-name]/policer/policer-id";
                                    }
                                    description "Policer ID";
                                }

                                ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-overrides/policer/cbs
                                leaf cbs {
                                    type types-qos:policer-burst-size-override-max;
                                    description "CBS tolerance allowed by the policer";
                                }

                                ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-overrides/policer/mbs
                                leaf mbs {
                                    type types-qos:policer-burst-size-override-max;
                                    description "Maximum burst tolerance allowed by the policer";
                                }

                                ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-overrides/policer/packet-byte-offset
                                leaf packet-byte-offset {
                                    type types-qos:ingress-per-packet-offset-override;
                                    description "Packet size adjustment";
                                }

                                ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-overrides/policer/stat-mode
                                leaf stat-mode {
                                    type types-qos:ingress-policer-stat-mode;
                                    description "Stat mode for the policer";
                                }

                                ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-overrides/policer/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-overrides/policer/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                                ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-overrides/policer/rate
                                container rate {
                                    description "Enter the rate context";

                                    ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-overrides/policer/rate/cir
                                    leaf cir {
                                        type types-qos:queue-cir-rate-override;
                                        units "kilobps";
                                        description "CIR for the policer";
                                    }

                                    ///nokia-conf/configure/card/fp/ingress/network/queue-group/policer-overrides/policer/rate/pir
                                    leaf pir {
                                        type types-qos:queue-pir-rate-override;
                                        units "kilobps";
                                        description "PIR for the policer";
                                    }

                                } // container rate

                            } // list policer

                        } // container policer-overrides

                    } // list queue-group

                    ///nokia-conf/configure/card/fp/ingress/network/pool
                    list pool {
                        key "name";
                        description "Enter the pool list instance";

                        ///nokia-conf/configure/card/fp/ingress/network/pool/name
                        leaf name {
                            type types-sros:named-item;
                            description "Network ingress pool name for the FP";
                        }

                        ///nokia-conf/configure/card/fp/ingress/network/pool/amber-alarm-threshold
                        leaf amber-alarm-threshold {
                            type uint32 {
                                range "1..1000";
                            }
                            units "percent";
                            description "Amber alarm threshold allowed on oversubscription";
                        }

                        ///nokia-conf/configure/card/fp/ingress/network/pool/red-alarm-threshold
                        leaf red-alarm-threshold {
                            type uint32 {
                                range "1..1000";
                            }
                            units "percent";
                            description "Red alarm threshold allowed on oversubscription";
                        }

                        ///nokia-conf/configure/card/fp/ingress/network/pool/slope-policy
                        leaf slope-policy {
                            type leafref {
                                path "../../../../../../qos/slope-policy/slope-policy-name";
                            }
                            description "Slope policy";
                        }

                        ///nokia-conf/configure/card/fp/ingress/network/pool/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/card/fp/ingress/network/pool/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/card/fp/ingress/network/pool/resv-cbs
                        container resv-cbs {
                            description "Enter the resv-cbs context";

                            ///nokia-conf/configure/card/fp/ingress/network/pool/resv-cbs/cbs
                            leaf cbs {
                                type int32 {
                                    range "0..100";
                                }
                                units "percent";
                                description "Pool size reserved for CBS";
                            }

                            ///nokia-conf/configure/card/fp/ingress/network/pool/resv-cbs/amber-alarm-action
                            container amber-alarm-action {
                                description "Enter the amber-alarm-action context";

                                ///nokia-conf/configure/card/fp/ingress/network/pool/resv-cbs/amber-alarm-action/step
                                leaf step {
                                    type uint32 {
                                        range "1..100";
                                    }
                                    units "percent";
                                    description "Step-size percentage for reserved CBS size of the pool";
                                }

                                ///nokia-conf/configure/card/fp/ingress/network/pool/resv-cbs/amber-alarm-action/max
                                leaf max {
                                    type uint32 {
                                        range "1..100";
                                    }
                                    units "percent";
                                    description "Maximum reserved CBS size of the pool";
                                }

                            } // container amber-alarm-action

                        } // container resv-cbs

                    } // list pool

                } // container network

                ///nokia-conf/configure/card/fp/ingress/policy-accounting
                container policy-accounting {
                    description "Enter the policy-accounting context";

                    ///nokia-conf/configure/card/fp/ingress/policy-accounting/policers
                    leaf policers {
                        type uint32 {
                            range "1..64000";
                        }
                        description "Number of policer resources for policy accounting";
                    }

                    ///nokia-conf/configure/card/fp/ingress/policy-accounting/classes
                    leaf classes {
                        type uint32 {
                            range "1000..128000";
                        }
                        description "Number of accounting classes for the forwarding plane";
                    }

                } // container policy-accounting

                ///nokia-conf/configure/card/fp/ingress/dist-cpu-protection
                container dist-cpu-protection {
                    description "Enter the dist-cpu-protection context";

                    ///nokia-conf/configure/card/fp/ingress/dist-cpu-protection/dynamic-enforcement-policer-pool
                    leaf dynamic-enforcement-policer-pool {
                        type uint32 {
                            range "1000..32000";
                        }
                        description "Number of policers reserved as enforcement policers use";
                    }

                } // container dist-cpu-protection

            } // container ingress

        } // list fp

    } // grouping conf-card-fp

}
