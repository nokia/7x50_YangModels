                        ///nokia-state/state/router/dhcp-server/dhcpv6/statistics/dropped/conflicting-relay-address
                        leaf conflicting-relay-address {
                            type yang:counter32;
                            description
                                "the number of DHCP requests dropped because they
                                 were received from a different Relay IP address 
                                 within a timeframe of 10 seconds after a previous 
                                 DHCPREQUEST.";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/statistics/dropped/failover-enabled
                        leaf failover-enabled {
                            type yang:counter32;
                            description
                                "The number of internal calls from the local address assignment
                                 infrastructure dropped by the DHCP server instance because
                                 failover is enabled for the server instance.";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/statistics/dropped/address-conflicts
                        leaf address-conflicts {
                            type yang:counter32;
                            description
                                "A DHCP host is trying to acquire an IP address that was handed through
                                 the local address assignment infrastructure, or the local address
                                 assignment infrastructure tries to use an IP address that was handed
                                 out to a DHCP client.";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/statistics/dropped/interface-id-map-enabled
                        leaf interface-id-map-enabled {
                            type yang:counter32;
                            description
                                "the number of internal calls from the local address assignment
                                 infrastructure dropped because
                                 interface ID mapping is enabled.";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/statistics/dropped/user-id-enabled
                        leaf user-id-enabled {
                            type yang:counter32;
                            description
                                "The number of internal calls from the local address assignment
                                 infrastructure dropped because the user-id is not set to duid (2).";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/statistics/dropped/audit
                        leaf audit {
                            type yang:counter64;
                            description
                                "The number of DHCP requests dropped by the server instance
                                 because this server instance is in auditing state.";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/statistics/dropped/lease-query-not-allowed
                        leaf lease-query-not-allowed {
                            type yang:counter32;
                            description
                                "The number of DHCP leasequery packets dropped
                                 because the server is configured not to allow leasequery packets.";
                        }

                    } // container dropped

                    ///nokia-state/state/router/dhcp-server/dhcpv6/statistics/internal-requests
                    container internal-requests {
                        description "Enter the internal-requests context";

                        ///nokia-state/state/router/dhcp-server/dhcpv6/statistics/internal-requests/ppp-slaac
                        leaf ppp-slaac {
                            type yang:counter64;
                            description
                                "the number of internal request calls received from
                                 the local address assignment
                                 infrastructure for PPP SLAAC.";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/statistics/internal-requests/ipoe-wan
                        leaf ipoe-wan {
                            type yang:counter64;
                            description
                                "the number of internal request calls received from the
                                 local address assignment infrastructure for IPoE.";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/statistics/internal-requests/releases
                        leaf releases {
                            type yang:counter64;
                            description
                                "The number of internal release calls received by the
                                 DHCP server instance from the local address assignment
                                 infrastructure.";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/statistics/internal-requests/ipoe-slaac
                        leaf ipoe-slaac {
                            type yang:counter64;
                            description
                                "The number of internal request calls received from the local
                                 address assignment infrastructure for IPoE SLAAC 
                                 (stateless autoconfiguration).";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/statistics/internal-requests/ipsec
                        leaf ipsec {
                            type yang:counter64;
                            description
                                "the number of internal request calls received from
                                 the local address assignment
                                 infrastructure for IPsec.";
                        }

                    } // container internal-requests

                } // container statistics

                ///nokia-state/state/router/dhcp-server/dhcpv6/lease
                list lease {
                    key "address";
                    description "Enter the lease list instance";

                    ///nokia-state/state/router/dhcp-server/dhcpv6/lease/address
                    leaf address {
                        type types-sros:ip-address;
                        description "Lease IP address";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv6/lease/state
                    leaf state {
                        type enumeration {
                            enum offered {
                                value 0;
                                description "The IP address was offered to the client. The client still has to acknowledge the offer by sending a DHCP request.";
                            }
                            enum stable {
                                value 1;
                                description "The IP address is now in use by the client.";
                            }
                            enum force-renew-pending {
                                value 2;
                                description "The IP address is in use by the client, but the server sends a DHCP FORCERENEW message to the client, because an option has changed for this client.";
                            }
                            enum remove-pending {
                                value 3;
                                description "The IP address is in use by the client, but it is no longer available in the configured IP address ranges. The server sends a FORCERENEW message to the client. When the client sends a renewal DHCP request message, it will be rejected.";
                            }
                            enum held {
                                value 4;
                                description "The IP address has been used by the client but the lease has timed out. The address is held so that the same client can acquire this address when requesting a new lease.";
                            }
                            enum internal {
                                value 5;
                                description "The IP address has been leased via the local pool management system and is now in use.";
                            }
                            enum internal-orphan {
                                value 6;
                                description "The IP address has been leased via the local pool management system and is now in use. There is however no configured subnet to which this lease belongs, because it has been removed or because this lease was installed through dual-homing.";
                            }
                            enum internal-offered {
                                value 7;
                                description "The IP address has been offered via the local pool management system. The client still has to acknowledge this offer.";
                            }
                            enum internal-held {
                                value 8;
                                description "The IP address has been used by local pool management system but the lease is currently not active. The address is now in the hold list so that the local pool management system client can get the exactly same IP address again upon the next request for a lease.";
                            }
                            enum sticky {
                                value 9;
                                description "The IP address is reserved for the client and will remain reserved until the reservation for it is cleared.  The client will get the exactly same IP address again upon the next request for a lease.";
                            }
                        }
                        description "The state of this lease.";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv6/lease/start
                    leaf start {
                        type yang:date-and-time;
                        description "When this lease was created";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv6/lease/last-renew
                    leaf last-renew {
                        type yang:date-and-time;
                        description "When the last renew was done for this lease";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv6/lease/remaining-lease-time
                    leaf remaining-lease-time {
                        type uint32;
                        units "seconds";
                        description "The time left for this lease";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv6/lease/client-type
                    leaf client-type {
                        type types-dhcp:dhcp-server-client-type;
                        description "Type of client";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ppp-user-name
                    leaf ppp-user-name {
                        type types-submgt:ppp-user-name;
                        description "PPP user name";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv6/lease/remaining-hold-time
                    leaf remaining-hold-time {
                        type uint32;
                        units "seconds";
                        description "The hold time left for this lease";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv6/lease/internal-client-type
                    leaf internal-client-type {
                        type types-dhcp:dhcp-server-internal-client-type;
                        description "Type of internal client";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv6/lease/sticky-host-name
                    leaf sticky-host-name {
                        type types-sros:named-item-or-empty;
                        description "The name of the sticky host";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv6/lease/failover
                    container failover {
                        description "Enter the failover context";

                        ///nokia-state/state/router/dhcp-server/dhcpv6/lease/failover/control-type
                        leaf control-type {
                            type types-dhcp:failover-control-type;
                            description "The type of failover control for this DHCP lease state";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/lease/failover/expiration-interval
                        leaf expiration-interval {
                            type uint32;
                            units "seconds";
                            description
                                "The lease expiration interval for this lease that is
                                 negotiated with the failover peer";
                        }

                    } // container failover

                    ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6
                    container ipv6 {
                        description "Enter the ipv6 context";

                        ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/client-prefix-length
                        leaf client-prefix-length {
                            type uint32 {
                                range "1..128";
                            }
                            description "The length of client address";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/ia-id
                        leaf ia-id {
                            type uint32;
                            description "The Identity Association of the client";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/ia-type
                        leaf ia-type {
                            type enumeration {
                                enum ia-na {
                                    value 1;
                                    description "The IA_NA option type is used to request the assignment of non-temporary addresses.";
                                }
                                enum ia-pd {
                                    value 2;
                                    description "The IA_PD option is used to carry a prefix delegation identity association, the parameters associated with the IA_PD and the prefixes associated with it.";
                                }
                            }
                            description "Type of the option.";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/link-local-address
                        leaf link-local-address {
                            type types-sros:ipv6-address;
                            description "The link local address of the client";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/duid
                        container duid {
                            description "Enter the duid context";

                            ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/duid/raw-data
                            leaf raw-data {
                                type binary;
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/duid/duid-llt
                            container duid-llt {
                                description "Enter the duid-llt context";

                                ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/duid/duid-llt/hardware-type
                                leaf hardware-type {
                                    type types-dhcp:dhcp6-duid-hardware-type;
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/duid/duid-llt/time
                                leaf time {
                                    type yang:date-and-time;
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/duid/duid-llt/link-layer-address
                                leaf link-layer-address {
                                    type binary;
                                }

                            } // container duid-llt

                            ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/duid/duid-en
                            container duid-en {
                                description "Enter the duid-en context";

                                ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/duid/duid-en/enterprise-number
                                leaf enterprise-number {
                                    type uint32;
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/duid/duid-en/identifier
                                leaf identifier {
                                    type binary;
                                }

                            } // container duid-en

                            ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/duid/duid-ll
                            container duid-ll {
                                description "Enter the duid-ll context";

                                ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/duid/duid-ll/hardware-type
                                leaf hardware-type {
                                    type types-dhcp:dhcp6-duid-hardware-type;
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/duid/duid-ll/link-layer-address
                                leaf link-layer-address {
                                    type binary;
                                }

                            } // container duid-ll

                        } // container duid

                        ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/relay-interface-id
                        container relay-interface-id {
                            description "Enter the relay-interface-id context";

                            ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/relay-interface-id/raw-data
                            leaf raw-data {
                                type binary;
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/relay-interface-id/string
                            leaf string {
                                type string {
                                    length "0..255";
                                }
                            }

                        } // container relay-interface-id

                        ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/ldra-interface-id
                        container ldra-interface-id {
                            description "Enter the ldra-interface-id context";

                            ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/ldra-interface-id/raw-data
                            leaf raw-data {
                                type binary;
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv6/lease/ipv6/ldra-interface-id/string
                            leaf string {
                                type string {
                                    length "0..255";
                                }
                            }

                        } // container ldra-interface-id

                    } // container ipv6

                } // list lease

            } // list dhcpv6

        } // container dhcp-server

    } // grouping state-router-dhcp-server

}
