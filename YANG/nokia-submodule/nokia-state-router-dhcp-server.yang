submodule nokia-state-router-dhcp-server {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-dhcp               { prefix "types-dhcp"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-subscriber-mgmt    { prefix "types-submgt"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for DHCP server on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-router-dhcp-server YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2021-10-04";

    grouping state-router-dhcp-server {
        ///nokia-state/state/router/dhcp-server
        container dhcp-server {
            description "Enter the dhcp-server context";

            ///nokia-state/state/router/dhcp-server/dhcpv4
            list dhcpv4 {
                key "name";
                description "Enter the dhcpv4 list instance";

                ///nokia-state/state/router/dhcp-server/dhcpv4/name
                leaf name {
                    type types-sros:named-item;
                    description "DHCP server name";
                }

                ///nokia-state/state/router/dhcp-server/dhcpv4/oper-state
                leaf oper-state {
                    type enumeration {
                        enum unknown                        { value 1; }
                        enum up                             { value 2; }
                        enum down                           { value 3; }
                        enum transition                     { value 4; }
                        enum wait-persistence               { value 5; }
                    }
                    description "Operational state of this local DHCP server.";
                }

                ///nokia-state/state/router/dhcp-server/dhcpv4/creation-origin
                leaf creation-origin {
                    type types-sros:creation-origin;
                    description "Creation origin of this local DHCP server.";
                }

                ///nokia-state/state/router/dhcp-server/dhcpv4/failover
                container failover {
                    description "Enter the failover context";

                    ///nokia-state/state/router/dhcp-server/dhcpv4/failover/state
                    leaf state {
                        type enumeration {
                            enum unknown {
                                value 1;
                                description "unknown";
                            }
                            enum init {
                                value 2;
                                description "preparing to startup";
                            }
                            enum start-up {
                                value 3;
                                description "starting up";
                            }
                            enum no-communication {
                                value 4;
                                description "communication with partner is lost";
                            }
                            enum partner-down {
                                value 5;
                                description "partner is assumed down";
                            }
                            enum normal {
                                value 6;
                                description "normal";
                            }
                            enum disabled {
                                value 7;
                                description "administrative state of server or failover facility is down";
                            }
                            enum transition {
                                value 8;
                                description "transition";
                            }
                            enum pre-normal {
                                value 9;
                                description "sub-state of normal, that applies to a pair of servers A and B, while B is recovering after it went down: - server A is responding to (instead of dropping) renew and rebind messages for remote leases it has allocated during partnerDown; - server B is dropping (instead of NACKing) renew and rebind messages for yet unknown local leases while resynchronizing with the MCS database.";
                            }
                        }
                        description "Failover state";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/failover/time-left
                    leaf time-left {
                        type union {
                            type enumeration {
                                enum infinite {
                                    value -1;
                                    description "none of the timers is running";
                                }
                            }
                            type int32;
                        }
                        units "seconds";
                        description "Time left until expiry of one of the timers";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/failover/actual-max-client-lead-time
                    leaf actual-max-client-lead-time {
                        type types-sros:time-duration {
                            range "600..86399";
                        }
                        units "seconds";
                        description "Actual maximum client lead time";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/failover/statistics
                    container statistics {
                        description "Enter the statistics context";

                        ///nokia-state/state/router/dhcp-server/dhcpv4/failover/statistics/lease-not-found
                        leaf lease-not-found {
                            type yang:counter32;
                            description "number of dropped binding updates because the corresponding lease cannot be found";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/failover/statistics/drop-invalid-packets
                        leaf drop-invalid-packets {
                            type yang:counter32;
                            description "number of dropped binding updates because the packet was malformed";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/failover/statistics/admin-disabled
                        leaf admin-disabled {
                            type yang:counter32;
                            description "number of dropped binding updates because the failover admin-state is disabled";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/failover/statistics/expired
                        leaf expired {
                            type yang:counter32;
                            description "number of dropped binding updates because the lease has expired";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/failover/statistics/max-reached
                        leaf max-reached {
                            type yang:counter32;
                            description "number of dropped binding updates because the maximum number of leases was reached";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/failover/statistics/subnet-not-found
                        leaf subnet-not-found {
                            type yang:counter32;
                            description "number of dropped binding updates because the subnet could not be found for the lease";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/failover/statistics/range-not-found
                        leaf range-not-found {
                            type yang:counter32;
                            description "number of dropped binding updates because a valid include range could not be found for the lease";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/failover/statistics/host-conflict
                        leaf host-conflict {
                            type yang:counter32;
                            description "number of dropped add binding updates because the address is leased to another host";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/failover/statistics/address-conflict
                        leaf address-conflict {
                            type yang:counter32;
                            description "number of dropped add binding updates because the host has already another address";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/failover/statistics/peer-conflict
                        leaf peer-conflict {
                            type yang:counter32;
                            description "number of dropped add binding updates because the failover peer has leased a conflicting address";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/failover/statistics/persistency-congestion
                        leaf persistency-congestion {
                            type yang:counter32;
                            description "number of dropped binding updates because of persistency congestion on the host";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/failover/statistics/zero-lease-hold-time
                        leaf zero-lease-hold-time {
                            type yang:counter32;
                            description "number of dropped binding updates because the lease-hold-time is zero";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/failover/statistics/invalid-prefix-length
                        leaf invalid-prefix-length {
                            type yang:counter32;
                            description "number of dropped binding updates because the lease is not inside the valid prefix-length range";
                        }

                    } // container statistics

                } // container failover

                ///nokia-state/state/router/dhcp-server/dhcpv4/pool
                list pool {
                    key "pool-name";
                    description "Enter the pool list instance";

                    ///nokia-state/state/router/dhcp-server/dhcpv4/pool/pool-name
                    leaf pool-name {
                        type types-sros:named-item;
                        description "DHCP server pool name";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover
                    container failover {
                        description "Enable the failover context";

                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/state
                        leaf state {
                            type enumeration {
                                enum unknown {
                                    value 1;
                                    description "unknown";
                                }
                                enum init {
                                    value 2;
                                    description "preparing to startup";
                                }
                                enum start-up {
                                    value 3;
                                    description "starting up";
                                }
                                enum no-communication {
                                    value 4;
                                    description "communication with partner is lost";
                                }
                                enum partner-down {
                                    value 5;
                                    description "partner is assumed down";
                                }
                                enum normal {
                                    value 6;
                                    description "normal";
                                }
                                enum disabled {
                                    value 7;
                                    description "administrative state of server or failover facility is down";
                                }
                                enum transition {
                                    value 8;
                                    description "transition";
                                }
                                enum pre-normal {
                                    value 9;
                                    description "sub-state of normal, that applies to a pair of servers A and B, while B is recovering after it went down: - server A is responding to (instead of dropping) renew and rebind messages for remote leases it has allocated during partnerDown; - server B is dropping (instead of NACKing) renew and rebind messages for yet unknown local leases while resynchronizing with the MCS database.";
                                }
                            }
                            description "Failover state";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/time-left
                        leaf time-left {
                            type union {
                                type enumeration {
                                    enum infinite {
                                        value -1;
                                        description "none of the timers is running";
                                    }
                                }
                                type int32;
                            }
                            units "seconds";
                            description "Time left until expiry of one of the timers";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/actual-max-client-lead-time
                        leaf actual-max-client-lead-time {
                            type types-sros:time-duration {
                                range "600..86399";
                            }
                            units "seconds";
                            description "Actual maximum client lead time";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/statistics
                        container statistics {
                            description "Enter the statistics context";

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/statistics/lease-not-found
                            leaf lease-not-found {
                                type yang:counter32;
                                description "number of dropped binding updates because the corresponding lease cannot be found";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/statistics/drop-invalid-packets
                            leaf drop-invalid-packets {
                                type yang:counter32;
                                description "number of dropped binding updates because the packet was malformed";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/statistics/admin-disabled
                            leaf admin-disabled {
                                type yang:counter32;
                                description "number of dropped binding updates because the failover admin-state is disabled";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/statistics/expired
                            leaf expired {
                                type yang:counter32;
                                description "number of dropped binding updates because the lease has expired";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/statistics/max-reached
                            leaf max-reached {
                                type yang:counter32;
                                description "number of dropped binding updates because the maximum number of leases was reached";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/statistics/subnet-not-found
                            leaf subnet-not-found {
                                type yang:counter32;
                                description "number of dropped binding updates because the subnet could not be found for the lease";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/statistics/range-not-found
                            leaf range-not-found {
                                type yang:counter32;
                                description "number of dropped binding updates because a valid include range could not be found for the lease";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/statistics/host-conflict
                            leaf host-conflict {
                                type yang:counter32;
                                description "number of dropped add binding updates because the address is leased to another host";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/statistics/address-conflict
                            leaf address-conflict {
                                type yang:counter32;
                                description "number of dropped add binding updates because the host has already another address";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/statistics/peer-conflict
                            leaf peer-conflict {
                                type yang:counter32;
                                description "number of dropped add binding updates because the failover peer has leased a conflicting address";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/statistics/persistency-congestion
                            leaf persistency-congestion {
                                type yang:counter32;
                                description "number of dropped binding updates because of persistency congestion on the host";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/statistics/zero-lease-hold-time
                            leaf zero-lease-hold-time {
                                type yang:counter32;
                                description "number of dropped binding updates because the lease-hold-time is zero";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/failover/statistics/invalid-prefix-length
                            leaf invalid-prefix-length {
                                type yang:counter32;
                                description "number of dropped binding updates because the lease is not inside the valid prefix-length range";
                            }

                        } // container statistics

                    } // container failover

                    ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet
                    list subnet {
                        key "ipv4-prefix";
                        description "Enter the subnet list instance";

                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/ipv4-prefix
                        leaf ipv4-prefix {
                            type types-sros:ipv4-unicast-prefix;
                            description "IPv4 prefix for the subnet";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/active
                        leaf active {
                            type boolean;
                            description "indicates if this subnet is currently being served by this system.";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/declined-address
                        list declined-address {
                            key "address";
                            description "Enter the declined-address list instance";

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/declined-address/address
                            leaf address {
                                type types-sros:ipv4-address;
                                description "IP address declined by client";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/declined-address/mac-address
                            leaf mac-address {
                                type yang:mac-address;
                                description "The MAC address of the client";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/declined-address/declined-time
                            leaf declined-time {
                                type yang:date-and-time {
                                    length "11";
                                }
                                description "When the client is declined";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/declined-address/option82
                            leaf option82 {
                                type binary {
                                    length "0..257";
                                }
                                description "Option 82 from the client message";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/declined-address/client-type
                            leaf client-type {
                                type types-dhcp:dhcp-server-client-type;
                                description "Type of client";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/declined-address/user-name
                            leaf user-name {
                                type types-submgt:ppp-user-name;
                                description "PPP user name";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/declined-address/user-name-hex
                            leaf user-name-hex {
                                type binary {
                                    length "1..253";
                                }
                                description "PPP user name";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/declined-address/circuit-id
                            leaf circuit-id {
                                type binary {
                                    length "0..255";
                                }
                                description "The circuit ID from the option 82";
                            }

                        } // list declined-address

                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics
                        container statistics {
                            description "Enter the statistics context";

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/local
                            container local {
                                description "Enter the local context";

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/local/free-addresses
                                leaf free-addresses {
                                    type yang:counter32;
                                    description "number of free addresses";
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/local/declined-addresses
                                leaf declined-addresses {
                                    type yang:counter32;
                                    description "number of declined addresses";
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/local/provisioned-addresses
                                leaf provisioned-addresses {
                                    type yang:counter32;
                                    description "number of provisioned addresses";
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/local/offered-state
                                leaf offered-state {
                                    type yang:counter32;
                                    description "number of leases in state offered";
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/local/stable-state
                                leaf stable-state {
                                    type yang:counter32;
                                    description "number of leases in state stable";
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/local/force-renew-pending-state
                                leaf force-renew-pending-state {
                                    type yang:counter32;
                                    description "number of leases in state force renew pending";
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/local/remove-pending-state
                                leaf remove-pending-state {
                                    type yang:counter32;
                                    description "number of leases in state remove pending";
                                }

                            } // container local

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/failover
                            container failover {
                                description "Enter the failover context";

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/failover/free-addresses
                                leaf free-addresses {
                                    type yang:counter32;
                                    description "number of free addresses";
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/failover/declined-addresses
                                leaf declined-addresses {
                                    type yang:counter32;
                                    description "number of declined addresses";
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/failover/provisioned-addresses
                                leaf provisioned-addresses {
                                    type yang:counter32;
                                    description "number of provisioned addresses";
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/failover/offered-state
                                leaf offered-state {
                                    type yang:counter32;
                                    description "number of leases in state offered";
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/failover/stable-state
                                leaf stable-state {
                                    type yang:counter32;
                                    description "number of leases in state stable";
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/failover/force-renew-pending-state
                                leaf force-renew-pending-state {
                                    type yang:counter32;
                                    description "number of leases in state force renew pending";
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/failover/remove-pending-state
                                leaf remove-pending-state {
                                    type yang:counter32;
                                    description "number of leases in state remove pending";
                                }

                            } // container failover

                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection
                            container extended-collection {
                                description "Enter the extended-collection context";

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/extended-collection-last-reset
                                leaf extended-collection-last-reset {
                                    type yang:date-and-time;
                                    description "time of last reset of the extended statistics";
                                }

                                ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local
                                container local {
                                    description "Enter the local context";

                                    ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/stable-state
                                    container stable-state {
                                        description "Enter the stable-state context";

                                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/stable-state/peak
                                        leaf peak {
                                            type yang:counter32;
                                            description "peak value of stable-state since last reset";
                                        }

                                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/stable-state/peak-time
                                        leaf peak-time {
                                            type yang:date-and-time;
                                            description "time of last changed peak-state-state";
                                        }

                                    } // container stable-state

                                    ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/leases-in-use
                                    container leases-in-use {
                                        description "Enter the leases-in-use context";

                                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/leases-in-use/current
                                        leaf current {
                                            type yang:counter32;
                                            description "current number of blocks";
                                        }

                                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/leases-in-use/peak
                                        leaf peak {
                                            type yang:counter32;
                                            description "peak value";
                                        }

                                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/leases-in-use/peak-time
                                        leaf peak-time {
                                            type yang:date-and-time;
                                            description "time of last change of the peak value";
                                        }

                                    } // container leases-in-use

                                    ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/free-addresses
                                    container free-addresses {
                                        description "Enter the free-addresses context";

                                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/free-addresses/peak
                                        leaf peak {
                                            type yang:counter32;
                                            description "peak value of free addresses since last reset";
                                        }

                                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/free-addresses/peak-time
                                        leaf peak-time {
                                            type yang:date-and-time;
                                            description "time of last changed peak-free-addresses";
                                        }

                                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/free-addresses/relative
                                        container relative {
                                            description "Enter the relative context";

                                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/free-addresses/relative/current
                                            leaf current {
                                                type types-sros:percent;
                                                description "current number of blocks";
                                            }

                                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/free-addresses/relative/peak
                                            leaf peak {
                                                type types-sros:percent;
                                                description "peak value";
                                            }

                                            ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/free-addresses/relative/peak-time
                                            leaf peak-time {
                                                type yang:date-and-time;
                                                description "time of last change of the peak value";
                                            }

                                        } // container relative

                                    } // container free-addresses

                                    ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/addresses-in-use
                                    container addresses-in-use {
                                        description "Enter the addresses-in-use context";

                                        ///nokia-state/state/router/dhcp-server/dhcpv4/pool/subnet/statistics/extended-collection/local/addresses-in-use/relative
