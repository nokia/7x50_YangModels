                ///nokia-state/state/router/dhcp-server/dhcpv4/lease
                list lease {
                    key "address";
                    description "Enter the lease list instance";

                    ///nokia-state/state/router/dhcp-server/dhcpv4/lease/address
                    leaf address {
                        type types-sros:ip-address;
                        description "Lease IP address";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/lease/state
                    leaf state {
                        type enumeration {
                            enum offered {
                                value 0;
                                description "The IP address was offered to the client. The client still has to acknowledge the offer by sending a DHCP request.";
                            }
                            enum stable {
                                value 1;
                                description "The IP address is now in use by the client.";
                            }
                            enum force-renew-pending {
                                value 2;
                                description "The IP address is in use by the client, but the server sends a DHCP FORCERENEW message to the client, because an option has changed for this client.";
                            }
                            enum remove-pending {
                                value 3;
                                description "The IP address is in use by the client, but it is no longer available in the configured IP address ranges. The server sends a FORCERENEW message to the client. When the client sends a renewal DHCP request message, it will be rejected.";
                            }
                            enum held {
                                value 4;
                                description "The IP address has been used by the client but the lease has timed out. The address is held so that the same client can acquire this address when requesting a new lease.";
                            }
                            enum internal {
                                value 5;
                                description "The IP address has been leased via the local pool management system and is now in use.";
                            }
                            enum internal-orphan {
                                value 6;
                                description "The IP address has been leased via the local pool management system and is now in use. There is however no configured subnet to which this lease belongs, because it has been removed or because this lease was installed through dual-homing.";
                            }
                            enum internal-offered {
                                value 7;
                                description "The IP address has been offered via the local pool management system. The client still has to acknowledge this offer.";
                            }
                            enum internal-held {
                                value 8;
                                description "The IP address has been used by local pool management system but the lease is currently not active. The address is now in the hold list so that the local pool management system client can get the exactly same IP address again upon the next request for a lease.";
                            }
                            enum sticky {
                                value 9;
                                description "The IP address is reserved for the client and will remain reserved until the reservation for it is cleared.  The client will get the exactly same IP address again upon the next request for a lease.";
                            }
                        }
                        description "The state of this lease.";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/lease/start
                    leaf start {
                        type yang:date-and-time;
                        description "When this lease was created";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/lease/last-renew
                    leaf last-renew {
                        type yang:date-and-time;
                        description "When the last renew was done for this lease";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/lease/remaining-lease-time
                    leaf remaining-lease-time {
                        type uint32;
                        units "seconds";
                        description "The time left for this lease";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/lease/client-type
                    leaf client-type {
                        type types-dhcp:dhcp-server-client-type;
                        description "Type of client";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ppp-user-name
                    leaf ppp-user-name {
                        type types-submgt:ppp-user-name;
                        description "PPP user name";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/lease/remaining-hold-time
                    leaf remaining-hold-time {
                        type uint32;
                        units "seconds";
                        description "The hold time left for this lease";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/lease/internal-client-type
                    leaf internal-client-type {
                        type types-dhcp:dhcp-server-internal-client-type;
                        description "Type of internal client";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/lease/sticky-host-name
                    leaf sticky-host-name {
                        type types-sros:named-item-or-empty;
                        description "The name of the sticky host";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/lease/failover
                    container failover {
                        description "Enter the failover context";

                        ///nokia-state/state/router/dhcp-server/dhcpv4/lease/failover/control-type
                        leaf control-type {
                            type types-dhcp:failover-control-type;
                            description "The type of failover control for this DHCP lease state";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/lease/failover/expiration-interval
                        leaf expiration-interval {
                            type uint32;
                            units "seconds";
                            description
                                "The lease expiration interval for this lease that is
                                 negotiated with the failover peer";
                        }

                    } // container failover

                    ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4
                    container ipv4 {
                        description "Enter the ipv4 context";

                        ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4/transaction-id
                        leaf transaction-id {
                            type uint32;
                            description
                                "The transaction ID that the client has sent in its last DHCP
                                 message to the server";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4/client-id
                        leaf client-id {
                            type string {
                                length "0..255";
                            }
                            description "The Client ID from the option 61";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4/client-mac-address
                        leaf client-mac-address {
                            type yang:mac-address;
                            description "The MAC address of the client";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4/vendor-class-id
                        leaf vendor-class-id {
                            type binary {
                                length "0..32";
                            }
                            description "The content of option 60 for this DHCP lease state";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4/option-82
                        container option-82 {
                            description "Enter the option-82 context";

                            ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4/option-82/value
                            leaf value {
                                type string {
                                    length "0..257";
                                }
                                description "The content of option 82 for this DHCP lease state";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4/option-82/circuit-id
                            leaf circuit-id {
                                type string {
                                    length "0..255";
                                }
                                description "Circuit ID from option 82";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4/option-82/remote-id
                            leaf remote-id {
                                type string {
                                    length "0..255";
                                }
                                description "The Remote ID from the option 82";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4/option-82/subscriber-id
                            leaf subscriber-id {
                                type string {
                                    length "0..255";
                                }
                                description "The subscriber id from the option 82";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4/option-82/vendor-specific-system
                            leaf vendor-specific-system {
                                type string {
                                    length "0..255";
                                }
                                description "The VSA-system from the option 82";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4/option-82/vendor-specific-client-mac
                            leaf vendor-specific-client-mac {
                                type string {
                                    length "0..255";
                                }
                                description "The VSA-client-mac from the option 82";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4/option-82/vendor-specific-service
                            leaf vendor-specific-service {
                                type string {
                                    length "0..255";
                                }
                                description "The VSA-service from the option 82";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4/option-82/vendor-specific-sap
                            leaf vendor-specific-sap {
                                type string {
                                    length "0..255";
                                }
                                description "The VSA-sap from the option 82";
                            }

                            ///nokia-state/state/router/dhcp-server/dhcpv4/lease/ipv4/option-82/vendor-specific-string
                            leaf vendor-specific-string {
                                type string {
                                    length "0..255";
                                }
                                description "The VSA-string from the option 82";
                            }

                        } // container option-82

                    } // container ipv4

                } // list lease

                ///nokia-state/state/router/dhcp-server/dhcpv4/sticky-lease
                list sticky-lease {
                    key "host-name";
                    description "Enter the sticky-lease list instance";

                    ///nokia-state/state/router/dhcp-server/dhcpv4/sticky-lease/host-name
                    leaf host-name {
                        type types-sros:named-item;
                        description "The host name of the sticky lease.";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv4/sticky-lease/address
                    leaf address {
                        type types-sros:ipv4-address;
                        description "The client IP address of this lease";
                    }

                } // list sticky-lease

            } // list dhcpv4

            ///nokia-state/state/router/dhcp-server/dhcpv6
            list dhcpv6 {
                key "name";
                description "Enter the dhcpv6 list instance";

                ///nokia-state/state/router/dhcp-server/dhcpv6/name
                leaf name {
                    type types-sros:named-item;
                    description "DHCP server name";
                }

                ///nokia-state/state/router/dhcp-server/dhcpv6/oper-state
                leaf oper-state {
                    type enumeration {
                        enum unknown                        { value 1; }
                        enum up                             { value 2; }
                        enum down                           { value 3; }
                        enum transition                     { value 4; }
                        enum wait-persistence               { value 5; }
                    }
                    description "Operational state of this local DHCP server.";
                }

                ///nokia-state/state/router/dhcp-server/dhcpv6/creation-origin
                leaf creation-origin {
                    type types-sros:creation-origin;
                    description "Creation origin of this local DHCP server.";
                }

                ///nokia-state/state/router/dhcp-server/dhcpv6/server-id
                container server-id {
                    description "Enable the server-id context";

                    ///nokia-state/state/router/dhcp-server/dhcpv6/server-id/enterprise-number
                    leaf enterprise-number {
                        type uint32;
                        description "The vendor's registered Private Enterprise Number as maintained by IANA";
                    }

                } // container server-id

                ///nokia-state/state/router/dhcp-server/dhcpv6/failover
                container failover {
                    description "Enter the failover context";

                    ///nokia-state/state/router/dhcp-server/dhcpv6/failover/state
                    leaf state {
                        type enumeration {
                            enum unknown {
                                value 1;
                                description "unknown";
                            }
                            enum init {
                                value 2;
                                description "preparing to startup";
                            }
                            enum start-up {
                                value 3;
                                description "starting up";
                            }
                            enum no-communication {
                                value 4;
                                description "communication with partner is lost";
                            }
                            enum partner-down {
                                value 5;
                                description "partner is assumed down";
                            }
                            enum normal {
                                value 6;
                                description "normal";
                            }
                            enum disabled {
                                value 7;
                                description "administrative state of server or failover facility is down";
                            }
                            enum transition {
                                value 8;
                                description "transition";
                            }
                            enum pre-normal {
                                value 9;
                                description "sub-state of normal, that applies to a pair of servers A and B, while B is recovering after it went down: - server A is responding to (instead of dropping) renew and rebind messages for remote leases it has allocated during partnerDown; - server B is dropping (instead of NACKing) renew and rebind messages for yet unknown local leases while resynchronizing with the MCS database.";
                            }
                        }
                        description "Failover state";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv6/failover/time-left
                    leaf time-left {
                        type union {
                            type enumeration {
                                enum infinite {
                                    value -1;
                                    description "none of the timers is running";
                                }
                            }
                            type int32;
                        }
                        units "seconds";
                        description "Time left until expiry of one of the timers";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv6/failover/actual-max-client-lead-time
                    leaf actual-max-client-lead-time {
                        type types-sros:time-duration {
                            range "600..86399";
                        }
                        units "seconds";
                        description "Actual maximum client lead time";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv6/failover/statistics
                    container statistics {
                        description "Enter the statistics context";

                        ///nokia-state/state/router/dhcp-server/dhcpv6/failover/statistics/lease-not-found
                        leaf lease-not-found {
                            type yang:counter32;
                            description "number of dropped binding updates because the corresponding lease cannot be found";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/failover/statistics/drop-invalid-packets
                        leaf drop-invalid-packets {
                            type yang:counter32;
                            description "number of dropped binding updates because the packet was malformed";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/failover/statistics/admin-disabled
                        leaf admin-disabled {
                            type yang:counter32;
                            description "number of dropped binding updates because the failover admin-state is disabled";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/failover/statistics/expired
                        leaf expired {
                            type yang:counter32;
                            description "number of dropped binding updates because the lease has expired";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/failover/statistics/max-reached
                        leaf max-reached {
                            type yang:counter32;
                            description "number of dropped binding updates because the maximum number of leases was reached";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/failover/statistics/subnet-not-found
                        leaf subnet-not-found {
                            type yang:counter32;
                            description "number of dropped binding updates because the subnet could not be found for the lease";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/failover/statistics/range-not-found
                        leaf range-not-found {
                            type yang:counter32;
                            description "number of dropped binding updates because a valid include range could not be found for the lease";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/failover/statistics/host-conflict
                        leaf host-conflict {
                            type yang:counter32;
                            description "number of dropped add binding updates because the address is leased to another host";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/failover/statistics/address-conflict
                        leaf address-conflict {
                            type yang:counter32;
                            description "number of dropped add binding updates because the host has already another address";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/failover/statistics/peer-conflict
                        leaf peer-conflict {
                            type yang:counter32;
                            description "number of dropped add binding updates because the failover peer has leased a conflicting address";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/failover/statistics/persistency-congestion
                        leaf persistency-congestion {
                            type yang:counter32;
                            description "number of dropped binding updates because of persistency congestion on the host";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/failover/statistics/zero-lease-hold-time
                        leaf zero-lease-hold-time {
                            type yang:counter32;
                            description "number of dropped binding updates because the lease-hold-time is zero";
                        }

                        ///nokia-state/state/router/dhcp-server/dhcpv6/failover/statistics/invalid-prefix-length
                        leaf invalid-prefix-length {
                            type yang:counter32;
                            description "number of dropped binding updates because the lease is not inside the valid prefix-length range";
                        }

                    } // container statistics

                } // container failover

                ///nokia-state/state/router/dhcp-server/dhcpv6/pool
                list pool {
                    key "pool-name";
                    description "Enter the pool list instance";

                    ///nokia-state/state/router/dhcp-server/dhcpv6/pool/pool-name
                    leaf pool-name {
                        type types-sros:named-item;
                        description "DHCP server pool name";
                    }

                    ///nokia-state/state/router/dhcp-server/dhcpv6/pool/failover
                    container failover {
                        description "Enable the failover context";

                        ///nokia-state/state/router/dhcp-server/dhcpv6/pool/failover/state
                        leaf state {
                            type enumeration {
                                enum unknown {
                                    value 1;
                                    description "unknown";
                                }
                                enum init {
                                    value 2;
                                    description "preparing to startup";
                                }
                                enum start-up {
                                    value 3;
                                    description "starting up";
                                }
                                enum no-communication {
                                    value 4;
                                    description "communication with partner is lost";
                                }
                                enum partner-down {
                                    value 5;
                                    description "partner is assumed down";
                                }
                                enum normal {
                                    value 6;
                                    description "normal";
                                }
                                enum disabled {
                                    value 7;
                                    description "administrative state of server or failover facility is down";
                                }
