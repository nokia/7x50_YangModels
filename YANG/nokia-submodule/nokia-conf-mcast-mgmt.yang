                            description "Enter the queue-parameters context";

                            ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/primary-path/queue-parameters/cbs
                            leaf cbs {
                                type decimal64 {
                                    range "0.00..100.00";
                                    fraction-digits 2;
                                }
                                units "percent";
                                default "5.00";
                                description "Override for default CBS for single queue for each path";
                            }

                            ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/primary-path/queue-parameters/mbs
                            leaf mbs {
                                type decimal64 {
                                    range "0.00..100.00";
                                    fraction-digits 2;
                                }
                                units "percent";
                                default "7.00";
                                description "Override for default MBS for single queue for each path";
                            }

                            ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/primary-path/queue-parameters/drop-tail
                            container drop-tail {
                                description "Enter the drop-tail context";

                                ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/primary-path/queue-parameters/drop-tail/low
                                container low {
                                    description "Enter the low context";

                                    ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/primary-path/queue-parameters/drop-tail/low/percent-reduction-from-mbs
                                    leaf percent-reduction-from-mbs {
                                        type types-sros:percent;
                                        default "10";
                                        description "Low drop-tail percent from MBS that is reduced";
                                    }

                                } // container low

                            } // container drop-tail

                        } // container queue-parameters

                    } // container primary-path

                    ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/secondary-path
                    container secondary-path {
                        description "Enable the secondary-path context";

                        ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/secondary-path/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/secondary-path/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/secondary-path/number-paths
                        container number-paths {
                            description "Enter the number-paths context";

                            ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/secondary-path/number-paths/number-of-paths
                            leaf number-of-paths {
                                type uint32 {
                                    range "1..15";
                                }
                                default "1";
                                description "Number of secondary paths";
                            }

                            ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/secondary-path/number-paths/redundant-sfm
                            leaf redundant-sfm {
                                type uint32 {
                                    range "1..15";
                                }
                                default "1";
                                description "Number of secondary paths in redundant SFM mode";
                            }

                        } // container number-paths

                        ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/secondary-path/queue-parameters
                        container queue-parameters {
                            description "Enter the queue-parameters context";

                            ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/secondary-path/queue-parameters/cbs
                            leaf cbs {
                                type decimal64 {
                                    range "0.00..100.00";
                                    fraction-digits 2;
                                }
                                units "percent";
                                default "30.00";
                                description "Override for default CBS for single queue for each path";
                            }

                            ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/secondary-path/queue-parameters/mbs
                            leaf mbs {
                                type decimal64 {
                                    range "0.00..100.00";
                                    fraction-digits 2;
                                }
                                units "percent";
                                default "40.00";
                                description "Override for default MBS for single queue for each path";
                            }

                            ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/secondary-path/queue-parameters/drop-tail
                            container drop-tail {
                                description "Enter the drop-tail context";

                                ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/secondary-path/queue-parameters/drop-tail/low
                                container low {
                                    description "Enter the low context";

                                    ///nokia-conf/configure/multicast-management/bandwidth-policy/impm-paths/secondary-path/queue-parameters/drop-tail/low/percent-reduction-from-mbs
                                    leaf percent-reduction-from-mbs {
                                        type types-sros:percent;
                                        default "10";
                                        description "Low drop-tail percent from MBS that is reduced";
                                    }

                                } // container low

                            } // container drop-tail

                        } // container queue-parameters

                    } // container secondary-path

                } // container impm-paths

            } // list bandwidth-policy

            ///nokia-conf/configure/multicast-management/chassis-level
            container chassis-level {
                description "Enter the chassis-level context";

                ///nokia-conf/configure/multicast-management/chassis-level/mmrp-impm-override
                leaf mmrp-impm-override {
                    type boolean;
                    default "false";
                    description "Manage MMRP traffic by IMPM";
                }

                ///nokia-conf/configure/multicast-management/chassis-level/round-robin-inactive-records
                leaf round-robin-inactive-records {
                    type boolean;
                    default "false";
                    description "Redistribute newly populated inactive records";
                }

                ///nokia-conf/configure/multicast-management/chassis-level/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/multicast-management/chassis-level/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/multicast-management/chassis-level/per-mcast-plane-capacity
                container per-mcast-plane-capacity {
                    description "Enter the per-mcast-plane-capacity context";

                    ///nokia-conf/configure/multicast-management/chassis-level/per-mcast-plane-capacity/total-capacity
                    leaf total-capacity {
                        type enumeration {
                            enum dynamic                        { value 0; }
                            enum 2000                           { value 2000; }
                            enum 4000                           { value 4000; }
                            enum 5250                           { value 5250; }
                            enum 8250                           { value 8250; }
                            enum 15000                          { value 15000; }
                            enum 17000                          { value 17000; }
                            enum 19000                          { value 19000; }
                            enum 27000                          { value 27000; }
                            enum 42000                          { value 42000; }
                            enum 90000                          { value 90000; }
                            enum 16500                          { status obsolete; }
                            enum 28000                          { status obsolete; }
                            enum 45000                          { status obsolete; }
                        }
                        units "megabps";
                        description "Total multicast plane bandwidth";
                    }

                    ///nokia-conf/configure/multicast-management/chassis-level/per-mcast-plane-capacity/mcast-capacity
                    container mcast-capacity {
                        description "Enter the mcast-capacity context";

                        ///nokia-conf/configure/multicast-management/chassis-level/per-mcast-plane-capacity/mcast-capacity/primary-percentage
                        leaf primary-percentage {
                            type decimal64 {
                                range "0.01..100.00";
                                fraction-digits 2;
                            }
                            units "percent";
                            description "Percent of plane capacity for primary multicast planes";
                        }

                        ///nokia-conf/configure/multicast-management/chassis-level/per-mcast-plane-capacity/mcast-capacity/secondary-percentage
                        leaf secondary-percentage {
                            type decimal64 {
                                range "0.01..100.00";
                                fraction-digits 2;
                            }
                            units "percent";
                            description "Percent of plane capacity for secondary multicast plane";
                        }

                    } // container mcast-capacity

                    ///nokia-conf/configure/multicast-management/chassis-level/per-mcast-plane-capacity/redundant-mcast-capacity
                    container redundant-mcast-capacity {
                        description "Enter the redundant-mcast-capacity context";

                        ///nokia-conf/configure/multicast-management/chassis-level/per-mcast-plane-capacity/redundant-mcast-capacity/primary-percentage
                        leaf primary-percentage {
                            type decimal64 {
                                range "0.01..100.00";
                                fraction-digits 2;
                            }
                            units "percent";
                            description "Percent of plane capacity for primary multicast planes";
                        }

                        ///nokia-conf/configure/multicast-management/chassis-level/per-mcast-plane-capacity/redundant-mcast-capacity/secondary-percentage
                        leaf secondary-percentage {
                            type decimal64 {
                                range "0.01..100.00";
                                fraction-digits 2;
                            }
                            units "percent";
                            description "Percent of plane capacity for secondary multicast plane";
                        }

                    } // container redundant-mcast-capacity

                } // container per-mcast-plane-capacity

            } // container chassis-level

        } // container multicast-management

    } // grouping conf-mcast-mgmt-multicast-management

}
