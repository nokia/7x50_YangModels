submodule nokia-conf-mcast-mgmt {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-mcast-mgmt         { prefix "types-mcast-mgmt"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-conf-groups;
    include nokia-conf-qos;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure mcast mgmt on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-mcast-mgmt YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2022-07-18";

    grouping conf-mcast-mgmt-multicast-management {
        ///nokia-conf/configure/multicast-management
        container multicast-management {
            description "Enter the multicast-management context";

            ///nokia-conf/configure/multicast-management/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/multicast-management/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/multicast-management/multicast-info-policy
            list multicast-info-policy {
                key "policy-name";
                max-elements 32;
                description "Enter the multicast-info-policy list instance";

                ///nokia-conf/configure/multicast-management/multicast-info-policy/policy-name
                leaf policy-name {
                    type types-sros:named-item;
                    description "Multicast information policy name";
                }

                ///nokia-conf/configure/multicast-management/multicast-info-policy/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/multicast-management/multicast-info-policy/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/multicast-management/multicast-info-policy/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle
                list bundle {
                    key "bundle-name";
                    max-elements 32;
                    description "Enter the bundle list instance";

                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/bundle-name
                    leaf bundle-name {
                        type types-sros:named-item;
                        description "Bundle name";
                    }

                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/admin-bw
                    leaf admin-bw {
                        type uint32 {
                            range "1..40000000";
                        }
                        units "kilobps";
                        description "Administrative bandwidth";
                    }

                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/cong-priority-threshold
                    leaf cong-priority-threshold {
                        type uint32 {
                            range "0..7";
                        }
                        default "4";
                        description "Preference level threshold from low to high priority";
                    }

                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/ecmp-opt-threshold
                    leaf ecmp-opt-threshold {
                        type uint32 {
                            range "0..7";
                        }
                        default "7";
                        description "ECMP threshold";
                    }

                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/explicit-sf-path
                    leaf explicit-sf-path {
                        type enumeration {
                            enum primary                        { value 1; }
                            enum secondary                      { value 2; }
                        }
                        description "Explicit ingress switch fabric multicast path";
                    }

                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/keepalive-override
                    leaf keepalive-override {
                        type uint32 {
                            range "10..86000";
                        }
                        units "seconds";
                        description "Keepalive timer override";
                    }

                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/preference
                    leaf preference {
                        type uint32 {
                            range "1..7";
                        }
                        description "Relative preference level for multicast channels";
                    }

                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/bw-activity
                    container bw-activity {
                        description "Enter the bw-activity context";

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/bw-activity/bw-activity-type
                        leaf bw-activity-type {
                            type enumeration {
                                enum use-admin-bw                   { value 1; }
                                enum dynamic                        { value 2; }
                            }
                            default "dynamic";
                            description "Bandwidth required by a multicast channel";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/bw-activity/black-hole-rate
                        leaf black-hole-rate {
                            type uint32 {
                                range "1..40000000";
                            }
                            units "kilobps";
                            description "Rate that a channel is placed in the blackhole state";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/bw-activity/falling-delay
                        leaf falling-delay {
                            type uint32 {
                                range "10..3600";
                            }
                            units "seconds";
                            default "30";
                            description "Falling delay threshold value until delay time expires";
                        }

                    } // container bw-activity

                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/primary-tunnel-interface
                    container primary-tunnel-interface {
                        description "Enter the primary-tunnel-interface context";

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/primary-tunnel-interface/sender
                        leaf sender {
                            type types-sros:ipv4-address;
                            description "Bundle set to receive from the primary tunnel interface";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/primary-tunnel-interface/p2mp
                        choice p2mp {
                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/primary-tunnel-interface/p2mp/rsvp-p2mp
                            case rsvp-p2mp {

                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/primary-tunnel-interface/p2mp/rsvp-p2mp/rsvp-p2mp
                                leaf rsvp-p2mp {
                                    type types-sros:named-item;
                                    description "P2MP name for the LDP P2MP";
                                }

                            }
                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/primary-tunnel-interface/p2mp/ldp-p2mp
                            case ldp-p2mp {

                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/primary-tunnel-interface/p2mp/ldp-p2mp/ldp-p2mp
                                leaf ldp-p2mp {
                                    type uint32 {
                                        range "1..4294967295";
                                    }
                                    description "ID for signaling MLDP P2MP LSP";
                                }

                            }
                        }
                    } // container primary-tunnel-interface

                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video
                    container video {
                        description "Enter the video context";

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/fcc-channel-type
                        leaf fcc-channel-type {
                            type types-mcast-mgmt:channel-type;
                            description "Channel type for the bundle or channel";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/fcc-min-duration
                        leaf fcc-min-duration {
                            type uint32 {
                                range "300..8000";
                            }
                            units "milliseconds";
                            default "300";
                            description "Minimum duration of the FCC burst";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/fcc-server
                        leaf fcc-server {
                            type boolean;
                            default "false";
                            description "Enable the FCC server for a multicast bundle";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/local-fcc-port
                        leaf local-fcc-port {
                            type uint32 {
                                range "1024..5999|6251..65535";
                            }
                            default "4098";
                            description "Local port for FCC requests";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/local-rt-port
                        leaf local-rt-port {
                            type uint32 {
                                range "1024..5999|6251..65535";
                            }
                            default "4096";
                            description "Local port for RET requests";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/local-rt-server
                        leaf local-rt-server {
                            type boolean;
                            default "false";
                            description "Enable local retransmission server capability";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/reorder-audio
                        leaf reorder-audio {
                            type uint32 {
                                range "100..1000";
                            }
                            units "milliseconds";
                            description "Time the audio packets are reordered in the ad stream";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/rt-buffer-size
                        leaf rt-buffer-size {
                            type uint32 {
                                range "300..8000";
                            }
                            units "milliseconds";
                            default "300";
                            description "Buffer size to store channel packets";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/video-group
                        leaf video-group {
                            type uint32 {
                                range "1..4";
                            }
                            description "Video group for the bundle";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/rt-server
                        container rt-server {
                            presence "Enable the rt-server context";
                            description "Enable the rt-server context";

                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/rt-server/ip-address
                            leaf ip-address {
                                type types-sros:ipv4-unicast-address;
                                mandatory true;
                                description "IP address of the upstream retransmission server";
                            }

                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/rt-server/port
                            leaf port {
                                type uint32 {
                                    range "1024..5999|6251..65535";
                                }
                                mandatory true;
                                description "RET server port number";
                            }

                        } // container rt-server

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/stream-selection
                        container stream-selection {
                            description "Enter the stream-selection context";

                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/stream-selection/source1
                            leaf source1 {
                                type types-sros:ipv4-address;
                                description "Primary source IP address";
                            }

                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/stream-selection/intf1
                            leaf intf1 {
                                type types-sros:named-item;
                                description "Primary interface";
                            }

                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/stream-selection/source2
                            leaf source2 {
                                type types-sros:ipv4-address;
                                description "Secondary source IP address";
                            }

                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/stream-selection/intf2
                            leaf intf2 {
                                type types-sros:named-item;
                                description "Secondary interface";
                            }

                        } // container stream-selection

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer
                        container analyzer {
                            presence "Enable the analyzer context";
                            description "Enable the analyzer context";

                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/description
                            leaf description {
                                type types-sros:description;
                                description "Text description";
                            }

                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms
                            container alarms {
                                description "Enter the alarms context";

                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/cc-error
                                leaf cc-error {
                                    type boolean;
                                    default "false";
                                    description "Check the continuity counter";
                                }

                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/non-vid-pid-absent
                                leaf non-vid-pid-absent {
                                    type uint32 {
                                        range "100..5000";
                                    }
                                    units "milliseconds";
                                    description "PID checked within the specified interval";
                                }

                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pat-syntax
                                leaf pat-syntax {
                                    type boolean;
                                    default "false";
                                    description "Check for PAT syntax errors";
                                }

                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pid-pmt-unref
                                leaf pid-pmt-unref {
                                    type boolean;
                                    default "false";
                                    description "Check for unreferenced PIDs in the PMT";
                                }

                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pmt-syntax
                                leaf pmt-syntax {
                                    type boolean;
                                    default "false";
                                    description "Check for PMT syntax errors";
                                }

                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/tei-set
                                leaf tei-set {
                                    type boolean;
                                    default "false";
                                    description "Check for TEI set errors";
                                }

                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/ts-sync-loss
                                leaf ts-sync-loss {
                                    type boolean;
                                    default "false";
                                    description "Check for synchronization loss errors";
                                }

                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/vid-pid-absent
                                leaf vid-pid-absent {
                                    type uint32 {
                                        range "100..5000";
                                    }
                                    units "milliseconds";
                                    description "VID PID checked within the specified time interval";
                                }

                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pat-repetition
                                container pat-repetition {
                                    presence "Enable the pat-repetition context";
                                    description "Enable the pat-repetition context";

                                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pat-repetition/tnc
                                    leaf tnc {
                                        type uint32 {
                                            range "100..800";
                                        }
                                        units "milliseconds";
                                        default "100";
                                        description "Time between two PATs for which a TNC alarm is raised";
                                    }

                                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pat-repetition/qos
                                    leaf qos {
                                        type uint32 {
                                            range "200..900";
                                        }
                                        units "milliseconds";
                                        default "200";
                                        description "Time between two PATs for which a QoS alarm is raised";
                                    }

                                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pat-repetition/poa
                                    leaf poa {
                                        type uint32 {
                                            range "300..1000";
                                        }
                                        units "milliseconds";
                                        default "500";
                                        description "Time between two PATs for which a POA alarm is raised";
                                    }

                                } // container pat-repetition

                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pcr-repetition
                                container pcr-repetition {
                                    presence "Enable the pcr-repetition context";
                                    description "Enable the pcr-repetition context";

                                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pcr-repetition/tnc
                                    leaf tnc {
                                        type uint32 {
                                            range "100..800";
                                        }
                                        units "milliseconds";
                                        default "100";
                                        description "Time between two PCRs for which a TNC alarm is raised";
                                    }

                                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pcr-repetition/qos
                                    leaf qos {
                                        type uint32 {
                                            range "200..900";
                                        }
                                        units "milliseconds";
                                        default "200";
                                        description "Time between two PCRs for which a QoS alarm is raised";
                                    }

                                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pcr-repetition/poa
                                    leaf poa {
                                        type uint32 {
                                            range "300..1000";
                                        }
                                        units "milliseconds";
                                        default "500";
                                        description "Time between two PCRs for which a POA alarm is raised";
                                    }

                                } // container pcr-repetition

                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pmt-repetition
                                container pmt-repetition {
                                    presence "Enable the pmt-repetition context";
                                    description "Enable the pmt-repetition context";

                                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pmt-repetition/tnc
                                    leaf tnc {
                                        type uint32 {
                                            range "100..4800";
                                        }
                                        units "milliseconds";
                                        default "400";
                                        description "Time between two PMTs for which a TNC alarm is raised";
                                    }

                                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pmt-repetition/qos
                                    leaf qos {
                                        type uint32 {
                                            range "200..4900";
                                        }
                                        units "milliseconds";
                                        default "800";
                                        description "Time between two PMTs for which a QoS alarm is raised";
                                    }

                                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/pmt-repetition/poa
                                    leaf poa {
                                        type uint32 {
                                            range "300..5000";
                                        }
                                        units "milliseconds";
                                        default "2000";
                                        description "Time between two PMTs for which a POA alarm is raised";
                                    }

                                } // container pmt-repetition

                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/report-alarm
                                container report-alarm {
                                    description "Enter the report-alarm context";

                                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/video/analyzer/alarms/report-alarm/severity
                                    leaf severity {
                                        type enumeration {
                                            enum tnc                            { value 1; }
                                            enum qos                            { value 2; }
                                            enum poa                            { value 3; }
                                        }
                                        description "Keyword for the type of alarm";
                                    }

                                } // container report-alarm

                            } // container alarms

                        } // container analyzer

                    } // container video

                    ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel
                    list channel {
                        key "start end";
                        description "Enter the channel list instance";

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/start
                        leaf start {
                            type types-sros:ip-multicast-address;
                            description "Starting IP address for the channel range";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/end
                        leaf end {
                            type types-sros:ip-multicast-address;
                            description "Ending IP address for the channel range";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/admin-bw
                        leaf admin-bw {
                            type uint32 {
                                range "1..40000000";
                            }
                            units "kilobps";
                            description "Administrative bandwidth";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/explicit-sf-path
                        leaf explicit-sf-path {
                            type enumeration {
                                enum primary                        { value 1; }
                                enum secondary                      { value 2; }
                            }
                            description "Explicit switch fabric path shared to multicast channel";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/keepalive-override
                        leaf keepalive-override {
                            type uint32 {
                                range "10..86000";
                            }
                            units "seconds";
                            description "Keepalive timer override";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/preference
                        leaf preference {
                            type uint32 {
                                range "1..7";
                            }
                            description "Relative preference level for multicast channels";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/bw-activity
                        container bw-activity {
                            description "Enter the bw-activity context";

                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/bw-activity/bw-activity-type
                            leaf bw-activity-type {
                                type enumeration {
                                    enum use-admin-bw                   { value 1; }
                                    enum dynamic                        { value 2; }
                                }
                                description "Bandwidth required by a multicast channel";
                            }

                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/bw-activity/black-hole-rate
                            leaf black-hole-rate {
                                type uint32 {
                                    range "0..40000000";
                                }
                                units "kilobps";
                                default "0";
                                description "Rate that a channel is placed in the blackhole state";
                            }

                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/bw-activity/falling-delay
                            leaf falling-delay {
                                type uint32 {
                                    range "10..3600";
                                }
                                units "seconds";
                                description "Falling delay threshold value until delay time expires";
                            }

                        } // container bw-activity

                        ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/primary-tunnel-interface
                        container primary-tunnel-interface {
                            description "Enter the primary-tunnel-interface context";

                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/primary-tunnel-interface/sender
                            leaf sender {
                                type types-sros:ipv4-address;
                                description "Bundle set to receive from the primary tunnel interface";
                            }

                            ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/primary-tunnel-interface/p2mp
                            choice p2mp {
                                ///nokia-conf/configure/multicast-management/multicast-info-policy/bundle/channel/primary-tunnel-interface/p2mp/rsvp-p2mp
