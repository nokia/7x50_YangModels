submodule nokia-conf-filter {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-filter             { prefix "types-filter"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-subscriber-mgmt    { prefix "types-submgt"; }

    include nokia-conf-cflowd;
    include nokia-conf-filter-dhcp;
    include nokia-conf-filter-ip-exception;
    include nokia-conf-groups;
    include nokia-conf-log;
    include nokia-conf-openflow;
    include nokia-conf-service;
    include nokia-conf-svc-nat;
    include nokia-conf-svc-vpls;
    include nokia-conf-svc-vprn;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure line card ACL filters on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-filter YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-05-31";

    grouping conf-filter {
        ///nokia-conf/configure/filter
        container filter {
            description "Enter the filter context";

            ///nokia-conf/configure/filter/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/filter/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/filter/redirect-policy
            list redirect-policy {
                key "redirect-policy-name";
                description "Enter the redirect-policy list instance";

                ///nokia-conf/configure/filter/redirect-policy/redirect-policy-name
                leaf redirect-policy-name {
                    type types-sros:named-item;
                    description "Redirect policy name";
                }

                ///nokia-conf/configure/filter/redirect-policy/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "disable";
                    description "Administrative state of the redirect policy";
                }

                ///nokia-conf/configure/filter/redirect-policy/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/filter/redirect-policy/router-instance
                leaf router-instance {
                    type string;
                    description "Routing context to use for route lookup";
                }

                ///nokia-conf/configure/filter/redirect-policy/sticky-dest
                leaf sticky-dest {
                    type types-filter:filter-sticky-dest;
                    units "seconds";
                    description "Time required by system before applying the current best destination as active destination";
                }

                ///nokia-conf/configure/filter/redirect-policy/notify-dest-change
                leaf notify-dest-change {
                    type boolean;
                    default "false";
                    description "Send notifications when the active destination changes";
                }

                ///nokia-conf/configure/filter/redirect-policy/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/filter/redirect-policy/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/filter/redirect-policy/destination
                list destination {
                    key "destination-address";
                    description "Enter the destination list instance";

                    ///nokia-conf/configure/filter/redirect-policy/destination/destination-address
                    leaf destination-address {
                        type types-sros:ip-unicast-address;
                        description "IP address and type of destination";
                    }

                    ///nokia-conf/configure/filter/redirect-policy/destination/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the destination";
                    }

                    ///nokia-conf/configure/filter/redirect-policy/destination/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/filter/redirect-policy/destination/priority
                    leaf priority {
                        type uint32 {
                            range "1..255";
                        }
                        default "100";
                        description "Priority for this destination";
                    }

                    ///nokia-conf/configure/filter/redirect-policy/destination/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/redirect-policy/destination/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/redirect-policy/destination/ping-test
                    container ping-test {
                        presence "Enable the ping-test context";
                        description "Enable the ping-test context";

                        ///nokia-conf/configure/filter/redirect-policy/destination/ping-test/source-address
                        leaf source-address {
                            type types-sros:ip-address;
                            description "Source address to use in the IP packet of the ping test";
                        }

                        ///nokia-conf/configure/filter/redirect-policy/destination/ping-test/interval
                        leaf interval {
                            type uint32 {
                                range "1..60";
                            }
                            units "seconds";
                            default "1";
                            description "Time between consecutive requests which are sent to the far end host";
                        }

                        ///nokia-conf/configure/filter/redirect-policy/destination/ping-test/timeout
                        leaf timeout {
                            type uint32 {
                                range "1..60";
                            }
                            units "seconds";
                            default "1";
                            description "Time required to receive a response from the far end host";
                        }

                        ///nokia-conf/configure/filter/redirect-policy/destination/ping-test/drop-count
                        leaf drop-count {
                            type uint32 {
                                range "1..60";
                            }
                            default "3";
                            description "Number of consecutive requests that fail before destination is declared unreachable";
                        }

                        ///nokia-conf/configure/filter/redirect-policy/destination/ping-test/hold-down
                        leaf hold-down {
                            type uint32 {
                                range "0..86400";
                            }
                            units "seconds";
                            default "0";
                            description "Time for the system to be held down if this test has marked it unreachable";
                        }

                        ///nokia-conf/configure/filter/redirect-policy/destination/ping-test/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/filter/redirect-policy/destination/ping-test/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container ping-test

                    ///nokia-conf/configure/filter/redirect-policy/destination/unicast-rt-test
                    container unicast-rt-test {
                        presence "Enable the unicast-rt-test context";
                        description "Enable the unicast-rt-test context";

                    } // container unicast-rt-test

                } // list destination

            } // list redirect-policy

            ///nokia-conf/configure/filter/redirect-policy-binding
            list redirect-policy-binding {
                key "binding-name";
                max-elements 16;
                description "Enter the redirect-policy-binding list instance";

                ///nokia-conf/configure/filter/redirect-policy-binding/binding-name
                leaf binding-name {
                    type types-sros:named-item;
                    description "Binding name";
                }

                ///nokia-conf/configure/filter/redirect-policy-binding/binding-operator
                leaf binding-operator {
                    type types-filter:filter-binding-operator;
                    default "and";
                    description "Logical operator used to obtain the master test result";
                }

                ///nokia-conf/configure/filter/redirect-policy-binding/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/filter/redirect-policy-binding/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/filter/redirect-policy-binding/redirect-policy
                list redirect-policy {
                    key "redirect-policy-name";
                    description "Enter the redirect-policy list instance";

                    ///nokia-conf/configure/filter/redirect-policy-binding/redirect-policy/redirect-policy-name
                    leaf redirect-policy-name {
                        type leafref {
                            path "../../../redirect-policy/redirect-policy-name";
                        }
                        description "Redirect policy name";
                    }

                    ///nokia-conf/configure/filter/redirect-policy-binding/redirect-policy/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/redirect-policy-binding/redirect-policy/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/redirect-policy-binding/redirect-policy/destination
                    list destination {
                        key "destination-address";
                        min-elements 1;
                        description "Add a list entry for destination";

                        ///nokia-conf/configure/filter/redirect-policy-binding/redirect-policy/destination/destination-address
                        leaf destination-address {
                            type leafref {
                                path "../../../../redirect-policy[redirect-policy-name=current()/../../redirect-policy-name]/destination/destination-address";
                            }
                            description "IP address of redirect policy destination to binding";
                        }

                    } // list destination

                } // list redirect-policy

            } // list redirect-policy-binding

            ///nokia-conf/configure/filter/log
            list log {
                key "log-id";
                description "Enter the log list instance";

                ///nokia-conf/configure/filter/log/log-id
                leaf log-id {
                    type types-filter:filter-log-id;
                    description "Filter log identifier";
                }

                ///nokia-conf/configure/filter/log/description
                leaf description {
                    type types-sros:description-or-empty;
                    description "Text description";
                }

                ///nokia-conf/configure/filter/log/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "enable";
                    description "Administrative state of filter logging";
                }

                ///nokia-conf/configure/filter/log/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/filter/log/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/filter/log/destination
                container destination {
                    description "Enter the destination context";

                    ///nokia-conf/configure/filter/log/destination/destination
                    choice destination {
                        default "memory";
                        ///nokia-conf/configure/filter/log/destination/destination/memory
                        case memory {

                            ///nokia-conf/configure/filter/log/destination/destination/memory/memory
                            container memory {
                                description "Enter the memory context";

                                ///nokia-conf/configure/filter/log/destination/destination/memory/memory/max-entries
                                leaf max-entries {
                                    type uint32 {
                                        range "1..50000";
                                    }
                                    default "1000";
                                    description "Maximum number of memory entries that the log can store";
                                }

                                ///nokia-conf/configure/filter/log/destination/destination/memory/memory/stop-on-full
                                leaf stop-on-full {
                                    type boolean;
                                    default "false";
                                    description "Stop logging when maximum number of memory entries is reached or wrap-around is used";
                                }

                            } // container memory

                        }
                        ///nokia-conf/configure/filter/log/destination/destination/syslog
                        case syslog {

                            ///nokia-conf/configure/filter/log/destination/destination/syslog/syslog
                            container syslog {
                                description "Enter the syslog context";

                                ///nokia-conf/configure/filter/log/destination/destination/syslog/syslog/syslog-id
                                leaf syslog-id {
                                    status obsolete;
                                    type leafref {
                                        path "../../../../../log/syslog/syslog-name";
                                    }
                                    description "ID of the Syslog server definition for filter logs";
                                }

                                ///nokia-conf/configure/filter/log/destination/destination/syslog/syslog/name
                                leaf name {
                                    type leafref {
                                        path "../../../../../log/syslog/syslog-name";
                                    }
                                    description "Syslog server definition ID";
                                }

                                ///nokia-conf/configure/filter/log/destination/destination/syslog/syslog/summary
                                container summary {
                                    description "Enter the summary context";

                                    ///nokia-conf/configure/filter/log/destination/destination/syslog/syslog/summary/admin-state
                                    leaf admin-state {
                                        type types-sros:admin-state;
                                        default "disable";
                                        description "Administrative state of filter log summarization";
                                    }

                                    ///nokia-conf/configure/filter/log/destination/destination/syslog/syslog/summary/summary-crit
                                    leaf summary-crit {
                                        type types-filter:filter-log-summary-criterion;
                                        default "src-addr";
                                        description "Summary for filter log entries";
                                    }

                                } // container summary

                            } // container syslog

                        }
                    }
                } // container destination

            } // list log

            ///nokia-conf/configure/filter/match-list
            container match-list {
                description "Enter the match-list context";

                ///nokia-conf/configure/filter/match-list/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/filter/match-list/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/filter/match-list/protocol-list
                list protocol-list {
                    key "protocol-list-name";
                    max-elements 512;
                    description "Enter the protocol-list list instance";

                    ///nokia-conf/configure/filter/match-list/protocol-list/protocol-list-name
                    leaf protocol-list-name {
                        type types-sros:named-item;
                        description "Protocol list name";
                    }

                    ///nokia-conf/configure/filter/match-list/protocol-list/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/filter/match-list/protocol-list/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/match-list/protocol-list/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/match-list/protocol-list/protocol
                    list protocol {
                        key "protocol-id";
                        max-elements 32;
                        description "Add a list entry for protocol";

                        ///nokia-conf/configure/filter/match-list/protocol-list/protocol/protocol-id
                        leaf protocol-id {
                            type types-sros:ip-protocol;
                            description "IP protocol identifier";
                        }

                    } // list protocol

                } // list protocol-list

                ///nokia-conf/configure/filter/match-list/ip-prefix-list
                list ip-prefix-list {
                    key "prefix-list-name";
                    description "Enter the ip-prefix-list list instance";

                    ///nokia-conf/configure/filter/match-list/ip-prefix-list/prefix-list-name
                    leaf prefix-list-name {
                        type types-sros:named-item;
                        description "IP prefix list name";
                    }

                    ///nokia-conf/configure/filter/match-list/ip-prefix-list/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/filter/match-list/ip-prefix-list/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/match-list/ip-prefix-list/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/match-list/ip-prefix-list/apply-path
                    container apply-path {
                        description "Enter the apply-path context";

                        ///nokia-conf/configure/filter/match-list/ip-prefix-list/apply-path/bgp-peers
                        list bgp-peers {
                            key "criterion-index";
                            description "Enter the bgp-peers list instance";

                            ///nokia-conf/configure/filter/match-list/ip-prefix-list/apply-path/bgp-peers/criterion-index
                            leaf criterion-index {
                                type uint32 {
                                    range "1..255";
                                }
                                description "BGP peers auto-generation configuration index";
                            }

                            ///nokia-conf/configure/filter/match-list/ip-prefix-list/apply-path/bgp-peers/group
                            leaf group {
                                type types-sros:regular-expression-not-all-spaces;
                                sros-ext:immutable;
                                mandatory true;
                                description "Regular expression to match against the base router BGP instance group configuration";
                            }

                            ///nokia-conf/configure/filter/match-list/ip-prefix-list/apply-path/bgp-peers/neighbor
                            leaf neighbor {
                                type types-sros:regular-expression-not-all-spaces;
                                sros-ext:immutable;
                                mandatory true;
                                description "Regular expression to match against the base router BGP instance neighbor configuration";
                            }

                            ///nokia-conf/configure/filter/match-list/ip-prefix-list/apply-path/bgp-peers/router-instance
                            leaf router-instance {
                                type string;
                                sros-ext:immutable;
                                default "Base";
                                description "Target routing instance";
                            }

                            ///nokia-conf/configure/filter/match-list/ip-prefix-list/apply-path/bgp-peers/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/filter/match-list/ip-prefix-list/apply-path/bgp-peers/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list bgp-peers



                    } // container apply-path

                    ///nokia-conf/configure/filter/match-list/ip-prefix-list/prefix
                    list prefix {
                        key "ip-prefix";
                        max-elements 8192;
                        description "Add a list entry for prefix";

                        ///nokia-conf/configure/filter/match-list/ip-prefix-list/prefix/ip-prefix
                        leaf ip-prefix {
                            type types-sros:ipv4-prefix;
                            description "IPv4 prefix to be added to the prefix list";
                        }

                    } // list prefix

                    ///nokia-conf/configure/filter/match-list/ip-prefix-list/prefix-exclude
                    list prefix-exclude {
                        key "ip-prefix";
                        max-elements 512;
                        description "Add a list entry for prefix-exclude";

                        ///nokia-conf/configure/filter/match-list/ip-prefix-list/prefix-exclude/ip-prefix
                        leaf ip-prefix {
                            type types-sros:ipv4-prefix;
                            description "IPv4 prefix to be added to the prefix list";
                        }

                    } // list prefix-exclude

                } // list ip-prefix-list

                ///nokia-conf/configure/filter/match-list/ipv6-prefix-list
                list ipv6-prefix-list {
                    key "prefix-list-name";
                    description "Enter the ipv6-prefix-list list instance";

                    ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/prefix-list-name
                    leaf prefix-list-name {
                        type types-sros:named-item;
                        description "IP prefix list name";
                    }

                    ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/apply-path
                    container apply-path {
                        description "Enter the apply-path context";

                        ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/apply-path/bgp-peers
                        list bgp-peers {
                            key "criterion-index";
                            description "Enter the bgp-peers list instance";

                            ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/apply-path/bgp-peers/criterion-index
                            leaf criterion-index {
                                type uint32 {
                                    range "1..255";
                                }
                                description "BGP peers auto-generation configuration index";
                            }

                            ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/apply-path/bgp-peers/group
                            leaf group {
                                type types-sros:regular-expression-not-all-spaces;
                                sros-ext:immutable;
                                mandatory true;
                                description "Regular expression to match against the base router BGP instance group configuration";
                            }

                            ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/apply-path/bgp-peers/neighbor
                            leaf neighbor {
                                type types-sros:regular-expression-not-all-spaces;
                                sros-ext:immutable;
                                mandatory true;
                                description "Regular expression to match against the base router BGP instance neighbor configuration";
                            }

                            ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/apply-path/bgp-peers/router-instance
                            leaf router-instance {
                                type string;
                                sros-ext:immutable;
                                default "Base";
                                description "Target routing instance";
                            }

                            ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/apply-path/bgp-peers/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/apply-path/bgp-peers/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list bgp-peers


                    } // container apply-path

                    ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/prefix
                    list prefix {
                        key "ipv6-prefix";
                        max-elements 8192;
                        description "Add a list entry for prefix";

                        ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/prefix/ipv6-prefix
                        leaf ipv6-prefix {
                            type types-sros:ipv6-prefix;
                            description "IPv6 prefix to be added to the prefix list";
                        }

                    } // list prefix

                    ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/prefix-exclude
                    list prefix-exclude {
                        key "ipv6-prefix";
                        max-elements 512;
                        description "Add a list entry for prefix-exclude";

                        ///nokia-conf/configure/filter/match-list/ipv6-prefix-list/prefix-exclude/ipv6-prefix
                        leaf ipv6-prefix {
                            type types-sros:ipv6-prefix;
                            description "IPv6 prefix to be added to the prefix list";
