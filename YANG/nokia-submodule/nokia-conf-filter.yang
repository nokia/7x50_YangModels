                                } // container dst-ip

                            }
                            ///nokia-conf/configure/filter/ipv6-filter/entry/match/ip-selector/ip
                            case ip {

                                ///nokia-conf/configure/filter/ipv6-filter/entry/match/ip-selector/ip/ip
                                container ip {
                                    description "Enter the ip context";

                                    ///nokia-conf/configure/filter/ipv6-filter/entry/match/ip-selector/ip/ip/match-address-choice
                                    choice match-address-choice {
                                        ///nokia-conf/configure/filter/ipv6-filter/entry/match/ip-selector/ip/ip/match-address-choice/address-and-prefix-or-mask
                                        case address-and-prefix-or-mask {

                                            ///nokia-conf/configure/filter/ipv6-filter/entry/match/ip-selector/ip/ip/match-address-choice/address-and-prefix-or-mask/address
                                            leaf address {
                                                type union {
                                                    type types-sros:ipv6-prefix-with-host-bits;
                                                    type types-sros:ipv6-address;
                                                }
                                                description "IPv6 address used as the match criterion";
                                            }

