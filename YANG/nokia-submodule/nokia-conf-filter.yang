                            case drop {

                                ///nokia-conf/configure/filter/ip-filter/entry/action/action/drop/drop
                                leaf drop {
                                    type empty;
                                    description "Drop a packet matching this entry";
                                }

                            }
                            ///nokia-conf/configure/filter/ip-filter/entry/action/action/forward
                            case forward {

                                ///nokia-conf/configure/filter/ip-filter/entry/action/action/forward/forward
                                container forward {
