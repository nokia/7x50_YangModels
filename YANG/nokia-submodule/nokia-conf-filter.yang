                                leaf end {
                                    type types-filter:entry-id;
                                    mandatory true;
                                    description "Upper bound of range for inserting shared host rules";
                                }

                            } // container range

                        } // container filter-rule

                        ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/shared-entry/pcc-rule
                        container pcc-rule {
                            description "Enter the pcc-rule context";

                            ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/shared-entry/pcc-rule/range
                            container range {
