                                            ///nokia-conf/configure/filter/ip-filter/entry/action/drop-when/criterion-1/packet-length/packet-length/packet-length/lt
                                            case lt {

                                                ///nokia-conf/configure/filter/ip-filter/entry/action/drop-when/criterion-1/packet-length/packet-length/packet-length/lt/lt
