                                                                "((([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\\.){3}([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5]))|
                                                                                  ((([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\\.){2}(([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\\.){1}([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5]){1})|
                                                                                  ((([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\\.){1}([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5]){1}(\\.([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){2})|
                                                                                  (([1-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5]){1}(\\.([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])){3})";
                                                        }
                                                        mandatory true;
                                                        description "Target BGP next hop IP address";
                                                    }

                                                    ///nokia-conf/configure/filter/ipv6-filter/entry/action/secondary/action/forward/forward/forward-action/vprn-target/vprn-target/vprn
                                                    leaf vprn {
                                                        type leafref {
                                                            path "../../../../../../../../service/vprn/service-name";
                                                        }
                                                        mandatory true;
                                                        description "Routing context used for route lookup";
                                                    }

                                                    ///nokia-conf/configure/filter/ipv6-filter/entry/action/secondary/action/forward/forward/forward-action/vprn-target/vprn-target/lsp
                                                    leaf lsp {
                                                        type types-sros:named-item-64;
                                                        description "LSP that is specified to forward a packet matching this entry";
                                                    }

                                                    ///nokia-conf/configure/filter/ipv6-filter/entry/action/secondary/action/forward/forward/forward-action/vprn-target/vprn-target/adv-prefix
                                                    leaf adv-prefix {
                                                        type types-sros:ipv6-prefix;
                                                        description "Advertised IP prefix for the target destination";
                                                    }

                                                } // container vprn-target

                                            }
                                        }
                                    } // container forward

                                }
                            }
                            ///nokia-conf/configure/filter/ipv6-filter/entry/action/secondary/remark
                            container remark {
                                presence "Enable the remark context";
                                description "Enable the remark context";

                                ///nokia-conf/configure/filter/ipv6-filter/entry/action/secondary/remark/dscp
                                leaf dscp {
                                    type types-qos:dscp-name;
                                    mandatory true;
                                    description "Destination SAP";
                                }

                            } // container remark

                        } // container secondary

                    } // container action

                } // list entry

                ///nokia-conf/configure/filter/ipv6-filter/embed
                container embed {
                    description "Enter the embed context";

                    ///nokia-conf/configure/filter/ipv6-filter/embed/filter
                    list filter {
                        key "name offset";
                        description "Enter the filter list instance";

                        ///nokia-conf/configure/filter/ipv6-filter/embed/filter/name
                        leaf name {
                            type leafref {
                                path "../../../../ipv6-filter/filter-name";
                            }
                            description "IPv6 policy to be embedded in the filter";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/filter/offset
                        leaf offset {
                            type types-filter:embed-offset {
                                range "0..2097150";
                            }
                            description "Offset of the inserted entries";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/filter/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "enable";
                            description "Administrative state of the embedded filter";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/filter/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/filter/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list filter

                    ///nokia-conf/configure/filter/ipv6-filter/embed/flowspec
                    list flowspec {
                        key "offset";
                        description "Enter the flowspec list instance";

                        ///nokia-conf/configure/filter/ipv6-filter/embed/flowspec/offset
                        leaf offset {
                            type types-filter:embed-offset;
                            description "Offset of the inserted entries";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/flowspec/group
                        leaf group {
                            type uint32 {
                                range "0..16383";
                            }
                            sros-ext:immutable;
                            description "Interface group ID for an external configured set of flowspec rules";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/flowspec/router-instance
                        leaf router-instance {
                            type string;
                            sros-ext:immutable;
                            mandatory true;
                            description "Virtual router for an external configured set of flowspec rules";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/flowspec/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "enable";
                            description "Administrative state of the embedded filter";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/flowspec/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/flowspec/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list flowspec

                    ///nokia-conf/configure/filter/ipv6-filter/embed/openflow
                    list openflow {
                        key "of-switch offset";
                        description "Enter the openflow list instance";

                        ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/of-switch
                        leaf of-switch {
                            type leafref {
                                path "../../../../../openflow/of-switch/name";
                            }
                            description "Referenced Hybrid OpenFlow Switch (OFS) name";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/offset
                        leaf offset {
                            type types-filter:embed-offset {
                                range "0..2097150";
                            }
                            description "Offset of the inserted entries";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "enable";
                            description "Administrative state of the embedded filter";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/context
                        choice context {
                            default "grt";
                            description
                                "Specifies the context in which the openflow embedding is inserted into filter.
                                 When no context is present, this object is set to grt value.";
                            ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/context/grt
                            case grt {

                                ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/context/grt/grt
                                leaf grt {
                                    type empty;
                                    sros-ext:immutable;
                                    description "Global routing context";
                                }

                            }
                            ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/context/system
                            case system {

                                ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/context/system/system
                                leaf system {
                                    type empty;
                                    sros-ext:immutable;
                                    description "System context";
                                }

                            }
                            ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/context/vprn
                            case vprn {

                                ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/context/vprn/vprn
                                leaf vprn {
                                    type leafref {
                                        path "../../../../../service/vprn/service-name";
                                    }
                                    sros-ext:immutable;
                                    description "VPRN context";
                                }

                            }
                            ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/context/vpls
                            case vpls {

                                ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/context/vpls/vpls
                                leaf vpls {
                                    type leafref {
                                        path "../../../../../service/vpls/service-name";
                                    }
                                    sros-ext:immutable;
                                    description "VPLS context";
                                }

                                ///nokia-conf/configure/filter/ipv6-filter/embed/openflow/context/vpls/sap
                                leaf sap {
                                    type leafref {
                                        path "../../../../../service/vpls[service-name=current()/../vpls]/sap/sap-id";
                                    }
                                    sros-ext:immutable;
                                    description "SAP context";
                                }

                            }
                        }
                    } // list openflow

                    ///nokia-conf/configure/filter/ipv6-filter/embed/vsd
                    list vsd {
                        status obsolete;
                        key "name offset";
                        description "Enter the vsd list instance";

                        ///nokia-conf/configure/filter/ipv6-filter/embed/vsd/name
                        leaf name {
                            status obsolete;
                            type types-sros:named-item {
                                pattern "_tmnx_vsd_([1-9]|[1-9][0-9]|[1-9][0-9][0-9]|[1-9][0-9][0-9][0-9]|[1-5][0-9][0-9][0-9][0-9]|6[0-4][0-9][0-9][0-9]|65[0-4][0-9][0-9]|655[0-2][0-9]|6553[0-5])";
                            }
                            description "Specifies the name of the filter to be inserted in this filter";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/vsd/offset
                        leaf offset {
                            status obsolete;
                            type types-filter:embed-offset {
                                range "0..2097150";
                            }
                            description "Offset of the inserted entries";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/vsd/admin-state
                        leaf admin-state {
                            status obsolete;
                            type types-sros:admin-state;
                            default "enable";
                            description "Administrative state of the embedded filter";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/vsd/apply-groups
                        leaf-list apply-groups {
                            status obsolete;
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/filter/ipv6-filter/embed/vsd/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            status obsolete;
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list vsd

                } // container embed

            } // list ipv6-filter

            ///nokia-conf/configure/filter/mac-filter
            list mac-filter {
                key "filter-name";
                description "Enter the mac-filter list instance";

                ///nokia-conf/configure/filter/mac-filter/filter-name
                leaf filter-name {
                    type types-filter:filter-name;
                    description "Filter name";
                }

                ///nokia-conf/configure/filter/mac-filter/default-action
                leaf default-action {
                    type types-filter:filter-default-action;
                    default "drop";
                    description "Action for packets that do not match any entry";
                }

                ///nokia-conf/configure/filter/mac-filter/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/filter/mac-filter/scope
                leaf scope {
                    type types-filter:filter-scope;
                    default "template";
                    description "Filter policy scope";
                }

                ///nokia-conf/configure/filter/mac-filter/type
                leaf type {
                    type types-qos:mac-filter-type;
                    sros-ext:immutable;
                    default "normal";
                    description "MAC filter policy";
                }

                ///nokia-conf/configure/filter/mac-filter/filter-id
                leaf filter-id {
                    type types-filter:filter-id;
                    sros-ext:immutable;
                    description "MAC filter ID";
                }

                ///nokia-conf/configure/filter/mac-filter/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/filter/mac-filter/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/filter/mac-filter/entry
                list entry {
                    key "entry-id";
                    description "Enter the entry list instance";

                    ///nokia-conf/configure/filter/mac-filter/entry/entry-id
                    leaf entry-id {
                        type types-filter:entry-id;
                        description "ID for a match criteria and the corresponding action";
                    }

                    ///nokia-conf/configure/filter/mac-filter/entry/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/filter/mac-filter/entry/pbr-down-action-override
                    leaf pbr-down-action-override {
                        type types-filter:filter-pbr-down-action-ovr;
                        description "Action when PBR or PBF target for this entry is not available";
                    }

                    ///nokia-conf/configure/filter/mac-filter/entry/sticky-dest
                    leaf sticky-dest {
                        type types-filter:filter-sticky-dest;
                        units "seconds";
                        description "Time before action with available PBR or PBF destination and highest priority";
                    }

                    ///nokia-conf/configure/filter/mac-filter/entry/log
                    leaf log {
                        type leafref {
                            path "../../../log/log-id";
                        }
                        description "Log that is used for packets matching this entry";
                    }

                    ///nokia-conf/configure/filter/mac-filter/entry/collect-stats
                    leaf collect-stats {
                        type boolean;
                        default "false";
                        description "Enable statistics collection for an entry";
                    }

                    ///nokia-conf/configure/filter/mac-filter/entry/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/mac-filter/entry/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/mac-filter/entry/match
                    container match {
                        description "Enter the match context";

                        ///nokia-conf/configure/filter/mac-filter/entry/match/frame-type
                        leaf frame-type {
                            type types-filter:mac-frame-type;
                            description "MAC frame as match criteria";
                        }

                        ///nokia-conf/configure/filter/mac-filter/entry/match/etype
                        leaf etype {
                            type types-services:etype-value;
                            description "Ethernet type";
                        }

                        ///nokia-conf/configure/filter/mac-filter/entry/match/snap-pid
                        leaf snap-pid {
                            type int32 {
                                range "0..65535";
                            }
                            description "Parameter snap-pid as a MAC filter match criteria";
                        }

                        ///nokia-conf/configure/filter/mac-filter/entry/match/snap-oui
                        leaf snap-oui {
                            type enumeration {
                                enum zero                           { value 2; }
                                enum non-zero                       { value 3; }
                            }
                            description "Parameter snap-oui as a MAC filter match criteria";
                        }

                        ///nokia-conf/configure/filter/mac-filter/entry/match/src-mac
                        container src-mac {
                            presence "Enable the src-mac context";
                            description "Enable the src-mac context";

                            ///nokia-conf/configure/filter/mac-filter/entry/match/src-mac/address
                            leaf address {
                                type yang:mac-address;
                                mandatory true;
                                description "MAC address used as the match criterion";
                            }

                            ///nokia-conf/configure/filter/mac-filter/entry/match/src-mac/mask
                            leaf mask {
                                type yang:mac-address;
                                default "ff:ff:ff:ff:ff:ff";
                                description "MAC address mask as the match criterion";
                            }

                        } // container src-mac

                        ///nokia-conf/configure/filter/mac-filter/entry/match/dst-mac
                        container dst-mac {
                            presence "Enable the dst-mac context";
                            description "Enable the dst-mac context";

                            ///nokia-conf/configure/filter/mac-filter/entry/match/dst-mac/address
                            leaf address {
                                type yang:mac-address;
                                mandatory true;
                                description "MAC address used as the match criterion";
                            }

                            ///nokia-conf/configure/filter/mac-filter/entry/match/dst-mac/mask
                            leaf mask {
                                type yang:mac-address;
                                default "ff:ff:ff:ff:ff:ff";
                                description "MAC address mask as the match criterion";
                            }

                        } // container dst-mac

                        ///nokia-conf/configure/filter/mac-filter/entry/match/dot1p
                        container dot1p {
                            presence "Enable the dot1p context";
                            description "Enable the dot1p context";

                            ///nokia-conf/configure/filter/mac-filter/entry/match/dot1p/priority
                            leaf priority {
                                type types-qos:dot1p-priority;
                                mandatory true;
                                description "IEEE 802.1p value used as a MAC filter match criterion";
                            }

                            ///nokia-conf/configure/filter/mac-filter/entry/match/dot1p/mask
                            leaf mask {
                                type types-qos:dot1p-priority {
                                    range "1..7";
                                }
                                default "7";
                                description "802.1p mask value used as a MAC filter match criterion";
                            }

                        } // container dot1p

                        ///nokia-conf/configure/filter/mac-filter/entry/match/llc-ssap
                        container llc-ssap {
                            presence "Enable the llc-ssap context";
                            description "Enable the llc-ssap context";

                            ///nokia-conf/configure/filter/mac-filter/entry/match/llc-ssap/ssap
                            leaf ssap {
                                type types-qos:service-access-point;
                                mandatory true;
                                description "Source or destination SAP value";
                            }

                            ///nokia-conf/configure/filter/mac-filter/entry/match/llc-ssap/mask
                            leaf mask {
                                type types-qos:service-access-point {
                                    range "1..255";
                                }
                                default "255";
                                description "Source SAP mask";
                            }

                        } // container llc-ssap

                        ///nokia-conf/configure/filter/mac-filter/entry/match/llc-dsap
                        container llc-dsap {
                            presence "Enable the llc-dsap context";
                            description "Enable the llc-dsap context";

                            ///nokia-conf/configure/filter/mac-filter/entry/match/llc-dsap/dsap
                            leaf dsap {
                                type types-qos:service-access-point;
                                mandatory true;
                                description "DSAP value";
                            }

                            ///nokia-conf/configure/filter/mac-filter/entry/match/llc-dsap/mask
                            leaf mask {
                                type types-qos:service-access-point {
                                    range "1..255";
                                }
                                default "255";
                                description "Destination SAP mask";
                            }

                        } // container llc-dsap

                        ///nokia-conf/configure/filter/mac-filter/entry/match/inner-tag
                        container inner-tag {
                            presence "Enable the inner-tag context";
                            description "Enable the inner-tag context";

                            ///nokia-conf/configure/filter/mac-filter/entry/match/inner-tag/tag
                            leaf tag {
                                type int32 {
                                    range "0..4095";
                                }
                                mandatory true;
                                description "Matching value against VID of the second or first VLAN tag in the packet carried transparently";
                            }

                            ///nokia-conf/configure/filter/mac-filter/entry/match/inner-tag/mask
                            leaf mask {
                                type uint32 {
                                    range "1..4095";
                                }
                                default "4095";
                                description "Mask to VID of the inner VLAN tag before comparing it with the inner-tag or outer-tag value";
                            }

                        } // container inner-tag

                        ///nokia-conf/configure/filter/mac-filter/entry/match/outer-tag
                        container outer-tag {
                            presence "Enable the outer-tag context";
                            description "Enable the outer-tag context";

                            ///nokia-conf/configure/filter/mac-filter/entry/match/outer-tag/tag
                            leaf tag {
                                type int32 {
                                    range "0..4095";
                                }
                                mandatory true;
                                description "Matching value against VID of the second or first VLAN tag in the packet carried transparently";
                            }

                            ///nokia-conf/configure/filter/mac-filter/entry/match/outer-tag/mask
                            leaf mask {
                                type uint32 {
                                    range "1..4095";
                                }
                                default "4095";
                                description "Mask to VID of the inner VLAN tag before comparing it with the inner-tag or outer-tag value";
                            }

                        } // container outer-tag

                        ///nokia-conf/configure/filter/mac-filter/entry/match/isid
                        container isid {
                            description "Enter the isid context";

                            ///nokia-conf/configure/filter/mac-filter/entry/match/isid/isid
                            choice isid {
                                ///nokia-conf/configure/filter/mac-filter/entry/match/isid/isid/isid-value
                                case isid-value {

                                    ///nokia-conf/configure/filter/mac-filter/entry/match/isid/isid/isid-value/value
                                    leaf value {
                                        type types-sros:svc-isid;
                                        description "Lowest value of 24-bit service instance identifier for the service matching this entry";
                                    }

                                }
                                ///nokia-conf/configure/filter/mac-filter/entry/match/isid/isid/isid-range
                                case isid-range {

                                    ///nokia-conf/configure/filter/mac-filter/entry/match/isid/isid/isid-range/range
                                    container range {
                                        presence "Enable the range context";
                                        description "Enable the range context";

                                        ///nokia-conf/configure/filter/mac-filter/entry/match/isid/isid/isid-range/range/start
                                        leaf start {
                                            type types-sros:svc-isid;
                                            mandatory true;
                                            description "Lower bound of the ISID range";
                                        }

                                        ///nokia-conf/configure/filter/mac-filter/entry/match/isid/isid/isid-range/range/end
                                        leaf end {
                                            type types-sros:svc-isid;
                                            mandatory true;
                                            description "Upper bound of the ISID range";
                                        }

                                    } // container range

                                }
                            }
                        } // container isid

                    } // container match

                    ///nokia-conf/configure/filter/mac-filter/entry/action
                    container action {
                        presence "Enable the action context";
                        description "Enable the action context";

                        ///nokia-conf/configure/filter/mac-filter/entry/action/fc
                        leaf fc {
                            type types-sros:fc-name;
                            description "Forwarding class for packets matching the entry";
                        }

                        ///nokia-conf/configure/filter/mac-filter/entry/action/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/filter/mac-filter/entry/action/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/filter/mac-filter/entry/action/action
                        choice action {
                            mandatory true;
                            ///nokia-conf/configure/filter/mac-filter/entry/action/action/ignore-match
                            case ignore-match {

                                ///nokia-conf/configure/filter/mac-filter/entry/action/action/ignore-match/ignore-match
                                leaf ignore-match {
                                    type empty;
                                    description "Ignore match criteria for the entry";
                                }

                            }
                            ///nokia-conf/configure/filter/mac-filter/entry/action/action/drop
                            case drop {

                                ///nokia-conf/configure/filter/mac-filter/entry/action/action/drop/drop
                                leaf drop {
                                    type empty;
                                    description "Drop a packet matching this entry";
                                }

                            }
                            ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward
                            case forward {

                                ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward/forward
                                container forward {
                                    description "Enter the forward context";

                                    ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward/forward/forward-action
                                    choice forward-action {
                                        ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward/forward/forward-action/esi-l2
                                        case esi-l2 {

                                            ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward/forward/forward-action/esi-l2/esi-l2
                                            container esi-l2 {
                                                presence "Enable the esi-l2 context";
                                                description "Enable the esi-l2 context";

                                                ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward/forward/forward-action/esi-l2/esi-l2/esi-value
                                                leaf esi-value {
                                                    type types-services:ethernet-segment-id {
                                                        pattern "(0x[0-9a-fA-F]{20})|([0-9a-fA-F]{2}(:[0-9a-fA-F]{2}){9})|([0-9a-fA-F]{2}(-[0-9a-fA-F]{2}){9})";
                                                    }
                                                    mandatory true;
                                                    description "ESI of the first ESI-identified appliance";
                                                }

                                                ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward/forward/forward-action/esi-l2/esi-l2/vpls
                                                leaf vpls {
                                                    type leafref {
                                                        path "../../../../../../../service/vpls/service-name";
                                                    }
                                                    mandatory true;
                                                    description "VPLS service name";
                                                }

                                            } // container esi-l2

                                        }
                                        ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward/forward/forward-action/sdp
                                        case sdp {

                                            ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward/forward/forward-action/sdp/sdp
                                            container sdp {
                                                presence "Enable the sdp context";
                                                description "Enable the sdp context";

                                                ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward/forward/forward-action/sdp/sdp/vpls
                                                leaf vpls {
                                                    type leafref {
                                                        path "../../../../../../../service/vpls/service-name";
                                                    }
                                                    mandatory true;
                                                    description "VPLS associated with the SDP";
                                                }

                                                ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward/forward/forward-action/sdp/sdp/sdp-bind-id
                                                leaf sdp-bind-id {
                                                    type types-services:sdp-bind-id;
                                                    mandatory true;
                                                    description "VPLS SDP bind ID used to forward matching packets";
                                                }

                                            } // container sdp

                                        }
                                        ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward/forward/forward-action/sap
                                        case sap {

                                            ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward/forward/forward-action/sap/sap
                                            container sap {
                                                presence "Enable the sap context";
                                                description "Enable the sap context";

                                                ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward/forward/forward-action/sap/sap/vpls
                                                leaf vpls {
                                                    type leafref {
                                                        path "../../../../../../../service/vpls/service-name";
                                                    }
                                                    mandatory true;
                                                    description "VPLS associated with the SAP";
                                                }

                                                ///nokia-conf/configure/filter/mac-filter/entry/action/action/forward/forward/forward-action/sap/sap/sap-id
                                                leaf sap-id {
                                                    type leafref {
                                                        path "../../../../../../../service/vpls[service-name=current()/../vpls]/sap/sap-id";
                                                    }
                                                    mandatory true;
                                                    description "VPLS Ethernet SAP ID used to forward matching packets";
                                                }

                                            } // container sap

                                        }
                                    }
                                } // container forward

                            }
                            ///nokia-conf/configure/filter/mac-filter/entry/action/action/http-redirect
                            case http-redirect {

                                ///nokia-conf/configure/filter/mac-filter/entry/action/action/http-redirect/http-redirect
                                container http-redirect {
                                    presence "Enable the http-redirect context";
                                    description "Enable the http-redirect context";

                                    ///nokia-conf/configure/filter/mac-filter/entry/action/action/http-redirect/http-redirect/url
                                    leaf url {
                                        type types-sros:http-redirect-url;
                                        sros-ext:immutable;
                                        mandatory true;
                                        description "URL that is used for redirecting";
                                    }

                                } // container http-redirect

                            }
                            ///nokia-conf/configure/filter/mac-filter/entry/action/action/accept
                            case accept {

                                ///nokia-conf/configure/filter/mac-filter/entry/action/action/accept/accept
                                leaf accept {
                                    type empty;
                                    description "Accept regular routing to forward a matching packet";
                                }

                            }
                        }
                        ///nokia-conf/configure/filter/mac-filter/entry/action/rate-limit
                        container rate-limit {
                            presence "Enable the rate-limit context";
                            description "Enable the rate-limit context";

                            ///nokia-conf/configure/filter/mac-filter/entry/action/rate-limit/pir
                            leaf pir {
                                type types-filter:rate-limit;
                                units "kilobps";
                                mandatory true;
                                description "Peak information rate";
                            }

                        } // container rate-limit

                        ///nokia-conf/configure/filter/mac-filter/entry/action/secondary
                        container secondary {
                            presence "Enable the secondary context";
                            description "Enable the secondary context";

                            ///nokia-conf/configure/filter/mac-filter/entry/action/secondary/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/filter/mac-filter/entry/action/secondary/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/filter/mac-filter/entry/action/secondary/action
                            choice action {
                                mandatory true;
                                ///nokia-conf/configure/filter/mac-filter/entry/action/secondary/action/forward
                                case forward {

                                    ///nokia-conf/configure/filter/mac-filter/entry/action/secondary/action/forward/forward
                                    container forward {
                                        description "Enter the forward context";

                                        ///nokia-conf/configure/filter/mac-filter/entry/action/secondary/action/forward/forward/forward-action
                                        choice forward-action {
                                            ///nokia-conf/configure/filter/mac-filter/entry/action/secondary/action/forward/forward/forward-action/sdp
                                            case sdp {

                                                ///nokia-conf/configure/filter/mac-filter/entry/action/secondary/action/forward/forward/forward-action/sdp/sdp
                                                container sdp {
                                                    presence "Enable the sdp context";
                                                    description "Enable the sdp context";

                                                    ///nokia-conf/configure/filter/mac-filter/entry/action/secondary/action/forward/forward/forward-action/sdp/sdp/vpls
                                                    leaf vpls {
                                                        type leafref {
                                                            path "../../../../../../../../service/vpls/service-name";
                                                        }
                                                        mandatory true;
                                                        description "VPLS associated with the SDP";
                                                    }

                                                    ///nokia-conf/configure/filter/mac-filter/entry/action/secondary/action/forward/forward/forward-action/sdp/sdp/sdp-bind-id
                                                    leaf sdp-bind-id {
                                                        type types-services:sdp-bind-id;
                                                        sros-ext:immutable;
                                                        mandatory true;
                                                        description "VPLS SDP bind ID used to forward matching packets";
                                                    }

                                                } // container sdp

                                            }
                                            ///nokia-conf/configure/filter/mac-filter/entry/action/secondary/action/forward/forward/forward-action/sap
                                            case sap {

                                                ///nokia-conf/configure/filter/mac-filter/entry/action/secondary/action/forward/forward/forward-action/sap/sap
                                                container sap {
                                                    presence "Enable the sap context";
                                                    description "Enable the sap context";

                                                    ///nokia-conf/configure/filter/mac-filter/entry/action/secondary/action/forward/forward/forward-action/sap/sap/vpls
                                                    leaf vpls {
                                                        type leafref {
                                                            path "../../../../../../../../service/vpls/service-name";
                                                        }
                                                        mandatory true;
                                                        description "VPLS the sdp-bind-id belongs to";
                                                    }

                                                    ///nokia-conf/configure/filter/mac-filter/entry/action/secondary/action/forward/forward/forward-action/sap/sap/sap-id
                                                    leaf sap-id {
                                                        type leafref {
                                                            path "../../../../../../../../service/vpls[service-name=current()/../vpls]/sap/sap-id";
                                                        }
                                                        sros-ext:immutable;
                                                        mandatory true;
                                                        description "SAP ID used to forward packets matching the entry";
                                                    }

                                                } // container sap

                                            }
                                        }
                                    } // container forward

                                }
                            }
                        } // container secondary

                    } // container action

                } // list entry

                ///nokia-conf/configure/filter/mac-filter/embed
                container embed {
                    description "Enter the embed context";

                    ///nokia-conf/configure/filter/mac-filter/embed/vsd
                    list vsd {
                        status obsolete;
                        key "name offset";
                        description "Enter the vsd list instance";

                        ///nokia-conf/configure/filter/mac-filter/embed/vsd/name
                        leaf name {
                            status obsolete;
                            type types-sros:named-item {
                                pattern "_tmnx_vsd_([1-9]|[1-9][0-9]|[1-9][0-9][0-9]|[1-9][0-9][0-9][0-9]|[1-5][0-9][0-9][0-9][0-9]|6[0-4][0-9][0-9][0-9]|65[0-4][0-9][0-9]|655[0-2][0-9]|6553[0-5])";
                            }
                            description "Specifies the name of the filter to be inserted in this filter";
                        }

                        ///nokia-conf/configure/filter/mac-filter/embed/vsd/offset
                        leaf offset {
                            status obsolete;
                            type types-filter:embed-offset {
                                range "0..2097150";
                            }
                            description "Offset of the inserted entries";
                        }

                        ///nokia-conf/configure/filter/mac-filter/embed/vsd/admin-state
                        leaf admin-state {
                            status obsolete;
                            type types-sros:admin-state;
                            default "enable";
                            description "Administrative state of the embedded filter";
                        }

                        ///nokia-conf/configure/filter/mac-filter/embed/vsd/apply-groups
                        leaf-list apply-groups {
                            status obsolete;
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/filter/mac-filter/embed/vsd/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            status obsolete;
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list vsd

                } // container embed

            } // list mac-filter

            ///nokia-conf/configure/filter/system-filter
            container system-filter {
                description "Enable the system-filter context";

                ///nokia-conf/configure/filter/system-filter/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/filter/system-filter/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/filter/system-filter/ip
                list ip {
                    key "ip-filter";
                    max-elements 1;
                    description "Add a list entry for ip";

                    ///nokia-conf/configure/filter/system-filter/ip/ip-filter
                    leaf ip-filter {
                        type leafref {
                            path "../../../ip-filter/filter-name";
                        }
                        description "Active IPv4 system filter policy";
                    }

                } // list ip

                ///nokia-conf/configure/filter/system-filter/ipv6
                list ipv6 {
                    key "ipv6-filter";
                    max-elements 1;
                    description "Add a list entry for ipv6";

                    ///nokia-conf/configure/filter/system-filter/ipv6/ipv6-filter
                    leaf ipv6-filter {
                        type leafref {
                            path "../../../ipv6-filter/filter-name";
                        }
                        description "Active IPv6 system filter policy";
                    }

                } // list ipv6

            } // container system-filter

            ///nokia-conf/configure/filter/gre-tunnel-template
            list gre-tunnel-template {
                key "gre-tunnel-template-name";
                max-elements 8191;
                description "Enter the gre-tunnel-template list instance";

                ///nokia-conf/configure/filter/gre-tunnel-template/gre-tunnel-template-name
                leaf gre-tunnel-template-name {
                    type types-sros:named-item;
                    description "GRE tunnel template ID";
                }

                ///nokia-conf/configure/filter/gre-tunnel-template/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/filter/gre-tunnel-template/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/filter/gre-tunnel-template/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/filter/gre-tunnel-template/ipvx
                choice ipvx {
                    ///nokia-conf/configure/filter/gre-tunnel-template/ipvx/ipv4
                    case ipv4 {

                        ///nokia-conf/configure/filter/gre-tunnel-template/ipvx/ipv4/ipv4
                        container ipv4 {
                            description "Enter the ipv4 context";

                            ///nokia-conf/configure/filter/gre-tunnel-template/ipvx/ipv4/ipv4/source-address
                            leaf source-address {
                                type types-sros:ipv4-address;
                                description "Source IP address of the GRE encapsulated";
                            }

                            ///nokia-conf/configure/filter/gre-tunnel-template/ipvx/ipv4/ipv4/gre-key
                            leaf gre-key {
                                type types-filter:filter-gre-tunnel-gre-key;
                                description "GRE key";
                            }

                            ///nokia-conf/configure/filter/gre-tunnel-template/ipvx/ipv4/ipv4/skip-ttl-decrement
                            leaf skip-ttl-decrement {
                                type boolean;
                                default "false";
                                description "Decrement TTL";
                            }

                            ///nokia-conf/configure/filter/gre-tunnel-template/ipvx/ipv4/ipv4/destination-address
                            list destination-address {
                                key "address";
                                max-elements 32;
                                description "Add a list entry for destination-address";

                                ///nokia-conf/configure/filter/gre-tunnel-template/ipvx/ipv4/ipv4/destination-address/address
                                leaf address {
                                    type types-sros:ipv4-address;
                                    description "Destination IPv4 address";
                                }

                            } // list destination-address

                        } // container ipv4

                    }
                    ///nokia-conf/configure/filter/gre-tunnel-template/ipvx/ipv6
                    case ipv6 {

                        ///nokia-conf/configure/filter/gre-tunnel-template/ipvx/ipv6/ipv6
                        container ipv6 {
                            description "Enter the ipv6 context";

                            ///nokia-conf/configure/filter/gre-tunnel-template/ipvx/ipv6/ipv6/source-address
                            leaf source-address {
                                type types-sros:ipv6-unicast-address;
                                description "Source IPv6 address of the GRE encapsulated";
                            }

                            ///nokia-conf/configure/filter/gre-tunnel-template/ipvx/ipv6/ipv6/gre-key
                            leaf gre-key {
                                type enumeration {
                                    enum if-index                       { value -2; }
                                }
                                description "Include a key value in GRE header";
                            }

                            ///nokia-conf/configure/filter/gre-tunnel-template/ipvx/ipv6/ipv6/skip-hop-decrement
                            leaf skip-hop-decrement {
                                type boolean;
                                default "false";
                                description "Decrement TTL of the received packet";
                            }

                            ///nokia-conf/configure/filter/gre-tunnel-template/ipvx/ipv6/ipv6/destination-address
                            list destination-address {
                                key "address";
                                max-elements 1;
                                description "Add a list entry for destination-address";

                                ///nokia-conf/configure/filter/gre-tunnel-template/ipvx/ipv6/ipv6/destination-address/address
                                leaf address {
                                    type types-sros:ipv6-unicast-address;
                                    description "IPv6 destination address";
                                }

                            } // list destination-address

                        } // container ipv6

                    }
                }
            } // list gre-tunnel-template

            ///nokia-conf/configure/filter/md-auto-id
            container md-auto-id {
                description "Enter the md-auto-id context";

                ///nokia-conf/configure/filter/md-auto-id/filter-id-range
                container filter-id-range {
                    presence "Enable the filter-id-range context";
                    description "Enable the filter-id-range context";

                    ///nokia-conf/configure/filter/md-auto-id/filter-id-range/start
                    leaf start {
                        type types-filter:filter-id;
                        sros-ext:immutable;
                        mandatory true;
                        description "Lower bound of the ID range";
                    }

                    ///nokia-conf/configure/filter/md-auto-id/filter-id-range/end
                    leaf end {
                        type types-filter:filter-id;
                        sros-ext:immutable;
                        mandatory true;
                        description "Upper bound of the ID range";
                    }

                    ///nokia-conf/configure/filter/md-auto-id/filter-id-range/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/md-auto-id/filter-id-range/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container filter-id-range

            } // container md-auto-id

            uses conf-filter-dhcp-dhcp-filter;
            uses conf-filter-dhcp-dhcp6-filter;
            uses conf-filter-ip-exception;
            uses conf-filter-ip-exception-ipv6-exception;
        } // container filter

    } // grouping conf-filter

}
