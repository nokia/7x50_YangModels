                                default "90";
                                description "Low watermark for host-specific entries, to clear a table full alarm";
                            }

                            ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/host-specific-entry/watermark/high
                            leaf high {
                                type int32 {
                                    range "0..100";
                                }
                                default "95";
                                description "High watermark for host-specific entries, to raise a table full alarm";
                            }

                        } // container watermark

                    } // container host-specific-entry

                    ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/shared-entry
                    container shared-entry {
                        description "Enter the shared-entry context";

                        ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/shared-entry/filter-rule
                        container filter-rule {
                            description "Enter the filter-rule context";

                            ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/shared-entry/filter-rule/range
                            container range {
                                presence "Enable the range context";
