                    ///nokia-conf/configure/filter/ipv6-filter/entry/interface-sample
                    leaf interface-sample {
                        type boolean;
                        default "true";
                        description "Sample matching traffic if IP interface is set to cflowd interface mode";
                    }

                    ///nokia-conf/configure/filter/ipv6-filter/entry/sample-profile
                    leaf sample-profile {
                        type leafref {
                            path "../../../../cflowd/sample-profile/profile-id";
