                                    ///nokia-conf/configure/filter/ip-filter/entry/action/action/forward/forward/forward-action
                                    choice forward-action {
                                        ///nokia-conf/configure/filter/ip-filter/entry/action/action/forward/forward/forward-action/bonding-connection
                                        case bonding-connection {

                                            ///nokia-conf/configure/filter/ip-filter/entry/action/action/forward/forward/forward-action/bonding-connection/bonding-connection
