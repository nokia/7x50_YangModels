                                            leaf port-list {
                                                type leafref {
                                                    path "../../../../../match-list/port-list/port-list-name";
                                                }
                                                description "Name of the port list as the match criterion";
                                            }

                                        }
                                    }
                                } // container dst-port

                            }
                            ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port
                            case port {

                                ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port/port
                                container port {
                                    description "Enter the port context";

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port/port/port
                                    choice port {
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port/port/port/eq
                                        case eq {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port/port/port/eq/eq
                                            leaf eq {
                                                type uint16 {
                                                    range "0..65535";
                                                }
                                                description "Exact match criterion for the port number";
                                            }

                                        }
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port/port/port/lt
                                        case lt {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port/port/port/lt/lt
                                            leaf lt {
                                                type uint16 {
                                                    range "1..65535";
                                                }
                                                description "Less than match criterion for the port number";
                                            }

                                        }
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port/port/port/gt
                                        case gt {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port/port/port/gt/gt
                                            leaf gt {
                                                type uint16 {
                                                    range "0..65534";
                                                }
                                                description "Greater than match criterion for the port number";
                                            }

                                        }
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port/port/port/range
                                        case range {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port/port/port/range/range
                                            container range {
                                                presence "Enable the range context";
                                                description "Enable the range context";

                                                ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port/port/port/range/range/start
                                                leaf start {
                                                    type uint16 {
                                                        range "0..65534";
                                                    }
                                                    mandatory true;
                                                    description "Lower bound of the port range as port match criterion";
                                                }

                                                ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port/port/port/range/range/end
                                                leaf end {
                                                    type uint16 {
                                                        range "1..65535";
                                                    }
                                                    mandatory true;
                                                    description "Upper bound of the port range as port match criterion";
                                                }

                                            } // container range

                                        }
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port/port/port/port-list
                                        case port-list {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/port/port/port/port-list/port-list
                                            leaf port-list {
                                                type leafref {
                                                    path "../../../../../match-list/port-list/port-list-name";
                                                }
                                                description "Name of the port list as the match criterion";
                                            }

                                        }
                                    }
                                } // container port

                            }
                        }
                        ///nokia-conf/configure/filter/ip-filter/entry/match/match-tcp-flags
                        choice match-tcp-flags {
                            ///nokia-conf/configure/filter/ip-filter/entry/match/match-tcp-flags/tcp-established
                            case tcp-established {

                                ///nokia-conf/configure/filter/ip-filter/entry/match/match-tcp-flags/tcp-established/tcp-established
                                leaf tcp-established {
                                    type empty;
                                    description "Match packets containing the TCP flag as ACK or RST";
                                }

                            }
                            ///nokia-conf/configure/filter/ip-filter/entry/match/match-tcp-flags/tcp-flags
                            case tcp-flags {

                                ///nokia-conf/configure/filter/ip-filter/entry/match/match-tcp-flags/tcp-flags/tcp-flags
                                container tcp-flags {
                                    description "Enter the tcp-flags context";

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/match-tcp-flags/tcp-flags/tcp-flags/ack
                                    leaf ack {
                                        type boolean;
                                        description "Match TCP ACK as per value of the ACK TCP flag bit";
                                    }

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/match-tcp-flags/tcp-flags/tcp-flags/syn
                                    leaf syn {
                                        type boolean;
                                        description "Match TCP SYN as per value of the SYN TCP flag bit";
                                    }

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/match-tcp-flags/tcp-flags/tcp-flags/fin
                                    leaf fin {
                                        type boolean;
                                        description "Match TCP FIN as per value of the FIN TCP flag bit";
                                    }

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/match-tcp-flags/tcp-flags/tcp-flags/rst
                                    leaf rst {
                                        type boolean;
                                        description "Match TCP RST as per value of the RST TCP flag bit";
                                    }

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/match-tcp-flags/tcp-flags/tcp-flags/psh
                                    leaf psh {
                                        type boolean;
                                        description "Match TCP PSH as per value of the PSH TCP flag bit";
                                    }

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/match-tcp-flags/tcp-flags/tcp-flags/urg
                                    leaf urg {
                                        type boolean;
                                        description "Match TCP URG as per value of the URG TCP flag bit";
                                    }

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/match-tcp-flags/tcp-flags/tcp-flags/ece
                                    leaf ece {
                                        type boolean;
                                        description "Match TCP ECE as per value of the ECE TCP flag bit";
                                    }

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/match-tcp-flags/tcp-flags/tcp-flags/cwr
                                    leaf cwr {
                                        type boolean;
                                        description "Match TCP CWR as per value of the CWR TCP flag bit";
                                    }

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/match-tcp-flags/tcp-flags/tcp-flags/ns
                                    leaf ns {
                                        type boolean;
                                        description "Match TCP NS as per value of the NS TCP flag bit";
                                    }

                                } // container tcp-flags

                            }
                        }
                        ///nokia-conf/configure/filter/ip-filter/entry/match/ip-option
                        container ip-option {
                            presence "Enable the ip-option context";
                            description "Enable the ip-option context";

                            ///nokia-conf/configure/filter/ip-filter/entry/match/ip-option/type
                            leaf type {
                                type types-filter:filter-match-ip-option;
                                mandatory true;
                                description "Specific IP option to match";
                            }

                            ///nokia-conf/configure/filter/ip-filter/entry/match/ip-option/mask
                            leaf mask {
                                type types-filter:filter-match-ip-option {
                                    range "1..255";
                                }
                                default "255";
                                description "Mask that is ANDed with ip-option value in the packet header";
                            }

                        } // container ip-option

                        ///nokia-conf/configure/filter/ip-filter/entry/match/src-mac
                        container src-mac {
                            presence "Enable the src-mac context";
                            description "Enable the src-mac context";

                            ///nokia-conf/configure/filter/ip-filter/entry/match/src-mac/address
                            leaf address {
                                type yang:mac-address;
                                mandatory true;
                                description "MAC address used as the match criterion";
                            }

                            ///nokia-conf/configure/filter/ip-filter/entry/match/src-mac/mask
                            leaf mask {
                                type yang:mac-address;
                                default "ff:ff:ff:ff:ff:ff";
                                description "MAC address mask as the match criterion";
                            }

                        } // container src-mac

                        ///nokia-conf/configure/filter/ip-filter/entry/match/icmp
                        container icmp {
                            description "Enter the icmp context";

                            ///nokia-conf/configure/filter/ip-filter/entry/match/icmp/icmp-code
                            choice icmp-code {
                                ///nokia-conf/configure/filter/ip-filter/entry/match/icmp/icmp-code/code
                                case code {

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/icmp/icmp-code/code/code
                                    leaf code {
                                        type types-filter:ipv4-match-icmp-codes;
                                        description "ICMP code value to match";
                                    }

                                }
                            }
                            ///nokia-conf/configure/filter/ip-filter/entry/match/icmp/icmp-type
                            choice icmp-type {
                                ///nokia-conf/configure/filter/ip-filter/entry/match/icmp/icmp-type/type
                                case type {

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/icmp/icmp-type/type/type
                                    leaf type {
                                        type types-filter:ipv4-match-icmp-types;
                                        description "ICMP type value to match";
                                    }

                                }
                            }
                        } // container icmp

                        ///nokia-conf/configure/filter/ip-filter/entry/match/packet-length
                        container packet-length {
                            presence "Enable the packet-length context";
                            description "Enable the packet-length context";

                            ///nokia-conf/configure/filter/ip-filter/entry/match/packet-length/packet-length
                            choice packet-length {
                                mandatory true;
                                ///nokia-conf/configure/filter/ip-filter/entry/match/packet-length/packet-length/eq
                                case eq {

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/packet-length/packet-length/eq/eq
                                    leaf eq {
                                        type types-filter:pkt-len-or-payload-len-value;
                                        description "Exact match criterion for the length";
                                    }

                                }
                                ///nokia-conf/configure/filter/ip-filter/entry/match/packet-length/packet-length/lt
                                case lt {

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/packet-length/packet-length/lt/lt
                                    leaf lt {
                                        type types-filter:pkt-len-or-payload-len-lt-value;
                                        description "Less than match criterion for the length";
                                    }

                                }
                                ///nokia-conf/configure/filter/ip-filter/entry/match/packet-length/packet-length/gt
                                case gt {

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/packet-length/packet-length/gt/gt
                                    leaf gt {
                                        type types-filter:pkt-len-or-payload-len-gt-value;
                                        description "Greater than match criterion for the length";
                                    }

                                }
                                ///nokia-conf/configure/filter/ip-filter/entry/match/packet-length/packet-length/range
                                case range {

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/packet-length/packet-length/range/range
                                    container range {
                                        presence "Enable the range context";
                                        description "Enable the range context";

                                        ///nokia-conf/configure/filter/ip-filter/entry/match/packet-length/packet-length/range/range/start
                                        leaf start {
                                            type types-filter:pkt-len-or-payload-len-gt-value;
                                            mandatory true;
                                            description "Lower bound of the length range";
                                        }

                                        ///nokia-conf/configure/filter/ip-filter/entry/match/packet-length/packet-length/range/range/end
                                        leaf end {
                                            type types-filter:pkt-len-or-payload-len-lt-value;
                                            mandatory true;
                                            description "Upper bound of the length range";
                                        }

                                    } // container range

                                }
                            }
                        } // container packet-length

                        ///nokia-conf/configure/filter/ip-filter/entry/match/ttl
                        container ttl {
                            presence "Enable the ttl context";
                            description "Enable the ttl context";

                            ///nokia-conf/configure/filter/ip-filter/entry/match/ttl/ttl
                            choice ttl {
                                mandatory true;
                                ///nokia-conf/configure/filter/ip-filter/entry/match/ttl/ttl/eq
                                case eq {

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/ttl/ttl/eq/eq
                                    leaf eq {
