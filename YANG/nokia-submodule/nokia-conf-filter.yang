                                ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/host-specific-entry/credit-control/range/end
                                leaf end {
                                    type types-filter:entry-id;
                                    mandatory true;
                                    description "Upper bound of range for credit control filter entries";
                                }

