                                }

                            } // container range

                        } // container pcc-rule

                        ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/shared-entry/watermark
                        container watermark {
                            presence "Enable the watermark context";
                            description "Enable the watermark context";

                            ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/shared-entry/watermark/low
                            leaf low {
                                type int32 {
                                    range "0..7999";
                                }
