                ///nokia-conf/configure/filter/ipv6-filter/shared-policer
                leaf shared-policer {
                    type boolean;
                    default "false";
                    description "Share policer among active ports in the LAG";
                }

                ///nokia-conf/configure/filter/ipv6-filter/filter-id
                leaf filter-id {
                    type types-filter:filter-id;
                    sros-ext:immutable;
                    description "IPv6 filter identifier";
                }

                ///nokia-conf/configure/filter/ipv6-filter/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/filter/ipv6-filter/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt
                container subscriber-mgmt {
                    description "Enter the subscriber-mgmt context";

                    ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/host-specific-entry
                    container host-specific-entry {
                        description "Enter the host-specific-entry context";

                        ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/host-specific-entry/filter-rule
                        container filter-rule {
                            description "Enter the filter-rule context";

                            ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/host-specific-entry/filter-rule/range
                            container range {
