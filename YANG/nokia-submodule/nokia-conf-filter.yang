                                            ///nokia-conf/configure/filter/ipv6-filter/entry/match/ip-selector/src-dst-ip/dst-ip/match-address-choice/address-and-prefix-or-mask/mask
                                            leaf mask {
                                                type types-sros:ipv6-address;
                                                description "IPv6 address mask used as the match criterion";
                                            }

                                        }
                                        ///nokia-conf/configure/filter/ipv6-filter/entry/match/ip-selector/src-dst-ip/dst-ip/match-address-choice/ipv6-prefix-list
                                        case ipv6-prefix-list {

                                            ///nokia-conf/configure/filter/ipv6-filter/entry/match/ip-selector/src-dst-ip/dst-ip/match-address-choice/ipv6-prefix-list/ipv6-prefix-list
                                            leaf ipv6-prefix-list {
