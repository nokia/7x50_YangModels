                                leaf start {
                                    type types-filter:entry-id;
                                    mandatory true;
                                    description "Lower bound of range for inserting filter rule entries";
                                }

                                ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/host-specific-entry/filter-rule/range/end
                                leaf end {
                                    type types-filter:entry-id;
                                    mandatory true;
                                    description "Upper bound of range for inserting filter rule entries";
                                }

                            } // container range

                        } // container filter-rule

                        ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/host-specific-entry/credit-control
                        container credit-control {
                            description "Enter the credit-control context";

                            ///nokia-conf/configure/filter/ipv6-filter/subscriber-mgmt/host-specific-entry/credit-control/range
                            container range {
                                presence "Enable the range context";
