                } // list ipv6-prefix-list

                ///nokia-conf/configure/filter/match-list/port-list
                list port-list {
                    key "port-list-name";
                    max-elements 5120;
                    description "Enter the port-list list instance";

                    ///nokia-conf/configure/filter/match-list/port-list/port-list-name
                    leaf port-list-name {
                        type types-sros:named-item;
                        description "Port list name";
                    }

                    ///nokia-conf/configure/filter/match-list/port-list/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/filter/match-list/port-list/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/match-list/port-list/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/match-list/port-list/port
                    list port {
                        key "value";
                        description "Add a list entry for port";

                        ///nokia-conf/configure/filter/match-list/port-list/port/value
                        leaf value {
                            type int32 {
                                range "0..65535";
                            }
                            description "Port value";
                        }

                    } // list port

                    ///nokia-conf/configure/filter/match-list/port-list/range
                    list range {
                        key "start end";
                        description "Add a list entry for range";

                        ///nokia-conf/configure/filter/match-list/port-list/range/start
                        leaf start {
                            type int32 {
                                range "0..65534";
                            }
                            description "Lower bound of the port list range";
                        }

                        ///nokia-conf/configure/filter/match-list/port-list/range/end
                        leaf end {
                            type int32 {
                                range "1..65535";
                            }
                            description "Upper bound of the port list range";
                        }

                    } // list range

                } // list port-list

            } // container match-list

            ///nokia-conf/configure/filter/ip-filter
            list ip-filter {
                key "filter-name";
                description "Enter the ip-filter list instance";

                ///nokia-conf/configure/filter/ip-filter/filter-name
                leaf filter-name {
                    type types-filter:filter-name;
                    description "Filter name";
                }

                ///nokia-conf/configure/filter/ip-filter/default-action
                leaf default-action {
                    type types-filter:filter-default-action;
                    default "drop";
                    description "Action for packets that do not match any entry";
                }

                ///nokia-conf/configure/filter/ip-filter/description
                leaf description {
                    type types-sros:description;
                    description "Text description";
                }

                ///nokia-conf/configure/filter/ip-filter/scope
                leaf scope {
                    type types-filter:filter-scope;
                    default "template";
                    description "Scope of the filter defintion";
                }

                ///nokia-conf/configure/filter/ip-filter/type
                leaf type {
                    type enumeration {
                        enum normal                         { value 0; }
                        enum src-mac                        { value 1; }
                        enum packet-length                  { value 2; }
                        enum destination-class              { value 3; }
                    }
                    default "normal";
                    description "Set of match criteria for the filter policy";
                }

                ///nokia-conf/configure/filter/ip-filter/chain-to-system-filter
                leaf chain-to-system-filter {
                    type boolean;
                    default "false";
                    description "Chain filter policy to the active IPvX system filter policy";
                }

                ///nokia-conf/configure/filter/ip-filter/shared-policer
                leaf shared-policer {
                    type boolean;
                    default "false";
                    description "Share policer among active ports in the LAG";
                }

                ///nokia-conf/configure/filter/ip-filter/filter-id
                leaf filter-id {
                    type types-filter:filter-id;
                    sros-ext:immutable;
                    description "IP filter ID";
                }

                ///nokia-conf/configure/filter/ip-filter/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/filter/ip-filter/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt
                container subscriber-mgmt {
                    description "Enter the subscriber-mgmt context";

                    ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/host-specific-entry
                    container host-specific-entry {
                        description "Enter the host-specific-entry context";

                        ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/host-specific-entry/filter-rule
                        container filter-rule {
                            description "Enter the filter-rule context";

                            ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/host-specific-entry/filter-rule/range
                            container range {
                                presence "Enable the range context";
                                description "Enable the range context";

                                ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/host-specific-entry/filter-rule/range/start
                                leaf start {
                                    type types-filter:entry-id;
                                    mandatory true;
                                    description "Lower bound of range for inserting filter rule entries";
                                }

                                ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/host-specific-entry/filter-rule/range/end
                                leaf end {
                                    type types-filter:entry-id;
                                    mandatory true;
                                    description "Upper bound of range for inserting filter rule entries";
                                }

                            } // container range

                        } // container filter-rule

                        ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/host-specific-entry/credit-control
                        container credit-control {
                            description "Enter the credit-control context";

                            ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/host-specific-entry/credit-control/range
                            container range {
                                presence "Enable the range context";
                                description "Enable the range context";

                                ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/host-specific-entry/credit-control/range/start
                                leaf start {
                                    type types-filter:entry-id;
                                    mandatory true;
                                    description "Lower bound of range for credit control filter entries";
                                }

                                ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/host-specific-entry/credit-control/range/end
                                leaf end {
                                    type types-filter:entry-id;
                                    mandatory true;
                                    description "Upper bound of range for credit control filter entries";
                                }

                            } // container range

                        } // container credit-control

                        ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/host-specific-entry/watermark
                        container watermark {
                            description "Enter the watermark context";

                            ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/host-specific-entry/watermark/low
                            leaf low {
                                type int32 {
                                    range "0..100";
                                }
                                default "90";
                                description "Low watermark for host-specific entries, to clear a table full alarm";
                            }

                            ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/host-specific-entry/watermark/high
                            leaf high {
                                type int32 {
                                    range "0..100";
                                }
                                default "95";
                                description "High watermark for host-specific entries, to raise a table full alarm";
                            }

                        } // container watermark

                    } // container host-specific-entry

                    ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/shared-entry
                    container shared-entry {
                        description "Enter the shared-entry context";

                        ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/shared-entry/filter-rule
                        container filter-rule {
                            description "Enter the filter-rule context";

                            ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/shared-entry/filter-rule/range
                            container range {
                                presence "Enable the range context";
                                description "Enable the range context";

                                ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/shared-entry/filter-rule/range/start
                                leaf start {
                                    type types-filter:entry-id;
                                    mandatory true;
                                    description "Lower bound of range for inserting shared host rules";
                                }

                                ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/shared-entry/filter-rule/range/end
                                leaf end {
                                    type types-filter:entry-id;
                                    mandatory true;
                                    description "Upper bound of range for inserting shared host rules";
                                }

                            } // container range

                        } // container filter-rule

                        ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/shared-entry/pcc-rule
                        container pcc-rule {
                            description "Enter the pcc-rule context";

                            ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/shared-entry/pcc-rule/range
                            container range {
                                presence "Enable the range context";
                                description "Enable the range context";

                                ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/shared-entry/pcc-rule/range/start
                                leaf start {
                                    type types-filter:entry-id;
                                    mandatory true;
                                    description "Lower bound of the range for PCC rule filter entries";
                                }

                                ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/shared-entry/pcc-rule/range/end
                                leaf end {
                                    type types-filter:entry-id;
                                    mandatory true;
                                    description "Upper bound of the range for PCC rule filter entries";
                                }

                            } // container range

                        } // container pcc-rule

                        ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/shared-entry/watermark
                        container watermark {
                            presence "Enable the watermark context";
                            description "Enable the watermark context";

                            ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/shared-entry/watermark/low
                            leaf low {
                                type int32 {
                                    range "0..7999";
                                }
                                mandatory true;
                                description "Limit of RADIUS or Diameter shared filters before clearing high watermark notification";
                            }

                            ///nokia-conf/configure/filter/ip-filter/subscriber-mgmt/shared-entry/watermark/high
                            leaf high {
                                type int32 {
                                    range "1..8000";
                                }
                                mandatory true;
                                description "Limit of RADIUS shared filters before generating high watermark notification";
                            }

                        } // container watermark

                    } // container shared-entry

                } // container subscriber-mgmt

                ///nokia-conf/configure/filter/ip-filter/entry
                list entry {
                    key "entry-id";
                    description "Enter the entry list instance";

                    ///nokia-conf/configure/filter/ip-filter/entry/entry-id
                    leaf entry-id {
                        type types-filter:entry-id;
                        description "ID for a match criteria and the corresponding action";
                    }

                    ///nokia-conf/configure/filter/ip-filter/entry/description
                    leaf description {
                        type types-sros:description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/filter/ip-filter/entry/pbr-down-action-override
                    leaf pbr-down-action-override {
                        type types-filter:filter-pbr-down-action-ovr;
                        description "Action when PBR or PBF target for this entry is not available";
                    }

                    ///nokia-conf/configure/filter/ip-filter/entry/sticky-dest
                    leaf sticky-dest {
                        type types-filter:filter-sticky-dest;
                        units "seconds";
                        description "Time before action with available PBR or PBF destination and highest priority";
                    }

                    ///nokia-conf/configure/filter/ip-filter/entry/log
                    leaf log {
                        type leafref {
                            path "../../../log/log-id";
                        }
                        description "Log that is used for packets matching this entry";
                    }

                    ///nokia-conf/configure/filter/ip-filter/entry/collect-stats
                    leaf collect-stats {
                        type boolean;
                        default "false";
                        description "Enable statistics collection for an entry";
                    }

                    ///nokia-conf/configure/filter/ip-filter/entry/egress-pbr
                    leaf egress-pbr {
                        type types-filter:filter-egress-pbr;
                        sros-ext:immutable;
                        description "PBR that has an effect when this filter is applied on egress";
                    }

                    ///nokia-conf/configure/filter/ip-filter/entry/filter-sample
                    leaf filter-sample {
                        type boolean;
                        default "false";
                        description "Sample matching traffic if IP interface is set to cflowd ACL mode";
                    }

                    ///nokia-conf/configure/filter/ip-filter/entry/interface-sample
                    leaf interface-sample {
                        type boolean;
                        default "true";
                        description "Sample matching traffic if IP interface is set to cflowd interface mode";
                    }

                    ///nokia-conf/configure/filter/ip-filter/entry/sample-profile
                    leaf sample-profile {
                        type leafref {
                            path "../../../../cflowd/sample-profile/profile-id";
                        }
                        description "Cflowd sample profile ID to match packets";
                    }

                    ///nokia-conf/configure/filter/ip-filter/entry/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/ip-filter/entry/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/filter/ip-filter/entry/match
                    container match {
                        description "Enter the match context";

                        ///nokia-conf/configure/filter/ip-filter/entry/match/dscp
                        leaf dscp {
                            type types-qos:dscp-name;
                            description "DSCP used as an IP filter match criterion";
                        }

                        ///nokia-conf/configure/filter/ip-filter/entry/match/fragment
                        leaf fragment {
                            type enumeration {
                                enum false                          { value 2; }
                                enum true                           { value 3; }
                                enum first-only                     { value 4; }
                                enum non-first-only                 { value 5; }
                            }
                            description "Match criterion for fragmented packets";
                        }

                        ///nokia-conf/configure/filter/ip-filter/entry/match/multiple-option
                        leaf multiple-option {
                            type boolean;
                            description "Match based on presence of multiple options in header";
                        }

                        ///nokia-conf/configure/filter/ip-filter/entry/match/option-present
                        leaf option-present {
                            type boolean;
                            description "Match on the presence of any IP option in the packet";
                        }

                        ///nokia-conf/configure/filter/ip-filter/entry/match/src-route-option
                        leaf src-route-option {
                            type boolean;
                            description "Match based on presence of source route option";
                        }

                        ///nokia-conf/configure/filter/ip-filter/entry/match/destination-class
                        leaf destination-class {
                            type types-sros:class-index;
                            description "Destination class as a match criterion";
                        }

                        ///nokia-conf/configure/filter/ip-filter/entry/match/match-protocol-choice
                        choice match-protocol-choice {
                            ///nokia-conf/configure/filter/ip-filter/entry/match/match-protocol-choice/protocol
                            case protocol {

                                ///nokia-conf/configure/filter/ip-filter/entry/match/match-protocol-choice/protocol/protocol
                                leaf protocol {
                                    type types-sros:match-ip-protocol;
                                    description "IP protocol identifier as a match criterion";
                                }

                            }
                            ///nokia-conf/configure/filter/ip-filter/entry/match/match-protocol-choice/protocol-list
                            case protocol-list {

                                ///nokia-conf/configure/filter/ip-filter/entry/match/match-protocol-choice/protocol-list/protocol-list
                                leaf protocol-list {
                                    type leafref {
                                        path "../../../../match-list/protocol-list/protocol-list-name";
                                    }
                                    description "Name of the protocol list as a match criterion";
                                }

                            }
                        }
                        ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector
                        choice ip-selector {
                            ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip
                            case src-dst-ip {

                                ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip/src-ip
                                container src-ip {
                                    description "Enter the src-ip context";

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip/src-ip/match-address-choice
                                    choice match-address-choice {
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip/src-ip/match-address-choice/address-and-prefix-or-mask
                                        case address-and-prefix-or-mask {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip/src-ip/match-address-choice/address-and-prefix-or-mask/address
                                            leaf address {
                                                type union {
                                                    type types-sros:ipv4-prefix-with-host-bits;
                                                    type types-sros:ipv4-address;
                                                }
                                                description "IPv4 address used as the match criterion";
                                            }

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip/src-ip/match-address-choice/address-and-prefix-or-mask/mask
                                            leaf mask {
                                                type types-sros:ipv4-address;
                                                description "IPv4 address mask used as the match criterion";
                                            }

                                        }
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip/src-ip/match-address-choice/ip-prefix-list
                                        case ip-prefix-list {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip/src-ip/match-address-choice/ip-prefix-list/ip-prefix-list
                                            leaf ip-prefix-list {
                                                type leafref {
                                                    path "../../../../../match-list/ip-prefix-list/prefix-list-name";
                                                }
                                                description "IP4 address prefix list used as match criterion";
                                            }

                                        }
                                    }
                                } // container src-ip

                                ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip/dst-ip
                                container dst-ip {
                                    description "Enter the dst-ip context";

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip/dst-ip/match-address-choice
                                    choice match-address-choice {
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip/dst-ip/match-address-choice/address-and-prefix-or-mask
                                        case address-and-prefix-or-mask {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip/dst-ip/match-address-choice/address-and-prefix-or-mask/address
                                            leaf address {
                                                type union {
                                                    type types-sros:ipv4-prefix-with-host-bits;
                                                    type types-sros:ipv4-address;
                                                }
                                                description "IPv4 address used as the match criterion";
                                            }

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip/dst-ip/match-address-choice/address-and-prefix-or-mask/mask
                                            leaf mask {
                                                type types-sros:ipv4-address;
                                                description "IPv4 address mask used as the match criterion";
                                            }

                                        }
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip/dst-ip/match-address-choice/ip-prefix-list
                                        case ip-prefix-list {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/src-dst-ip/dst-ip/match-address-choice/ip-prefix-list/ip-prefix-list
                                            leaf ip-prefix-list {
                                                type leafref {
                                                    path "../../../../../match-list/ip-prefix-list/prefix-list-name";
                                                }
                                                description "IP4 address prefix list used as match criterion";
                                            }

                                        }
                                    }
                                } // container dst-ip

                            }
                            ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/ip
                            case ip {

                                ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/ip/ip
                                container ip {
                                    description "Enter the ip context";

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/ip/ip/match-address-choice
                                    choice match-address-choice {
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/ip/ip/match-address-choice/address-and-prefix-or-mask
                                        case address-and-prefix-or-mask {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/ip/ip/match-address-choice/address-and-prefix-or-mask/address
                                            leaf address {
                                                type union {
                                                    type types-sros:ipv4-prefix-with-host-bits;
                                                    type types-sros:ipv4-address;
                                                }
                                                description "IPv4 address used as the match criterion";
                                            }

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/ip/ip/match-address-choice/address-and-prefix-or-mask/mask
                                            leaf mask {
                                                type types-sros:ipv4-address;
                                                description "IPv4 address mask used as the match criterion";
                                            }

                                        }
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/ip/ip/match-address-choice/ip-prefix-list
                                        case ip-prefix-list {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/ip-selector/ip/ip/match-address-choice/ip-prefix-list/ip-prefix-list
                                            leaf ip-prefix-list {
                                                type leafref {
                                                    path "../../../../../match-list/ip-prefix-list/prefix-list-name";
                                                }
                                                description "IP4 address prefix list used as match criterion";
                                            }

                                        }
                                    }
                                } // container ip

                            }
                        }
                        ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector
                        choice port-selector {
                            ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/src-dst-port
                            case src-dst-port {

                                ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/src-dst-port/src-port
                                container src-port {
                                    description "Enter the src-port context";

                                    ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/src-dst-port/src-port/port
                                    choice port {
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/src-dst-port/src-port/port/eq
                                        case eq {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/src-dst-port/src-port/port/eq/eq
                                            leaf eq {
                                                type uint16 {
                                                    range "0..65535";
                                                }
                                                description "Exact match criterion for the port number";
                                            }

                                        }
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/src-dst-port/src-port/port/lt
                                        case lt {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/src-dst-port/src-port/port/lt/lt
                                            leaf lt {
                                                type uint16 {
                                                    range "1..65535";
                                                }
                                                description "Less than match criterion for the port number";
                                            }

                                        }
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/src-dst-port/src-port/port/gt
                                        case gt {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/src-dst-port/src-port/port/gt/gt
                                            leaf gt {
                                                type uint16 {
                                                    range "0..65534";
                                                }
                                                description "Greater than match criterion for the port number";
                                            }

                                        }
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/src-dst-port/src-port/port/range
                                        case range {

                                            ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/src-dst-port/src-port/port/range/range
                                            container range {
                                                presence "Enable the range context";
                                                description "Enable the range context";

                                                ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/src-dst-port/src-port/port/range/range/start
                                                leaf start {
                                                    type uint16 {
                                                        range "0..65534";
                                                    }
                                                    mandatory true;
                                                    description "Lower bound of the port range as port match criterion";
                                                }

                                                ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/src-dst-port/src-port/port/range/range/end
                                                leaf end {
                                                    type uint16 {
                                                        range "1..65535";
                                                    }
                                                    mandatory true;
                                                    description "Upper bound of the port range as port match criterion";
                                                }

                                            } // container range

                                        }
                                        ///nokia-conf/configure/filter/ip-filter/entry/match/port-selector/src-dst-port/src-port/port/port-list
