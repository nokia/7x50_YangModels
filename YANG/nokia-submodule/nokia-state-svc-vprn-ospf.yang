                        ///nokia-state/state/service/vprn/ospf/area/interface/last-packet-drop/packet-type
                        leaf packet-type {
                            type uint8;
                            description "the OSPF packet type of the dropped packet from the packet header.";
                        }

                        ///nokia-state/state/service/vprn/ospf/area/interface/last-packet-drop/router-id
                        leaf router-id {
                            type types-sros:ipv4-address;
                            description "the OSPF router ID of the dropped packet from the packet header.";
                        }

                        ///nokia-state/state/service/vprn/ospf/area/interface/last-packet-drop/area-id
                        leaf area-id {
                            type types-sros:ipv4-address;
