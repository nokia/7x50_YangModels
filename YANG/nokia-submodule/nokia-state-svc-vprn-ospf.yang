                ///nokia-state/state/service/vprn/ospf/as-capabilities/ls-id
                leaf ls-id {
                    type types-sros:ipv4-address;
                    description "Link state advertisement identity.";
                }

                ///nokia-state/state/service/vprn/ospf/as-capabilities/sequence-nr
                leaf sequence-nr {
                    type uint32;
                }

                ///nokia-state/state/service/vprn/ospf/as-capabilities/age
                leaf age {
                    type int32;
                }

                ///nokia-state/state/service/vprn/ospf/as-capabilities/checksum
                leaf checksum {
                    type uint32;
                }

                ///nokia-state/state/service/vprn/ospf/as-capabilities/advertisement
                leaf advertisement {
                    type string {
                        length "1..9778";
                    }
                    description "Contents of the link state advertisement.";
                }

            } // list as-capabilities

            ///nokia-state/state/service/vprn/ospf/route
            list route {
                key "route-type destination area";
                description "Enter the route list instance";

                ///nokia-state/state/service/vprn/ospf/route/route-type
                leaf route-type {
                    type types-ospf:route-type;
                    description "The route type os this route, network or router.";
                }

                ///nokia-state/state/service/vprn/ospf/route/destination
                leaf destination {
                    type types-sros:ip-prefix;
                    description "The value specifies the IP address and mask of the route destination.";
                }

                ///nokia-state/state/service/vprn/ospf/route/area
                leaf area {
                    type types-sros:ipv4-address;
                    description "Area through which the destination is reachable.";
                }

                ///nokia-state/state/service/vprn/ospf/route/path-type
                leaf path-type {
                    type types-ospf:route-path-type;
                    description "Select path type for the destination.";
                }

                ///nokia-state/state/service/vprn/ospf/route/dest-type
                leaf dest-type {
                    type types-ospf:route-dest-type;
                }

                ///nokia-state/state/service/vprn/ospf/route/rtm-status
                leaf rtm-status {
                    type types-ospf:rtm-status;
                    description "Indicates status in RTM.";
                }

                ///nokia-state/state/service/vprn/ospf/route/cost
                leaf cost {
                    type uint32;
                    description "The cost of this route, calculated during SPF.";
                }

                ///nokia-state/state/service/vprn/ospf/route/type2-cost
                leaf type2-cost {
                    type uint32;
                    description "The external type2 cost of this route, calculated during SPF.";
                }

                ///nokia-state/state/service/vprn/ospf/route/next-hop
                list next-hop {
                    key "ecmp-index";
                    description "Enter the next-hop list instance";

                    ///nokia-state/state/service/vprn/ospf/route/next-hop/ecmp-index
                    leaf ecmp-index {
                        type types-router:ip-ecmp;
                        description "ECMP index of this route next-hop.";
                    }

                    ///nokia-state/state/service/vprn/ospf/route/next-hop/type
                    leaf type {
                        type enumeration {
                            enum unknown                        { value 0; }
                            enum direct                         { value 1; }
                            enum rsvp                           { value 2; }
                            enum srte                           { value 3; }
                            enum shortcut                       { value 4; }
                            enum blackhole                      { value 5; }
                            enum unnumbered                     { value 6; }
                            enum ip                             { value 7; }
                        }
                        description "Indicates the next hop type";
                    }

                    ///nokia-state/state/service/vprn/ospf/route/next-hop/ip-address
                    leaf ip-address {
                        type types-sros:ip-address;
                        description "The value specifies the IP address of the next hop for this route";
                    }

                    ///nokia-state/state/service/vprn/ospf/route/next-hop/interface
                    leaf interface {
                        type types-sros:interface-name;
                        description "The value specifies the interface of the next hop for this route";
                    }

                    ///nokia-state/state/service/vprn/ospf/route/next-hop/tunnel-id
                    leaf tunnel-id {
                        type uint32;
                        description "The tunnel-id given to this tunnelled next hop";
                    }

                    ///nokia-state/state/service/vprn/ospf/route/next-hop/area
                    leaf area {
                        type types-sros:ipv4-address;
                        description "The value specifies the area the shortest path was found in";
                    }

                    ///nokia-state/state/service/vprn/ospf/route/next-hop/rtm-status
                    leaf rtm-status {
                        type enumeration {
                            enum success                        { value 0; }
                            enum discarded                      { value 1; }
                        }
                        description "Indicates the next hop status in RTM";
                    }

                    ///nokia-state/state/service/vprn/ospf/route/next-hop/weight
                    container weight {
                        description "Enter the weight context";

                        ///nokia-state/state/service/vprn/ospf/route/next-hop/weight/configured
                        leaf configured {
                            type uint32;
                            description "The weight given to this next hop";
                        }

                        ///nokia-state/state/service/vprn/ospf/route/next-hop/weight/normalised
                        leaf normalised {
                            type uint32;
                            description "The normalised weight of this next hop for this route";
                        }

                    } // container weight

                } // list next-hop

            } // list route

            ///nokia-state/state/service/vprn/ospf/hostname
            list hostname {
                key "router-id";
                description "Enter the hostname list instance";

                ///nokia-state/state/service/vprn/ospf/hostname/router-id
                leaf router-id {
                    type types-igp:router-id;
                    description "Specifies the router-id for which we map the hostname";
                }

                ///nokia-state/state/service/vprn/ospf/hostname/name
                leaf name {
                    type types-sros:display-string;
                    description "The value indicates the mapping hostname for the corresponding router-id";
                }

            } // list hostname

        } // list ospf

    } // grouping state-svc-vprn-ospf

}
