                        leaf ls-type {
                            type uint32;
                            description "Link state type.";
                        }

                        ///nokia-state/state/service/vprn/ospf/area/interface/capabilities/adv-rtr-id
                        leaf adv-rtr-id {
                            type types-sros:ipv4-address;
