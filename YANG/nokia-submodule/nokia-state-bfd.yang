submodule nokia-state-bfd {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-bfd                { prefix "types-bfd"; }
    import nokia-types-card               { prefix "types-card"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for BFD protocol on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-bfd YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-02-28";

    grouping state-bfd {
        ///nokia-state/state/bfd
        container bfd {
            description "Enter the bfd context";

            ///nokia-state/state/bfd/oper-bfd-template
            list oper-bfd-template {
                key "name";
                description "Enter the oper-bfd-template list instance";

                ///nokia-state/state/bfd/oper-bfd-template/name
                leaf name {
                    type types-sros:named-item;
                    description "BFD template name.";
                }

                ///nokia-state/state/bfd/oper-bfd-template/type
                leaf type {
                    type types-bfd:cfg-endpoint-type;
                    description "The BFD session endpoint type.";
                }

                ///nokia-state/state/bfd/oper-bfd-template/transmit-interval
                leaf transmit-interval {
                    type uint32 {
                        range "10..100000";
                    }
                    description "Transmit interval.";
                }

                ///nokia-state/state/bfd/oper-bfd-template/receive-interval
                leaf receive-interval {
                    type uint32 {
                        range "10..100000";
                    }
                    description "Receive interval.";
                }

                ///nokia-state/state/bfd/oper-bfd-template/multiplier
                leaf multiplier {
                    type uint32 {
                        range "1..20";
                    }
                    description "Multiplier.";
                }

                ///nokia-state/state/bfd/oper-bfd-template/echo-receive
                leaf echo-receive {
                    type uint32 {
                        range "100..100000";
                    }
                    description "Echo receive interval.";
                }

                ///nokia-state/state/bfd/oper-bfd-template/associations
                container associations {
                    description "Enter the associations context";

                    ///nokia-state/state/bfd/oper-bfd-template/associations/lsp-ldp
                    leaf lsp-ldp {
                        type uint32;
                        description "Number of LSP LDP references to the BFD template.";
                    }

                    ///nokia-state/state/bfd/oper-bfd-template/associations/dynamic-lsp-rsvp
                    leaf dynamic-lsp-rsvp {
                        type uint32;
                        description "Number of dynamic LSP RSVP references to the BFD template.";
                    }

                    ///nokia-state/state/bfd/oper-bfd-template/associations/template-lsp-rsvp
                    leaf template-lsp-rsvp {
                        type uint32;
                        description "Number of template LSP RSVP references to the BFD template.";
                    }

                    ///nokia-state/state/bfd/oper-bfd-template/associations/lsp-sr-te
                    leaf lsp-sr-te {
                        type uint32;
                        description "Number of LSP SR-TE references to the BFD template.";
                    }

                    ///nokia-state/state/bfd/oper-bfd-template/associations/template-lsp-sr-te
                    leaf template-lsp-sr-te {
                        type uint32;
                        description "Number of template LSP SR-TE references to the BFD template.";
                    }

                    ///nokia-state/state/bfd/oper-bfd-template/associations/static-sr-policy
                    leaf static-sr-policy {
                        type uint32;
                        description "Number of static sr-policy references to the BFD template.";
                    }

                    ///nokia-state/state/bfd/oper-bfd-template/associations/bgp-sr-policy
                    leaf bgp-sr-policy {
                        type uint32;
                        description "Number of BGP sr-policy references to the BFD template.";
                    }

                    ///nokia-state/state/bfd/oper-bfd-template/associations/oam-template
                    leaf oam-template {
                        type uint32;
                        description "Number of OAM template references to the BFD template.";
                    }

                    ///nokia-state/state/bfd/oper-bfd-template/associations/sdp-bind
                    leaf sdp-bind {
                        type uint32;
                        description "Number of SDP-bind references to the BFD template.";
                    }

                } // container associations

                ///nokia-state/state/bfd/oper-bfd-template/sdp-bind-ref
                list sdp-bind-ref {
                    key "svc-id sdp-bind-id";
                    description "Enter the sdp-bind-ref list instance";

                    ///nokia-state/state/bfd/oper-bfd-template/sdp-bind-ref/svc-id
                    leaf svc-id {
                        type types-services:service-id;
                        description "Service-id of the associated item.";
                    }

                    ///nokia-state/state/bfd/oper-bfd-template/sdp-bind-ref/sdp-bind-id
                    leaf sdp-bind-id {
                        type types-services:sdp-bind-id;
                        description "SDP Bind identifier of the associated item.";
                    }

                    ///nokia-state/state/bfd/oper-bfd-template/sdp-bind-ref/enabled
                    leaf enabled {
                        type boolean;
                        description "Indicates whether BFD is enabled on this SDP binding.";
                    }

                    ///nokia-state/state/bfd/oper-bfd-template/sdp-bind-ref/encap
                    leaf encap {
                        type types-services:bfd-encap;
                        description "BFD encapsulation used on this SDP binding.";
                    }

                } // list sdp-bind-ref

                ///nokia-state/state/bfd/oper-bfd-template/oam-template-ref
                list oam-template-ref {
                    key "router-instance template-name";
                    description "Enter the oam-template-ref list instance";

                    ///nokia-state/state/bfd/oper-bfd-template/oam-template-ref/router-instance
                    leaf router-instance {
                        type types-sros:router-instance;
                        description
                            "Administrative name for this virtual router. The router
                             name must be unique among all virtual routers in the system.";
                    }

                    ///nokia-state/state/bfd/oper-bfd-template/oam-template-ref/template-name
                    leaf template-name {
                        type types-sros:named-item;
                        description "OAM template name.";
                    }

                } // list oam-template-ref

            } // list oper-bfd-template

            ///nokia-state/state/bfd/lsp-tail-session
            list lsp-tail-session {
                key "router-instance session-fec-type remote-address address lsp-id tunnel-id";
                description "Enter the lsp-tail-session list instance";

                ///nokia-state/state/bfd/lsp-tail-session/router-instance
                leaf router-instance {
                    type types-sros:router-instance;
                    description
                        "The administrative name for this virtual router. The router
                         name must be unique among all virtual routers in the system.";
                }

                ///nokia-state/state/bfd/lsp-tail-session/session-fec-type
                leaf session-fec-type {
                    type types-bfd:fec-type;
                    description "Fec type of this BFD session.";
                }

                ///nokia-state/state/bfd/lsp-tail-session/remote-address
                leaf remote-address {
                    type types-sros:ip-address-with-zone;
                    description "Address of the remote endpoint of this BFD session.";
                }

                ///nokia-state/state/bfd/lsp-tail-session/address
                leaf address {
                    type types-sros:ip-address-with-zone;
                    description "Address of the local endpoint of this BFD session.";
                }

                ///nokia-state/state/bfd/lsp-tail-session/lsp-id
                leaf lsp-id {
                    type uint32;
                    description "Path-id when applicable. 0 otherwise.";
                }

                ///nokia-state/state/bfd/lsp-tail-session/tunnel-id
                leaf tunnel-id {
                    type uint32;
                    description "Tunnel-id when applicable. 0 otherwise.";
                }

                ///nokia-state/state/bfd/lsp-tail-session/session-name
                leaf session-name {
                    type string {
                        length "1..160";
                    }
                    description "Session name";
                }

                ///nokia-state/state/bfd/lsp-tail-session/state
                container state {
                    description "Enter the state context";

                    ///nokia-state/state/bfd/lsp-tail-session/state/location
                    leaf location {
                        type types-bfd:act-endpoint-type;
                        description "Location of the BFD session.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/state/configured-location
                    leaf configured-location {
                        type types-bfd:cfg-endpoint-type;
                        description "Configured location of the BFD session.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/state/oper-state
                    leaf oper-state {
                        type types-sros:oper-state;
                        description "Current operational state of this session.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/state/protocols
                    leaf protocols {
                        type types-bfd:protocols;
                        description "Protocols using this BFD session.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/state/session-state
                    leaf session-state {
                        type types-bfd:session-state;
                        description "State of the BFD protocol.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/state/down-reasons
                    leaf down-reasons {
                        type bits {
                            bit no-protocols                   { position 0; }
                            bit no-heart-beat                  { position 1; }
                            bit echo-failed                    { position 2; }
                            bit nbr-signal-down                { position 3; }
                            bit fwd-plane-reset                { position 4; }
                            bit path-down                      { position 5; }
                            bit nbr-admin-down                 { position 6; }
                            bit admin-clear                    { position 7; }
                            bit mis-conn-defect                { position 8; }
                        }
                        description "Reasons why a BFD session is not up.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/state/transmit-interval
                    leaf transmit-interval {
                        type uint32;
                        units "milliseconds";
                        description "Negotiated transmit interval.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/state/receive-interval
                    leaf receive-interval {
                        type uint32;
                        units "milliseconds";
                        description "Negotiated receive interval.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/state/multiplier
                    leaf multiplier {
                        type uint32;
                        units "milliseconds";
                        description "Negotiated multiplier.";
                    }

                } // container state

                ///nokia-state/state/bfd/lsp-tail-session/statistics
                container statistics {
                    description "Enter the statistics context";

                    ///nokia-state/state/bfd/lsp-tail-session/statistics/up-count
                    leaf up-count {
                        type yang:counter32;
                        description "Number of times the session-state changed to 'Up'.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/statistics/down-count
                    leaf down-count {
                        type yang:counter32;
                        description "Number of times the session-state changed to 'Down'.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/statistics/up-time
                    leaf up-time {
                        type types-sros:time-interval;
                        description "Time interval elapsed since the session is in state 'Up'.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/statistics/down-time
                    leaf down-time {
                        type types-sros:time-interval;
                        description "Time interval elapsed since the session is in state 'Down'.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/statistics/last-down-duration
                    leaf last-down-duration {
                        type types-sros:time-interval;
                        description "Time interval the session was in another state before it went 'Up'";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/statistics/last-up-duration
                    leaf last-up-duration {
                        type types-sros:time-interval;
                        description "Time interval the session was in state 'Up' before it left this state";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/statistics/received-messages
                    leaf received-messages {
                        type yang:counter32;
                        description "Number of messages received for the bfd session.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/statistics/version-mismatch
                    leaf version-mismatch {
                        type yang:counter32;
                        description "Number of version mismatch errors for the bfd session.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/statistics/last-received
                    leaf last-received {
                        type yang:date-and-time;
                        description
                            "Time when the most recent message was received for the
                             bfd session that was not handled by the network processor.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/statistics/time-since-np-receive
                    leaf time-since-np-receive {
                        type uint32;
                        units "milliseconds";
                        description
                            "Time elapsed since the most recent message was received
                             by the network processor for the BFD session. Zero is returned
                             if the session is Down.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/statistics/sent-messages
                    leaf sent-messages {
                        type yang:counter32;
                        description "Number of messages sent in this session.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/statistics/last-sent
                    leaf last-sent {
                        type yang:date-and-time;
                        description
                            "Time when the most recent message was sent
                             (not including the ones transmitted by the network processor).";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/statistics/finals-sent
                    leaf finals-sent {
                        type uint32;
                        description
                            "Number of protocol messages sent with the final
                             flag set.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/statistics/time-since-np-sent
                    leaf time-since-np-sent {
                        type uint32;
                        units "milliseconds";
                        description
                            "Time elapsed since the most recent message was sent
                             by the network processor for the BFD session. Zero is returned
                             if the session is Down.";
                    }

                } // container statistics

                ///nokia-state/state/bfd/lsp-tail-session/local-parameters
                container local-parameters {
                    description "Enter the local-parameters context";

                    ///nokia-state/state/bfd/lsp-tail-session/local-parameters/discriminator
                    leaf discriminator {
                        type uint32;
                        description "Unique discriminator for this session.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/local-parameters/state
                    leaf state {
                        type types-bfd:session-state;
                        description "State of this session.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/local-parameters/diag
                    leaf diag {
                        type enumeration {
                            enum detect-time-expired            { value 1; }
                            enum echo-func-failed               { value 2; }
                            enum neighbor-down                  { value 3; }
                            enum forwarding-plane-reset         { value 4; }
                            enum path-down                      { value 5; }
                            enum concatenated-path-down         { value 6; }
                            enum admin-down                     { value 7; }
                            enum reverse-concat-path-down       { value 8; }
                            enum mis-connectivity-defect        { value 9; }
                            enum admin-clear                    { value 39; }
                        }
                        description "Diagnostic value.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/local-parameters/mode
                    leaf mode {
                        type enumeration {
                            enum async                          { value 0; }
                            enum demand                         { value 1; }
                        }
                        description "Mode.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/local-parameters/tx-interval
                    leaf tx-interval {
                        type uint32;
                        description "Minimal transmit interval.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/local-parameters/multiplier
                    leaf multiplier {
                        type uint32;
                        description "Multiplier.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/local-parameters/rx-interval
                    leaf rx-interval {
                        type uint32;
                        description "Minimal receive interval.";
                    }

                } // container local-parameters

                ///nokia-state/state/bfd/lsp-tail-session/remote-parameters
                container remote-parameters {
                    description "Enter the remote-parameters context";

                    ///nokia-state/state/bfd/lsp-tail-session/remote-parameters/discriminator
                    leaf discriminator {
                        type uint32;
                        description "Unique discriminator for this session.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/remote-parameters/state
                    leaf state {
                        type types-bfd:session-state;
                        description "State of this session.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/remote-parameters/diag
                    leaf diag {
                        type enumeration {
                            enum detect-time-expired            { value 1; }
                            enum echo-func-failed               { value 2; }
                            enum neighbor-down                  { value 3; }
                            enum forwarding-plane-reset         { value 4; }
                            enum path-down                      { value 5; }
                            enum concatenated-path-down         { value 6; }
                            enum admin-down                     { value 7; }
                            enum reverse-concat-path-down       { value 8; }
                            enum mis-connectivity-defect        { value 9; }
                            enum admin-clear                    { value 39; }
                        }
                        description "Diagnostic value.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/remote-parameters/mode
                    leaf mode {
                        type enumeration {
                            enum async                          { value 0; }
                            enum demand                         { value 1; }
                        }
                        description "Mode.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/remote-parameters/tx-interval
                    leaf tx-interval {
                        type uint32;
                        description "Minimal transmit interval.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/remote-parameters/multiplier
                    leaf multiplier {
                        type uint32;
                        description "Multiplier.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/remote-parameters/rx-interval
                    leaf rx-interval {
                        type uint32;
                        description "Minimal receive interval.";
                    }

                    ///nokia-state/state/bfd/lsp-tail-session/remote-parameters/c-flag
                    leaf c-flag {
                        type boolean;
                        description "BFD is independent of the control plane.";
                    }

                } // container remote-parameters

            } // list lsp-tail-session

            ///nokia-state/state/bfd/lsp-head-session
            list lsp-head-session {
                status deprecated;
                key "router-instance session-fec-type remote-address address lsp-id tunnel-id";
                description "Enter the lsp-head-session list instance";

                ///nokia-state/state/bfd/lsp-head-session/router-instance
                leaf router-instance {
                    status deprecated;
                    type types-sros:router-instance;
                    description
                        "The administrative name for this virtual router. The router
                         name must be unique among all virtual routers in the system.";
                }

                ///nokia-state/state/bfd/lsp-head-session/session-fec-type
                leaf session-fec-type {
                    status deprecated;
                    type types-bfd:fec-type;
                    description "Fec type of this BFD session.";
                }

                ///nokia-state/state/bfd/lsp-head-session/remote-address
                leaf remote-address {
                    status deprecated;
                    type types-sros:ip-address-with-zone;
                    description "Address of the remote endpoint of this BFD session.";
                }

                ///nokia-state/state/bfd/lsp-head-session/address
                leaf address {
                    status deprecated;
                    type types-sros:ip-address-with-zone;
                    description "Address of the local endpoint of this BFD session.";
                }

                ///nokia-state/state/bfd/lsp-head-session/lsp-id
                leaf lsp-id {
                    status deprecated;
                    type uint32;
                    description "Path-id when applicable. 0 otherwise.";
                }

                ///nokia-state/state/bfd/lsp-head-session/tunnel-id
                leaf tunnel-id {
                    status deprecated;
                    type uint32;
                    description "Tunnel-id when applicable. 0 otherwise.";
                }

                ///nokia-state/state/bfd/lsp-head-session/session-name
                leaf session-name {
                    status deprecated;
                    type string {
                        length "1..160";
                    }
                    description "Session name";
                }

                ///nokia-state/state/bfd/lsp-head-session/state
                container state {
                    status deprecated;
                    description "Enter the state context";

                    ///nokia-state/state/bfd/lsp-head-session/state/location
                    leaf location {
                        status deprecated;
                        type types-bfd:act-endpoint-type;
                        description "Location of the BFD session.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/state/configured-location
                    leaf configured-location {
                        status deprecated;
                        type types-bfd:cfg-endpoint-type;
                        description "Configured location of the BFD session.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/state/oper-state
                    leaf oper-state {
                        status deprecated;
                        type types-sros:oper-state;
                        description "Current operational state of this session.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/state/protocols
                    leaf protocols {
                        status deprecated;
                        type types-bfd:protocols;
                        description "Protocols using this BFD session.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/state/session-state
                    leaf session-state {
                        status deprecated;
                        type types-bfd:session-state;
                        description "State of the BFD protocol.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/state/down-reasons
                    leaf down-reasons {
                        status deprecated;
                        type bits {
                            bit no-protocols                   { position 0; }
                            bit no-heart-beat                  { position 1; }
                            bit echo-failed                    { position 2; }
                            bit nbr-signal-down                { position 3; }
                            bit fwd-plane-reset                { position 4; }
                            bit path-down                      { position 5; }
                            bit nbr-admin-down                 { position 6; }
                            bit admin-clear                    { position 7; }
                            bit mis-conn-defect                { position 8; }
                        }
                        description "Reasons why a BFD session is not up.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/state/transmit-interval
                    leaf transmit-interval {
                        status deprecated;
                        type uint32;
                        units "milliseconds";
                        description "Negotiated transmit interval.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/state/receive-interval
                    leaf receive-interval {
                        status deprecated;
                        type uint32;
                        units "milliseconds";
                        description "Negotiated receive interval.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/state/multiplier
                    leaf multiplier {
                        status deprecated;
                        type uint32;
                        units "milliseconds";
                        description "Negotiated multiplier.";
                    }

                } // container state

                ///nokia-state/state/bfd/lsp-head-session/local-parameters
                container local-parameters {
                    status deprecated;
                    description "Enter the local-parameters context";

                    ///nokia-state/state/bfd/lsp-head-session/local-parameters/discriminator
                    leaf discriminator {
                        status deprecated;
                        type uint32;
                        description "Unique discriminator for this session.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/local-parameters/state
                    leaf state {
                        status deprecated;
                        type types-bfd:session-state;
                        description "State of this session.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/local-parameters/diag
                    leaf diag {
                        status deprecated;
                        type enumeration {
                            enum detect-time-expired            { value 1; }
                            enum echo-func-failed               { value 2; }
                            enum neighbor-down                  { value 3; }
                            enum forwarding-plane-reset         { value 4; }
                            enum path-down                      { value 5; }
                            enum concatenated-path-down         { value 6; }
                            enum admin-down                     { value 7; }
                            enum reverse-concat-path-down       { value 8; }
                            enum mis-connectivity-defect        { value 9; }
                            enum admin-clear                    { value 39; }
                        }
                        description "Diagnostic value.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/local-parameters/mode
                    leaf mode {
                        status deprecated;
                        type enumeration {
                            enum async                          { value 0; }
                            enum demand                         { value 1; }
                        }
                        description "Mode.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/local-parameters/tx-interval
                    leaf tx-interval {
                        status deprecated;
                        type uint32;
                        description "Minimal transmit interval.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/local-parameters/multiplier
                    leaf multiplier {
                        status deprecated;
                        type uint32;
                        description "Multiplier.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/local-parameters/rx-interval
                    leaf rx-interval {
                        status deprecated;
                        type uint32;
                        description "Minimal receive interval.";
                    }

                } // container local-parameters

                ///nokia-state/state/bfd/lsp-head-session/remote-parameters
                container remote-parameters {
                    status deprecated;
                    description "Enter the remote-parameters context";

                    ///nokia-state/state/bfd/lsp-head-session/remote-parameters/discriminator
                    leaf discriminator {
                        status deprecated;
                        type uint32;
                        description "Unique discriminator for this session.";
                    }

                    ///nokia-state/state/bfd/lsp-head-session/remote-parameters/state
                    leaf state {
