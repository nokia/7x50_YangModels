                            ///nokia-state/state/router/ldp/bindings/service-fec129/out-label/cep-tdm/differential-timestamping
                            leaf differential-timestamping {
                                type boolean;
                                description "The value indicates the differential (or dynamic) timestamping mode bit.";
                            }

                            ///nokia-state/state/router/ldp/bindings/service-fec129/out-label/cep-tdm/signalling-packets-mode
                            leaf signalling-packets-mode {
                                type types-services:tdm-options-sig-pkts;
                                description "The value indicates the CE application signalling packets mode.";
                            }

                            ///nokia-state/state/router/ldp/bindings/service-fec129/out-label/cep-tdm/cas-trunk-framing-type
                            leaf cas-trunk-framing-type {
                                type types-services:tdm-options-cas-trunk-framing;
                                description "The value indicates the CAS trunk framing mode.";
                            }

                            ///nokia-state/state/router/ldp/bindings/service-fec129/out-label/cep-tdm/timestamping-frequency
                            leaf timestamping-frequency {
                                type uint32;
                                units "kilohertz";
                                description "The value indicates the value of the differential timestamp frequency.";
                            }

                            ///nokia-state/state/router/ldp/bindings/service-fec129/out-label/cep-tdm/payload-type
                            leaf payload-type {
                                type uint32;
                                description "The value indicates the local or the remote payload type.";
                            }

                            ///nokia-state/state/router/ldp/bindings/service-fec129/out-label/cep-tdm/ssrc-id
                            leaf ssrc-id {
                                type uint32;
                                description "The value indicates the local or the remote value of SSRC ID.";
                            }

                        } // container cep-tdm

                    } // container out-label

                } // list service-fec129

                ///nokia-state/state/router/ldp/bindings/active
                container active {
                    description "Enter the active context";

                    ///nokia-state/state/router/ldp/bindings/active/prefixes
                    list prefixes {
                        key "fec-type ip-prefix operation-type";
                        description "Enter the prefixes list instance";

                        ///nokia-state/state/router/ldp/bindings/active/prefixes/fec-type
                        leaf fec-type {
                            type types-ldp:fec-type;
                            description "The value specifies the FEC type of this address FEC.";
                        }

                        ///nokia-state/state/router/ldp/bindings/active/prefixes/ip-prefix
                        leaf ip-prefix {
                            type types-sros:ip-prefix;
                            description "The value specifies the FEC IP Prefix.";
                        }

                        ///nokia-state/state/router/ldp/bindings/active/prefixes/operation-type
                        leaf operation-type {
                            type enumeration {
                                enum pop                            { value 1; }
                                enum push                           { value 2; }
                                enum swap                           { value 3; }
                            }
                            description "The value indicates operation type of this FEC.";
                        }

                        ///nokia-state/state/router/ldp/bindings/active/prefixes/flags
                        leaf flags {
                            type types-ldp:fec-flags;
                            description "The bit flags indicate the characteristics of the FEC.";
                        }

                        ///nokia-state/state/router/ldp/bindings/active/prefixes/in-label
                        list in-label {
                            key "id";
                            description "Enter the in-label list instance";

                            ///nokia-state/state/router/ldp/bindings/active/prefixes/in-label/id
                            leaf id {
                                type uint32;
                                description "The value indicates the label number of the exchanged label.";
                            }

                            ///nokia-state/state/router/ldp/bindings/active/prefixes/in-label/label
                            leaf label {
                                type uint32;
                                description "The value indicates the label exchanged.";
                            }

                        } // list in-label

                        ///nokia-state/state/router/ldp/bindings/active/prefixes/out-label
                        list out-label {
                            key "id";
                            description "Enter the out-label list instance";

                            ///nokia-state/state/router/ldp/bindings/active/prefixes/out-label/id
                            leaf id {
                                type uint32;
                                description "The value indicates the label number of the exchanged label.";
                            }

                            ///nokia-state/state/router/ldp/bindings/active/prefixes/out-label/interface-name
                            leaf interface-name {
                                type types-sros:named-item-or-empty;
                                description "The value specifies the interface name for egress label.";
                            }

                            ///nokia-state/state/router/ldp/bindings/active/prefixes/out-label/next-hop
                            leaf next-hop {
                                type types-sros:ip-address;
                                description "The value specifies the next hop IP address for egress label.";
                            }

                            ///nokia-state/state/router/ldp/bindings/active/prefixes/out-label/lsp-name
                            leaf lsp-name {
                                type types-sros:named-item-64-or-empty;
                                description "The value specifies the LSP name associated with egress label.";
                            }

                            ///nokia-state/state/router/ldp/bindings/active/prefixes/out-label/label
                            leaf label {
                                type uint32;
                                description "The value indicates the label exchanged.";
                            }

                            ///nokia-state/state/router/ldp/bindings/active/prefixes/out-label/status
                            leaf status {
                                type types-ldp:label-status;
                                description "The value indicates the status of the label.";
                            }

                            ///nokia-state/state/router/ldp/bindings/active/prefixes/out-label/mtu
                            leaf mtu {
                                type uint32;
                                description
                                    "The value indicates the maximum transfer unit for payloads represented by
                                     this FEC";
                            }

                            ///nokia-state/state/router/ldp/bindings/active/prefixes/out-label/metric
                            leaf metric {
                                type uint32;
                                description
                                    "The value indicates the relative cost of the tunnel represented by this
                                     FEC";
                            }

                            ///nokia-state/state/router/ldp/bindings/active/prefixes/out-label/class-based-forwarding
                            leaf class-based-forwarding {
                                type types-mpls:lsp-fc-bits;
                                description
                                    "The value specifies (if lsp-name has value) the forwarding classes
                                     installed for the RSVP tunnel used by LDP in LDP over RSVP
                                     associated with egress label.

                                     If the 'default-lsp' bit is set, it indicates if the LSP for the RSVP
                                     tunnel used by LDP in LDP over RSVP associated with egress label is
                                     used as the class-based-forwarding default.";
                            }

                        } // list out-label

                    } // list prefixes

                } // container active

            } // container bindings

        } // container ldp

    } // grouping state-router-ldp

}
