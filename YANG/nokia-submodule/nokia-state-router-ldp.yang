submodule nokia-state-router-ldp {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-ldp                { prefix "types-ldp"; }
    import nokia-types-mpls               { prefix "types-mpls"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-state-policy-options;
    include nokia-state-router;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for ldp protocol on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-router-ldp YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-01-23";

    grouping state-router-ldp {
        ///nokia-state/state/router/ldp
        container ldp {
            description "Enable the ldp context";

            ///nokia-state/state/router/ldp/backoff-time
            leaf backoff-time {
                type types-ldp:backoff-time {
                    range "1..2592000";
                }
                description "The value specifies the initial value for the backoff interval.";
            }

            ///nokia-state/state/router/ldp/max-backoff-time
            leaf max-backoff-time {
                type types-ldp:backoff-time;
                description "The value specifies the maximum value for the backoff interval.";
            }

            ///nokia-state/state/router/ldp/ipv4-up-time
            leaf ipv4-up-time {
                type yang:timeticks {
                    range "0..2147483647";
                }
                description
                    "The value indicates the time, in centiseconds, that
                     the IPv4 LDP instance has been in current operational state.";
            }

            ///nokia-state/state/router/ldp/ipv6-up-time
            leaf ipv6-up-time {
                type yang:timeticks {
                    range "0..2147483647";
                }
                description
                    "The value indicates the time, in centiseconds,
                     that the IPv6 LDP instance has been in current operational state.";
            }

            ///nokia-state/state/router/ldp/ipv4-oper-state
            leaf ipv4-oper-state {
                type types-sros:oper-state;
                description "The value indicates the current operational state of this IPv4 LDP instance.";
            }

            ///nokia-state/state/router/ldp/ipv6-oper-state
            leaf ipv6-oper-state {
                type types-sros:oper-state;
                description "The value indicates the current operational state of this IPv6 LDP instance.";
            }

            ///nokia-state/state/router/ldp/ipv4-down-reason
            leaf ipv4-down-reason {
                type types-ldp:gen-down-reason-code;
                description
                    "The value indicates the reason
                     that the IPv4 LDP instance is operationally down.";
            }

            ///nokia-state/state/router/ldp/ipv6-down-reason
            leaf ipv6-down-reason {
                type types-ldp:gen-down-reason-code;
                description
                    "The value indicates the reason
                     that the IPv6 LDP instance is operationally down.";
            }

            ///nokia-state/state/router/ldp/ipv4-label-switch-router-id
            leaf ipv4-label-switch-router-id {
                type types-sros:ipv4-address;
                description
                    "The value is the IPv4 Label Switch Router
                     (LSR) identifier which is used as the first 4 bytes or the
                     Router Id component of the Label Distribution Protocol (LDP)
                     identifier.";
            }

            ///nokia-state/state/router/ldp/ipv6-label-switch-router-id
            leaf ipv6-label-switch-router-id {
                type types-sros:ipv6-address;
                description
                    "The value is the IPv6 Label Switch Router
                     (LSR) identifier which is used as the first 16 bytes or the
                     Router Id component of the Label Distribution Protocol (LDP)
                     identifier.";
            }

            ///nokia-state/state/router/ldp/protocol-version
            leaf protocol-version {
                type uint32;
                description
                    "The value specifies the version of the LDP protocol. A value of
                     zero (0) indicates that the version of the protocol is unknown.";
            }

            ///nokia-state/state/router/ldp/deaggregate-fec
            leaf deaggregate-fec {
                type boolean;
                description
                    "When deaggregate-fec has a value of 'false', LDP aggregates
                     multiple prefixes into a single Forwarding Equivalence Class
                     (FEC) and advertises a single label for the FEC. When the
                     value is 'true', LDP deaggregates prefixes into multiple FECs.";
            }

            ///nokia-state/state/router/ldp/control-mode
            leaf control-mode {
                type enumeration {
                    enum ordered                        { value 1; }
                    enum independent                    { value 2; }
                }
                description
                    "The value specifies the mode used for distributing labels
                     in response to label binding requests.";
            }

            ///nokia-state/state/router/ldp/distribution-method
            leaf distribution-method {
                type types-ldp:distribution-method;
                description "The value specifies the label distribution method used.";
            }

            ///nokia-state/state/router/ldp/retention-mode
            leaf retention-mode {
                type enumeration {
                    enum conservative                   { value 1; }
                    enum liberal                        { value 2; }
                }
                description "The value specifies the label retention mode used.";
            }

            ///nokia-state/state/router/ldp/propagate-policy
            leaf propagate-policy {
                type enumeration {
                    enum system                         { value 1; }
                    enum interface                      { value 2; }
                    enum all                            { value 3; }
                    enum none                           { value 4; }
                }
                description
                    "The value determines whether the LSR should generate
                     FECs and which FECs it should generate.";
            }

            ///nokia-state/state/router/ldp/loop-detection-cap
            leaf loop-detection-cap {
                type enumeration {
                    enum none                           { value 1; }
                    enum other                          { value 2; }
                    enum hop-count                      { value 3; }
                    enum path-vector                    { value 4; }
                    enum hop-count-and-path-vector      { value 5; }
                }
                description
                    "The value indicates whether this LSR supports loop detection
                     and if so, what type of loop detection is supported.";
            }

            ///nokia-state/state/router/ldp/hop-limit
            leaf hop-limit {
                type uint32 {
                    range "0..255";
                }
                description "The value specifies the maximum allowable value for the hop count.";
            }

            ///nokia-state/state/router/ldp/path-vector-limit
            leaf path-vector-limit {
                type uint32 {
                    range "0..255";
                }
                description
                    "The value of path-vector-limit specifies the maximum
                     allowable value for the path vector count.";
            }

            ///nokia-state/state/router/ldp/route-preference
            leaf route-preference {
                type uint32 {
                    range "1..255";
                }
                description
                    "The value of route preference assigned to LDP routes. When multiple routes are
                     available to a destination, the route with the lowest preference will be used.";
            }

            ///nokia-state/state/router/ldp/oper-ecmp
            leaf oper-ecmp {
                type uint32 {
                    range "1..64";
                }
                description
                    "The operational ECMP value. This value is the smaller of the configured router
                     ECMP value and the configured LDP max-ecmp-routes value.";
            }

            ///nokia-state/state/router/ldp/capability
            container capability {
                description "Enter the capability context";

                ///nokia-state/state/router/ldp/capability/p2mp
                leaf p2mp {
                    type boolean;
                    description
                        "The value indicates whether Point-to-Multipoint (P2MP) FEC capability is
                         supported in the LDP instance or not.";
                }

                ///nokia-state/state/router/ldp/capability/multipoint-make-before-break
                leaf multipoint-make-before-break {
                    type boolean;
                    description
                        "The value indicates whether Multipoint Make-Before-Break (MBB) capability
                         is supported in the LDP instance or not.";
                }

                ///nokia-state/state/router/ldp/capability/dynamic
                leaf dynamic {
                    type boolean;
                    description
                        "The value indicates whether dynamic capability negotiation is supported
                         or not in LDP sessions associated with the LDP instance.";
                }

                ///nokia-state/state/router/ldp/capability/overload
                leaf overload {
                    type boolean;
                    description
                        "The value indicates whether LSR Overload is supported or not in LDP
                         sessions associated with the LDP instance.";
                }

                ///nokia-state/state/router/ldp/capability/fec128
                leaf fec128 {
                    type boolean;
                    description
                        "The value indicates whether or not PW (Pseudowire) ID FEC capability is
                         supported in LDP instance.";
                }

                ///nokia-state/state/router/ldp/capability/fec129
                leaf fec129 {
                    type boolean;
                    description
                        "The value indicates whether or not General PW ID FEC capability is
                         supported in LDP instance.";
                }

                ///nokia-state/state/router/ldp/capability/ipv4-prefix-fec
                leaf ipv4-prefix-fec {
                    type boolean;
                    description
                        "The value indicates whether or not IPv4 Prefix FEC capability is
                         supported in LDP instance.";
                }

                ///nokia-state/state/router/ldp/capability/ipv6-prefix-fec
                leaf ipv6-prefix-fec {
                    type boolean;
                    description
                        "The value indicates whether or not IPv6 Prefix FEC capability is
                         supported in LDP instance.";
                }

                ///nokia-state/state/router/ldp/capability/unrecognized-notification
                leaf unrecognized-notification {
                    type boolean;
                    description
                        "The value indicates whether or not Unrecognized Notification
                         capability is supported in LDP instance.";
                }

            } // container capability

            ///nokia-state/state/router/ldp/statistics
            container statistics {
                description "Enter the statistics context";

                ///nokia-state/state/router/ldp/statistics/targeted-peer
                container targeted-peer {
                    description "Enter the targeted-peer context";

                    ///nokia-state/state/router/ldp/statistics/targeted-peer/ipv4
                    container ipv4 {
                        description "Enter the ipv4 context";

                        ///nokia-state/state/router/ldp/statistics/targeted-peer/ipv4/active
                        leaf active {
                            type uint32;
                            description
                                "The value indicates the number of active (i.e. operationally
                                 up) peers or interfaces associated with the LDP instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/targeted-peer/ipv4/inactive
                        leaf inactive {
                            type uint32;
                            description
                                "The value indicates the number of inactive (i.e. operationally
                                 down) peers or interfaces associated with the LDP instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/targeted-peer/ipv4/overloaded
                        leaf overloaded {
                            type uint32;
                            description
                                "The value indicates the number of overloaded peers or interfaces
                                 in the LDP instance.";
                        }

                    } // container ipv4

                    ///nokia-state/state/router/ldp/statistics/targeted-peer/ipv6
                    container ipv6 {
                        description "Enter the ipv6 context";

                        ///nokia-state/state/router/ldp/statistics/targeted-peer/ipv6/active
                        leaf active {
                            type uint32;
                            description
                                "The value indicates the number of active (i.e. operationally
                                 up) peers or interfaces associated with the LDP instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/targeted-peer/ipv6/inactive
                        leaf inactive {
                            type uint32;
                            description
                                "The value indicates the number of inactive (i.e. operationally
                                 down) peers or interfaces associated with the LDP instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/targeted-peer/ipv6/overloaded
                        leaf overloaded {
                            type uint32;
                            description
                                "The value indicates the number of overloaded peers or interfaces
                                 in the LDP instance.";
                        }

                    } // container ipv6

                } // container targeted-peer

                ///nokia-state/state/router/ldp/statistics/interface
                container interface {
                    description "Enter the interface context";

                    ///nokia-state/state/router/ldp/statistics/interface/ipv4
                    container ipv4 {
                        description "Enter the ipv4 context";

                        ///nokia-state/state/router/ldp/statistics/interface/ipv4/active
                        leaf active {
                            type uint32;
                            description
                                "The value indicates the number of active (i.e. operationally
                                 up) peers or interfaces associated with the LDP instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/interface/ipv4/inactive
                        leaf inactive {
                            type uint32;
                            description
                                "The value indicates the number of inactive (i.e. operationally
                                 down) peers or interfaces associated with the LDP instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/interface/ipv4/overloaded
                        leaf overloaded {
                            type uint32;
                            description
                                "The value indicates the number of overloaded peers or interfaces
                                 in the LDP instance.";
                        }

                    } // container ipv4

                    ///nokia-state/state/router/ldp/statistics/interface/ipv6
                    container ipv6 {
                        description "Enter the ipv6 context";

                        ///nokia-state/state/router/ldp/statistics/interface/ipv6/active
                        leaf active {
                            type uint32;
                            description
                                "The value indicates the number of active (i.e. operationally
                                 up) peers or interfaces associated with the LDP instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/interface/ipv6/inactive
                        leaf inactive {
                            type uint32;
                            description
                                "The value indicates the number of inactive (i.e. operationally
                                 down) peers or interfaces associated with the LDP instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/interface/ipv6/overloaded
                        leaf overloaded {
                            type uint32;
                            description
                                "The value indicates the number of overloaded peers or interfaces
                                 in the LDP instance.";
                        }

                    } // container ipv6

                } // container interface

                ///nokia-state/state/router/ldp/statistics/adjacency
                container adjacency {
                    description "Enter the adjacency context";

                    ///nokia-state/state/router/ldp/statistics/adjacency/ipv4
                    container ipv4 {
                        description "Enter the ipv4 context";

                        ///nokia-state/state/router/ldp/statistics/adjacency/ipv4/link
                        leaf link {
                            type uint32;
                            description
                                "The value indicates the number of active link adjacencies (i.e
                                 established adjacencies) associated with the LDP instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/adjacency/ipv4/targeted
                        leaf targeted {
                            type uint32;
                            description
                                "The value indicates the number of active targeted adjacencies
                                 (i.e established adjacencies) associated with the LDP instance.";
                        }

                    } // container ipv4

                    ///nokia-state/state/router/ldp/statistics/adjacency/ipv6
                    container ipv6 {
                        description "Enter the ipv6 context";

                        ///nokia-state/state/router/ldp/statistics/adjacency/ipv6/link
                        leaf link {
                            type uint32;
                            description
                                "The value indicates the number of active link adjacencies (i.e
                                 established adjacencies) associated with the LDP instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/adjacency/ipv6/targeted
                        leaf targeted {
                            type uint32;
                            description
                                "The value indicates the number of active targeted adjacencies
                                 (i.e established adjacencies) associated with the LDP instance.";
                        }

                    } // container ipv6

                } // container adjacency

                ///nokia-state/state/router/ldp/statistics/session
                container session {
                    description "Enter the session context";

                    ///nokia-state/state/router/ldp/statistics/session/ipv4
                    container ipv4 {
                        description "Enter the ipv4 context";

                        ///nokia-state/state/router/ldp/statistics/session/ipv4/active
                        leaf active {
                            type uint32;
                            description
                                "The value indicates the number of active sessions (i.e. session
                                 in some form of creation) associated with the LDP instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/session/ipv4/attempted
                        leaf attempted {
                            type uint32;
                            description
                                "The value indicates the number of attempted sessions for LDP
                                 instance.";
                        }

                    } // container ipv4

                    ///nokia-state/state/router/ldp/statistics/session/ipv6
                    container ipv6 {
                        description "Enter the ipv6 context";

                        ///nokia-state/state/router/ldp/statistics/session/ipv6/active
                        leaf active {
                            type uint32;
                            description
                                "The value indicates the number of active sessions (i.e. session
                                 in some form of creation) associated with the LDP instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/session/ipv6/attempted
                        leaf attempted {
                            type uint32;
                            description
                                "The value indicates the number of attempted sessions for LDP
                                 instance.";
                        }

                    } // container ipv6

                } // container session

                ///nokia-state/state/router/ldp/statistics/protocol
                container protocol {
                    description "Enter the protocol context";

                    ///nokia-state/state/router/ldp/statistics/protocol/bad-ldp-id-errors
                    leaf bad-ldp-id-errors {
                        type uint32;
                        description
                            "The value indicates the number of Bad LDP Identifier
                             Fatal Errors detected for sessions associated with the
                             LDP instance.";
                    }

                    ///nokia-state/state/router/ldp/statistics/protocol/bad-pdu-length-errors
                    leaf bad-pdu-length-errors {
                        type uint32;
                        description
                            "The value indicates the number of Bad PDU Length Fatal
                             Errors detected for sessions associated with the LDP
                             instance.";
                    }

                    ///nokia-state/state/router/ldp/statistics/protocol/bad-msg-length-errors
                    leaf bad-msg-length-errors {
                        type uint32;
                        description
                            "The value indicates the number of Bad Message Length
                             Fatal Errors detected for sessions associated with the
                             LDP instance.";
                    }

                    ///nokia-state/state/router/ldp/statistics/protocol/bad-tlv-length-errors
                    leaf bad-tlv-length-errors {
                        type uint32;
                        description
                            "The value indicates the number of Bad TLV Length
                             Fatal Errors detected for sessions associated with the
                             LDP instance.";
                    }

                    ///nokia-state/state/router/ldp/statistics/protocol/malformed-tlv-errors
                    leaf malformed-tlv-errors {
                        type uint32;
                        description
                            "The value indicates the number of Malformed TLV Value
                             Fatal Errors detected for sessions associated with the
                             LDP instance.";
                    }

                    ///nokia-state/state/router/ldp/statistics/protocol/keepalive-expired-errors
                    leaf keepalive-expired-errors {
                        type uint32;
                        description
                            "The value indicates the number of Session Keep Alive
                             Timer Expired Errors detected for sessions associated
                             with the LDP instance.";
                    }

                    ///nokia-state/state/router/ldp/statistics/protocol/unknown-tlv-errors
                    leaf unknown-tlv-errors {
                        type uint32;
                        description
                            "The value indicates the number of Unknown TLV Fatal
                             Errors detected for sessions associated with the LDP
                             instance.";
                    }

                    ///nokia-state/state/router/ldp/statistics/protocol/bad-protocol-version-errors
                    leaf bad-protocol-version-errors {
                        type uint32;
                        description
                            "The value indicates the number of bad protocol version
                             errors detected for sessions associated with the LDP
                             instance.";
                    }

                    ///nokia-state/state/router/ldp/statistics/protocol/rejected-sessions
                    container rejected-sessions {
                        description "Enter the rejected-sessions context";

                        ///nokia-state/state/router/ldp/statistics/protocol/rejected-sessions/hello-errors
                        leaf hello-errors {
                            type uint32;
                            description
                                "The value indicates the total number of session
                                 rejected/No Hello Error Notification Messages sent
                                 or received by the LDP instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/protocol/rejected-sessions/parameters-advertisement-errors
                        leaf parameters-advertisement-errors {
                            type uint32;
                            description
                                "The value indicates total number of Session
                                 rejected/Parameters Advertisement Mode Error
                                 Notification Messages sent or received by the LDP
                                 instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/protocol/rejected-sessions/max-pdu-errors
                        leaf max-pdu-errors {
                            type uint32;
                            description
                                "The value indicates total number of Session
                                 Rejected/Parameters Advertisement Mode Error
                                 Notifications Messages sent or received by the LDP
                                 instance.";
                        }

                        ///nokia-state/state/router/ldp/statistics/protocol/rejected-sessions/label-range-errors
                        leaf label-range-errors {
                            type uint32;
                            description
                                "The value indicates total number of Session
                                 Rejected/Parameters Label Range  Error
                                 Notifications Messages sent or received by the LDP
                                 instance.";
                        }

                    } // container rejected-sessions

                    ///nokia-state/state/router/ldp/statistics/protocol/shutdown-notify
                    container shutdown-notify {
                        description "Enter the shutdown-notify context";

                        ///nokia-state/state/router/ldp/statistics/protocol/shutdown-notify/sent
                        leaf sent {
                            type uint32;
                            description "The value indicates LDP shutdown notify sent count.";
                        }

                        ///nokia-state/state/router/ldp/statistics/protocol/shutdown-notify/received
                        leaf received {
                            type uint32;
                            description
                                "The value indicates LDP shutdown notify receive
                                 count.";
                        }

                    } // container shutdown-notify

                } // container protocol

                ///nokia-state/state/router/ldp/statistics/fec-egress-prefixes
                container fec-egress-prefixes {
                    description "Enter the fec-egress-prefixes context";

                    ///nokia-state/state/router/ldp/statistics/fec-egress-prefixes/ipv4
                    leaf ipv4 {
                        type uint32;
                        description
                            "The value indicates the number of IPv4 egress FEC
                             prefixes configured for statistics collection on the LDP instance.";
                    }

                    ///nokia-state/state/router/ldp/statistics/fec-egress-prefixes/ipv6
                    leaf ipv6 {
                        type uint32;
                        description
                            "The value indicates the number of IPv6 egress FEC
                             prefixes configured for statistics collection on the LDP instance.";
                    }

                } // container fec-egress-prefixes

                ///nokia-state/state/router/ldp/statistics/oper-down-events
                container oper-down-events {
                    description "Enter the oper-down-events context";

                    ///nokia-state/state/router/ldp/statistics/oper-down-events/ipv4
                    leaf ipv4 {
                        type uint32;
                        description
                            "The value indicates the number of times the IPv4 LDP
                             instance has gone operationally down since the instance
                             was created.";
                    }

                    ///nokia-state/state/router/ldp/statistics/oper-down-events/ipv6
                    leaf ipv6 {
                        type uint32;
                        description
                            "The value indicates the number of times the IPv6 LDP
                             instance has gone operationally down since the instance
                             was created.";
                    }

                } // container oper-down-events

                ///nokia-state/state/router/ldp/statistics/fec-type
                container fec-type {
                    description "Enter the fec-type context";

                    ///nokia-state/state/router/ldp/statistics/fec-type/prefixes
                    container prefixes {
                        description "Enter the prefixes context";

                        ///nokia-state/state/router/ldp/statistics/fec-type/prefixes/ipv4
                        container ipv4 {
                            description "Enter the ipv4 context";

                            ///nokia-state/state/router/ldp/statistics/fec-type/prefixes/ipv4/sent
                            leaf sent {
                                type uint32;
                                description
                                    "The value indicates the number of FECs sent by the LDP instance
                                     to its neighbors.";
                            }

                            ///nokia-state/state/router/ldp/statistics/fec-type/prefixes/ipv4/received
                            leaf received {
                                type uint32;
                                description
                                    "The value indicates the number of FECs received by the LDP
                                     instance from its neighbors.";
                            }

                            ///nokia-state/state/router/ldp/statistics/fec-type/prefixes/ipv4/session-overload-sent
                            leaf session-overload-sent {
                                type uint32;
                                description
                                    "The value indicates the number of Overload Notifications sent
                                     over sessions in the LDP instance.";
                            }

                            ///nokia-state/state/router/ldp/statistics/fec-type/prefixes/ipv4/session-overload-received
                            leaf session-overload-received {
                                type uint32;
                                description
                                    "The value indicates the number of Overload Notifications
                                     received over sessions in the LDP instance.";
                            }

                            ///nokia-state/state/router/ldp/statistics/fec-type/prefixes/ipv4/current-fecs-overloaded
                            leaf current-fecs-overloaded {
                                type uint32;
                                description
                                    "The value indicates the number of FEC in overload state in the
                                     LDP instance.";
                            }

                        } // container ipv4

                        ///nokia-state/state/router/ldp/statistics/fec-type/prefixes/ipv6
                        container ipv6 {
                            description "Enter the ipv6 context";

                            ///nokia-state/state/router/ldp/statistics/fec-type/prefixes/ipv6/sent
                            leaf sent {
                                type uint32;
                                description
                                    "The value indicates the number of FECs sent by the LDP instance
                                     to its neighbors.";
                            }

                            ///nokia-state/state/router/ldp/statistics/fec-type/prefixes/ipv6/received
                            leaf received {
                                type uint32;
                                description
                                    "The value indicates the number of FECs received by the LDP
