                    ///nokia-conf/configure/service/ies/redundant-interface/spoke-sdp/egress
                    container egress {
                        description "Enter the egress context";

                        ///nokia-conf/configure/service/ies/redundant-interface/spoke-sdp/egress/vc-label
                        leaf vc-label {
                            type types-services:egress-vc-label;
