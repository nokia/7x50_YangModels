                                        case ascii-tuple {

                                            ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/ascii-tuple/ascii-tuple
                                            leaf ascii-tuple {
                                                type empty;
                                                description "Use ASCII-encoded concatenated tuple";
                                            }

                                        }
                                        ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/sap-id
                                        case sap-id {

                                            ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/sap-id/sap-id
                                            leaf sap-id {
                                                type empty;
                                                description "Use SAP ID in interface ID option in relay packet";
                                            }

                                        }
                                        ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/if-index
                                        case if-index {

                                            ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/if-index/if-index
                                            leaf if-index {
                                                type empty;
                                                description "Use interface index in the DHCPv6 relay packet";
                                            }

                                        }
                                        ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/string
                                        case string {

                                            ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/relay/option/interface-id/interface-id/string/string
                                            leaf string {
                                                type types-sros:string-not-all-spaces {
                                                    length "1..80";
                                                }
                                                description "String for interface ID option in DHCPv6 relay packet";
                                            }

                                        }
                                    }
                                } // container interface-id

                            } // container option

                        } // container relay

                        ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server
                        container server {
                            description "Enter the server context";

                            ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server/max-nbr-of-leases
                            leaf max-nbr-of-leases {
                                type int32 {
                                    range "0..8000";
                                }
                                default "8000";
                                description "DHCPv6 leases allowed";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server/prefix-delegation
                            container prefix-delegation {
                                description "Enter the prefix-delegation context";

                                ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server/prefix-delegation/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    default "disable";
                                    description "Administrative state of the prefix delegation";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server/prefix-delegation/prefix
                                list prefix {
                                    key "ipv6-prefix";
                                    description "Enter the prefix list instance";

                                    ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server/prefix-delegation/prefix/ipv6-prefix
                                    leaf ipv6-prefix {
                                        type types-sros:ipv6-prefix;
                                        description "IPv6 address and prefix";
                                    }

                                    ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server/prefix-delegation/prefix/preferred-lifetime
                                    leaf preferred-lifetime {
                                        type types-services:ipv6-prefix-lifetime;
                                        units "seconds";
                                        default "604800";
                                        description "Preferred lifetime of the prefix";
                                    }

                                    ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server/prefix-delegation/prefix/valid-lifetime
                                    leaf valid-lifetime {
                                        type types-services:ipv6-prefix-lifetime;
                                        units "seconds";
                                        default "2592000";
                                        description "Valid lifetime of the prefix";
                                    }

                                    ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server/prefix-delegation/prefix/apply-groups
                                    leaf-list apply-groups {
                                        type leafref {
                                            path "../../../../../../../../../groups/group/name";
                                        }
                                        max-elements 8;
                                        ordered-by user;
                                        description "Apply a configuration group at this level";
                                    }

                                    ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server/prefix-delegation/prefix/apply-groups-exclude
                                    leaf-list apply-groups-exclude {
                                        type leafref {
                                            path "../../../../../../../../../groups/group/name";
                                        }
                                        max-elements 8;
                                        ordered-by user;
                                        description "Exclude a configuration group at this level";
                                    }

                                    ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server/prefix-delegation/prefix/client-id
                                    container client-id {
                                        description "Enter the client-id context";

                                        ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server/prefix-delegation/prefix/client-id/duid
                                        leaf duid {
                                            type string {
                                                length "1..130";
                                            }
                                            description "Requesting router ID";
                                        }

                                        ///nokia-conf/configure/service/ies/interface/ipv6/dhcp6/server/prefix-delegation/prefix/client-id/iaid
                                        leaf iaid {
                                            type uint32 {
                                                range "1..4294967295";
                                            }
                                            description "IAID from the requesting router to match";
                                        }

                                    } // container client-id

                                } // list prefix

                            } // container prefix-delegation

                        } // container server

                    } // container dhcp6

                    ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery
                    container neighbor-discovery {
                        when "../../tunnel = 'false'" {
                            description "not allowed in a tunnel interface";
                        }
                        description "Enter the neighbor-discovery context";

                        ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/reachable-time
                        leaf reachable-time {
                            type types-router:neighbor-discovery-reachable-time;
                            description "Neighbor reachability detection timer";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/stale-time
                        leaf stale-time {
                            type types-router:neighbor-discovery-stale-time;
                            description "Time a Neighbor Discovery cache entry remains stale";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/learn-unsolicited
                        leaf learn-unsolicited {
                            type types-router:ipv6-address-type-selection;
                            description "Type of entries learned from unsolicited NA messages";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/proactive-refresh
                        leaf proactive-refresh {
                            type types-router:ipv6-address-type-selection;
                            description "Proactive refresh of neighbor entries";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/populate-host
                        leaf populate-host {
                            status obsolete;
                            type boolean;
                            default "false";
                            description "Allow addition or deletion of host routes in route table derived from ARP entries in ARP cache";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/route-tag
                        leaf route-tag {
                            status obsolete;
                            type types-router:route-tag;
                            description "ARP route tag";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/local-proxy-nd
                        leaf local-proxy-nd {
                            type boolean;
                            default "false";
                            description "Enable local proxy neighbor discovery on the interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/proxy-nd-policy
                        leaf-list proxy-nd-policy {
                            type leafref {
                                path "../../../../../../policy-options/policy-statement/name";
                            }
                            max-elements 5;
                            ordered-by user;
                            description "Proxy Neighbor Discovery policy name for the interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/host-route
                        container host-route {
                            description "Enter the host-route context";

                            ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/host-route/populate
                            list populate {
                                key "route-type";
                                description "Enter the populate list instance";

                                ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/host-route/populate/route-type
                                leaf route-type {
                                    type enumeration {
                                        enum static                         { value 1; }
                                        enum dynamic                        { value 2; }
                                        enum evpn                           { value 3; }
                                    }
                                    description "Type of ARP or ND entries that generate host routes";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/host-route/populate/route-tag
                                leaf route-tag {
                                    type uint32 {
                                        range "1..255";
                                    }
                                    sros-ext:immutable;
                                    description "Tag value used with the host route from an ARP/ND entry";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/host-route/populate/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/host-route/populate/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                            } // list populate

                        } // container host-route

                        ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/secure-nd
                        container secure-nd {
                            description "Enter the secure-nd context";

                            ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/secure-nd/allow-unsecured-msgs
                            leaf allow-unsecured-msgs {
                                type boolean;
                                default "true";
                                description "Accept unsecured messages";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/secure-nd/public-key-min-bits
                            leaf public-key-min-bits {
                                type uint32 {
                                    range "512..1024";
                                }
                                default "1024";
                                description "Minimum acceptable key length for public keys in CGA";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/secure-nd/security-parameter
                            leaf security-parameter {
                                type uint32 {
                                    range "0..1";
                                }
                                default "1";
                                description "Security parameter used in the generation of a CGA";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/secure-nd/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "disable";
                                description "Administrative state of Secure Neighbor Discovery";
                            }

                        } // container secure-nd

                        ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/limit
                        container limit {
                            description "Enter the limit context";

                            ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/limit/max-entries
                            leaf max-entries {
                                type types-router:ipv6-neighbor-limit;
                                description "Maximum number of entries learned on an IP interface";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/limit/log-only
                            leaf log-only {
                                type boolean;
                                default "false";
                                description "Generate log entries when limit is reached";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/limit/threshold
                            leaf threshold {
                                type uint32 {
                                    range "1..100";
                                }
                                units "percent";
                                default "90";
                                description "Threshold percentage that triggers a warning message";
                            }

                        } // container limit

                        ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/static-neighbor
                        list static-neighbor {
                            key "ipv6-address";
                            description "Enter the static-neighbor list instance";

                            ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/static-neighbor/ipv6-address
                            leaf ipv6-address {
                                type types-sros:ipv6-address;
                                description "IPv6 address corresponding to the physical address";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/static-neighbor/mac-address
                            leaf mac-address {
                                type yang:mac-address;
                                mandatory true;
                                description "MAC address for the static neighbor";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/static-neighbor/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/neighbor-discovery/static-neighbor/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list static-neighbor

                    } // container neighbor-discovery

                    ///nokia-conf/configure/service/ies/interface/ipv6/vrrp
                    list vrrp {
                        key "virtual-router-id";
                        max-elements 4;
                        description "Enter the vrrp list instance";

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/virtual-router-id
                        leaf virtual-router-id {
                            type int32 {
                                range "1..255";
                            }
                            description "Virtual Router Identifier (VRID) for the IP interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/backup
                        leaf-list backup {
                            type types-sros:ipv6-address;
                            max-elements 4;
                            description "Virtual router IP addresses for the interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/message-interval
                        leaf message-interval {
                            type uint32 {
                                range "10..4095";
                            }
                            units "centiseconds";
                            default "100";
                            description "Interval for sending VRRP advertisement messages";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/owner
                        leaf owner {
                            type boolean;
                            sros-ext:immutable;
                            default "false";
                            description "Designate the virtual router instance as owner";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/passive
                        leaf passive {
                            type boolean;
                            sros-ext:immutable;
                            default "false";
                            description "Suppress the processing of VRRP advertisement messages";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/init-delay
                        leaf init-delay {
                            type uint32 {
                                range "1..65535";
                            }
                            units "seconds";
                            description "VRRP initialization delay timer";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/mac
                        leaf mac {
                            type types-sros:mac-unicast-address;
                            description "Virtual MAC address to use in ARP responses";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/priority
                        leaf priority {
                            type uint32 {
                                range "1..255";
                            }
                            description "Base priority for the VRRP";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "enable";
                            description "Administrative state of VRRP";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/master-int-inherit
                        leaf master-int-inherit {
                            type boolean;
                            description "Allow master instance to dictate the master down timer";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/ping-reply
                        leaf ping-reply {
                            type boolean;
                            default "false";
                            description "Allow non-owner master to reply to ICMP echo requests";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/policy
                        leaf policy {
                            type leafref {
                                path "../../../../../../vrrp/policy/policy-id";
                            }
                            description "VRRP priority control policy";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/preempt
                        leaf preempt {
                            type boolean;
                            default "true";
                            description "Allow the VRRP to override an existing non-owner master";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/telnet-reply
                        leaf telnet-reply {
                            type boolean;
                            default "false";
                            description "Allow non-owner master to reply to Telnet requests";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/traceroute-reply
                        leaf traceroute-reply {
                            type boolean;
                            default "false";
                            description "Allow non-owner master to reply to traceroute requests";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/standby-forwarding
                        leaf standby-forwarding {
                            type boolean;
                            default "false";
                            description "Allow standby router to forward traffic";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/ntp-reply
                        leaf ntp-reply {
                            type boolean;
                            default "false";
                            description "Allow processing of NTP requests";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/oper-group
                        leaf oper-group {
                            type leafref {
                                path "../../../../../oper-group/name";
                            }
                            description "Operational group name associated with the VRRP";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/monitor-oper-group
                        leaf monitor-oper-group {
                            type leafref {
                                path "../../../../../oper-group/name";
                            }
                            sros-ext:immutable;
                            description "VRRP instance to follow a specified operational group";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/bfd-liveness
                        container bfd-liveness {
                            presence "Enable the bfd-liveness context";
                            description "Enable the bfd-liveness context";

                            ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/bfd-liveness/dest-ip
                            leaf dest-ip {
                                type types-sros:ip-address;
                                sros-ext:immutable;
                                mandatory true;
                                description "Destination address for the BFD session";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/bfd-liveness/service-name
                            leaf service-name {
                                type types-services:service-name;
                                sros-ext:immutable;
                                description "Administrative service name";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/bfd-liveness/interface-name
                            leaf interface-name {
                                type types-sros:interface-name;
                                sros-ext:immutable;
                                mandatory true;
                                description "Name of the interface running BFD";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/bfd-liveness/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv6/vrrp/bfd-liveness/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // container bfd-liveness

                    } // list vrrp

                } // container ipv6

                ///nokia-conf/configure/service/ies/interface/if-attribute
                container if-attribute {
                    description "Enter the if-attribute context";

                    ///nokia-conf/configure/service/ies/interface/if-attribute/admin-group
                    leaf-list admin-group {
                        type leafref {
                            path "../../../../../routing-options/if-attribute/admin-group/group-name";
                        }
                        max-elements 32;
                        description "Administrative group name for the interface";
                    }

                    ///nokia-conf/configure/service/ies/interface/if-attribute/srlg-group
                    list srlg-group {
                        key "name";
                        description "Add a list entry for srlg-group";

                        ///nokia-conf/configure/service/ies/interface/if-attribute/srlg-group/name
                        leaf name {
                            type leafref {
                                path "../../../../../../routing-options/if-attribute/srlg-group/name";
                            }
                            description "SRLG name";
                        }

                    } // list srlg-group

                } // container if-attribute

            } // list interface

            ///nokia-conf/configure/service/ies/redundant-interface
            list redundant-interface {
                key "interface-name";
                description "Enter the redundant-interface list instance";

                ///nokia-conf/configure/service/ies/redundant-interface/interface-name
                leaf interface-name {
                    type types-sros:interface-name;
                    description "Interface name";
                }

                ///nokia-conf/configure/service/ies/redundant-interface/description
                leaf description {
                    type types-sros:very-long-description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/ies/redundant-interface/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "enable";
                    description "Administrative state of the interface";
                }

                ///nokia-conf/configure/service/ies/redundant-interface/ip-mtu
                leaf ip-mtu {
                    type types-router:ip-mtu-ext;
                    units "bytes";
                    description "IP MTU applied to outgoing packets";
                }

                ///nokia-conf/configure/service/ies/redundant-interface/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/ies/redundant-interface/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/ies/redundant-interface/hold-time
                container hold-time {
                    description "Enter the hold-time context";

                    ///nokia-conf/configure/service/ies/redundant-interface/hold-time/ipv4
                    container ipv4 {
                        description "Enter the ipv4 context";

                        ///nokia-conf/configure/service/ies/redundant-interface/hold-time/ipv4/up
                        container up {
                            description "Enter the up context";

                            ///nokia-conf/configure/service/ies/redundant-interface/hold-time/ipv4/up/seconds
                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Up hold time for the IP interface";
                            }

                        } // container up

                        ///nokia-conf/configure/service/ies/redundant-interface/hold-time/ipv4/down
                        container down {
                            description "Enter the down context";

                            ///nokia-conf/configure/service/ies/redundant-interface/hold-time/ipv4/down/seconds
                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Down hold time for the IP interface";
                            }

                            ///nokia-conf/configure/service/ies/redundant-interface/hold-time/ipv4/down/init-only
                            leaf init-only {
                                type boolean;
                                default "false";
                                description "Apply delay only at interface configuration or reboot";
                            }

                        } // container down

                    } // container ipv4

