                                type types-submgt:int-dest-id;
                                description "Intermediate destination ID";
                            }

                            ///nokia-conf/configure/service/ies/interface/sap/static-host/ipv4/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/ies/interface/sap/static-host/ipv4/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/ies/interface/sap/static-host/ipv4/app-profile
                            container app-profile {
                                description "Enter the app-profile context";

                                ///nokia-conf/configure/service/ies/interface/sap/static-host/ipv4/app-profile/profile
                                leaf profile {
                                    type leafref {
                                        path "../../../../../../../../application-assurance/group/partition/policy/app-profile/app-profile-name";
                                    }
                                    description "Application profile used by the static host";
                                }

                            } // container app-profile

                            ///nokia-conf/configure/service/ies/interface/sap/static-host/ipv4/subscriber-id
                            container subscriber-id {
                                description "Enter the subscriber-id context";

                                ///nokia-conf/configure/service/ies/interface/sap/static-host/ipv4/subscriber-id/subscriber-id
                                choice subscriber-id {
                                    ///nokia-conf/configure/service/ies/interface/sap/static-host/ipv4/subscriber-id/subscriber-id/string
                                    case string {

                                        ///nokia-conf/configure/service/ies/interface/sap/static-host/ipv4/subscriber-id/subscriber-id/string/string
                                        leaf string {
                                            type types-submgt:subscriber-id;
                                            description "Subscriber identification";
                                        }

                                    }
                                    ///nokia-conf/configure/service/ies/interface/sap/static-host/ipv4/subscriber-id/subscriber-id/use-sap-id
                                    case use-sap-id {

                                        ///nokia-conf/configure/service/ies/interface/sap/static-host/ipv4/subscriber-id/subscriber-id/use-sap-id/use-sap-id
                                        leaf use-sap-id {
                                            type empty;
                                            description "Use the SAP id as subscriber ID";
                                        }

                                    }
                                }
                            } // container subscriber-id

                        } // list ipv4

                    } // container static-host

                } // list sap

                ///nokia-conf/configure/service/ies/interface/vpls
                list vpls {
                    when "../tunnel = 'false'" {
                        description "vpls not allowed in a tunnel interface";
                    }
                    key "vpls-name";
                    max-elements 1;
                    description "Enter the vpls list instance";

                    ///nokia-conf/configure/service/ies/interface/vpls/vpls-name
                    leaf vpls-name {
                        type types-sros:named-item-64;
                        description "VPLS service";
                    }

                    ///nokia-conf/configure/service/ies/interface/vpls/ip-stats
                    leaf ip-stats {
                        type boolean;
                        default "false";
                        description "Enable statistics collection";
                    }

                    ///nokia-conf/configure/service/ies/interface/vpls/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/ies/interface/vpls/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/ies/interface/vpls/evpn
                    container evpn {
                        description "Enter the evpn context";

                        ///nokia-conf/configure/service/ies/interface/vpls/evpn/arp
                        container arp {
                            description "Enter the arp context";

                            ///nokia-conf/configure/service/ies/interface/vpls/evpn/arp/learn-dynamic
                            leaf learn-dynamic {
                                type boolean;
                                default "true";
                                description "Process ARP or ND messages on EVPN tunnels";
                            }

                            ///nokia-conf/configure/service/ies/interface/vpls/evpn/arp/flood-garp-and-unknown-req
                            leaf flood-garp-and-unknown-req {
                                type boolean;
                                default "true";
                                description "Allow CPM originated ARP frames to flood R-VPLS service";
                            }

                            ///nokia-conf/configure/service/ies/interface/vpls/evpn/arp/advertise
                            list advertise {
                                key "route-type";
                                description "Enter the advertise list instance";

                                ///nokia-conf/configure/service/ies/interface/vpls/evpn/arp/advertise/route-type
                                leaf route-type {
                                    type enumeration {
                                        enum static                         { value 1; }
                                        enum dynamic                        { value 2; }
                                    }
                                    description "Type of ARP or ND entries that generate host routes";
                                }

                                ///nokia-conf/configure/service/ies/interface/vpls/evpn/arp/advertise/route-tag
                                leaf route-tag {
                                    type uint32 {
                                        range "0..255";
                                    }
                                    sros-ext:immutable;
                                    description "Tag value used with the host route from an ARP/ND entry";
                                }

                                ///nokia-conf/configure/service/ies/interface/vpls/evpn/arp/advertise/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/service/ies/interface/vpls/evpn/arp/advertise/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                            } // list advertise

                        } // container arp

                        ///nokia-conf/configure/service/ies/interface/vpls/evpn/nd
                        container nd {
                            description "Enter the nd context";

                            ///nokia-conf/configure/service/ies/interface/vpls/evpn/nd/learn-dynamic
                            leaf learn-dynamic {
                                type boolean;
                                default "true";
                                description "Process ARP or ND messages on EVPN tunnels";
                            }

                            ///nokia-conf/configure/service/ies/interface/vpls/evpn/nd/advertise
                            list advertise {
                                key "route-type";
                                description "Enter the advertise list instance";

                                ///nokia-conf/configure/service/ies/interface/vpls/evpn/nd/advertise/route-type
                                leaf route-type {
                                    type enumeration {
                                        enum static                         { value 1; }
                                        enum dynamic                        { value 2; }
                                    }
                                    description "Type of ARP or ND entries that generate host routes";
                                }

                                ///nokia-conf/configure/service/ies/interface/vpls/evpn/nd/advertise/route-tag
                                leaf route-tag {
                                    type uint32 {
                                        range "0..255";
                                    }
                                    sros-ext:immutable;
                                    description "Tag value used with the host route from an ARP/ND entry";
                                }

                                ///nokia-conf/configure/service/ies/interface/vpls/evpn/nd/advertise/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/service/ies/interface/vpls/evpn/nd/advertise/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                            } // list advertise

                        } // container nd

                    } // container evpn

                    ///nokia-conf/configure/service/ies/interface/vpls/egress
                    container egress {
                        description "Enter the egress context";

                        ///nokia-conf/configure/service/ies/interface/vpls/egress/reclassify-using-qos
                        leaf reclassify-using-qos {
                            type leafref {
                                path "../../../../../../qos/sap-egress/sap-egress-policy-name";
                            }
                            description "Egress QoS policy";
                        }

                        ///nokia-conf/configure/service/ies/interface/vpls/egress/routed-override-filter
                        container routed-override-filter {
                            description "Enter the routed-override-filter context";

                            ///nokia-conf/configure/service/ies/interface/vpls/egress/routed-override-filter/ip
                            leaf ip {
                                type leafref {
                                    path "../../../../../../../filter/ip-filter/filter-name";
                                }
                                description "IPv4 filter policy name";
                            }

                            ///nokia-conf/configure/service/ies/interface/vpls/egress/routed-override-filter/ipv6
                            leaf ipv6 {
                                type leafref {
                                    path "../../../../../../../filter/ipv6-filter/filter-name";
                                }
                                description "IPv6 filter policy name";
                            }

                        } // container routed-override-filter

                    } // container egress

                    ///nokia-conf/configure/service/ies/interface/vpls/ingress
                    container ingress {
                        description "Enter the ingress context";

                        ///nokia-conf/configure/service/ies/interface/vpls/ingress/routed-override-filter
                        container routed-override-filter {
                            description "Enter the routed-override-filter context";

                            ///nokia-conf/configure/service/ies/interface/vpls/ingress/routed-override-filter/ip
                            leaf ip {
                                type leafref {
                                    path "../../../../../../../filter/ip-filter/filter-name";
                                }
                                description "IPv4 filter policy name";
                            }

                            ///nokia-conf/configure/service/ies/interface/vpls/ingress/routed-override-filter/ipv6
                            leaf ipv6 {
                                type leafref {
                                    path "../../../../../../../filter/ipv6-filter/filter-name";
                                }
                                description "IPv6 filter policy name";
                            }

                        } // container routed-override-filter

                    } // container ingress

                } // list vpls

                ///nokia-conf/configure/service/ies/interface/hold-time
                container hold-time {
                    when "../tunnel = 'false' or not(../loopback = 'false')" {
                        description "not allowed in a tunnel interface unless it is a loopback interface";
                    }
                    description "Enter the hold-time context";

                    ///nokia-conf/configure/service/ies/interface/hold-time/ipv4
                    container ipv4 {
                        description "Enter the ipv4 context";

                        ///nokia-conf/configure/service/ies/interface/hold-time/ipv4/up
                        container up {
                            description "Enter the up context";

                            ///nokia-conf/configure/service/ies/interface/hold-time/ipv4/up/seconds
                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Up hold time for the IP interface";
                            }

                        } // container up

                        ///nokia-conf/configure/service/ies/interface/hold-time/ipv4/down
                        container down {
                            description "Enter the down context";

                            ///nokia-conf/configure/service/ies/interface/hold-time/ipv4/down/seconds
                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Down hold time for the IP interface";
                            }

                            ///nokia-conf/configure/service/ies/interface/hold-time/ipv4/down/init-only
                            leaf init-only {
                                type boolean;
                                default "false";
                                description "Apply delay only at interface configuration or reboot";
                            }

                        } // container down

                    } // container ipv4

                    ///nokia-conf/configure/service/ies/interface/hold-time/ipv6
                    container ipv6 {
                        description "Enter the ipv6 context";

                        ///nokia-conf/configure/service/ies/interface/hold-time/ipv6/up
                        container up {
                            description "Enter the up context";

                            ///nokia-conf/configure/service/ies/interface/hold-time/ipv6/up/seconds
                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Up hold time for the IP interface";
                            }

                        } // container up

                        ///nokia-conf/configure/service/ies/interface/hold-time/ipv6/down
                        container down {
                            description "Enter the down context";

                            ///nokia-conf/configure/service/ies/interface/hold-time/ipv6/down/seconds
                            leaf seconds {
                                type types-router:hold-time-seconds;
                                units "seconds";
                                description "Down hold time for the IP interface";
                            }

                            ///nokia-conf/configure/service/ies/interface/hold-time/ipv6/down/init-only
                            leaf init-only {
                                type boolean;
                                default "false";
                                description "Apply delay only at interface configuration or reboot";
                            }

                        } // container down

                    } // container ipv6

                } // container hold-time

                ///nokia-conf/configure/service/ies/interface/ipv4
                container ipv4 {
                    description "Enter the ipv4 context";

                    ///nokia-conf/configure/service/ies/interface/ipv4/tcp-mss
                    leaf tcp-mss {
                        type types-router:ipv4-tcp-mss-ext;
                        description "TCP maximum segment size for the interface";
                    }

                    ///nokia-conf/configure/service/ies/interface/ipv4/allow-directed-broadcasts
                    leaf allow-directed-broadcasts {
                        type boolean;
                        default "false";
                        description "Forward directed broadcasts";
                    }

                    ///nokia-conf/configure/service/ies/interface/ipv4/local-dhcp-server
                    leaf local-dhcp-server {
                        when "../../tunnel = 'false'" {
                            description "not allowed in a tunnel interface";
                        }
                        type leafref {
                            path "../../../../../router/dhcp-server/dhcpv4/name";
                        }
                        description "DHCP server for the interface";
                    }

                    ///nokia-conf/configure/service/ies/interface/ipv4/qos-route-lookup
                    leaf qos-route-lookup {
                        type types-router:qos-route-lookup;
                        description "QoS Route lookup";
                    }

                    ///nokia-conf/configure/service/ies/interface/ipv4/ip-helper-address
                    leaf ip-helper-address {
                        type types-sros:ipv4-unicast-address;
                        description "Gateway address";
                    }

                    ///nokia-conf/configure/service/ies/interface/ipv4/icmp
                    container icmp {
                        description "Enter the icmp context";

                        ///nokia-conf/configure/service/ies/interface/ipv4/icmp/mask-reply
                        leaf mask-reply {
                            type boolean;
                            default "true";
                            description "Allow responses to ICMP mask requests on the interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/icmp/redirects
                        container redirects {
                            description "Enter the redirects context";

                            ///nokia-conf/configure/service/ies/interface/ipv4/icmp/redirects/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sending ICMP redirect messages";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/icmp/redirects/number
                            leaf number {
                                type uint16 {
                                    range "10..1000";
                                }
                                default "100";
                                description "Maximum number of ICMP redirect messages to send";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/icmp/redirects/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time used to limit the number of ICMP redirect messages";
                            }

                        } // container redirects

                        ///nokia-conf/configure/service/ies/interface/ipv4/icmp/ttl-expired
                        container ttl-expired {
                            description "Enter the ttl-expired context";

                            ///nokia-conf/configure/service/ies/interface/ipv4/icmp/ttl-expired/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sending TTL expired messages";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/icmp/ttl-expired/number
                            leaf number {
                                type uint16 {
                                    range "10..2000";
                                }
                                default "100";
                                description "Maximum number of TTL expired messages to send";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/icmp/ttl-expired/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time used to limit the number of TTL expired messages";
                            }

                        } // container ttl-expired

                        ///nokia-conf/configure/service/ies/interface/ipv4/icmp/unreachables
                        container unreachables {
                            description "Enter the unreachables context";

                            ///nokia-conf/configure/service/ies/interface/ipv4/icmp/unreachables/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sending unreachable messages";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/icmp/unreachables/number
                            leaf number {
                                type uint16 {
                                    range "10..2000";
                                }
                                default "100";
                                description "Maximum number of unreachable messages to send";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/icmp/unreachables/seconds
                            leaf seconds {
                                type uint16 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time to limit the number of ICMP unreachable messages";
                            }

                        } // container unreachables

                        ///nokia-conf/configure/service/ies/interface/ipv4/icmp/param-problem
                        container param-problem {
                            description "Enter the param-problem context";

                            ///nokia-conf/configure/service/ies/interface/ipv4/icmp/param-problem/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "enable";
                                description "Administrative state of sent Parameter Problem messages";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/icmp/param-problem/number
                            leaf number {
                                type uint32 {
                                    range "10..1000";
                                }
                                default "100";
                                description "Maximum number of Parameter Problem messages to send";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/icmp/param-problem/seconds
                            leaf seconds {
                                type uint32 {
                                    range "1..60";
                                }
                                units "seconds";
                                default "10";
                                description "Time used to limit number of Parameter Problem messages";
                            }

                        } // container param-problem

                    } // container icmp

                    ///nokia-conf/configure/service/ies/interface/ipv4/unnumbered
                    container unnumbered {
                        description "Enter the unnumbered context";

                        ///nokia-conf/configure/service/ies/interface/ipv4/unnumbered/source
                        choice source {
                            ///nokia-conf/configure/service/ies/interface/ipv4/unnumbered/source/ip-address
                            case ip-address {

                                ///nokia-conf/configure/service/ies/interface/ipv4/unnumbered/source/ip-address/ip-address
                                leaf ip-address {
                                    type types-sros:ipv4-unicast-address;
                                    description "IP address of the unnumbered interface";
                                }

                            }
                            ///nokia-conf/configure/service/ies/interface/ipv4/unnumbered/source/ip-int-name
                            case ip-int-name {

                                ///nokia-conf/configure/service/ies/interface/ipv4/unnumbered/source/ip-int-name/ip-int-name
                                leaf ip-int-name {
                                    type types-sros:interface-name;
                                    description "IP interface name";
                                }

                            }
                            ///nokia-conf/configure/service/ies/interface/ipv4/unnumbered/source/ip-int-system
                            case ip-int-system {

                                ///nokia-conf/configure/service/ies/interface/ipv4/unnumbered/source/ip-int-system/system
                                leaf system {
                                    type empty;
                                    description "IP interface as an unnumbered interface";
                                }

                            }
                        }
                    } // container unnumbered

                    ///nokia-conf/configure/service/ies/interface/ipv4/urpf-check
                    container urpf-check {
                        presence "Enable the urpf-check context";
                        description "Enable the urpf-check context";

                        ///nokia-conf/configure/service/ies/interface/ipv4/urpf-check/ignore-default
                        leaf ignore-default {
                            type boolean;
                            default "false";
                            description "Ignore default route when performing a uRPF check";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/urpf-check/mode
                        leaf mode {
                            type types-router:urpf-check-mode;
                            default "strict";
                            description "Unicast RPF check mode";
                        }

                    } // container urpf-check

                    ///nokia-conf/configure/service/ies/interface/ipv4/bfd
                    container bfd {
                        description "Enter the bfd context";

                        ///nokia-conf/configure/service/ies/interface/ipv4/bfd/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of BFD sessions";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/bfd/transmit-interval
                        leaf transmit-interval {
                            type uint32 {
                                range "10..100000";
                            }
                            units "milliseconds";
                            default "100";
                            description "BFD transmit interval over this interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/bfd/receive
                        leaf receive {
                            type uint32 {
                                range "10..100000";
                            }
                            units "milliseconds";
                            default "100";
                            description "BFD receive interval over this interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/bfd/multiplier
                        leaf multiplier {
                            type uint32 {
                                range "1..20";
                            }
                            default "3";
                            description "Number of consecutive BFD messages missed from the peer";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/bfd/echo-receive
                        leaf echo-receive {
                            type uint32 {
                                range "100..100000";
                            }
                            units "milliseconds";
                            description "Minimum echo interval over this interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/bfd/type
                        leaf type {
                            type enumeration {
                                enum cpm-np                         { value 1; }
                                enum auto                           { value 2; }
                                enum fp                             { value 3; }
                            }
                            default "auto";
                            description "Local termination point for the BFD session";
                        }

                    } // container bfd

                    ///nokia-conf/configure/service/ies/interface/ipv4/primary
                    container primary {
                        presence "Enable the primary context";
                        description "Enable the primary context";

                        ///nokia-conf/configure/service/ies/interface/ipv4/primary/address
                        leaf address {
                            type types-sros:ipv4-unicast-address;
                            mandatory true;
                            description "Primary IPv4 address assigned to the interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/primary/prefix-length
                        leaf prefix-length {
                            type uint32 {
                                range "0..32";
                            }
                            mandatory true;
                            description "IPv4 address prefix length";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/primary/broadcast
                        leaf broadcast {
                            type types-services:broadcast-address-format;
                            default "host-ones";
                            description "Broadcast address format";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/primary/track-srrp
                        leaf track-srrp {
                            type uint32 {
                                range "1..4294967295";
                            }
                            description "SRRP instance whose state is tracked on the IP address";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/primary/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/primary/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container primary

                    ///nokia-conf/configure/service/ies/interface/ipv4/secondary
                    list secondary {
                        key "address";
                        description "Enter the secondary list instance";

                        ///nokia-conf/configure/service/ies/interface/ipv4/secondary/address
                        leaf address {
                            type types-sros:ipv4-unicast-address;
                            description "Secondary IPv4 address assigned to the interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/secondary/prefix-length
                        leaf prefix-length {
                            type uint32 {
                                range "0..32";
                            }
                            mandatory true;
                            description "IPv4 address prefix length";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/secondary/igp-inhibit
                        leaf igp-inhibit {
                            type boolean;
                            default "false";
                            description "Disable the running IGP from recognizing secondary IP";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/secondary/broadcast
                        leaf broadcast {
                            type types-services:broadcast-address-format;
                            default "host-ones";
                            description "Broadcast address format";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/secondary/track-srrp
                        leaf track-srrp {
                            type uint32 {
                                range "1..4294967295";
                            }
                            description "SRRP instance whose state is tracked on the IP address";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/secondary/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/secondary/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list secondary

                    ///nokia-conf/configure/service/ies/interface/ipv4/addresses
                    container addresses {
                        description "Enter the addresses context";

                        ///nokia-conf/configure/service/ies/interface/ipv4/addresses/address
                        list address {
                            key "ipv4-address";
                            description "Enter the address list instance";

                            ///nokia-conf/configure/service/ies/interface/ipv4/addresses/address/ipv4-address
                            leaf ipv4-address {
                                type types-sros:ipv4-unicast-address;
                                description "IPv4 address for the interface";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/addresses/address/prefix-length
                            leaf prefix-length {
                                type uint32 {
                                    range "0..32";
                                }
                                mandatory true;
                                description "IPv4 address prefix length";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/addresses/address/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/addresses/address/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list address

                    } // container addresses

                    ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery
                    container neighbor-discovery {
                        when "../../tunnel = 'false'" {
                            description "not allowed in a tunnel interface";
                        }
                        description "Enter the neighbor-discovery context";

                        ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/timeout
                        leaf timeout {
                            type uint32 {
                                range "0..65535";
                            }
                            units "seconds";
                            default "14400";
                            description "Timeout for an ARP entry learned on the interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/retry-timer
                        leaf retry-timer {
                            type uint32 {
                                range "1..300";
                            }
                            units "deciseconds";
                            default "50";
                            description "ARP retry interval";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/learn-unsolicited
                        leaf learn-unsolicited {
                            type boolean;
                            default "false";
                            description "Learn new entries from any received NA message";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/proactive-refresh
                        leaf proactive-refresh {
                            type boolean;
                            default "false";
                            description "Send a single refresh message before entry timeout";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/populate
                        leaf populate {
                            type boolean;
                            default "false";
                            description "Allow static and dynamic hosts to be populated in system ARP cache";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/populate-host
                        leaf populate-host {
                            status obsolete;
                            type boolean;
                            default "false";
                            description "Allow addition or deletion of host routes in route table derived from ARP entries in ARP cache";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/route-tag
                        leaf route-tag {
                            status obsolete;
                            type types-router:route-tag;
                            description "ARP route tag";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/local-proxy-arp
                        leaf local-proxy-arp {
                            type boolean;
                            description "Enable local proxy ARP on interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/remote-proxy-arp
                        leaf remote-proxy-arp {
                            type boolean;
                            default "false";
                            description "Enable remote proxy ARP on the interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/proxy-arp-policy
                        leaf-list proxy-arp-policy {
                            type leafref {
                                path "../../../../../../policy-options/policy-statement/name";
                            }
                            max-elements 5;
                            ordered-by user;
                            description "Proxy ARP policy name";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/host-route
                        container host-route {
                            description "Enter the host-route context";

                            ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/host-route/populate
                            list populate {
                                key "route-type";
                                description "Enter the populate list instance";

                                ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/host-route/populate/route-type
                                leaf route-type {
                                    type enumeration {
                                        enum static                         { value 1; }
                                        enum dynamic                        { value 2; }
                                        enum evpn                           { value 3; }
                                    }
                                    description "Type of ARP or ND entries that generate host routes";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/host-route/populate/route-tag
                                leaf route-tag {
                                    type uint32 {
                                        range "1..255";
                                    }
                                    sros-ext:immutable;
                                    description "Tag value used with the host route from an ARP/ND entry";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/host-route/populate/apply-groups
                                leaf-list apply-groups {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Apply a configuration group at this level";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/host-route/populate/apply-groups-exclude
                                leaf-list apply-groups-exclude {
                                    type leafref {
                                        path "../../../../../../../../groups/group/name";
                                    }
                                    max-elements 8;
                                    ordered-by user;
                                    description "Exclude a configuration group at this level";
                                }

                            } // list populate

                        } // container host-route

                        ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/limit
                        container limit {
                            description "Enter the limit context";

                            ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/limit/max-entries
                            leaf max-entries {
                                type types-router:ipv4-neighbor-limit;
                                description "Maximum number of entries learned on an IP interface";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/limit/log-only
                            leaf log-only {
                                type boolean;
                                default "false";
                                description "Generate log entries only if limit is reached";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/limit/threshold
                            leaf threshold {
                                type uint32 {
                                    range "1..100";
                                }
                                units "percent";
                                default "90";
                                description "Threshold value that triggers a warning message";
                            }

                        } // container limit

                        ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/static-neighbor-unnumbered
                        container static-neighbor-unnumbered {
                            when "../../unnumbered/ip-address or ../../unnumbered/ip-int-name or ../../unnumbered/system" {
                                description "only allowed in unnumbered interfaces";
                            }
                            presence "Enable the static-neighbor-unnumbered context";
                            description "Enable the static-neighbor-unnumbered context";

                            ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/static-neighbor-unnumbered/mac-address
                            leaf mac-address {
                                type yang:mac-address;
                                mandatory true;
                                description "MAC address for the static neighbor";
                            }

                        } // container static-neighbor-unnumbered

                        ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/static-neighbor
                        list static-neighbor {
                            when "not(../../unnumbered/ip-address) and not(../../unnumbered/ip-int-name) and not(../../unnumbered/system)" {
                                description "not allowed in unnumbered interfaces";
                            }
                            key "ipv4-address";
                            description "Enter the static-neighbor list instance";

                            ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/static-neighbor/ipv4-address
                            leaf ipv4-address {
                                type types-sros:ipv4-address;
                                description "IPv4 address that corresponds to the physical address";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/static-neighbor/mac-address
                            leaf mac-address {
                                type yang:mac-address;
                                mandatory true;
                                description "MAC address for the static neighbor";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/static-neighbor/apply-groups
                            leaf-list apply-groups {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Apply a configuration group at this level";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/neighbor-discovery/static-neighbor/apply-groups-exclude
                            leaf-list apply-groups-exclude {
                                type leafref {
                                    path "../../../../../../../groups/group/name";
                                }
                                max-elements 8;
                                ordered-by user;
                                description "Exclude a configuration group at this level";
                            }

                        } // list static-neighbor

                    } // container neighbor-discovery

                    ///nokia-conf/configure/service/ies/interface/ipv4/dhcp
                    container dhcp {
                        description "Enter the dhcp context";

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            description "Administrative state of DHCP";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/python-policy
                        leaf python-policy {
                            when "../../../tunnel = 'false'" {
                                description "not allowed in a tunnel interface";
                            }
                            type leafref {
                                path "../../../../../../python/python-policy/name";
                            }
                            description "Python policy";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/server
                        leaf-list server {
                            type types-sros:ipv4-unicast-address;
                            max-elements 8;
                            ordered-by user;
                            description "IP addresses for DHCP server requests";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/trusted
                        leaf trusted {
                            type boolean;
                            default "false";
                            description "Relay untrusted packets";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/gi-address
                        leaf gi-address {
                            type types-sros:ipv4-unicast-address;
                            description "GI address for the DHCP relay";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/release-include-gi-address
                        leaf release-include-gi-address {
                            type boolean;
                            default "false";
                            description "Include gateway IP address in DHCP Release messages";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/src-ip-addr
                        leaf src-ip-addr {
                            type enumeration {
                                enum auto                           { value 0; }
                                enum gi-address                     { value 1; }
                            }
                            default "auto";
                            description "Type of source address to use for DHCP relay";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/relay-plain-bootp
                        leaf relay-plain-bootp {
                            when "../../../tunnel = 'false'" {
                                description "not allowed in a tunnel interface";
                            }
                            type boolean;
                            default "false";
                            description "Enable relaying of plain BOOTP packets";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/use-arp
                        leaf use-arp {
                            when "../../../tunnel = 'false'" {
                                description "not allowed in a tunnel interface";
                            }
                            type boolean;
                            default "false";
                            description "Use ARP to determine the destination hardware address";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/proxy-server
                        container proxy-server {
                            description "Enter the proxy-server context";

                            ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/proxy-server/admin-state
                            leaf admin-state {
                                type types-sros:admin-state;
                                default "disable";
                                description "Administrative state of the DHCP proxy server";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/proxy-server/emulated-server
                            leaf emulated-server {
                                type types-sros:ipv4-unicast-address;
                                description "IP address used as the DHCP server address for the SAP";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/proxy-server/lease-time
                            container lease-time {
                                description "Enter the lease-time context";

                                ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/proxy-server/lease-time/value
                                leaf value {
                                    type types-sros:time-duration {
                                        range "300..315446399";
                                    }
                                    units "seconds";
                                    description "DHCP lease time";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/proxy-server/lease-time/radius-override
                                leaf radius-override {
                                    type boolean;
                                    default "false";
                                    description "Use lease time information provided by RADIUS server";
                                }

                            } // container lease-time

                        } // container proxy-server

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82
                        container option-82 {
                            description "Enter the option-82 context";

                            ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/action
                            leaf action {
                                type enumeration {
                                    enum replace                        { value 1; }
                                    enum drop                           { value 2; }
                                    enum keep                           { value 3; }
                                }
                                default "keep";
                                description "Action to take with received DHCP Option 82";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/circuit-id
                            container circuit-id {
                                description "Enter the circuit-id context";

                                ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/circuit-id/circuit-id
                                choice circuit-id {
                                    default "ascii-tuple";
                                    ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/none
                                    case none {

                                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/none/none
                                        leaf none {
                                            type empty;
                                            description "Do not include the circuit ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ifindex
                                    case ifindex {

                                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ifindex/ifindex
                                        leaf ifindex {
                                            type empty;
                                            description "Use the interface index for the circuit ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ascii-tuple
                                    case ascii-tuple {

                                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/ascii-tuple/ascii-tuple
                                        leaf ascii-tuple {
                                            type empty;
                                            description "Use the ASCII-encoded tuple for the circuit ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/sap-id
                                    case sap-id {

                                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/sap-id/sap-id
                                        leaf sap-id {
                                            type empty;
                                            description "Use the SAP ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/vlan-ascii-tuple
                                    case vlan-ascii-tuple {

                                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/circuit-id/circuit-id/vlan-ascii-tuple/vlan-ascii-tuple
                                        leaf vlan-ascii-tuple {
                                            type empty;
                                            description "Include the VLAN ID and dot1p bits in the ASCII tuple";
                                        }

                                    }
                                }
                            } // container circuit-id

                            ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/remote-id
                            container remote-id {
                                description "Enter the remote-id context";

                                ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/remote-id/remote-id
                                choice remote-id {
                                    default "none";
                                    ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/remote-id/remote-id/none
                                    case none {

                                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/remote-id/remote-id/none/none
                                        leaf none {
                                            type empty;
                                            description "Do not include the remote ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/remote-id/remote-id/mac
                                    case mac {

                                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/remote-id/remote-id/mac/mac
                                        leaf mac {
                                            type empty;
                                            description "Use the MAC address for the remote ID";
                                        }

                                    }
                                    ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/remote-id/remote-id/ascii-string
                                    case ascii-string {

                                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/remote-id/remote-id/ascii-string/ascii-string
                                        leaf ascii-string {
                                            type types-sros:string-not-all-spaces {
                                                length "1..32";
                                            }
                                            description "User-defined ASCII string for the remote ID";
                                        }

                                    }
                                }
                            } // container remote-id

                            ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/vendor-specific-option
                            container vendor-specific-option {
                                description "Enter the vendor-specific-option context";

                                ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/vendor-specific-option/system-id
                                leaf system-id {
                                    type boolean;
                                    default "false";
                                    description "Send the system ID in the VSO";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/vendor-specific-option/client-mac-address
                                leaf client-mac-address {
                                    type boolean;
                                    default "false";
                                    description "Send the MAC address in the VSO";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/vendor-specific-option/pool-name
                                leaf pool-name {
                                    type boolean;
                                    default "false";
                                    description "Send the pool name in the VSO";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/vendor-specific-option/service-id
                                leaf service-id {
                                    type boolean;
                                    default "false";
                                    description "Send the service ID in the Vendor Specific Option";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/vendor-specific-option/sap-id
                                leaf sap-id {
                                    type boolean;
                                    default "false";
                                    description "Send SAP ID in the sub-option of the DHCP relay packet";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/option-82/vendor-specific-option/string
                                leaf string {
                                    type types-sros:string-not-all-spaces {
                                        length "1..32";
                                    }
                                    description "User-defined ASCII string for the VSO";
                                }

                            } // container vendor-specific-option

                        } // container option-82

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/lease-populate
                        container lease-populate {
                            description "Enter the lease-populate context";

                            ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/lease-populate/max-leases
                            leaf max-leases {
                                type uint32 {
                                    range "0..511999";
                                }
                                description "Maximum number of DHCPv4 leases allowed";
                            }

                        } // container lease-populate

                        ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/relay-proxy
                        container relay-proxy {
                            presence "Enable the relay-proxy context";
                            description "Enable the relay-proxy context";

                            ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/relay-proxy/release-update-src-ip
                            leaf release-update-src-ip {
                                type boolean;
                                default "false";
                                description "Update the source IP address of a DHCP RELEASE message";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipv4/dhcp/relay-proxy/siaddr-override
                            leaf siaddr-override {
                                type types-sros:ipv4-unicast-address;
                                description "DHCP server IP address for address hiding function";
                            }

                        } // container relay-proxy

                    } // container dhcp

                    ///nokia-conf/configure/service/ies/interface/ipv4/vrrp
                    list vrrp {
                        key "virtual-router-id";
                        description "Enter the vrrp list instance";

                        ///nokia-conf/configure/service/ies/interface/ipv4/vrrp/virtual-router-id
                        leaf virtual-router-id {
                            type int32 {
                                range "1..255";
                            }
                            description "Virtual Router Identifier (VRID) for the IP interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/vrrp/authentication-key
                        leaf authentication-key {
                            type types-sros:encrypted-leaf {
                                length "1..38";
                            }
                            description "Password for simple text authentication";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/vrrp/backup
                        leaf-list backup {
                            type types-sros:ipv4-unicast-address;
                            max-elements 16;
                            description "Virtual router IP addresses for the interface";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/vrrp/message-interval
                        leaf message-interval {
                            type uint32 {
                                range "1..2559";
                            }
                            units "deciseconds";
                            default "10";
                            description "Interval for sending VRRP advertisement messages";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/vrrp/owner
                        leaf owner {
                            type boolean;
                            sros-ext:immutable;
                            default "false";
                            description "Designate the virtual router instance as owner";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/vrrp/passive
                        leaf passive {
                            type boolean;
                            sros-ext:immutable;
                            default "false";
                            description "Suppress the processing of VRRP advertisement messages";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipv4/vrrp/init-delay
                        leaf init-delay {
