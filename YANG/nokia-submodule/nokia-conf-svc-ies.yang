                ///nokia-conf/configure/service/ies/redundant-interface/ipv4
                container ipv4 {
                    description "Enter the ipv4 context";

                    ///nokia-conf/configure/service/ies/redundant-interface/ipv4/primary
                    container primary {
                        presence "Enable the primary context";
                        description "Enable the primary context";

                        ///nokia-conf/configure/service/ies/redundant-interface/ipv4/primary/address
                        leaf address {
                            type types-sros:ipv4-unicast-address;
