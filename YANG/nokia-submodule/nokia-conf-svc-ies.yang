                    ///nokia-conf/configure/service/ies/aarp-interface/spoke-sdp/egress
                    container egress {
                        description "Enter the egress context";

                        ///nokia-conf/configure/service/ies/aarp-interface/spoke-sdp/egress/vc-label
                        leaf vc-label {
                            type types-services:egress-vc-label;
                            sros-ext:auto-restart-to-modify;
                            description "VC egress value that indicates a specific connection";
                        }

                        ///nokia-conf/configure/service/ies/aarp-interface/spoke-sdp/egress/filter
