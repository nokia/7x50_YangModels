submodule nokia-conf-svc-ies {

    yang-version "1.1";

    belongs-to nokia-conf { prefix "conf"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-eth-cfm            { prefix "types-eth-cfm"; }
    import nokia-types-ipsec              { prefix "types-ipsec"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-router             { prefix "types-router"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-subscriber-mgmt    { prefix "types-submgt"; }

    include nokia-conf-aa-group;
    include nokia-conf-application-assurance;
    include nokia-conf-bfd;
    include nokia-conf-eth-cfm;
    include nokia-conf-filter;
    include nokia-conf-filter-ip-exception;
    include nokia-conf-groups;
    include nokia-conf-ipsec;
    include nokia-conf-isa;
    include nokia-conf-log;
    include nokia-conf-policy-options;
    include nokia-conf-port;
    include nokia-conf-port-eth-access;
    include nokia-conf-port-ethernet;
    include nokia-conf-python;
    include nokia-conf-qos;
    include nokia-conf-router;
    include nokia-conf-router-dhcp-server;
    include nokia-conf-routing-options;
    include nokia-conf-service;
    include nokia-conf-submgt-local-user-db;
    include nokia-conf-subscriber-mgmt;
    include nokia-conf-svc-epipe;
    include nokia-conf-svc-ies-sub-if;
    include nokia-conf-svc-ies-vdo-if;
    include nokia-conf-system;
    include nokia-conf-system-security;
    include nokia-conf-test-oam;
    include nokia-conf-vrrp;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model to configure ies services on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-conf-svc-ies YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-02-20";

    grouping conf-svc-ies {
        ///nokia-conf/configure/service/ies
        list ies {
            key "service-name";
            description "Enter the ies list instance";

            ///nokia-conf/configure/service/ies/service-name
            leaf service-name {
                type types-services:service-name;
                description "Administrative service name";
            }

            ///nokia-conf/configure/service/ies/service-id
            leaf service-id {
                type types-services:external-service-id;
                sros-ext:immutable;
                description "Service ID";
            }

            ///nokia-conf/configure/service/ies/description
            leaf description {
                type types-sros:description;
                description "Text description";
            }

            ///nokia-conf/configure/service/ies/customer
            leaf customer {
                type leafref {
                    path "../../customer/customer-name";
                }
                sros-ext:immutable;
                mandatory true;
                description "Service customer ID";
            }

            ///nokia-conf/configure/service/ies/admin-state
            leaf admin-state {
                type types-sros:admin-state;
                default "disable";
                description "Administrative state of the service";
            }

            ///nokia-conf/configure/service/ies/vpn-id
            leaf vpn-id {
                type types-services:vpn-id;
                sros-ext:immutable;
                description "VPN identifier for the service";
            }

            ///nokia-conf/configure/service/ies/apply-groups
            leaf-list apply-groups {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Apply a configuration group at this level";
            }

            ///nokia-conf/configure/service/ies/apply-groups-exclude
            leaf-list apply-groups-exclude {
                type leafref {
                    path "../../../groups/group/name";
                }
                max-elements 8;
                ordered-by user;
                description "Exclude a configuration group at this level";
            }

            ///nokia-conf/configure/service/ies/eth-cfm
            container eth-cfm {
                description "Enable the eth-cfm context";

                ///nokia-conf/configure/service/ies/eth-cfm/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/ies/eth-cfm/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

            } // container eth-cfm

            ///nokia-conf/configure/service/ies/subscriber-mgmt
            container subscriber-mgmt {
                description "Enter the subscriber-mgmt context";

                ///nokia-conf/configure/service/ies/subscriber-mgmt/multi-chassis-shunt-id
                leaf multi-chassis-shunt-id {
                    type uint32 {
                        range "1..8191";
                    }
                    description "Shunt ID for a pair of resilient nodes";
                }

                ///nokia-conf/configure/service/ies/subscriber-mgmt/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/ies/subscriber-mgmt/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/ies/subscriber-mgmt/up-resiliency
                container up-resiliency {
                    description "Enter the up-resiliency context";

                    ///nokia-conf/configure/service/ies/subscriber-mgmt/up-resiliency/monitor-oper-group
                    list monitor-oper-group {
                        key "oper-group";
                        max-elements 2;
                        description "Enter the monitor-oper-group list instance";

                        ///nokia-conf/configure/service/ies/subscriber-mgmt/up-resiliency/monitor-oper-group/oper-group
                        leaf oper-group {
                            type leafref {
                                path "../../../../../oper-group/name";
                            }
                            description "Operational group name";
                        }

                        ///nokia-conf/configure/service/ies/subscriber-mgmt/up-resiliency/monitor-oper-group/health-drop
                        leaf health-drop {
                            type uint32 {
                                range "1..255";
                            }
                            default "1";
                            description "Number subtracted from the health value per failure";
                        }

                        ///nokia-conf/configure/service/ies/subscriber-mgmt/up-resiliency/monitor-oper-group/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/ies/subscriber-mgmt/up-resiliency/monitor-oper-group/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // list monitor-oper-group

                } // container up-resiliency

            } // container subscriber-mgmt

            ///nokia-conf/configure/service/ies/aa-interface
            list aa-interface {
                key "interface-name";
                description "Enter the aa-interface list instance";

                ///nokia-conf/configure/service/ies/aa-interface/interface-name
                leaf interface-name {
                    type types-sros:interface-name;
                    description "Interface name";
                }

                ///nokia-conf/configure/service/ies/aa-interface/description
                leaf description {
                    type types-sros:very-long-description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/ies/aa-interface/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "enable";
                    description "Administrative state of the interface";
                }

                ///nokia-conf/configure/service/ies/aa-interface/ip-mtu
                leaf ip-mtu {
                    type types-router:ip-mtu-ext;
                    units "bytes";
                    description "IP MTU applied to outgoing packets";
                }

                ///nokia-conf/configure/service/ies/aa-interface/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/ies/aa-interface/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/ies/aa-interface/sap
                list sap {
                    key "sap-id";
                    max-elements 1;
                    description "Enter the sap list instance";

                    ///nokia-conf/configure/service/ies/aa-interface/sap/sap-id
                    leaf sap-id {
                        type types-sros:sap;
                        description "SAP ID";
                    }

                    ///nokia-conf/configure/service/ies/aa-interface/sap/description
                    leaf description {
                        type types-sros:long-description;
                        description "Text description";
                    }

                    ///nokia-conf/configure/service/ies/aa-interface/sap/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "enable";
                        description "Administrative state of the SAP";
                    }

                    ///nokia-conf/configure/service/ies/aa-interface/sap/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/ies/aa-interface/sap/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/ies/aa-interface/sap/ingress
                    container ingress {
                        description "Enter the ingress context";

                        ///nokia-conf/configure/service/ies/aa-interface/sap/ingress/qos
                        container qos {
                            description "Enter the qos context";

                            ///nokia-conf/configure/service/ies/aa-interface/sap/ingress/qos/sap-ingress
                            container sap-ingress {
                                description "Enter the sap-ingress context";

                                ///nokia-conf/configure/service/ies/aa-interface/sap/ingress/qos/sap-ingress/policy-name
                                leaf policy-name {
                                    type leafref {
                                        path "../../../../../../../../qos/sap-ingress/sap-ingress-policy-name";
                                    }
                                    description "Policy ID";
                                }

                                ///nokia-conf/configure/service/ies/aa-interface/sap/ingress/qos/sap-ingress/overrides
                                container overrides {
                                    description "Enter the overrides context";

                                } // container overrides

                            } // container sap-ingress

                        } // container qos

                    } // container ingress

                    ///nokia-conf/configure/service/ies/aa-interface/sap/egress
                    container egress {
                        description "Enter the egress context";

                        ///nokia-conf/configure/service/ies/aa-interface/sap/egress/qos
                        container qos {
                            description "Enter the qos context";

                            ///nokia-conf/configure/service/ies/aa-interface/sap/egress/qos/sap-egress
                            container sap-egress {
                                description "Enter the sap-egress context";

                                ///nokia-conf/configure/service/ies/aa-interface/sap/egress/qos/sap-egress/policy-name
                                leaf policy-name {
                                    type leafref {
                                        path "../../../../../../../../qos/sap-egress/sap-egress-policy-name";
                                    }
                                    description "Policy ID to associate with SAP for mirrored service";
                                }

                            } // container sap-egress

                        } // container qos

                        ///nokia-conf/configure/service/ies/aa-interface/sap/egress/filter
                        container filter {
                            description "Enter the filter context";

                            ///nokia-conf/configure/service/ies/aa-interface/sap/egress/filter/ip
                            leaf ip {
                                type leafref {
                                    path "../../../../../../../filter/ip-filter/filter-name";
                                }
                                description "IPv4 filter policy name";
                            }

                        } // container filter


                    } // container egress

                    ///nokia-conf/configure/service/ies/aa-interface/sap/lag
                    container lag {
                        description "Enter the lag context";

                    } // container lag

                    ///nokia-conf/configure/service/ies/aa-interface/sap/fwd-wholesale
                    container fwd-wholesale {
                        description "Enter the fwd-wholesale context";

                        ///nokia-conf/configure/service/ies/aa-interface/sap/fwd-wholesale/pppoe-service
                        leaf pppoe-service {
                            type leafref {
                                path "../../../../../epipe/service-name";
                            }
                            description "PPPoE service name";
                        }

                    } // container fwd-wholesale

                } // list sap

                ///nokia-conf/configure/service/ies/aa-interface/ipv4
                container ipv4 {
                    description "Enter the ipv4 context";

                    ///nokia-conf/configure/service/ies/aa-interface/ipv4/primary
                    container primary {
                        presence "Enable the primary context";
                        description "Enable the primary context";

                        ///nokia-conf/configure/service/ies/aa-interface/ipv4/primary/address
                        leaf address {
                            type types-sros:ipv4-unicast-address;
                            mandatory true;
                            description "Primary IPv4 address assigned to the interface";
                        }

                        ///nokia-conf/configure/service/ies/aa-interface/ipv4/primary/prefix-length
                        leaf prefix-length {
                            type uint32 {
                                range "0..32";
                            }
                            mandatory true;
                            description "IPv4 address prefix length";
                        }

                        ///nokia-conf/configure/service/ies/aa-interface/ipv4/primary/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/ies/aa-interface/ipv4/primary/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Exclude a configuration group at this level";
                        }

                    } // container primary

                } // container ipv4

            } // list aa-interface

            ///nokia-conf/configure/service/ies/interface
            list interface {
                key "interface-name";
                description "Enter the interface list instance";

                ///nokia-conf/configure/service/ies/interface/interface-name
                leaf interface-name {
                    type types-sros:interface-name;
                    description "Interface name";
                }

                ///nokia-conf/configure/service/ies/interface/description
                leaf description {
                    type types-sros:very-long-description;
                    description "Text description";
                }

                ///nokia-conf/configure/service/ies/interface/admin-state
                leaf admin-state {
                    type types-sros:admin-state;
                    default "enable";
                    description "Administrative state of the interface";
                }

                ///nokia-conf/configure/service/ies/interface/radius-auth-policy
                leaf radius-auth-policy {
                    type leafref {
                        path "../../../../subscriber-mgmt/radius-authentication-policy/name";
                    }
                    description "Authentication policy";
                }

                ///nokia-conf/configure/service/ies/interface/tunnel
                leaf tunnel {
                    type boolean;
                    sros-ext:immutable;
                    default "false";
                    description "Enable/disable tunnel interface";
                }

                ///nokia-conf/configure/service/ies/interface/loopback
                leaf loopback {
                    type boolean;
                    default "false";
                    description "Use interface as a loopback interface";
                }

                ///nokia-conf/configure/service/ies/interface/monitor-oper-group
                leaf monitor-oper-group {
                    type leafref {
                        path "../../../oper-group/name";
                    }
                    description "Operational group to monitor";
                }

                ///nokia-conf/configure/service/ies/interface/shcv-policy-ipv4
                leaf shcv-policy-ipv4 {
                    when "not(../tunnel = 'true')" {
                        description "shcv-policy can't be used in a tunnel interface";
                    }
                    type leafref {
                        path "../../../../subscriber-mgmt/shcv-policy/name";
                    }
                    description "Host connectivity IPv4 policy";
                }

                ///nokia-conf/configure/service/ies/interface/cpu-protection
                leaf cpu-protection {
                    type leafref {
                        path "../../../../system/security/cpu-protection/policy/policy-id";
                    }
                    description "CPU protection policy";
                }

                ///nokia-conf/configure/service/ies/interface/ip-mtu
                leaf ip-mtu {
                    type types-router:ip-mtu-ext;
                    units "bytes";
                    description "IP MTU applied to outgoing packets";
                }

                ///nokia-conf/configure/service/ies/interface/mac
                leaf mac {
                    type types-sros:mac-unicast-address;
                    description "MAC address for the interface";
                }

                ///nokia-conf/configure/service/ies/interface/tos-marking-state
                leaf tos-marking-state {
                    type types-router:tos-marking-state;
                    default "untrusted";
                    description "TOS marking state";
                }

                ///nokia-conf/configure/service/ies/interface/multicast-network-domain
                leaf multicast-network-domain {
                    type leafref {
                        path "../../../../router/network-domains/network-domain/domain-name";
                    }
                    description "Network domain name";
                }

                ///nokia-conf/configure/service/ies/interface/ingress-stats
                leaf ingress-stats {
                    type boolean;
                    default "false";
                    description "Collect ingress statistics";
                }

                ///nokia-conf/configure/service/ies/interface/mac-accounting
                leaf mac-accounting {
                    type boolean;
                    default "false";
                    description "Enable MAC accounting functionality";
                }

                ///nokia-conf/configure/service/ies/interface/vas-if-type
                leaf vas-if-type {
                    when "../tunnel = 'false'" {
                        description "not allowed in a tunnel interface";
                    }
                    type types-router:vas-if-type;
                    description "VAS interface type";
                }

                ///nokia-conf/configure/service/ies/interface/apply-groups
                leaf-list apply-groups {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Apply a configuration group at this level";
                }

                ///nokia-conf/configure/service/ies/interface/apply-groups-exclude
                leaf-list apply-groups-exclude {
                    type leafref {
                        path "../../../../groups/group/name";
                    }
                    max-elements 8;
                    ordered-by user;
                    description "Exclude a configuration group at this level";
                }

                ///nokia-conf/configure/service/ies/interface/shunting
                choice shunting {
                    description "shunting options";
                    ///nokia-conf/configure/service/ies/interface/shunting/shunting-profile
                    case shunting-profile {

                        ///nokia-conf/configure/service/ies/interface/shunting/shunting-profile/multi-chassis-shunting-profile
                        leaf multi-chassis-shunting-profile {
                            type leafref {
                                path "../../../../router/ipsec/multi-chassis-shunting-profile/name";
                            }
                            description "Multi-chassis shunting profile name";
                        }

                    }
                    ///nokia-conf/configure/service/ies/interface/shunting/nexthop
                    case nexthop {

                        ///nokia-conf/configure/service/ies/interface/shunting/nexthop/static-tunnel-redundant-nexthop
                        leaf static-tunnel-redundant-nexthop {
                            type types-sros:ipv4-unicast-address;
                            description "Address for the static ISA tunnel redundant next-hop";
                        }

                        ///nokia-conf/configure/service/ies/interface/shunting/nexthop/dynamic-tunnel-redundant-nexthop
                        leaf dynamic-tunnel-redundant-nexthop {
                            type types-sros:ipv4-unicast-address;
                            description "Redundant next-hop address for the dynamic IPsec tunnel";
                        }

                    }
                }
                ///nokia-conf/configure/service/ies/interface/ptp-hw-assist
                container ptp-hw-assist {
                    description "Enter the ptp-hw-assist context";

                    ///nokia-conf/configure/service/ies/interface/ptp-hw-assist/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the PTP time stamping assist";
                    }

                    ///nokia-conf/configure/service/ies/interface/ptp-hw-assist/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/ies/interface/ptp-hw-assist/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container ptp-hw-assist

                ///nokia-conf/configure/service/ies/interface/ingress
                container ingress {
                    description "Enter the ingress context";

                    ///nokia-conf/configure/service/ies/interface/ingress/policy-accounting
                    leaf policy-accounting {
                        type leafref {
                            path "../../../../../routing-options/policy-accounting/policy-acct-template/name";
                        }
                        description "Ingress policy accounting template name";
                    }

                    ///nokia-conf/configure/service/ies/interface/ingress/destination-class-lookup
                    leaf destination-class-lookup {
                        type boolean;
                        default "false";
                        description "Enable BGP destination class lookup";
                    }

                } // container ingress

                ///nokia-conf/configure/service/ies/interface/load-balancing
                container load-balancing {
                    description "Enter the load-balancing context";

                    ///nokia-conf/configure/service/ies/interface/load-balancing/ip-load-balancing
                    leaf ip-load-balancing {
                        type types-router:ip-load-balancing;
                        default "both";
                        description "IP load-balancing algorithm";
                    }

                    ///nokia-conf/configure/service/ies/interface/load-balancing/spi-load-balancing
                    leaf spi-load-balancing {
                        type boolean;
                        default "false";
                        description "Enable SPI use in hashing";
                    }

                    ///nokia-conf/configure/service/ies/interface/load-balancing/teid-load-balancing
                    leaf teid-load-balancing {
                        type boolean;
                        default "false";
                        description "Enable use of TEID in hashing";
                    }

                    ///nokia-conf/configure/service/ies/interface/load-balancing/flow-label-load-balancing
                    leaf flow-label-load-balancing {
                        type boolean;
                        default "false";
                        description "Enable flow label load balancing";
                    }

                } // container load-balancing

                ///nokia-conf/configure/service/ies/interface/ping-template
                container ping-template {
                    presence "Enable the ping-template context";
                    description "Enable the ping-template context";

                    ///nokia-conf/configure/service/ies/interface/ping-template/name
                    leaf name {
                        type leafref {
                            path "../../../../../test-oam/icmp/ping-template/name";
                        }
                        sros-ext:immutable;
                        mandatory true;
                        description "Ping template name";
                    }

                    ///nokia-conf/configure/service/ies/interface/ping-template/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of the ping template";
                    }

                    ///nokia-conf/configure/service/ies/interface/ping-template/destination-address
                    leaf destination-address {
                        type types-sros:ipv4-unicast-address;
                        sros-ext:auto-restart-to-modify;
                        description "Ping template destination address";
                    }

                    ///nokia-conf/configure/service/ies/interface/ping-template/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/ies/interface/ping-template/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                } // container ping-template

                ///nokia-conf/configure/service/ies/interface/ipsec
                container ipsec {
                    presence "Enable the ipsec context";
                    description "Enable the ipsec context";

                    ///nokia-conf/configure/service/ies/interface/ipsec/admin-state
                    leaf admin-state {
                        type types-sros:admin-state;
                        default "disable";
                        description "Administrative state of IPsec secured interface";
                    }

                    ///nokia-conf/configure/service/ies/interface/ipsec/tunnel-group
                    leaf tunnel-group {
                        type leafref {
                            path "../../../../../isa/tunnel-group/id";
                        }
                        sros-ext:immutable;
                        mandatory true;
                        description "Tunnel group ID";
                    }

                    ///nokia-conf/configure/service/ies/interface/ipsec/public-sap
                    leaf public-sap {
                        type uint32 {
                            range "0..4094";
                        }
                        sros-ext:immutable;
                        mandatory true;
                        description "Public SAP ID";
                    }

                    ///nokia-conf/configure/service/ies/interface/ipsec/ip-exception
                    leaf ip-exception {
                        type leafref {
                            path "../../../../../filter/ip-exception/filter-name";
                        }
                        description "IP exception filter";
                    }

                    ///nokia-conf/configure/service/ies/interface/ipsec/ipv6-exception
                    leaf ipv6-exception {
                        type leafref {
                            path "../../../../../filter/ipv6-exception/filter-name";
                        }
                        description "IPv6 filter exception used to bypass encryption";
                    }

                    ///nokia-conf/configure/service/ies/interface/ipsec/apply-groups
                    leaf-list apply-groups {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Apply a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/ies/interface/ipsec/apply-groups-exclude
                    leaf-list apply-groups-exclude {
                        type leafref {
                            path "../../../../../groups/group/name";
                        }
                        max-elements 8;
                        ordered-by user;
                        description "Exclude a configuration group at this level";
                    }

                    ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel
                    list ipsec-tunnel {
                        key "name";
                        description "Enter the ipsec-tunnel list instance";

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/name
                        leaf name {
                            type types-sros:named-item {
                                pattern "[a-zA-Z]+.*" {
                                    error-message "IPSec tunnel name cannot start with a number";
                                }
                            }
                            description "IPsec tunnel name";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/private-sap
                        leaf private-sap {
                            type uint32 {
                                range "0..4094";
                            }
                            sros-ext:immutable;
                            sros-ext:auto-restart-to-modify;
                            mandatory true;
                            description "Private SAP ID";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/admin-state
                        leaf admin-state {
                            type types-sros:admin-state;
                            default "disable";
                            description "Administrative state of the IPsec tunnel";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/description
                        leaf description {
                            type types-sros:description;
                            description "Text description";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/clear-df-bit
                        leaf clear-df-bit {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "false";
                            description "Reset the DF bit to 0 in all payload IP packets";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/copy-traffic-class-upon-decapsulation
                        leaf copy-traffic-class-upon-decapsulation {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "false";
                            description "Enable traffic class copy upon decapsulation";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/encapsulated-ip-mtu
                        leaf encapsulated-ip-mtu {
                            type uint32 {
                                range "512..9000";
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "Maximum size of the encapsulated tunnel packet";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/ip-mtu
                        leaf ip-mtu {
                            type uint32 {
                                range "512..9000";
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "Private MTU of the IPsec tunnel";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/replay-window
                        leaf replay-window {
                            type uint32 {
                                range "32|64|128|256|512";
                            }
                            units "packets";
                            sros-ext:auto-restart-to-modify;
                            description "Anti-replay window size";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/public-tcp-mss-adjust
                        leaf public-tcp-mss-adjust {
                            type union {
                                type uint32 {
                                    range "512..9000";
                                }
                                type enumeration {
                                    enum auto                           { value 0; }
                                }
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "TCP maximum segment size (MSS) on the public network";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/private-tcp-mss-adjust
                        leaf private-tcp-mss-adjust {
                            type int32 {
                                range "512..9000";
                            }
                            units "bytes";
                            sros-ext:auto-restart-to-modify;
                            description "TCP maximum segment size (MSS) adjustment";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/local-gateway-address-override
                        leaf local-gateway-address-override {
                            type types-sros:ip-address;
                            sros-ext:auto-restart-to-modify;
                            description "Local IPsec tunnel endpoint address";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/remote-gateway-address
                        leaf remote-gateway-address {
                            type types-sros:ip-address;
                            sros-ext:auto-restart-to-modify;
                            description "Remote IPsec tunnel endpoint address";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/private-service
                        leaf private-service {
                            type types-services:service-name;
                            sros-ext:immutable;
                            sros-ext:auto-restart-to-modify;
                            description "Private service name";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/propagate-pmtu-v4
                        leaf propagate-pmtu-v4 {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "true";
                            description "Enable propagation of the path MTU to IPv4 hosts";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/propagate-pmtu-v6
                        leaf propagate-pmtu-v6 {
                            type boolean;
                            sros-ext:auto-restart-to-modify;
                            default "true";
                            description "Enable propagation of the path MTU to IPv6 hosts";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/pmtu-discovery-aging
                        leaf pmtu-discovery-aging {
                            type uint32 {
                                range "900..3600";
                            }
                            units "seconds";
                            sros-ext:auto-restart-to-modify;
                            default "900";
                            description "Aging out time of the learned path MTU";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/apply-groups
                        leaf-list apply-groups {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            sros-ext:auto-restart-to-modify;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            type leafref {
                                path "../../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            sros-ext:auto-restart-to-modify;
                            description "Exclude a configuration group at this level";
                        }

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/bfd
                        container bfd {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the bfd context";

                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/bfd/bfd-designate
                            leaf bfd-designate {
                                type boolean;
                                sros-ext:auto-restart-to-modify;
                                default "false";
                                description "Designate IPsec tunnel to carry BFD traffic";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/bfd/bfd-liveness
                            container bfd-liveness {
                                presence "Enable the bfd-liveness context";
                                sros-ext:auto-restart-to-modify;
                                description "Enable the bfd-liveness context";

                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/bfd/bfd-liveness/dest-ip
                                leaf dest-ip {
                                    type types-sros:ipv4-unicast-address;
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Destination address used for the BFD session";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/bfd/bfd-liveness/interface
                                leaf interface {
                                    type types-sros:interface-name;
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Name of the interface used by the BFD session";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/bfd/bfd-liveness/service-name
                                leaf service-name {
                                    type types-services:service-name;
                                    sros-ext:auto-restart-to-modify;
                                    mandatory true;
                                    description "Administrative service name";
                                }

                            } // container bfd-liveness

                        } // container bfd

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/icmp6-generation
                        container icmp6-generation {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the icmp6-generation context";

                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/icmp6-generation/packet-too-big
                            container packet-too-big {
                                sros-ext:auto-restart-to-modify;
                                description "Enter the packet-too-big context";

                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/icmp6-generation/packet-too-big/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    sros-ext:auto-restart-to-modify;
                                    default "enable";
                                    description "Administrative state of Packet Too Big message sends";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/icmp6-generation/packet-too-big/message-count
                                leaf message-count {
                                    type uint32 {
                                        range "10..1000";
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    default "100";
                                    description "Maximum number of ICMPv6 PTB messages that can be sent";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/icmp6-generation/packet-too-big/interval
                                leaf interval {
                                    type uint32 {
                                        range "1..60";
                                    }
                                    units "seconds";
                                    sros-ext:auto-restart-to-modify;
                                    default "10";
                                    description "Interval for sending Packet Too Big messages";
                                }

                            } // container packet-too-big

                        } // container icmp6-generation

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange
                        container key-exchange {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the key-exchange context";

                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type
                            choice type {
                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic
                                case dynamic {

                                    ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic
                                    container dynamic {
                                        presence "Enable the dynamic context";
                                        sros-ext:auto-restart-to-modify;
                                        description "Enable the dynamic context";

                                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/auto-establish
                                        leaf auto-establish {
                                            type boolean;
                                            sros-ext:auto-restart-to-modify;
                                            default "false";
                                            description "Attempt to establish a phase 1 exchange automatically";
                                        }

                                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ike-policy
                                        leaf ike-policy {
                                            type leafref {
                                                path "../../../../../../../../ipsec/ike-policy/id";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            description "IKE policy ID";
                                        }

                                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/ipsec-transform
                                        leaf-list ipsec-transform {
                                            type leafref {
                                                path "../../../../../../../../ipsec/ipsec-transform/id";
                                            }
                                            max-elements 4;
                                            sros-ext:auto-restart-to-modify;
                                            description "IPsec transform IDs used by the dynamic key";
                                        }

                                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/pre-shared-key
                                        leaf pre-shared-key {
                                            type types-sros:encrypted-leaf {
                                                length "1..115";
                                            }
                                            sros-ext:auto-restart-to-modify;
                                            description "Pre-shared key for authentication";
                                        }

                                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert
                                        container cert {
                                            sros-ext:auto-restart-to-modify;
                                            description "Enter the cert context";

                                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/cert-profile
                                            leaf cert-profile {
                                                type leafref {
                                                    path "../../../../../../../../../ipsec/cert-profile/name";
                                                }
                                                description "Certificate profile name";
                                            }

                                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/trust-anchor-profile
                                            leaf trust-anchor-profile {
                                                type leafref {
                                                    path "../../../../../../../../../ipsec/trust-anchor-profile/name";
                                                }
                                                description "Trust anchor profile name";
                                            }

                                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify
                                            container status-verify {
                                                sros-ext:auto-restart-to-modify;
                                                description "Enter the status-verify context";

                                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify/default-result
                                                leaf default-result {
                                                    type enumeration {
                                                        enum revoked                        { value 0; }
                                                        enum good                           { value 1; }
                                                    }
                                                    default "revoked";
                                                    description "Default result for Certificate Status Verification";
                                                }

                                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify/primary
                                                leaf primary {
                                                    type types-ipsec:cert-revocation-status;
                                                    default "crl";
                                                    description "Primary method of CSV to verify the revocation status";
                                                }

                                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/cert/status-verify/secondary
                                                leaf secondary {
                                                    type types-ipsec:cert-revocation-status-with-none;
                                                    default "none";
                                                    description "Secondary method used to verify certificate revocation";
                                                }

                                            } // container status-verify

                                        } // container cert

                                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id
                                        container id {
                                            sros-ext:auto-restart-to-modify;
                                            description "Enter the id context";

                                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id
                                            choice id {
                                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv4
                                                case ipv4 {

                                                    ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv4/ipv4
                                                    leaf ipv4 {
                                                        type types-sros:ipv4-unicast-address;
                                                        sros-ext:auto-restart-to-modify;
                                                        description "IPv4 as the local ID type";
                                                    }

                                                }
                                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv6
                                                case ipv6 {

                                                    ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/ipv6/ipv6
                                                    leaf ipv6 {
                                                        type types-sros:ip-address;
                                                        sros-ext:auto-restart-to-modify;
                                                        description "IPv6 used as the local IKE ID type";
                                                    }

                                                }
                                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/fqdn
                                                case fqdn {

                                                    ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/dynamic/dynamic/id/id/fqdn/fqdn
                                                    leaf fqdn {
                                                        type types-sros:fully-qualified-domain-name;
                                                        sros-ext:auto-restart-to-modify;
                                                        description "FQDN used as the local ID IKE type";
                                                    }

                                                }
                                            }
                                        } // container id

                                    } // container dynamic

                                }
                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/manual
                                case manual {

                                    ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual
                                    container manual {
                                        presence "Enable the manual context";
                                        sros-ext:auto-restart-to-modify;
                                        description "Enable the manual context";

                                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys
                                        list keys {
                                            key "security-association direction";
                                            description "Enter the keys list instance";

                                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/security-association
                                            leaf security-association {
                                                type uint32 {
                                                    range "1..16";
                                                }
                                                description "SA entry ID";
                                            }

                                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/direction
                                            leaf direction {
                                                type enumeration {
                                                    enum inbound                        { value 1; }
                                                    enum outbound                       { value 2; }
                                                }
                                                description "Direction of the IPsec tunnel";
                                            }

                                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/ipsec-transform
                                            leaf ipsec-transform {
                                                type leafref {
                                                    path "../../../../../../../../../ipsec/ipsec-transform/id";
                                                }
                                                sros-ext:immutable;
                                                mandatory true;
                                                description "Transform entry used by manual SAs";
                                            }

                                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/spi
                                            leaf spi {
                                                type uint32 {
                                                    range "256..16383";
                                                }
                                                sros-ext:immutable;
                                                mandatory true;
                                                description "SPI of inbound and outbound packets";
                                            }

                                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/authentication-key
                                            leaf authentication-key {
                                                type types-sros:hex-string {
                                                    length "1..130";
                                                }
                                                sros-ext:immutable;
                                                description "Key used for the authentication algorithm";
                                            }

                                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/encryption-key
                                            leaf encryption-key {
                                                type types-sros:hex-string {
                                                    length "1..66";
                                                }
                                                sros-ext:immutable;
                                                description "Key used for the encryption algorithm";
                                            }

                                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/apply-groups
                                            leaf-list apply-groups {
                                                type leafref {
                                                    path "../../../../../../../../../groups/group/name";
                                                }
                                                max-elements 8;
                                                ordered-by user;
                                                description "Apply a configuration group at this level";
                                            }

                                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/key-exchange/type/manual/manual/keys/apply-groups-exclude
                                            leaf-list apply-groups-exclude {
                                                type leafref {
                                                    path "../../../../../../../../../groups/group/name";
                                                }
                                                max-elements 8;
                                                ordered-by user;
                                                description "Exclude a configuration group at this level";
                                            }

                                        } // list keys

                                    } // container manual

                                }
                            }
                        } // container key-exchange

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/max-history-key-records
                        container max-history-key-records {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the max-history-key-records context";

                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/max-history-key-records/ike
                            leaf ike {
                                type uint32 {
                                    range "1..3";
                                }
                                sros-ext:auto-restart-to-modify;
                                description "Maximum number of historical IKE key records";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/max-history-key-records/esp
                            leaf esp {
                                type uint32 {
                                    range "1..48";
                                }
                                sros-ext:auto-restart-to-modify;
                                description "Maximum number of recent records";
                            }

                        } // container max-history-key-records

                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/icmp-generation
                        container icmp-generation {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the icmp-generation context";

                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/icmp-generation/frag-required
                            container frag-required {
                                sros-ext:auto-restart-to-modify;
                                description "Enter the frag-required context";

                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/icmp-generation/frag-required/admin-state
                                leaf admin-state {
                                    type types-sros:admin-state;
                                    sros-ext:auto-restart-to-modify;
                                    default "enable";
                                    description "Administrative state of sending ICMP messages";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/icmp-generation/frag-required/message-count
                                leaf message-count {
                                    type uint32 {
                                        range "10..1000";
                                    }
                                    sros-ext:auto-restart-to-modify;
                                    default "100";
                                    description "Maximum number of ICMP messages that can be sent";
                                }

                                ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/icmp-generation/frag-required/interval
                                leaf interval {
                                    type uint32 {
                                        range "1..60";
                                    }
                                    units "seconds";
                                    sros-ext:auto-restart-to-modify;
                                    default "10";
                                    description "Interval for sending ICMP messages";
                                }

                            } // container frag-required

                        } // container icmp-generation


                        ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/security-policy
                        container security-policy {
                            sros-ext:auto-restart-to-modify;
                            description "Enter the security-policy context";

                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/security-policy/id
                            leaf id {
                                type uint32;
                                sros-ext:auto-restart-to-modify;
                                description "Security policy ID for use by the tunnel";
                            }

                            ///nokia-conf/configure/service/ies/interface/ipsec/ipsec-tunnel/security-policy/strict-match
                            leaf strict-match {
                                type boolean;
                                sros-ext:auto-restart-to-modify;
                                default "false";
                                description "Enable strict match of the security policy entry";
                            }

                        } // container security-policy

                    } // list ipsec-tunnel

                } // container ipsec

