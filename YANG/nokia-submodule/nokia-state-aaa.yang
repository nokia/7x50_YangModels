submodule nokia-state-aaa {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-inet-types                { prefix "inet"; }
    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-diameter           { prefix "types-diam"; }
    import nokia-types-nat                { prefix "types-nat"; }
    import nokia-types-services           { prefix "types-services"; }
    import nokia-types-sros               { prefix "types-sros"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for authentication, authorization and accounting on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-aaa YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2022-11-14";

    grouping state-aaa {
        ///nokia-state/state/aaa
        container aaa {
            description "Enable the aaa context";

            ///nokia-state/state/aaa/radius
            container radius {
                description "Enter the radius context";

                ///nokia-state/state/aaa/radius/server-policy
                list server-policy {
                    key "name";
                    description "Enter the server-policy list instance";

                    ///nokia-state/state/aaa/radius/server-policy/name
                    leaf name {
                        type types-sros:named-item;
                        description "RADIUS server policy name";
                    }

                    ///nokia-state/state/aaa/radius/server-policy/blocked-state
                    leaf blocked-state {
                        type enumeration {
                            enum not-blocked                    { value 0; }
                            enum blocked                        { value 1; }
                        }
                        description "Indicates whether or not the RADIUS server policy is blocked by the acct-on-off functions.";
                    }

                    ///nokia-state/state/aaa/radius/server-policy/servers
                    container servers {
                        description "Enter the servers context";

                        ///nokia-state/state/aaa/radius/server-policy/servers/server
                        list server {
                            key "server-index";
                            description "Enter the server list instance";

                            ///nokia-state/state/aaa/radius/server-policy/servers/server/server-index
                            leaf server-index {
                                type uint32 {
                                    range "1..16";
                                }
                                description "RADIUS server index";
                            }

                            ///nokia-state/state/aaa/radius/server-policy/servers/server/oper-state
                            leaf oper-state {
                                type enumeration {
                                    enum unknown                        { value 1; }
                                    enum in-service                     { value 2; }
                                    enum out-of-service                 { value 3; }
                                    enum transition                     { value 4; }
                                    enum overloaded                     { value 5; }
                                    enum probing                        { value 6; }
                                }
                                description "Operational state of RADIUS server";
                            }

                            ///nokia-state/state/aaa/radius/server-policy/servers/server/out-of-service-time
                            leaf out-of-service-time {
                                type types-sros:time-duration;
                                description "Number of seconds that RADIUS server was out-of-service";
                            }

                            ///nokia-state/state/aaa/radius/server-policy/servers/server/overload-time
                            leaf overload-time {
                                type types-sros:time-duration;
                                description "Number of seconds that RADIUS server was overloaded";
                            }

                            ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics
                            container statistics {
                                description "Enter the statistics context";

                                ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/tx-request
                                container tx-request {
                                    description "Enter the tx-request context";

                                    ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/tx-request/total-packets
                                    leaf total-packets {
                                        type yang:counter32;
                                        description "The total number of RADIUS request packets transmitted for this server.";
                                    }

                                    ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/tx-request/pending
                                    leaf pending {
                                        type yang:counter32;
                                        description
                                            "The number of RADIUS request packets that are currently waiting
                                             for reply from this server.";
                                    }

                                    ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/tx-request/timeout
                                    leaf timeout {
                                        type yang:counter32;
                                        description "The number of RADIUS request packets that have timed out for this server.";
                                    }

                                    ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/tx-request/failed
                                    leaf failed {
                                        type yang:counter32;
                                        description
                                            "The number of RADIUS request packets that could not be transmitted
                                             for this server.";
                                    }

                                    ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/tx-request/failed-out-of-ids
                                    leaf failed-out-of-ids {
                                        type yang:counter32;
                                        description
                                            "The number of RADIUS request packets that could not be
                                             transmitted due to out of ids";
                                    }

                                } // container tx-request

                                ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/rx-response
                                container rx-response {
                                    description "Enter the rx-response context";

                                    ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/rx-response/total-packets
                                    leaf total-packets {
                                        type yang:counter32;
                                        description "The total number of RADIUS response packets received from this server.";
                                    }

                                    ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/rx-response/invalid-authenticator
                                    leaf invalid-authenticator {
                                        type yang:counter32;
                                        description
                                            "The number of RADIUS response packets with an invalid Authenticator
                                             received from this server.";
                                    }

                                    ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/rx-response/invalid-message-authenticator-attribute
                                    leaf invalid-message-authenticator-attribute {
                                        type yang:counter32;
                                        description
                                            "The number of RADIUS response packets with an invalid Message-Authenticator
                                             attribute received from this server.";
                                    }

                                } // container rx-response

                                ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/accounting
                                container accounting {
                                    description "Enter the accounting context";

                                    ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/accounting/failed
                                    leaf failed {
                                        type yang:counter32;
                                        description "The number of accounting failures for this server.";
                                    }

                                    ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/accounting/average-delay
                                    container average-delay {
                                        description "Enter the average-delay context";

                                        ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/accounting/average-delay/last-10-packets
                                        leaf last-10-packets {
                                            type yang:gauge32;
                                            description
                                                "This indicates (in microseconds) the average response delay
                                                 for the last 10 accounting packets.";
                                        }

                                        ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/accounting/average-delay/last-100-packets
                                        leaf last-100-packets {
                                            type yang:gauge32;
                                            description
                                                "This indicates (in microseconds) the average response delay
                                                 for the last 100 accounting packets.";
                                        }

                                        ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/accounting/average-delay/last-1000-packets
                                        leaf last-1000-packets {
                                            type yang:gauge32;
                                            description
                                                "This indicates (in microseconds) the average response delay
                                                 for the last 1000 accounting packets.";
                                        }

                                        ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/accounting/average-delay/last-10000-packets
                                        leaf last-10000-packets {
                                            type yang:gauge32;
                                            description
                                                "This indicates (in microseconds) the average response delay
                                                 for the last 10000 accounting packets.";
                                        }

                                    } // container average-delay

                                } // container accounting

                                ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/authentication
                                container authentication {
                                    description "Enter the authentication context";

                                    ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/authentication/failed
                                    leaf failed {
                                        type yang:counter32;
                                        description "The number of authentication failures for this server.";
                                    }

                                    ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/authentication/average-delay
                                    container average-delay {
                                        description "Enter the average-delay context";

                                        ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/authentication/average-delay/last-10-packets
                                        leaf last-10-packets {
                                            type yang:gauge32;
                                            description
                                                "This indicates (in microseconds) the average response delay
                                                 for the last 10 authentication packets.";
                                        }

                                        ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/authentication/average-delay/last-100-packets
                                        leaf last-100-packets {
                                            type yang:gauge32;
                                            description
                                                "This indicates (in microseconds) the average response delay
                                                 for the last 100 authentication packets.";
                                        }

                                        ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/authentication/average-delay/last-1000-packets
                                        leaf last-1000-packets {
                                            type yang:gauge32;
                                            description
                                                "This indicates (in microseconds) the average response delay
                                                 for the last 1000 authentication packets.";
                                        }

                                        ///nokia-state/state/aaa/radius/server-policy/servers/server/statistics/authentication/average-delay/last-10000-packets
                                        leaf last-10000-packets {
                                            type yang:gauge32;
                                            description
                                                "This indicates (in microseconds) the average response delay
                                                 for the last 10000 authentication packets.";
                                        }

                                    } // container average-delay

                                } // container authentication

                            } // container statistics

                        } // list server

                    } // container servers

                    ///nokia-state/state/aaa/radius/server-policy/acct-on-off
                    container acct-on-off {
                        description "Enable the acct-on-off context";

                        ///nokia-state/state/aaa/radius/server-policy/acct-on-off/oper-state
                        leaf oper-state {
                            type enumeration {
                                enum not-applicable                 { value 0; }
                                enum off                            { value 1; }
                                enum off-no-response                { value 2; }
                                enum on                             { value 3; }
                                enum sending-acct-on                { value 4; }
                                enum sending-acct-off               { value 5; }
                            }
                            description "Indicates the operational state related to the sending of RADIUS Accounting-On and Accounting-Off messages for this RADIUS server policy.";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/acct-on-off/session-id
                        leaf session-id {
                            type types-sros:display-string-or-empty;
                            description "Indicates the accounting session id used in the RADIUS Accounting-On and Accounting-Off messages sent for this RADIUS server policy.";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/acct-on-off/trigger
                        leaf trigger {
                            type enumeration {
                                enum not-applicable                 { value 0; }
                                enum start-up                       { value 1; }
                                enum tools                          { value 2; }
                                enum tools-forced                   { value 3; }
                                enum reboot                         { value 4; }
                                enum group                          { value 5; }
                            }
                            description "Indicates the event that caused the most recent operational state change as defined in oper-state.";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/acct-on-off/server-name
                        leaf server-name {
                            type types-sros:named-item-or-empty;
                            description "Indicates the name of the RADIUS server from which the most recent RADIUS response has been received.";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/acct-on-off/retry-count
                        leaf retry-count {
                            type uint32;
                            description "Indicates the number of times the acct-on client has already sent a new RADIUS Accounting-On message after no accounting response was received in time.";
                        }

                    } // container acct-on-off

                    ///nokia-state/state/aaa/radius/server-policy/statistics
                    container statistics {
                        description "Enter the statistics context";

                        ///nokia-state/state/aaa/radius/server-policy/statistics/tx-request
                        leaf tx-request {
                            type yang:counter32;
                            description "The number of RADIUS transaction requests transmitted.";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/statistics/rx-response
                        leaf rx-response {
                            type yang:counter32;
                            description "The number of RADIUS transaction responses received.";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/statistics/request-timeout
                        leaf request-timeout {
                            type yang:counter32;
                            description "The number of RADIUS transaction requests that have timed out.";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/statistics/request-send-fail
                        leaf request-send-fail {
                            type yang:counter32;
                            description "The number of RADIUS transaction requests that could not be transmitted.";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/statistics/request-send-retry
                        leaf request-send-retry {
                            type yang:counter32;
                            description "The number of times a RADIUS request packet was retransmitted to a server.";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/statistics/request-rejected
                        leaf request-rejected {
                            type yang:counter32;
                            description "The number of RADIUS transaction requests rejected";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/statistics/accounting-fail
                        leaf accounting-fail {
                            type yang:counter32;
                            description "The number of accounting failures for this policy.";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/statistics/authentication-fail
                        leaf authentication-fail {
                            type yang:counter32;
                            description "The number of authentication failures for this policy.";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/statistics/access-reject-ratio
                        leaf access-reject-ratio {
                            type uint32 {
                                range "0..100";
                            }
                            description "The ratio of access-rejects in the authentication responses for this policy.";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/statistics/failure-ratio
                        leaf failure-ratio {
                            type uint32 {
                                range "0..100";
                            }
                            description "The transaction failure ratio for this policy.";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/statistics/success-ratio
                        leaf success-ratio {
                            type uint32 {
                                range "0..100";
                            }
                            description "The transaction success ratio for this policy.";
                        }

                        ///nokia-state/state/aaa/radius/server-policy/statistics/buffered-messages
                        container buffered-messages {
                            description "Enter the buffered-messages context";

                            ///nokia-state/state/aaa/radius/server-policy/statistics/buffered-messages/accounting-interim
                            leaf accounting-interim {
                                type yang:counter32;
                                description "The number of buffered accounting interim update messages.";
                            }

                            ///nokia-state/state/aaa/radius/server-policy/statistics/buffered-messages/accounting-interim-drop
                            leaf accounting-interim-drop {
                                type yang:counter32;
                                description
                                    "The number of accounting interim update messages dropped from
                                     the buffer because their lifetime expired.";
                            }

                            ///nokia-state/state/aaa/radius/server-policy/statistics/buffered-messages/accounting-stop
                            leaf accounting-stop {
                                type yang:counter32;
                                description "The number of buffered accounting stop messages.";
                            }

                            ///nokia-state/state/aaa/radius/server-policy/statistics/buffered-messages/accounting-stop-drop
                            leaf accounting-stop-drop {
                                type yang:counter32;
                                description
                                    "The number of accounting stop messages dropped from
                                     the buffer because their lifetime expired.";
                            }

                            ///nokia-state/state/aaa/radius/server-policy/statistics/buffered-messages/last-cleared
                            leaf last-cleared {
                                type yang:date-and-time;
                                description "The time of the most recent reset (empty) of the message buffer.";
                            }

                            ///nokia-state/state/aaa/radius/server-policy/statistics/buffered-messages/statistics-last-cleared
                            leaf statistics-last-cleared {
                                type yang:date-and-time;
                                description "The time of the most recent reset of the message buffer statistics.";
                            }

                            ///nokia-state/state/aaa/radius/server-policy/statistics/buffered-messages/accounting-start
                            leaf accounting-start {
                                type yang:counter32;
                                description "The number of buffered accounting start messages.";
                            }

                            ///nokia-state/state/aaa/radius/server-policy/statistics/buffered-messages/accounting-start-drop
                            leaf accounting-start-drop {
                                type yang:counter32;
                                description
                                    "The number of accounting start messages dropped from
                                     the buffer because their lifetime expired.";
                            }

                        } // container buffered-messages

                    } // container statistics

                } // list server-policy

                ///nokia-state/state/aaa/radius/route-downloader
                list route-downloader {
                    key "name";
                    description "Enter the route-downloader list instance";

                    ///nokia-state/state/aaa/radius/route-downloader/name
                    leaf name {
                        type types-sros:named-item;
                        description "RADIUS route downloader name";
                    }

                    ///nokia-state/state/aaa/radius/route-downloader/statistics
                    container statistics {
                        description "Enter the statistics context";

                        ///nokia-state/state/aaa/radius/route-downloader/statistics/last-access-reject-received
                        leaf last-access-reject-received {
                            type yang:date-and-time;
                            description
                                "When this RADIUS Route Downloader last received a RADIUS
                                 Access-Reject packet";
                        }

                        ///nokia-state/state/aaa/radius/route-downloader/statistics/last-access-accept-received
                        leaf last-access-accept-received {
                            type yang:date-and-time;
                            description
                                "When this RADIUS Route Downloader last received a RADIUS
                                 Access-Accept packet";
                        }

                        ///nokia-state/state/aaa/radius/route-downloader/statistics/last-access-request-sent
                        leaf last-access-request-sent {
                            type yang:date-and-time;
                            description
                                "When this RADIUS Route Downloader last sent a RADIUS
                                 Access-Request packet";
                        }

                        ///nokia-state/state/aaa/radius/route-downloader/statistics/last-access-request-retried
                        leaf last-access-request-retried {
                            type yang:date-and-time;
                            description
                                "When this RADIUS Route Downloader last retried a RADIUS
                                 Access-Request packet";
                        }

                        ///nokia-state/state/aaa/radius/route-downloader/statistics/remaining-time-before-next-download-attempt
                        leaf remaining-time-before-next-download-attempt {
                            type types-sros:time-interval;
                            description "The remaining time before the next download attempt";
                        }

                        ///nokia-state/state/aaa/radius/route-downloader/statistics/remaining-time-before-next-download-retry
                        leaf remaining-time-before-next-download-retry {
                            type types-sros:time-interval;
                            description "The remaining time before the next download retry";
                        }

                        ///nokia-state/state/aaa/radius/route-downloader/statistics/routes-received-count
                        leaf routes-received-count {
                            type yang:counter32;
                            description
                                "The number of routes received in the last completed
                                 route download process";
                        }

                        ///nokia-state/state/aaa/radius/route-downloader/statistics/access-reject-received-count
                        leaf access-reject-received-count {
                            type yang:counter32;
                            description
                                "The number of Access-Reject packets received
                                 by this RADIUS Route Downloader";
                        }

                        ///nokia-state/state/aaa/radius/route-downloader/statistics/access-accept-received-count
                        leaf access-accept-received-count {
                            type yang:counter32;
                            description
                                "The number of Access-Accept packets received
                                 by this RADIUS Route Downloader";
                        }

                        ///nokia-state/state/aaa/radius/route-downloader/statistics/access-accept-dropped-count
                        leaf access-accept-dropped-count {
                            type yang:counter32;
                            description
                                "The number of Access-Accept packets received but dropped
                                 by this RADIUS Route Downloader";
                        }

                        ///nokia-state/state/aaa/radius/route-downloader/statistics/access-request-sent-count
                        leaf access-request-sent-count {
                            type yang:counter32;
                            description
                                "The number of Access-Request packets sent
                                 by this RADIUS Route Downloader";
                        }

                        ///nokia-state/state/aaa/radius/route-downloader/statistics/access-request-retried-count
                        leaf access-request-retried-count {
                            type yang:counter32;
                            description
                                "The number of Access-Request packets retried
                                 by this RADIUS Route Downloader";
                        }

                        ///nokia-state/state/aaa/radius/route-downloader/statistics/download-count
                        leaf download-count {
                            type yang:counter32;
                            description
                                "The number of downloads started
                                 by this RADIUS Route Downloader";
                        }

                        ///nokia-state/state/aaa/radius/route-downloader/statistics/route-failed-to-install-count
                        leaf route-failed-to-install-count {
                            type yang:counter32;
                            description
                                "The number of times installing received routes failed
                                 for this RADIUS Route Downloader";
                        }

                    } // container statistics

                } // list route-downloader

                ///nokia-state/state/aaa/radius/isa-policy
                list isa-policy {
                    key "name";
                    description "Enter the isa-policy list instance";

                    ///nokia-state/state/aaa/radius/isa-policy/name
                    leaf name {
                        type types-sros:named-item;
                        description "ISA RADIUS policy name referenced by a NAT application";
                    }

                    ///nokia-state/state/aaa/radius/isa-policy/servers
                    container servers {
                        description "Enter the servers context";

                        ///nokia-state/state/aaa/radius/isa-policy/servers/source-address-range-end
                        leaf source-address-range-end {
                            type types-sros:ipv4-unicast-address;
                            description "End of the source address range of the RADIUS messages";
                        }

                        ///nokia-state/state/aaa/radius/isa-policy/servers/server
                        list server {
                            key "index";
                            description "Enter the server list instance";

                            ///nokia-state/state/aaa/radius/isa-policy/servers/server/index
                            leaf index {
                                type uint32 {
                                    range "1..10";
                                }
                                description "RADIUS server ID";
                            }

                            ///nokia-state/state/aaa/radius/isa-policy/servers/server/group-member
                            list group-member {
                                key "group-id member-id";
                                description "Enter the group-member list instance";

                                ///nokia-state/state/aaa/radius/isa-policy/servers/server/group-member/group-id
                                leaf group-id {
                                    type uint32;
                                    description "The identifier of the Integrated Service Adaptor group.";
                                }

                                ///nokia-state/state/aaa/radius/isa-policy/servers/server/group-member/member-id
                                leaf member-id {
                                    type types-nat:bb-group-member;
                                    description "The identifier of the ISA Group member.";
                                }

                                ///nokia-state/state/aaa/radius/isa-policy/servers/server/group-member/source-address
                                leaf source-address {
                                    type types-sros:ip-address;
                                    description
                                        "The source IP address used by this member for this connection
                                         with this RADIUS server.";
                                }

                                ///nokia-state/state/aaa/radius/isa-policy/servers/server/group-member/purpose-up
                                leaf purpose-up {
                                    type bits {
                                        bit accounting                     { position 0; }
                                        bit authentication                 { position 1; }
                                        bit coa                            { position 2; }
                                    }
                                    description
                                        "Indicates for what purpose this RADIUS server is operational up
                                         on this group member.";
                                }

                                ///nokia-state/state/aaa/radius/isa-policy/servers/server/group-member/purpose-hold-down
                                leaf purpose-hold-down {
                                    type bits {
                                        bit accounting                     { position 0; }
                                        bit authentication                 { position 1; }
                                        bit coa                            { position 2; }
                                    }
                                    description
                                        "Indicates for what purpose this RADIUS server is in hold down
                                         on this group member.";
                                }

                                ///nokia-state/state/aaa/radius/isa-policy/servers/server/group-member/statistics
                                container statistics {
                                    description "Enter the statistics context";

                                    ///nokia-state/state/aaa/radius/isa-policy/servers/server/group-member/statistics/accounting
                                    container accounting {
                                        description "Enter the accounting context";

                                        ///nokia-state/state/aaa/radius/isa-policy/servers/server/group-member/statistics/accounting/rx-replies
                                        leaf rx-replies {
                                            type yang:counter32;
                                            description "Number of received accounting replies";
                                        }

                                        ///nokia-state/state/aaa/radius/isa-policy/servers/server/group-member/statistics/accounting/tx-requests
                                        leaf tx-requests {
                                            type yang:counter32;
                                            description "Number of transmitted accounting requests";
                                        }

                                        ///nokia-state/state/aaa/radius/isa-policy/servers/server/group-member/statistics/accounting/tx-retries
                                        leaf tx-retries {
                                            type yang:counter32;
                                            description "Number of re-transmitted accounting requests";
                                        }

                                        ///nokia-state/state/aaa/radius/isa-policy/servers/server/group-member/statistics/accounting/tx-timeouts
                                        leaf tx-timeouts {
                                            type yang:counter32;
                                            description
                                                "Number of transmitted accounting requests that
                                                 timed out, not including retries";
