submodule nokia-state-test-oam {

    yang-version "1.1";

    belongs-to nokia-state { prefix "state"; }

    import ietf-yang-types                { prefix "yang"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-types-sros               { prefix "types-sros"; }

    include nokia-state-router;
    include nokia-state-testoam-service-activation-testhead;

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model with operational state for OAM test parameters configuration on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-state-test-oam YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2023-02-28";

    grouping state-test-oam {
        ///nokia-state/state/test-oam
        container test-oam {
            description "Enable the test-oam context";

            ///nokia-state/state/test-oam/link-measurement
            container link-measurement {
                description "Enter the link-measurement context";

                ///nokia-state/state/test-oam/link-measurement/router
                list router {
                    key "router-instance";
                    description "Enter the router list instance";

                    ///nokia-state/state/test-oam/link-measurement/router/router-instance
                    leaf router-instance {
                        type leafref {
                            path "../../../../router/router-name";
                        }
                        description "Name of the virtual router on which the measurement template is configured";
                    }

                    ///nokia-state/state/test-oam/link-measurement/router/interface
                    list interface {
                        key "interface-name";
                        description "Enter the interface list instance";

                        ///nokia-state/state/test-oam/link-measurement/router/interface/interface-name
                        leaf interface-name {
                            type leafref {
                                path "../../../../../router/interface/interface-name";
                            }
                            description "Name of the interface on which the measurement template is configured";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/oper-source-address
                        leaf oper-source-address {
                            type types-sros:ip-address;
                            description "Indicates the source IP address from which TWAMP packets are being sent";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/source-auto-assigned
                        leaf source-auto-assigned {
                            type boolean;
                            description "Indicates whether the oper-source-address was assigned (true) or configured (false)";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/oper-destination-address
                        leaf oper-destination-address {
                            type types-sros:ip-address;
                            description "Indicates the destination IP address to which TWAMP packets are being sent";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/destination-auto-assigned
                        leaf destination-auto-assigned {
                            type boolean;
                            description
                                "Indicates whether the oper-destination-address was assigned (true) or configured
                                 (false)";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/oper-state
                        leaf oper-state {
                            type types-sros:up-down-status;
                            description "Indicates the operational state of link measurement on this interface";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/failure-conditions
                        leaf failure-conditions {
                            type bits {
                                bit no-protocol                    { position 0; }
                                bit template-admin-down            { position 1; }
                                bit udp-port-unavailable           { position 2; }
                                bit internal-error                 { position 3; }
                            }
                            description
                                "Indicates the reason(s) why this interface's link measurement is operationally
                                 disabled";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/detectable-tx-error
                        leaf detectable-tx-error {
                            type enumeration {
                                enum none                           { value 1; }
                                enum interface-down                 { value 2; }
                                enum unexpected-error               { value 3; }
                                enum no-route                       { value 4; }
                                enum source-ip-not-local            { value 5; }
                                enum invalid-dest-ip                { value 6; }
                                enum invalid-interface-type         { value 7; }
                            }
                            description "Indicates a detectable error in delay measurement transmission";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/reporting
                        leaf reporting {
                            type boolean;
                            description "Indicates whether the interface is currently reporting to the routing engine";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/last-reported-delay
                        leaf last-reported-delay {
                            type union {
                                type int32 {
                                    range "0|1..2147483647";
                                }
                                type enumeration {
                                    enum none                           { value -1; }
                                }
                            }
                            units "microseconds";
                            description "Indicates the last delay measurement reported to the routing engine";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/report-timestamp
                        leaf report-timestamp {
                            type yang:date-and-time {
                                length "8";
                            }
                            description
                                "When ./last-reported-delay > 0, report-timestamp holds the time at which
                                 ./last-reported-delay was reported to the routing engine.

                                 When ./last-reported-delay = 0, report-timestamp holds the time at which
                                 ./last-reported-delay was cleared (because its aging timer expired).

                                 When ./last-reported-delay = -1, report-timestamp holds '1970-Jan-01 00:00:00.0'
                                 UTC as a placeholder representing a delay has not yet been reported to the routing
                                 engine.";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/triggered-by
                        leaf triggered-by {
                            type enumeration {
                                enum none                           { value 1; }
                                enum sample-threshold-absolute      { value 2; }
                                enum sample-threshold-relative      { value 3; }
                                enum aggregate-threshold-absolute   { value 4; }
                                enum aggregate-threshold-relative   { value 5; }
                                enum expired                        { value 6; }
                                enum reporting-disabled             { value 7; }
                            }
                            description "Indicates the reason for the report to the routing engine";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/aggregate-newest-index
                        leaf aggregate-newest-index {
                            type uint32;
                            description
                                "Indicates the index of the newest aggregate sample window for this interface (at
                                 /test-oam/link-measurement/results/aggregate/index)";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/sample-newest-index
                        leaf sample-newest-index {
                            type uint32;
                            description
                                "Indicates the index of the newest sample window for this interface (at
                                 /test-oam/link-measurement/results/sample/index)";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/in-use-source-udp-port
                        leaf in-use-source-udp-port {
                            type uint32;
                            description "Indicates the source UDP port in use for the session.";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/in-use-destination-udp-port
                        leaf in-use-destination-udp-port {
                            type uint32;
                            description "Indicates the destination UDP port in use for the session.";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/stamp-session-id
                        leaf stamp-session-id {
                            type uint32;
                            description "Indicates the autogenerated session-ID assigned to the session.";
                        }

                        ///nokia-state/state/test-oam/link-measurement/router/interface/statistics
                        container statistics {
                            description "Enter the statistics context";

                            ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/aggregate-sample-window
                            container aggregate-sample-window {
                                description "Enter the aggregate-sample-window context";

                                ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/aggregate-sample-window/index
                                list index {
                                    key "index";
                                    description "Enter the index list instance";

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/aggregate-sample-window/index/index
                                    leaf index {
                                        type uint32 {
                                            range "1..4294967295";
                                        }
                                        description "Specifies the index value of aggregate sample windows on the same interface";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/aggregate-sample-window/index/end-timestamp-utc
                                    leaf end-timestamp-utc {
                                        type yang:date-and-time {
                                            length "8";
                                        }
                                        description "Indicates the time (UTC) at which this aggregate sample window closed";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/aggregate-sample-window/index/window-state
                                    leaf window-state {
                                        type enumeration {
                                            enum completed                      { value 1; }
                                            enum in-progress                    { value 2; }
                                            enum sw-reported                    { value 3; }
                                            enum asw-reported                   { value 4; }
                                            enum terminated                     { value 5; }
                                        }
                                        description "Indicates the state of this aggregate sample window";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/aggregate-sample-window/index/sample-window-count
                                    leaf sample-window-count {
                                        type uint32;
                                        description
                                            "Indicates the number of sample windows that have completed and met their integrity
                                             value within this aggregate sample window";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/aggregate-sample-window/index/min
                                    leaf min {
                                        type uint32;
                                        units "microseconds";
                                        description
                                            "Indicates the minimum delay measurement reported to the aggregate sample window (by the
                                             sample windows that have been open during its lifetime)";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/aggregate-sample-window/index/max
                                    leaf max {
                                        type uint32;
                                        units "microseconds";
                                        description
                                            "Indicates the maximum delay measurement reported to the aggregate sample window (by the
                                             sample windows that have been open during its lifetime)";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/aggregate-sample-window/index/avg
                                    leaf avg {
                                        type uint32;
                                        units "microseconds";
                                        description
                                            "Indicates the average delay measurement reported to the aggregate sample window (by the
                                             sample windows that have been open during its lifetime)";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/aggregate-sample-window/index/integrity
                                    leaf integrity {
                                        type boolean;
                                        description
                                            "Indicates whether a sufficient percentage of this aggregate sample window's child
                                             sample windows met their configured integrity value";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/aggregate-sample-window/index/result
                                    leaf result {
                                        type uint32;
                                        units "microseconds";
                                        description
                                            "The delay that was being evaluated for report according to
                                             /test-oam/link-measurement/measurement-template/delay when this aggregate sample window
                                             was open";
                                    }

                                } // list index

                            } // container aggregate-sample-window

                            ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window
                            container sample-window {
                                description "Enter the sample-window context";

                                ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index
                                list index {
                                    key "index";
                                    description "Enter the index list instance";

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/index
                                    leaf index {
                                        type uint32 {
                                            range "1..4294967295";
                                        }
                                        description
                                            "Specifies an index used to differentiate sample windows on the same interface over
                                             time";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/end-timestamp-utc
                                    leaf end-timestamp-utc {
                                        type yang:date-and-time {
                                            length "8";
                                        }
                                        description "Indicates the time (UTC) at which this sample window closed";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/window-state
                                    leaf window-state {
                                        type enumeration {
                                            enum completed                      { value 1; }
                                            enum in-progress                    { value 2; }
                                            enum sw-reported                    { value 3; }
                                            enum terminated                     { value 4; }
                                        }
                                        description "Indicates the state of this sample window";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/sent
                                    leaf sent {
                                        type uint32;
                                        description "Indicates the number of TWAMP packets that have been sent for this sample window";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/received
                                    leaf received {
                                        type uint32;
                                        description "Indicates the number of TWAMP packets that have been received for this sample window";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/min
                                    leaf min {
                                        type uint32;
                                        units "microseconds";
                                        description
                                            "Indicates the minimum of the series of delay measurements that occurred during this
                                             sample window.";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/max
                                    leaf max {
                                        type uint32;
                                        units "microseconds";
                                        description
                                            "Indicates the maximum of the series of delay measurements that occurred during this
                                             sample window";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/avg
                                    leaf avg {
                                        type uint32;
                                        units "microseconds";
                                        description
                                            "Indicates the average of the series of delay measurements that occurred during this
                                             sample window";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/errors
                                    leaf errors {
                                        type yang:counter32;
                                        description "Indicates the number of erroneous delay measurements that occurred in this sample window";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/integrity
                                    leaf integrity {
                                        type boolean;
                                        description
                                            "Indicates whether the percentage of successful request-response pairs of test packets
                                             met this sample window's configured integrity value";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/result
                                    leaf result {
                                        type uint32;
                                        units "microseconds";
                                        description
                                            "The delay that was being evaluated for report according to
                                             /test-oam/link-measurement/measurement-template/delay when this sample window was open";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/stamp-u-flag
                                    leaf stamp-u-flag {
                                        type uint32;
                                        description
                                            "Indicates the count of packets in this sample window with the U (Unrecognized) bit set
                                             in the flags field of the TLV.";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/stamp-m-flag
                                    leaf stamp-m-flag {
                                        type uint32;
                                        description
                                            "Indicates the count of packets in this sample window with the M (Malformed) bit set in
                                             the flags field of the TLV.";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/zero-or-negative-delay-packets
                                    leaf zero-or-negative-delay-packets {
                                        type uint32;
                                        description
                                            "Indicates the count of packets that have a zero or negative computed delay that have
                                             arrived during this sample window.";
                                    }

                                    ///nokia-state/state/test-oam/link-measurement/router/interface/statistics/sample-window/index/duplicate-packets
                                    leaf duplicate-packets {
                                        type uint32;
                                        description "Indicates the count of duplicate packets that have arrived during this sample window.";
                                    }

                                } // list index

                            } // container sample-window

                        } // container statistics

                    } // list interface

                } // list router

            } // container link-measurement

            ///nokia-state/state/test-oam/twamp
            container twamp {
                description "Enter the twamp context";

                ///nokia-state/state/test-oam/twamp/server
                container server {
                    description "Enable the server context";

                    ///nokia-state/state/test-oam/twamp/server/statistics
                    container statistics {
                        description "Enter the statistics context";

                        ///nokia-state/state/test-oam/twamp/server/statistics/session-count
                        leaf session-count {
                            type uint32 {
                                range "0..128";
                            }
                            description "The number of currently in-progress sessions";
                        }

                        ///nokia-state/state/test-oam/twamp/server/statistics/session-completed
                        leaf session-completed {
                            type yang:counter32;
                            description "The number of test sessions completed";
                        }

                        ///nokia-state/state/test-oam/twamp/server/statistics/session-rejected
                        leaf session-rejected {
                            type yang:counter32;
                            description "The number of test sessions rejected";
                        }

                        ///nokia-state/state/test-oam/twamp/server/statistics/test-packet-received
                        leaf test-packet-received {
                            type yang:counter32;
                            description "The number of TWAMP test packets received";
                        }

                        ///nokia-state/state/test-oam/twamp/server/statistics/test-packet-transmitted
                        leaf test-packet-transmitted {
                            type yang:counter32;
                            description "The number of TWAMP test packets sent";
                        }

                        ///nokia-state/state/test-oam/twamp/server/statistics/oper-state
                        leaf oper-state {
                            type enumeration {
                                enum unknown                        { value 1; }
                                enum in-service                     { value 2; }
                                enum out-of-service                 { value 3; }
                                enum transition                     { value 4; }
                            }
                            description "State of TWAMP server";
                        }

                        ///nokia-state/state/test-oam/twamp/server/statistics/up-time
                        leaf up-time {
                            type uint32;
                            units "seconds";
                            description "Time elapsed since the most recent transition";
                        }

                        ///nokia-state/state/test-oam/twamp/server/statistics/connection-count
                        leaf connection-count {
                            type uint32 {
                                range "0..64";
                            }
                            description "Control connections currently managed by the TWAMP server";
                        }

                        ///nokia-state/state/test-oam/twamp/server/statistics/connections-rejected
                        leaf connections-rejected {
                            type yang:counter32;
                            description "Control connection requests which have been rejected by the TWAMP server";
                        }

                        ///nokia-state/state/test-oam/twamp/server/statistics/capabilities
                        leaf capabilities {
                            type bits {
                                bit unauthenticated                { position 0; }
                                bit authenticated                  { position 1; }
                                bit encrypted                      { position 2; }
                                bit unauthTestEncryptControl       { position 3; }
                                bit individualSessionControl       { position 4; }
                                bit reflectOctets                  { position 5; }
                                bit symmetricalSizeSenderTestPkt   { position 6; }
                                bit ikeV2DerivedMode               { position 7; }
                                bit dscpAndEcnMonitoring           { position 8; }
                            }
                            description "The set of standard TWAMP protocol options supported by the TWAMP server";
                        }

                        ///nokia-state/state/test-oam/twamp/server/statistics/sessions-aborted
                        leaf sessions-aborted {
                            type yang:counter32;
                            description "Test sessions aborted by the TWAMP server";
                        }

                    } // container statistics

                    ///nokia-state/state/test-oam/twamp/server/prefix
                    list prefix {
                        key "ip-prefix";
                        description "Enter the prefix list instance";

                        ///nokia-state/state/test-oam/twamp/server/prefix/ip-prefix
                        leaf ip-prefix {
                            type types-sros:ip-unicast-prefix;
                            description "Prefix to match against TWAMP client address";
                        }

                        ///nokia-state/state/test-oam/twamp/server/prefix/statistics
                        container statistics {
                            description "Enter the statistics context";

                            ///nokia-state/state/test-oam/twamp/server/prefix/statistics/session-count
                            leaf session-count {
                                type uint32 {
                                    range "0..128";
                                }
                                description "The number of currently in-progress sessions";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/statistics/session-completed
                            leaf session-completed {
                                type yang:counter32;
                                description "The number of test sessions completed";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/statistics/session-rejected
                            leaf session-rejected {
                                type yang:counter32;
                                description "The number of test sessions rejected";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/statistics/test-packet-received
                            leaf test-packet-received {
                                type yang:counter32;
                                description "The number of TWAMP test packets received";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/statistics/test-packet-transmitted
                            leaf test-packet-transmitted {
                                type yang:counter32;
                                description "The number of TWAMP test packets sent";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/statistics/sessions-aborted
                            leaf sessions-aborted {
                                type yang:counter32;
                                description "Test sessions aborted by the TWAMP server for the prefix";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/statistics/connection-count
                            leaf connection-count {
                                type uint32 {
                                    range "0..64";
                                }
                                description "The number of control connections currently managed by the TWAMP server for the prefix";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/statistics/connections-rejected
                            leaf connections-rejected {
                                type yang:counter32;
                                description "Control connection requests which have been rejected by the TWAMP server for the prefix";
                            }

                        } // container statistics

                        ///nokia-state/state/test-oam/twamp/server/prefix/connection-statistics
                        list connection-statistics {
                            key "client-address sequence-number";
                            description "Enter the connection-statistics list instance";

                            ///nokia-state/state/test-oam/twamp/server/prefix/connection-statistics/client-address
                            leaf client-address {
                                type types-sros:ip-address;
                                description "The address of the specified session's TWAMP Session-Sender";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/connection-statistics/sequence-number
                            leaf sequence-number {
                                type uint32 {
                                    range "1..4294967295";
                                }
                                description "Control connection's sequence number";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/connection-statistics/session-count
                            leaf session-count {
                                type uint32 {
                                    range "0..128";
                                }
                                description "The number of currently in-progress sessions";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/connection-statistics/session-completed
                            leaf session-completed {
                                type yang:counter32;
                                description "The number of test sessions completed";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/connection-statistics/session-rejected
                            leaf session-rejected {
                                type yang:counter32;
                                description "The number of test sessions rejected";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/connection-statistics/test-packet-received
                            leaf test-packet-received {
                                type yang:counter32;
                                description "The number of TWAMP test packets received";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/connection-statistics/test-packet-transmitted
                            leaf test-packet-transmitted {
                                type yang:counter32;
                                description "The number of TWAMP test packets sent";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/connection-statistics/oper-state
                            leaf oper-state {
                                type enumeration {
                                    enum setting-up                     { value 1; }
                                    enum ready                          { value 2; }
                                    enum running                        { value 3; }
                                }
                                description "State of a control connection managed by the TWAMP server";
                            }

                            ///nokia-state/state/test-oam/twamp/server/prefix/connection-statistics/idle-time
                            leaf idle-time {
                                type uint32;
                                units "seconds";
                                description "Elapsed time since a TWAMP message was received on this control connection";
                            }

                        } // list connection-statistics

                    } // list prefix

                    ///nokia-state/state/test-oam/twamp/server/client
                    list client {
                        key "client-address connection-sequence-number session-sequence-number";
                        description "Enter the client list instance";

                        ///nokia-state/state/test-oam/twamp/server/client/client-address
                        leaf client-address {
                            type types-sros:ip-address;
                            description "The address of the specified session's TWAMP Session-Sender.";
                        }

                        ///nokia-state/state/test-oam/twamp/server/client/connection-sequence-number
                        leaf connection-sequence-number {
                            type uint32 {
                                range "1..4294967295";
                            }
                            description "Control connection's sequence number";
                        }

                        ///nokia-state/state/test-oam/twamp/server/client/session-sequence-number
                        leaf session-sequence-number {
                            type uint32 {
                                range "1..4294967295";
                            }
                            description "TWAMP test session's sequence number";
                        }

                        ///nokia-state/state/test-oam/twamp/server/client/session-id
                        leaf session-id {
                            type binary {
                                length "16";
                            }
                            description "The session identifier (SID) for the specified session";
                        }

                        ///nokia-state/state/test-oam/twamp/server/client/oper-state
                        leaf oper-state {
                            type enumeration {
                                enum create                         { value 1; }
                                enum active                         { value 2; }
                                enum stop                           { value 3; }
                            }
                            description "State of the specified session";
                        }

                        ///nokia-state/state/test-oam/twamp/server/client/sender-address
                        leaf sender-address {
                            type types-sros:ip-unicast-address;
                            description "The address of the specified session's TWAMP Session-Sender";
                        }

                        ///nokia-state/state/test-oam/twamp/server/client/sender-udp-port
                        leaf sender-udp-port {
                            type types-sros:tcp-udp-port;
                            description
                                "The source UDP port field of test packets sent by the specified
                                 session's TWAMP Session-Sender";
                        }

                        ///nokia-state/state/test-oam/twamp/server/client/reflector-address
                        leaf reflector-address {
                            type types-sros:ip-unicast-address;
                            description "The address of the specified session's TWAMP Session-Reflector";
                        }

                        ///nokia-state/state/test-oam/twamp/server/client/reflector-udp-port
                        leaf reflector-udp-port {
                            type types-sros:tcp-udp-port;
                            description "The UDP listen port of the specified session's TWAMP Session-Reflector";
                        }

                    } // list client

                } // container server

                ///nokia-state/state/test-oam/twamp/twamp-light
                container twamp-light {
                    description "Enable the twamp-light context";

                    ///nokia-state/state/test-oam/twamp/twamp-light/source-udp-port-pools
                    container source-udp-port-pools {
                        description "Enter the source-udp-port-pools context";

                        ///nokia-state/state/test-oam/twamp/twamp-light/source-udp-port-pools/port
                        list port {
                            key "port-number";
                            description "Enter the port list instance";

                            ///nokia-state/state/test-oam/twamp/twamp-light/source-udp-port-pools/port/port-number
                            leaf port-number {
                                type types-sros:tcp-udp-port {
                                    range "64374..64383";
                                }
                                description "Source UDP port pools port number";
                            }

                            ///nokia-state/state/test-oam/twamp/twamp-light/source-udp-port-pools/port/statistics
                            container statistics {
                                description "Enter the statistics context";

                                ///nokia-state/state/test-oam/twamp/twamp-light/source-udp-port-pools/port/statistics/in-use
                                leaf in-use {
                                    type boolean;
                                    description
                                        "Indicates if the mapped source UDP port is in use under
                                         the application.";
                                }

                            } // container statistics

                        } // list port

                    } // container source-udp-port-pools

                } // container twamp-light

            } // container twamp

            uses state-testoam-service-activation-testhead;
        } // container test-oam

    } // grouping state-test-oam

}
