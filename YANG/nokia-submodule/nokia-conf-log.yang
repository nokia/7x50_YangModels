                            description "Enter the aa-sub-counters context";

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/aa-sub-counters/long-duration-flow-count
                            leaf long-duration-flow-count {
                                type boolean;
                                default "false";
                                description "Include the long duration flow count";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/aa-sub-counters/medium-duration-flow-count
                            leaf medium-duration-flow-count {
                                type boolean;
                                default "false";
                                description "Include the medium duration flow";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/aa-sub-counters/short-duration-flow-count
                            leaf short-duration-flow-count {
                                type boolean;
                                default "false";
                                description "Include the short duration flow count";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/aa-sub-counters/total-flow-duration
                            leaf total-flow-duration {
                                type boolean;
                                default "false";
                                description "Include the total flow duration count";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/aa-sub-counters/total-flows-completed-count
                            leaf total-flows-completed-count {
                                type boolean;
                                default "false";
                                description "Include the total of completed flows count";
                            }

                        } // container aa-sub-counters

                        ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/from-aa-sub-counters
                        container from-aa-sub-counters {
                            description "Enter the from-aa-sub-counters context";

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/from-aa-sub-counters/flows-active-count
                            leaf flows-active-count {
                                type boolean;
                                default "false";
                                description "Include the active flows";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/from-aa-sub-counters/flows-admitted-count
                            leaf flows-admitted-count {
                                type boolean;
                                default "false";
                                description "Include the admitted flows";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/from-aa-sub-counters/flows-denied-count
                            leaf flows-denied-count {
                                type boolean;
                                default "false";
