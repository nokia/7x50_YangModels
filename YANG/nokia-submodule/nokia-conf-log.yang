                        description "Enter the aa-specific context";

                        ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/aa-sub-attributes
                        container aa-sub-attributes {
                            description "Enter the aa-sub-attributes context";

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/aa-sub-attributes/app-profile
                            leaf app-profile {
                                type boolean;
                                default "false";
                                description "Include application profile to be exported";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/aa-sub-attributes/app-service-options
                            leaf app-service-options {
                                type boolean;
                                default "false";
                                description "Include application service options";
                            }

                        } // container aa-sub-attributes

                        ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/aa-sub-counters
