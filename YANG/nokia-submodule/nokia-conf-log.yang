                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/from-aa-sub-counters/max-throughput-octet-count
                            leaf max-throughput-octet-count {
                                type boolean;
                                default "false";
                                description "Include maximum throughput as measured in octet count";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/from-aa-sub-counters/max-throughput-packet-count
                            leaf max-throughput-packet-count {
                                type boolean;
                                default "false";
                                description "Include maximum throughput as measured in packet count";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/from-aa-sub-counters/max-throughput-timestamp
                            leaf max-throughput-timestamp {
                                type boolean;
                                default "false";
                                description "Include timestamp of the maximum throughput";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/from-aa-sub-counters/octets-admitted-count
                            leaf octets-admitted-count {
                                type boolean;
                                default "false";
                                description "Include the admitted octets count";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/from-aa-sub-counters/octets-denied-count
                            leaf octets-denied-count {
                                type boolean;
                                default "false";
                                description "Include the denied octets count";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/from-aa-sub-counters/packets-admitted-count
                            leaf packets-admitted-count {
                                type boolean;
                                default "false";
                                description "Include the admitted packets count";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/from-aa-sub-counters/packets-denied-count
                            leaf packets-denied-count {
                                type boolean;
                                default "false";
                                description "Include the denied packets count";
                            }

                        } // container from-aa-sub-counters

                        ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/to-aa-sub-counters
                        container to-aa-sub-counters {
                            description "Enter the to-aa-sub-counters context";

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/to-aa-sub-counters/flows-active-count
                            leaf flows-active-count {
                                type boolean;
                                default "false";
                                description "Include the active flows";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/to-aa-sub-counters/flows-admitted-count
                            leaf flows-admitted-count {
                                type boolean;
                                default "false";
                                description "Include the admitted flows";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/to-aa-sub-counters/flows-denied-count
                            leaf flows-denied-count {
                                type boolean;
                                default "false";
                                description "Include the denied flows";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/to-aa-sub-counters/forwarding-class
                            leaf forwarding-class {
                                type boolean;
                                default "false";
                                description "Include forwarding class";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/to-aa-sub-counters/max-throughput-octet-count
                            leaf max-throughput-octet-count {
                                type boolean;
                                default "false";
                                description "Include maximum throughput as measured in octet count";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/to-aa-sub-counters/max-throughput-packet-count
                            leaf max-throughput-packet-count {
                                type boolean;
                                default "false";
                                description "Include maximum throughput as measured in packet count";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/to-aa-sub-counters/max-throughput-timestamp
                            leaf max-throughput-timestamp {
                                type boolean;
                                default "false";
                                description "Include timestamp of the maximum throughput";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/to-aa-sub-counters/octets-admitted-count
                            leaf octets-admitted-count {
                                type boolean;
                                default "false";
                                description "Include the admitted octets count";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/to-aa-sub-counters/octets-denied-count
                            leaf octets-denied-count {
                                type boolean;
                                default "false";
                                description "Include the denied octets count";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/to-aa-sub-counters/packets-admitted-count
                            leaf packets-admitted-count {
                                type boolean;
                                default "false";
                                description "Include the admitted packets count";
                            }

                            ///nokia-conf/configure/log/accounting-policy/custom-record/aa-specific/to-aa-sub-counters/packets-denied-count
                            leaf packets-denied-count {
                                type boolean;
                                default "false";
                                description "Include the denied packets count";
                            }

                        } // container to-aa-sub-counters

                    } // container aa-specific

                    ///nokia-conf/configure/log/accounting-policy/custom-record/override-counter
                    list override-counter {
                        status obsolete;
                        key "id";
                        description "Enter the override-counter list instance";

                        ///nokia-conf/configure/log/accounting-policy/custom-record/override-counter/id
                        leaf id {
                            status obsolete;
                            type uint32 {
                                range "1..8";
                            }
                            description "Override counter ID";
                        }

                        ///nokia-conf/configure/log/accounting-policy/custom-record/override-counter/apply-groups
                        leaf-list apply-groups {
                            status obsolete;
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
                            max-elements 8;
                            ordered-by user;
                            description "Apply a configuration group at this level";
                        }

                        ///nokia-conf/configure/log/accounting-policy/custom-record/override-counter/apply-groups-exclude
                        leaf-list apply-groups-exclude {
                            status obsolete;
                            type leafref {
                                path "../../../../../groups/group/name";
                            }
