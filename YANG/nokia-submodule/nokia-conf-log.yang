                        container e-counters {
                            status obsolete;
                            description "Enter the e-counters context";

                            ///nokia-conf/configure/log/accounting-policy/custom-record/override-counter/e-counters/in-profile-octets-discarded-count
                            leaf in-profile-octets-discarded-count {
                                status obsolete;
