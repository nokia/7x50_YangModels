                    description "Enter the remote-management context";

                    ///reset/system/management-interface/remote-management/managers
                    container managers {
                        description "Enter the managers context";

                        ///reset/system/management-interface/remote-management/managers/all
                        action all {
                            description "Reset connections for all remote NISH managers";

                            output {

                                ///reset/system/management-interface/remote-management/managers/all/output/operation-id
                                leaf operation-id {
                                    type types-operation:operation-id;
                                    description "ID assigned to the operation.";
