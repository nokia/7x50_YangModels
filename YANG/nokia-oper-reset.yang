    ///reset
    container reset {
        config false;
        description "Clear statistics or reset operational state";

        ///reset/lag
        list lag {
            key "lag-name";
            description "Enter the lag list instance";

            ///reset/lag/lag-name
            leaf lag-name {
                type leafref {
                    path "/state:state/state:lag/state:lag-name";
                }
                description "LAG name, starting with lag-";
            }

            ///reset/lag/statistics
            action statistics {
                description "Reset LAG statistics";

                output {

                    ///reset/lag/statistics/output/operation-id
                    leaf operation-id {
                        type types-operation:operation-id;
                        description "ID assigned to the operation.";
                    }

                    ///reset/lag/statistics/output/start-time
                    leaf start-time {
                        type types-operation:operation-timestamp;
                        description "Time when the operation started its execution.";
                    }

                    ///reset/lag/statistics/output/results-path
                    leaf results-path {
                        type types-operation:operation-path;
                        description "Path to the results of the operation formatted as xpath.";
                    }

                    ///reset/lag/statistics/output/status
                    leaf status {
                        type types-operation:operation-status;
                        description "Status of the operation";
                    }

                    ///reset/lag/statistics/output/error-message
                    leaf-list error-message {
                        type types-operation:operation-message;
                        description "Error messages reported during execution of the operation.";
                    }

                    ///reset/lag/statistics/output/warning-message
                    leaf-list warning-message {
                        type types-operation:operation-message;
                        description "Warning messages reported during execution of the operation.";
                    }

                    ///reset/lag/statistics/output/info-message
                    leaf-list info-message {
                        type types-operation:operation-message;
                        description "Info messages reported during execution of the operation.";
                    }

                    ///reset/lag/statistics/output/end-time
                    leaf end-time {
                        type types-operation:operation-timestamp;
                        description "Time when the operation ended its execution.";
                    }

                }
            } // action statistics

            ///reset/lag/lacp-statistics
            action lacp-statistics {
                description "Reset LACP statistics";

                output {

                    ///reset/lag/lacp-statistics/output/operation-id
                    leaf operation-id {
                        type types-operation:operation-id;
                        description "ID assigned to the operation.";
                    }

                    ///reset/lag/lacp-statistics/output/start-time
