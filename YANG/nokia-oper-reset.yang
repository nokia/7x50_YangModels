            container host-lockout {
                description "Enter the host-lockout context";

                action policy {
                    description "Reset the policy related host lockout entries";

                    input {

                        leaf name {
                            type types-sros:named-item;
                            mandatory true;
                            description "Policy name";
                        }

                        leaf state {
                            type enumeration {
                                enum all                            { value 0; }
                                enum active                         { value 1; }
                                enum graceful                       { value 2; }
                            }
                            mandatory true;
                            description "Lockout state to match for hosts";
                        }

                    }
                    output {

                        leaf operation-id {
                            type types-operation:operation-id;
                            description "ID assigned to the operation.";
                        }

                        leaf start-time {
                            type types-operation:operation-timestamp;
                            description "Time when the operation started its execution.";
