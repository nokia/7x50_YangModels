    typedef dms-deployment-model {
        type enumeration {
            enum centralized                    { value 0; }
            enum inline                         { value 1; }
        }
        description "DMS advanced counter-measures group network deployment model.";
    }

    typedef esa-vm-oper-flags-type {
        type bits {
            bit esa-vm-down                    { position 0; }
            bit group-admin-down               { position 1; }
            bit config-in-progress             { position 2; }
            bit pxc-down                       { position 3; }
        }
        description "DMS advanced counter-measures group esa-vm failure types.";
    }

    typedef genome-list-import-conditions-type {
        type bits {
            bit admin-down                     { position 0; }
            bit http-client-prof-error         { position 1; }
            bit http-client-url-open-failed    { position 2; }
            bit http-client-read-failed        { position 3; }
            bit json-parsing-error             { position 4; }
            bit json-version-error             { position 5; }
            bit json-tags-error                { position 6; }
            bit json-no-extracted-ips          { position 7; }
            bit ipv4-trie-limit-exceeded       { position 8; }
            bit ipv6-trie-limit-exceeded       { position 9; }
            bit ip-addr-limit-exceeded         { position 10; }
            bit http-client-file-too-large     { position 11; }
        }
        description "Genome list import condition types.";
    }

    typedef match-operator {
        type enumeration {
