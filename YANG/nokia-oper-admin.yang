                ///admin/satellite/ethernet-satellite/synchronize
                action synchronize {
                    description "Synchronize the chassis to the boot image";

                    output {

                        ///admin/satellite/ethernet-satellite/synchronize/output/operation-id
                        leaf operation-id {
                            type types-operation:operation-id;
                            description "ID assigned to the operation.";
                        }

                        ///admin/satellite/ethernet-satellite/synchronize/output/start-time
                        leaf start-time {
                            type types-operation:operation-timestamp;
