    typedef configuration-region {
        type enumeration {
            enum configure                      { value 0; }
            enum li                             { value 1; }
        }
        default "configure";
        description "The configuration region specify region which is used";
    }

    typedef cli-path-type {
        type string;
        description "Path type used for path infrastructure";
    }

    ///admin
    container admin {
        config false;
        description "Enter the administrative context for system operations";

        ///admin/disconnect
        action disconnect {
            description "Disconnect a user session";

            input {

                ///admin/disconnect/input/op-table-bypass
                leaf op-table-bypass {
                    type types-operation:operation-op-table-bypass;
                    description "Avoid operation ID allocation";
                }

                ///admin/disconnect/input/address
                leaf address {
                    type types-sros:ip-address;
                    description "IP address of the session to disconnect";
                }

                ///admin/disconnect/input/session-id
                leaf session-id {
                    type uint32 {
                        range "1..4294967295";
                    }
                    description "ID of the session to disconnect";
                }

                ///admin/disconnect/input/session-type
                leaf session-type {
                    type enumeration {
                        enum console                        { value 1; }
                        enum bluetooth                      { value 2; }
                        enum telnet                         { value 3; }
                        enum ssh                            { value 4; }
                        enum ftp                            { value 5; }
                        enum netconf                        { value 6; }
                        enum grpc                           { value 7; }
                        enum cron-ehs                       { value 8; }
                    }
                    description "Type of session to disconnect";
                }

                ///admin/disconnect/input/username
                leaf username {
                    type types-sros:named-item;
                    description "Username to disconnect";
                }

            }
            output {

                ///admin/disconnect/output/operation-id
                leaf operation-id {
                    type types-operation:operation-id;
                    description "ID assigned to the operation.";
                }

                ///admin/disconnect/output/start-time
                leaf start-time {
                    type types-operation:operation-timestamp;
                    description "Time when the operation started its execution.";
                }

                ///admin/disconnect/output/results-path
                leaf results-path {
                    type types-operation:operation-path;
                    description "Path to the results of the operation formatted as xpath.";
                }

                ///admin/disconnect/output/status
                leaf status {
                    type types-operation:operation-status;
                    description "Status of the operation";
                }

                ///admin/disconnect/output/error-message
                leaf-list error-message {
                    type types-operation:operation-message;
                    description "Error messages reported during execution of the operation.";
                }

                ///admin/disconnect/output/warning-message
                leaf-list warning-message {
                    type types-operation:operation-message;
                    description "Warning messages reported during execution of the operation.";
                }

                ///admin/disconnect/output/info-message
                leaf-list info-message {
                    type types-operation:operation-message;
                    description "Info messages reported during execution of the operation.";
                }

                ///admin/disconnect/output/end-time
                leaf end-time {
                    type types-operation:operation-timestamp;
                    description "Time when the operation ended its execution.";
                }

            }
        } // action disconnect

        ///admin/reboot
        action reboot {
            description "Reboot CPM or force an upgrade of system boot ROMs";

            input {

                ///admin/reboot/input/now
                leaf now {
                    type empty;
                    description "Reboot immediately without prompts or confirmation";
                }

                ///admin/reboot/input/card
                leaf card {
                    type enumeration {
                        enum active                         { value 1; }
                        enum standby                        { value 2; }
                        enum upgrade                        { value 3; }
                    }
                    description "Card to reboot";
                }

                ///admin/reboot/input/hold
                leaf hold {
                    type empty;
                    description "Hold a rebooted standby CPM from coming back online";
                }

            }
            output {

                ///admin/reboot/output/operation-id
                leaf operation-id {
                    type types-operation:operation-id;
                    description "ID assigned to the operation.";
                }

                ///admin/reboot/output/start-time
                leaf start-time {
                    type types-operation:operation-timestamp;
                    description "Time when the operation started its execution.";
                }

                ///admin/reboot/output/results-path
                leaf results-path {
                    type types-operation:operation-path;
                    description "Path to the results of the operation formatted as xpath.";
                }

                ///admin/reboot/output/status
                leaf status {
                    type types-operation:operation-status;
                    description "Status of the operation";
                }

                ///admin/reboot/output/error-message
                leaf-list error-message {
                    type types-operation:operation-message;
                    description "Error messages reported during execution of the operation.";
                }

                ///admin/reboot/output/warning-message
                leaf-list warning-message {
                    type types-operation:operation-message;
                    description "Warning messages reported during execution of the operation.";
                }

                ///admin/reboot/output/info-message
                leaf-list info-message {
                    type types-operation:operation-message;
                    description "Info messages reported during execution of the operation.";
                }

                ///admin/reboot/output/end-time
                leaf end-time {
                    type types-operation:operation-timestamp;
                    description "Time when the operation ended its execution.";
                }

            }
        } // action reboot

        ///admin/application-assurance
        container application-assurance {
            description "Enter the application-assurance context";

            ///admin/application-assurance/upgrade
            action upgrade {
                description "Upgrade to a new isa-aa.tim file";

                output {

                    ///admin/application-assurance/upgrade/output/operation-id
                    leaf operation-id {
                        type types-operation:operation-id;
                        description "ID assigned to the operation.";
                    }

                    ///admin/application-assurance/upgrade/output/start-time
                    leaf start-time {
                        type types-operation:operation-timestamp;
                        description "Time when the operation started its execution.";
                    }

                    ///admin/application-assurance/upgrade/output/results-path
                    leaf results-path {
                        type types-operation:operation-path;
                        description "Path to the results of the operation formatted as xpath.";
                    }

                    ///admin/application-assurance/upgrade/output/status
                    leaf status {
                        type types-operation:operation-status;
                        description "Status of the operation";
                    }

                    ///admin/application-assurance/upgrade/output/error-message
                    leaf-list error-message {
                        type types-operation:operation-message;
                        description "Error messages reported during execution of the operation.";
                    }

                    ///admin/application-assurance/upgrade/output/warning-message
                    leaf-list warning-message {
                        type types-operation:operation-message;
                        description "Warning messages reported during execution of the operation.";
                    }

                    ///admin/application-assurance/upgrade/output/info-message
                    leaf-list info-message {
                        type types-operation:operation-message;
                        description "Info messages reported during execution of the operation.";
                    }

                    ///admin/application-assurance/upgrade/output/end-time
                    leaf end-time {
                        type types-operation:operation-timestamp;
                        description "Time when the operation ended its execution.";
                    }

                }
            } // action upgrade

            ///admin/application-assurance/group
            list group {
                key "aa-group-id";
                description "Enter the group list instance";

                ///admin/application-assurance/group/aa-group-id
                leaf aa-group-id {
                    type leafref {
                        path "/state:state/state:application-assurance/state:group/state:aa-group-id";
                    }
                    description "AA group ID";
                }

                ///admin/application-assurance/group/url-list
                list url-list {
                    key "url-list-name";
                    description "Enter the url-list list instance";

                    ///admin/application-assurance/group/url-list/url-list-name
                    leaf url-list-name {
                        type leafref {
                            path "/state:state/state:application-assurance/state:group[state:aa-group-id=current()/../../aa-group-id]/state:url-list/state:url-list-name";
                        }
                        description "AA URL list name";
                    }

                    ///admin/application-assurance/group/url-list/upgrade
                    action upgrade {
                        description "Upgrade the referenced URL list";

                        output {

                            ///admin/application-assurance/group/url-list/upgrade/output/operation-id
                            leaf operation-id {
                                type types-operation:operation-id;
                                description "ID assigned to the operation.";
                            }

                            ///admin/application-assurance/group/url-list/upgrade/output/start-time
                            leaf start-time {
                                type types-operation:operation-timestamp;
                                description "Time when the operation started its execution.";
                            }

                            ///admin/application-assurance/group/url-list/upgrade/output/results-path
                            leaf results-path {
                                type types-operation:operation-path;
                                description "Path to the results of the operation formatted as xpath.";
                            }

                            ///admin/application-assurance/group/url-list/upgrade/output/status
                            leaf status {
                                type types-operation:operation-status;
                                description "Status of the operation";
                            }

                            ///admin/application-assurance/group/url-list/upgrade/output/error-message
                            leaf-list error-message {
                                type types-operation:operation-message;
                                description "Error messages reported during execution of the operation.";
                            }

                            ///admin/application-assurance/group/url-list/upgrade/output/warning-message
                            leaf-list warning-message {
                                type types-operation:operation-message;
                                description "Warning messages reported during execution of the operation.";
                            }

                            ///admin/application-assurance/group/url-list/upgrade/output/info-message
                            leaf-list info-message {
                                type types-operation:operation-message;
                                description "Info messages reported during execution of the operation.";
                            }

                            ///admin/application-assurance/group/url-list/upgrade/output/end-time
                            leaf end-time {
                                type types-operation:operation-timestamp;
                                description "Time when the operation ended its execution.";
                            }

                        }
                    } // action upgrade

                } // list url-list

            } // list group

        } // container application-assurance

        ///admin/clear
        container clear {
            description "Enter the clear context";

            ///admin/clear/security
            container security {
                description "Enter the security context";

                ///admin/clear/security/lockout
                action lockout {
                    description "Reset the lockout timer";

                    input {

                        ///admin/clear/security/lockout/input/lockout-options
                        choice lockout-options {
                            mandatory true;
                            ///admin/clear/security/lockout/input/lockout-options/user-name
                            case user-name {

                                ///admin/clear/security/lockout/input/lockout-options/user-name/user
                                leaf user {
                                    type types-sros:named-item;
                                    description "User to be cleared of lockout";
                                }

                            }
                            ///admin/clear/security/lockout/input/lockout-options/all
                            case all {

                                ///admin/clear/security/lockout/input/lockout-options/all/all
                                leaf all {
                                    type empty;
                                    description "Clear lockout of all users";
                                }

                            }
                        }
                    }
                    output {

                        ///admin/clear/security/lockout/output/operation-id
                        leaf operation-id {
                            type types-operation:operation-id;
                            description "ID assigned to the operation.";
                        }

                        ///admin/clear/security/lockout/output/start-time
                        leaf start-time {
                            type types-operation:operation-timestamp;
                            description "Time when the operation started its execution.";
                        }

                        ///admin/clear/security/lockout/output/results-path
                        leaf results-path {
                            type types-operation:operation-path;
                            description "Path to the results of the operation formatted as xpath.";
                        }

                        ///admin/clear/security/lockout/output/status
                        leaf status {
                            type types-operation:operation-status;
                            description "Status of the operation";
                        }

                        ///admin/clear/security/lockout/output/error-message
                        leaf-list error-message {
                            type types-operation:operation-message;
                            description "Error messages reported during execution of the operation.";
                        }

                        ///admin/clear/security/lockout/output/warning-message
                        leaf-list warning-message {
                            type types-operation:operation-message;
                            description "Warning messages reported during execution of the operation.";
                        }

                        ///admin/clear/security/lockout/output/info-message
                        leaf-list info-message {
                            type types-operation:operation-message;
                            description "Info messages reported during execution of the operation.";
                        }

                        ///admin/clear/security/lockout/output/end-time
                        leaf end-time {
                            type types-operation:operation-timestamp;
                            description "Time when the operation ended its execution.";
                        }

                    }
                } // action lockout

                ///admin/clear/security/password-history
                action password-history {
                    description "Clear the password history";

                    input {

                        ///admin/clear/security/password-history/input/password-history-options
                        choice password-history-options {
                            mandatory true;
                            ///admin/clear/security/password-history/input/password-history-options/user-name
                            case user-name {

                                ///admin/clear/security/password-history/input/password-history-options/user-name/user
                                leaf user {
                                    type types-sros:named-item;
                                    description "User to be cleared of password history information";
                                }

                            }
                            ///admin/clear/security/password-history/input/password-history-options/all
                            case all {

                                ///admin/clear/security/password-history/input/password-history-options/all/all
                                leaf all {
                                    type empty;
                                    description "Clear password history of all users";
                                }

                            }
                        }
                    }
                    output {

                        ///admin/clear/security/password-history/output/operation-id
                        leaf operation-id {
                            type types-operation:operation-id;
                            description "ID assigned to the operation.";
                        }

                        ///admin/clear/security/password-history/output/start-time
                        leaf start-time {
                            type types-operation:operation-timestamp;
                            description "Time when the operation started its execution.";
                        }

                        ///admin/clear/security/password-history/output/results-path
                        leaf results-path {
                            type types-operation:operation-path;
                            description "Path to the results of the operation formatted as xpath.";
                        }

                        ///admin/clear/security/password-history/output/status
                        leaf status {
                            type types-operation:operation-status;
                            description "Status of the operation";
                        }

                        ///admin/clear/security/password-history/output/error-message
                        leaf-list error-message {
                            type types-operation:operation-message;
                            description "Error messages reported during execution of the operation.";
                        }

                        ///admin/clear/security/password-history/output/warning-message
                        leaf-list warning-message {
                            type types-operation:operation-message;
                            description "Warning messages reported during execution of the operation.";
                        }

                        ///admin/clear/security/password-history/output/info-message
                        leaf-list info-message {
                            type types-operation:operation-message;
                            description "Info messages reported during execution of the operation.";
                        }

                        ///admin/clear/security/password-history/output/end-time
                        leaf end-time {
                            type types-operation:operation-timestamp;
                            description "Time when the operation ended its execution.";
                        }

                    }
                } // action password-history

            } // container security

        } // container clear

        ///admin/nat
        container nat {
            description "Enter the nat context";

            ///admin/nat/save-deterministic-script
            action save-deterministic-script {
                description "Save script that computes deterministic NAT map entries";

                output {

                    ///admin/nat/save-deterministic-script/output/operation-id
                    leaf operation-id {
                        type types-operation:operation-id;
                        description "ID assigned to the operation.";
                    }

                    ///admin/nat/save-deterministic-script/output/start-time
                    leaf start-time {
                        type types-operation:operation-timestamp;
