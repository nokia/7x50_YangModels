                        description "Error messages reported during execution of the operation.";
                    }

                    ///admin/redundancy/force-switchover/output/warning-message
                    leaf-list warning-message {
                        type types-operation:operation-message;
                        description "Warning messages reported during execution of the operation.";
                    }

                    ///admin/redundancy/force-switchover/output/info-message
                    leaf-list info-message {
                        type types-operation:operation-message;
                        description "Info messages reported during execution of the operation.";
                    }

                    ///admin/redundancy/force-switchover/output/end-time
                    leaf end-time {
                        type types-operation:operation-timestamp;
                        description "Time when the operation ended its execution.";
                    }

                }
            } // action force-switchover

            ///admin/redundancy/synchronize
            action synchronize {
                description "Synchronize the standby CPM";

                input {

                    ///admin/redundancy/synchronize/input/synchronize
                    choice synchronize {
                        mandatory true;
                        ///admin/redundancy/synchronize/input/synchronize/boot-environment
                        case boot-environment {

                            ///admin/redundancy/synchronize/input/synchronize/boot-environment/boot-environment
                            leaf boot-environment {
                                type empty;
                                description "Synchronize all files required for the boot process";
                            }

                        }
                        ///admin/redundancy/synchronize/input/synchronize/configuration
                        case configuration {

                            ///admin/redundancy/synchronize/input/synchronize/configuration/configuration
                            leaf configuration {
                                type empty;
                                description "Synchronize the configuration files";
                            }

                        }
                        ///admin/redundancy/synchronize/input/synchronize/certificate
                        case certificate {

                            ///admin/redundancy/synchronize/input/synchronize/certificate/certificate
                            leaf certificate {
                                type empty;
                                description "Synchronize imported certificate, key, and CRL files";
                            }

                        }
                    }
                }
                output {

                    ///admin/redundancy/synchronize/output/operation-id
                    leaf operation-id {
                        type types-operation:operation-id;
                        description "ID assigned to the operation.";
                    }

                    ///admin/redundancy/synchronize/output/start-time
                    leaf start-time {
                        type types-operation:operation-timestamp;
                        description "Time when the operation started its execution.";
                    }

                    ///admin/redundancy/synchronize/output/results-path
                    leaf results-path {
                        type types-operation:operation-path;
