                                                    ///global-operations/oam/lsp-trace/sr-ospf/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/fec-stack-entry/operation-type
                                                    leaf operation-type {
                                                        type enumeration {
                                                            enum push                           { value 1; }
                                                            enum pop                            { value 2; }
