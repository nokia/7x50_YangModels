                                                    }

                                                    ///global-operations/oam/lsp-trace/sr-ospf/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/fec-stack-entry/sub-type
                                                    leaf sub-type {
                                                        type types-sros:named-item-or-empty;
                                                        description "The value of Subtype field in RFC 4379 FEC-type-specific sub-TLV";
                                                    }

                                                    ///global-operations/oam/lsp-trace/sr-ospf/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/fec-stack-entry/prefix
                                                    leaf prefix {
                                                        type types-sros:ip-address;
                                                        description
                                                            "The value of FEC-type-specific IP prefix (or IP address) field
                                                             in RFC 6424 FEC Stack Change sub-TLV";
                                                    }

                                                    ///global-operations/oam/lsp-trace/sr-ospf/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/fec-stack-entry/remote-peer-address
                                                    leaf remote-peer-address {
