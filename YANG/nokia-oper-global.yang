                    description "Perform an SR-TE LSP ping test";

                    input {

                        ///global-operations/oam/lsp-ping/sr-te/input/lsp-name
                        leaf lsp-name {
                            type leafref {
                                path "/state:state/state:router/state:mpls/state:lsp/state:lsp-name";
                                require-instance "false";
                            }
                            mandatory true;
                            description "Name of the LSP to test";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/input/path
                        leaf path {
                            type leafref {
                                path "/state:state/state:router/state:mpls/state:path/state:path-name";
                                require-instance "false";
                            }
                            description "Name of the MPLS path to test";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/input/path-destination
                        leaf path-destination {
                            type types-sros:ip-address;
                            description "IP address of the path destination";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/input/path-destination-egress
                        choice path-destination-egress {
                            ///global-operations/oam/lsp-ping/sr-te/input/path-destination-egress/case-interface
                            case case-interface {

                                ///global-operations/oam/lsp-ping/sr-te/input/path-destination-egress/case-interface/interface
                                leaf interface {
                                    type leafref {
                                        path "/state:state/state:router/state:interface/state:interface-name";
                                        require-instance "false";
                                    }
                                    description "Egress router interface to use with path destination";
                                }

                            }
                            ///global-operations/oam/lsp-ping/sr-te/input/path-destination-egress/case-next-hop
                            case case-next-hop {

                                ///global-operations/oam/lsp-ping/sr-te/input/path-destination-egress/case-next-hop/next-hop
                                leaf next-hop {
                                    type types-sros:ip-address;
                                    description "Egress IP next hop address to use with path destination";
                                }

                            }
                        }
                        ///global-operations/oam/lsp-ping/sr-te/input/source-ip-address
                        leaf source-ip-address {
                            type types-sros:ip-address;
                            description "Source IP address";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/input/interval
                        leaf interval {
                            type uint32 {
                                range "1..10";
                            }
                            units "seconds";
                            default "1";
                            description "Minimum time to expire before next echo request is sent";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/input/size
                        leaf size {
                            type uint32 {
                                range "1..9786";
                            }
                            units "bytes";
                            default "1";
                            description "Echo request packet pad size";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/input/fc
                        leaf fc {
                            type types-sros:fc-name;
                            default "be";
                            description "QoS forwarding class for the sent echo request packet";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/input/profile
                        leaf profile {
                            type types-qos:profile-in-out;
                            default "out";
                            description "QoS profile for the transmitted echo request packets";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/input/timeout
                        leaf timeout {
                            type uint32 {
                                range "1..10";
                            }
                            units "seconds";
                            default "5";
                            description "Time to wait for a reply after sending an echo request";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/input/ttl
                        leaf ttl {
                            type uint32 {
                                range "1..255";
                            }
                            default "255";
                            description "TTL value for the MPLS label of the echo request";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/input/send-count
                        leaf send-count {
                            type uint32 {
                                range "1..100";
                            }
                            units "packets";
                            default "1";
                            description "Number of echo request packets to send";
                        }

                    }
                    output {

                        ///global-operations/oam/lsp-ping/sr-te/output/operation-id
                        leaf operation-id {
                            type types-operation:operation-id;
                            description "ID assigned to the operation.";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/output/start-time
                        leaf start-time {
                            type types-operation:operation-timestamp;
                            description "Time when the operation started its execution.";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/output/results-path
                        leaf results-path {
                            type types-operation:operation-path;
                            description "Path to the results of the operation formatted as xpath.";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/output/results
                        container results {
                            description "Enter the results context";

                            ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters
                            container test-parameters {
                                description "Enter the test-parameters context";

                                ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/lsp-name
                                leaf lsp-name {
                                    type types-sros:named-item-64;
                                    description "The name of the LSP to be tested";
                                }

                                ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/path
                                leaf path {
                                    type types-sros:named-item-64;
                                    description "The name of the MPLS path to be tested";
                                }

                                ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/path-destination
                                leaf path-destination {
                                    type types-sros:ip-address;
                                    description "The IP address of the path destination";
                                }

                                ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/path-destination-egress
                                choice path-destination-egress {
                                    ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/path-destination-egress/case-interface
                                    case case-interface {

                                        ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/path-destination-egress/case-interface/interface
                                        leaf interface {
                                            type types-sros:interface-name;
                                            description "The egress router interface, used in conjunction with path-destination";
                                        }

                                    }
                                    ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/path-destination-egress/case-next-hop
                                    case case-next-hop {

                                        ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/path-destination-egress/case-next-hop/next-hop
                                        leaf next-hop {
                                            type types-sros:ip-address;
                                            description "The egress IP next hop address, used in conjunction with path-destination";
                                        }

                                    }
                                }
                                ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/source-ip-address
                                leaf source-ip-address {
                                    type types-sros:ip-address;
                                    description "Source IP address";
                                }

                                ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/interval
                                leaf interval {
                                    type uint32 {
                                        range "1..10";
                                    }
                                    units "seconds";
                                    description "Minimum time to expire before the next request is sent";
                                }

                                ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/size
                                leaf size {
                                    type uint32 {
                                        range "1..9786";
                                    }
                                    units "bytes";
                                    description "Packet pad size of request";
                                }

                                ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/fc
                                leaf fc {
                                    type types-sros:fc-name;
                                    description "QoS Forwarding Class for the transmitted request packet(s)";
                                }

                                ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/profile
                                leaf profile {
                                    type types-qos:profile-in-out;
                                    description "QoS profile for the transmitted request packet(s)";
                                }

                                ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/timeout
                                leaf timeout {
                                    type uint32 {
                                        range "1..10";
                                    }
                                    units "seconds";
                                    description "Time router waits for a reply after sending a request";
                                }

                                ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/ttl
                                leaf ttl {
                                    type uint32 {
                                        range "1..255";
                                    }
                                    description "TTL value for the request's MPLS label";
                                }

                                ///global-operations/oam/lsp-ping/sr-te/output/results/test-parameters/send-count
                                leaf send-count {
                                    type uint32 {
                                        range "1..100";
                                    }
                                    units "packets";
                                    description "Number of request packets to send";
                                }

                            } // container test-parameters

                            ///global-operations/oam/lsp-ping/sr-te/output/results/probe
                            list probe {
                                key "probe-index";
                                description "Probe data for the specified test run";

                                ///global-operations/oam/lsp-ping/sr-te/output/results/probe/probe-index
                                leaf probe-index {
                                    type uint32 {
                                        range "1..max";
                                    }
                                    description "Probe index number";
                                }

                                ///global-operations/oam/lsp-ping/sr-te/output/results/probe/egress-interface
                                leaf egress-interface {
                                    type types-sros:interface-name;
                                    description "Indicates the interface name this probe was transmitted from";
                                }

                                ///global-operations/oam/lsp-ping/sr-te/output/results/probe/round-trip-time
                                leaf round-trip-time {
                                    type uint32;
                                    units "microseconds";
                                    description "Indicates the round trip time for the probe";
                                }

                                ///global-operations/oam/lsp-ping/sr-te/output/results/probe/status
                                leaf status {
                                    type types-oam:response-status;
                                    description "Indicates the OAM response status code for the probe";
                                }

                                ///global-operations/oam/lsp-ping/sr-te/output/results/probe/response-packet
                                container response-packet {
                                    description "Data from packet received as a response to the transmitted probe";

                                    ///global-operations/oam/lsp-ping/sr-te/output/results/probe/response-packet/size
                                    leaf size {
                                        type uint32;
                                        units "bytes";
                                        description "Indicates the number of bytes in the response packet for the specified probe";
                                    }

                                    ///global-operations/oam/lsp-ping/sr-te/output/results/probe/response-packet/source-address
                                    leaf source-address {
                                        type types-sros:ip-address;
                                        description "Indicates the source address in the response packet for the specified probe";
                                    }

                                    ///global-operations/oam/lsp-ping/sr-te/output/results/probe/response-packet/sequence-number
                                    leaf sequence-number {
                                        type uint32;
                                        description
                                            "Indicates the sequence number in the response packet for the
                                             specified probe";
                                    }

                                    ///global-operations/oam/lsp-ping/sr-te/output/results/probe/response-packet/return-code
                                    leaf return-code {
                                        type int32;
                                        description "Indicates the OAM return code received in the probe response";
                                    }

                                    ///global-operations/oam/lsp-ping/sr-te/output/results/probe/response-packet/return-code-string
                                    leaf return-code-string {
                                        type types-oam:lsp-ping-trace-return-code;
                                        description "Description of the OAM return code";
                                    }

                                } // container response-packet

                            } // list probe

                            ///global-operations/oam/lsp-ping/sr-te/output/results/summary
                            container summary {
                                description "Test run summary data";

                                ///global-operations/oam/lsp-ping/sr-te/output/results/summary/statistics
                                container statistics {
                                    description "Test run summary statistics";

                                    ///global-operations/oam/lsp-ping/sr-te/output/results/summary/statistics/packets
                                    container packets {
                                        description "Packet statistics for the specified test run";

                                        ///global-operations/oam/lsp-ping/sr-te/output/results/summary/statistics/packets/sent
                                        leaf sent {
                                            type uint32;
                                            units "packets";
                                            description "Indicates the number of packets sent for the specified test run";
                                        }

                                        ///global-operations/oam/lsp-ping/sr-te/output/results/summary/statistics/packets/received
                                        leaf received {
                                            type uint32;
                                            units "packets";
                                            description "Indicates the number of packets received for the specified test run";
                                        }

                                        ///global-operations/oam/lsp-ping/sr-te/output/results/summary/statistics/packets/loss
                                        leaf loss {
                                            type decimal64 {
                                                range "0.00..100.00";
                                                fraction-digits 2;
                                            }
                                            units "percent";
                                            description "Indicates the number of packet loss percentage for the specified test run";
                                        }

                                    } // container packets

                                    ///global-operations/oam/lsp-ping/sr-te/output/results/summary/statistics/round-trip-time
                                    container round-trip-time {
                                        description "Test run round trip time statistics";

                                        ///global-operations/oam/lsp-ping/sr-te/output/results/summary/statistics/round-trip-time/minimum
                                        leaf minimum {
                                            type uint32;
                                            units "microseconds";
                                            description "Indicates the minimum round trip time for the specified test run";
                                        }

                                        ///global-operations/oam/lsp-ping/sr-te/output/results/summary/statistics/round-trip-time/average
                                        leaf average {
                                            type uint32;
                                            units "microseconds";
                                            description "Indicates the average round trip time for the specified test run";
                                        }

                                        ///global-operations/oam/lsp-ping/sr-te/output/results/summary/statistics/round-trip-time/maximum
                                        leaf maximum {
                                            type uint32;
                                            units "microseconds";
                                            description "Indicates the maximum round trip time for the specified test run";
                                        }

                                        ///global-operations/oam/lsp-ping/sr-te/output/results/summary/statistics/round-trip-time/standard-deviation
                                        leaf standard-deviation {
                                            type uint32;
                                            units "microseconds";
                                            description
                                                "Indicates the standard deviation for the round trip times for the
                                                 specified test run";
                                        }

                                    } // container round-trip-time

                                } // container statistics

                            } // container summary

                        } // container results

                        ///global-operations/oam/lsp-ping/sr-te/output/status
                        leaf status {
                            type types-operation:operation-status;
                            description "Status of the operation";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/output/error-message
                        leaf-list error-message {
                            type types-operation:operation-message;
                            description "Error messages reported during execution of the operation.";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/output/warning-message
                        leaf-list warning-message {
                            type types-operation:operation-message;
                            description "Warning messages reported during execution of the operation.";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/output/info-message
                        leaf-list info-message {
                            type types-operation:operation-message;
                            description "Info messages reported during execution of the operation.";
                        }

                        ///global-operations/oam/lsp-ping/sr-te/output/end-time
                        leaf end-time {
                            type types-operation:operation-timestamp;
                            description "Time when the operation ended its execution.";
                        }

                    }
                } // action sr-te

            } // container lsp-ping

            ///global-operations/oam/lsp-trace
            container lsp-trace {
                description "Perform a Label Switched Path (LSP) trace test";

