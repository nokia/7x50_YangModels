module nokia-oper-global {

    yang-version "1.1";

    namespace "urn:nokia.com:sros:ns:yang:sr:oper-global";

    prefix "oper-global";

    import nokia-conf                     { prefix "conf"; }
    import nokia-sros-yang-extensions     { prefix "sros-ext"; }
    import nokia-state                    { prefix "state"; }
    import nokia-types-eth-cfm            { prefix "types-eth-cfm"; }
    import nokia-types-oam                { prefix "types-oam"; }
    import nokia-types-operation          { prefix "types-operation"; }
    import nokia-types-qos                { prefix "types-qos"; }
    import nokia-types-sros               { prefix "types-sros"; }
    import nokia-types-subscriber-mgmt    { prefix "types-subscriber-mgmt"; }

    sros-ext:sros-major-release "rel23";
    organization "Nokia";
    contact
        "Nokia SR OS Support
         Web: <http://www.nokia.com>";
    description
        "Nokia YANG Data model for Nokia global operations on SR OS based routers.

         Copyright 2016 Nokia. All rights reserved. Reproduction of this document
         is authorized on the condition that the foregoing copyright notice is included.

         This nokia-oper-global YANG module embodies Nokia's proprietary
         intellectual property. Nokia retains all title and ownership in the
         specification, including any revisions.

         Nokia grants all interested parties a non-exclusive license to use
         and distribute an unmodified copy of this specification in connection with
         management of Nokia products, and without fee, provided this
         copyright notice and license appear on all copies.

         This specification is supplied `as is', and Nokia makes no warranty,
         either express or implied, as to the use, operation, condition, or performance
         of the specification.";
    revision "2020-10-15";

    ///global-operations
    container global-operations {
        config false;
        description "Global operations";

        ///global-operations/md-cli-raw-command
        action md-cli-raw-command {
            description "Trigger MD-CLI command.";

            input {

                ///global-operations/md-cli-raw-command/input/md-cli-input-line
                leaf md-cli-input-line {
                    type string;
                    mandatory true;
                    description "MD-CLI command to be executed..";
                }

            }
            output {

                ///global-operations/md-cli-raw-command/output/operation-id
                leaf operation-id {
                    type types-operation:operation-id;
                    description "ID assigned to the operation.";
                }

                ///global-operations/md-cli-raw-command/output/start-time
                leaf start-time {
                    type types-operation:operation-timestamp;
                    description "Time when the operation started its execution.";
                }

                ///global-operations/md-cli-raw-command/output/results-path
                leaf results-path {
                    type types-operation:operation-path;
                    description "Path to the results of the operation formatted as xpath.";
                }

                ///global-operations/md-cli-raw-command/output/results
                container results {
                    description "Enter the results context";

                    ///global-operations/md-cli-raw-command/output/results/md-cli-output-block
                    leaf md-cli-output-block {
                        type string;
                        description "Output of executed MD-CLI command.";
                    }

                } // container results

                ///global-operations/md-cli-raw-command/output/status
                leaf status {
                    type types-operation:operation-status;
                    description "Status of the operation";
                }

                ///global-operations/md-cli-raw-command/output/error-message
                leaf-list error-message {
                    type types-operation:operation-message;
                    description "Error messages reported during execution of the operation.";
                }

                ///global-operations/md-cli-raw-command/output/warning-message
                leaf-list warning-message {
                    type types-operation:operation-message;
                    description "Warning messages reported during execution of the operation.";
                }

                ///global-operations/md-cli-raw-command/output/info-message
                leaf-list info-message {
                    type types-operation:operation-message;
                    description "Info messages reported during execution of the operation.";
                }

                ///global-operations/md-cli-raw-command/output/end-time
                leaf end-time {
                    type types-operation:operation-timestamp;
                    description "Time when the operation ended its execution.";
                }

            }
        } // action md-cli-raw-command

        ///global-operations/md-compare
        action md-compare {
            description "Trigger md-compare command.";

            input {

                ///global-operations/md-compare/input/path
                container path {
                    description "This parameter contains the path to the target node to compare.";

                    ///global-operations/md-compare/input/path/path-type
                    choice path-type {
                        ///global-operations/md-compare/input/path/path-type/subtree-path
                        case subtree-path {

                            ///global-operations/md-compare/input/path/path-type/subtree-path/subtree-path
                            anyxml subtree-path {
                                description " The source datastore for the compare operation.";

                            } // anyxml subtree-path

                        }
                    }
                } // container path

                ///global-operations/md-compare/input/configuration-region
                leaf configuration-region {
                    type types-sros:configuration-region;
                    description "This parameter contains the configuration region to operate on.";
                }

                ///global-operations/md-compare/input/source
                container source {
                    description " The source datastore for the compare operation.";

                    ///global-operations/md-compare/input/source/destination-type
                    choice destination-type {
                        ///global-operations/md-compare/input/source/destination-type/candidate
                        case candidate {

                            ///global-operations/md-compare/input/source/destination-type/candidate/candidate
                            leaf candidate {
                                type empty;
                            }

                        }
                        ///global-operations/md-compare/input/source/destination-type/baseline
                        case baseline {

                            ///global-operations/md-compare/input/source/destination-type/baseline/baseline
                            leaf baseline {
                                type empty;
                            }

                        }
                        ///global-operations/md-compare/input/source/destination-type/url
                        case url {

                            ///global-operations/md-compare/input/source/destination-type/url/url
                            leaf url {
                                type string;
                            }

                        }
                        ///global-operations/md-compare/input/source/destination-type/running
                        case running {

                            ///global-operations/md-compare/input/source/destination-type/running/running
                            leaf running {
                                type empty;
                            }

                        }
                        ///global-operations/md-compare/input/source/destination-type/startup
                        case startup {

                            ///global-operations/md-compare/input/source/destination-type/startup/startup
                            leaf startup {
                                type empty;
                            }

                        }
                        ///global-operations/md-compare/input/source/destination-type/booted
                        case booted {

                            ///global-operations/md-compare/input/source/destination-type/booted/booted
                            leaf booted {
                                type empty;
                            }

                        }
                        ///global-operations/md-compare/input/source/destination-type/rollback
                        case rollback {
                            status obsolete;

                            ///global-operations/md-compare/input/source/destination-type/rollback/rollback
                            container rollback {
                                status obsolete;
                                description "Enter the rollback context";

                                ///global-operations/md-compare/input/source/destination-type/rollback/rollback/checkpoint-id
                                leaf checkpoint-id {
                                    status obsolete;
                                    type uint32;
                                }

                            } // container rollback

                        }
                        ///global-operations/md-compare/input/source/destination-type/rollback-id
                        case rollback-id {

                            ///global-operations/md-compare/input/source/destination-type/rollback-id/rollback-id
                            leaf rollback-id {
                                type uint32 {
                                    range "0..200";
                                }
                            }

                        }
                        ///global-operations/md-compare/input/source/destination-type/commit-id
                        case commit-id {

                            ///global-operations/md-compare/input/source/destination-type/commit-id/commit-id
                            leaf commit-id {
                                type uint32 {
                                    range "1..max";
                                }
                            }

                        }
                    }
                } // container source

                ///global-operations/md-compare/input/destination
                container destination {
                    description "The destination datastore for the compare operation.";

                    ///global-operations/md-compare/input/destination/destination-type
                    choice destination-type {
                        ///global-operations/md-compare/input/destination/destination-type/candidate
                        case candidate {

                            ///global-operations/md-compare/input/destination/destination-type/candidate/candidate
                            leaf candidate {
                                type empty;
                            }

                        }
                        ///global-operations/md-compare/input/destination/destination-type/baseline
                        case baseline {

                            ///global-operations/md-compare/input/destination/destination-type/baseline/baseline
                            leaf baseline {
                                type empty;
                            }

                        }
                        ///global-operations/md-compare/input/destination/destination-type/url
                        case url {

                            ///global-operations/md-compare/input/destination/destination-type/url/url
                            leaf url {
                                type string;
                            }

                        }
                        ///global-operations/md-compare/input/destination/destination-type/running
                        case running {

                            ///global-operations/md-compare/input/destination/destination-type/running/running
                            leaf running {
                                type empty;
                            }

                        }
                        ///global-operations/md-compare/input/destination/destination-type/startup
                        case startup {

                            ///global-operations/md-compare/input/destination/destination-type/startup/startup
                            leaf startup {
                                type empty;
                            }

                        }
                        ///global-operations/md-compare/input/destination/destination-type/booted
                        case booted {

                            ///global-operations/md-compare/input/destination/destination-type/booted/booted
                            leaf booted {
                                type empty;
                            }

                        }
                        ///global-operations/md-compare/input/destination/destination-type/rollback
                        case rollback {
                            status obsolete;

                            ///global-operations/md-compare/input/destination/destination-type/rollback/rollback
                            container rollback {
                                status obsolete;
                                description "Enter the rollback context";

                                ///global-operations/md-compare/input/destination/destination-type/rollback/rollback/checkpoint-id
                                leaf checkpoint-id {
                                    status obsolete;
                                    type uint32;
                                }

                            } // container rollback

                        }
                        ///global-operations/md-compare/input/destination/destination-type/rollback-id
                        case rollback-id {

                            ///global-operations/md-compare/input/destination/destination-type/rollback-id/rollback-id
                            leaf rollback-id {
                                type uint32 {
                                    range "0..200";
                                }
                            }

                        }
                        ///global-operations/md-compare/input/destination/destination-type/commit-id
                        case commit-id {

                            ///global-operations/md-compare/input/destination/destination-type/commit-id/commit-id
                            leaf commit-id {
                                type uint32 {
                                    range "1..max";
                                }
                            }

                        }
                    }
                } // container destination

                ///global-operations/md-compare/input/format
                leaf format {
                    type enumeration {
                        enum xml                            { value 0; }
                        enum md-cli                         { value 1; }
                    }
                    default "xml";
                    description "RPC output format tag.";
                }

            }
            output {

                ///global-operations/md-compare/output/operation-id
                leaf operation-id {
                    type types-operation:operation-id;
                    description "ID assigned to the operation.";
                }

                ///global-operations/md-compare/output/start-time
                leaf start-time {
                    type types-operation:operation-timestamp;
                    description "Time when the operation started its execution.";
                }

                ///global-operations/md-compare/output/results-path
                leaf results-path {
                    type types-operation:operation-path;
                    description "Path to the results of the operation formatted as xpath.";
                }

                ///global-operations/md-compare/output/results
                container results {
                    description "Enter the results context";

                    ///global-operations/md-compare/output/results/md-compare-output
                    leaf md-compare-output {
                        type string;
                        description "Output of executed md-compare command.";
                    }

                } // container results

                ///global-operations/md-compare/output/status
                leaf status {
                    type types-operation:operation-status;
                    description "Status of the operation";
                }

                ///global-operations/md-compare/output/error-message
                leaf-list error-message {
                    type types-operation:operation-message;
                    description "Error messages reported during execution of the operation.";
                }

                ///global-operations/md-compare/output/warning-message
                leaf-list warning-message {
                    type types-operation:operation-message;
                    description "Warning messages reported during execution of the operation.";
                }

                ///global-operations/md-compare/output/info-message
                leaf-list info-message {
                    type types-operation:operation-message;
                    description "Info messages reported during execution of the operation.";
                }

                ///global-operations/md-compare/output/end-time
                leaf end-time {
                    type types-operation:operation-timestamp;
                    description "Time when the operation ended its execution.";
                }

            }
        } // action md-compare

        ///global-operations/oam
        container oam {
            description "Enter the oam context for operations, administration and maintenance";

            ///global-operations/oam/eth-cfm
            container eth-cfm {
                description "Enter the Eth-CFM context";

                ///global-operations/oam/eth-cfm/linktrace
                action linktrace {
                    description "Initiate a linktrace test";

                    input {

                        ///global-operations/oam/eth-cfm/linktrace/input/asynchronous
                        leaf asynchronous {
                            type types-operation:operation-asynchronous;
                            description "Controls execution type of the operation.";
                        }

                        ///global-operations/oam/eth-cfm/linktrace/input/retention-timeout
                        leaf retention-timeout {
                            type types-operation:operation-timeout;
                            description "Timeout for operation data retention in global operation table.";
                        }

                        ///global-operations/oam/eth-cfm/linktrace/input/destination
                        leaf destination {
                            type union {
                                type types-sros:mac-unicast-address-no-zero;
                                type types-eth-cfm:mep-id-type;
                            }
                            mandatory true;
                            description "Destination MAC address or remote MEP ID of the peer";
                        }

                        ///global-operations/oam/eth-cfm/linktrace/input/md-admin-name
                        leaf md-admin-name {
                            type leafref {
                                path "/state:state/state:eth-cfm/state:domain/state:md-admin-name";
                            }
                            mandatory true;
                            description "Domain name";
                        }

                        ///global-operations/oam/eth-cfm/linktrace/input/ma-admin-name
                        leaf ma-admin-name {
                            type leafref {
                                path "/state:state/state:eth-cfm/state:domain[state:md-admin-name=current()/../md-admin-name]/state:association/state:ma-admin-name";
                            }
                            mandatory true;
                            description "Association name";
                        }

                        ///global-operations/oam/eth-cfm/linktrace/input/mep-id
                        leaf mep-id {
                            type types-eth-cfm:mep-id-type;
                            mandatory true;
                            description "Local Maintenance Association Endpoint (MEP) ID";
                        }

                        ///global-operations/oam/eth-cfm/linktrace/input/ttl
                        leaf ttl {
                            type uint32 {
                                range "0..255";
                            }
                            default "64";
                            description "Time to live for a returned linktrace";
                        }

                    }
                    output {

                        ///global-operations/oam/eth-cfm/linktrace/output/operation-id
                        leaf operation-id {
                            type types-operation:operation-id;
                            description "ID assigned to the operation.";
                        }

                        ///global-operations/oam/eth-cfm/linktrace/output/start-time
                        leaf start-time {
                            type types-operation:operation-timestamp;
                            description "Time when the operation started its execution.";
                        }

                        ///global-operations/oam/eth-cfm/linktrace/output/results-path
                        leaf results-path {
                            type types-operation:operation-path;
                            description "Path to the results of the operation formatted as xpath.";
                        }

                        ///global-operations/oam/eth-cfm/linktrace/output/status
                        leaf status {
                            type types-operation:operation-status;
                            description "Status of the operation";
                        }

                        ///global-operations/oam/eth-cfm/linktrace/output/error-message
                        leaf-list error-message {
                            type types-operation:operation-message;
                            description "Error messages reported during execution of the operation.";
                        }

                        ///global-operations/oam/eth-cfm/linktrace/output/warning-message
                        leaf-list warning-message {
                            type types-operation:operation-message;
                            description "Warning messages reported during execution of the operation.";
                        }

                        ///global-operations/oam/eth-cfm/linktrace/output/info-message
                        leaf-list info-message {
                            type types-operation:operation-message;
                            description "Info messages reported during execution of the operation.";
                        }

                        ///global-operations/oam/eth-cfm/linktrace/output/end-time
                        leaf end-time {
                            type types-operation:operation-timestamp;
                            description "Time when the operation ended its execution.";
                        }

                    }
                } // action linktrace

                ///global-operations/oam/eth-cfm/loopback
                action loopback {
                    description "Initiate a loopback test";

                    input {

                        ///global-operations/oam/eth-cfm/loopback/input/asynchronous
                        leaf asynchronous {
                            type types-operation:operation-asynchronous;
                            description "Controls execution type of the operation.";
                        }

                        ///global-operations/oam/eth-cfm/loopback/input/execution-timeout
                        leaf execution-timeout {
                            type types-operation:operation-timeout;
                            description "Timeout for operation execution.";
                        }

                        ///global-operations/oam/eth-cfm/loopback/input/retention-timeout
                        leaf retention-timeout {
                            type types-operation:operation-timeout;
                            description "Timeout for operation data retention in global operation table.";
                        }

                        ///global-operations/oam/eth-cfm/loopback/input/destination
                        leaf destination {
                            type union {
                                type types-sros:mac-unicast-address-no-zero;
                                type enumeration {
                                    enum multicast {
                                        value 0;
                                        description "Class one destination multicast address based on the level of the local MEP";
                                    }
                                }
                                type types-eth-cfm:mep-id-type;
                            }
                            mandatory true;
                            description "Destination MAC address or remote MEP ID of the peer";
                        }

                        ///global-operations/oam/eth-cfm/loopback/input/md-admin-name
                        leaf md-admin-name {
                            type leafref {
                                path "/state:state/state:eth-cfm/state:domain/state:md-admin-name";
                            }
                            mandatory true;
                            description "Domain name";
                        }

                        ///global-operations/oam/eth-cfm/loopback/input/ma-admin-name
                        leaf ma-admin-name {
                            type leafref {
                                path "/state:state/state:eth-cfm/state:domain[state:md-admin-name=current()/../md-admin-name]/state:association/state:ma-admin-name";
                            }
                            mandatory true;
                            description "Association name";
                        }

                        ///global-operations/oam/eth-cfm/loopback/input/mep-id
                        leaf mep-id {
                            type types-eth-cfm:mep-id-type;
                            mandatory true;
                            description "Local Maintenance Association Endpoint (MEP) ID";
                        }

                        ///global-operations/oam/eth-cfm/loopback/input/send-count
                        leaf send-count {
                            type int32 {
                                range "1..1024";
                            }
                            default "1";
                            description "Number of loopback messages to send";
                        }

                        ///global-operations/oam/eth-cfm/loopback/input/size
                        leaf size {
                            type uint32 {
                                range "0..1500";
                            }
                            units "bytes";
                            default "0";
                            description "Data portion size of the data TLV, octet string allowed";
                        }

                        ///global-operations/oam/eth-cfm/loopback/input/priority
                        leaf priority {
                            type int32 {
                                range "0..7";
                            }
                            description "Frame priority that can be manipulated by QoS policies";
                        }

                        ///global-operations/oam/eth-cfm/loopback/input/lbm-padding
                        leaf lbm-padding {
                            type uint32 {
                                range "0|3..9778";
                            }
                            default "0";
                            description "Data portion size of the data TLV, no octet string";
                        }

                        ///global-operations/oam/eth-cfm/loopback/input/timeout
                        leaf timeout {
                            type uint32 {
                                range "1..10";
                            }
                            units "seconds";
                            default "5";
                            description "Wait time for a reply to a sent message request";
                        }

                        ///global-operations/oam/eth-cfm/loopback/input/interval
                        leaf interval {
                            type uint32 {
                                range "0..600";
                            }
                            units "deciseconds";
                            default "0";
                            description "Time between probes within the test run";
                        }

                    }
                    output {

                        ///global-operations/oam/eth-cfm/loopback/output/operation-id
                        leaf operation-id {
                            type types-operation:operation-id;
                            description "ID assigned to the operation.";
                        }

                        ///global-operations/oam/eth-cfm/loopback/output/start-time
                        leaf start-time {
                            type types-operation:operation-timestamp;
                            description "Time when the operation started its execution.";
                        }

                        ///global-operations/oam/eth-cfm/loopback/output/results-path
                        leaf results-path {
                            type types-operation:operation-path;
                            description "Path to the results of the operation formatted as xpath.";
                        }

                        ///global-operations/oam/eth-cfm/loopback/output/status
                        leaf status {
                            type types-operation:operation-status;
                            description "Status of the operation";
                        }

                        ///global-operations/oam/eth-cfm/loopback/output/error-message
                        leaf-list error-message {
                            type types-operation:operation-message;
                            description "Error messages reported during execution of the operation.";
                        }

                        ///global-operations/oam/eth-cfm/loopback/output/warning-message
                        leaf-list warning-message {
                            type types-operation:operation-message;
                            description "Warning messages reported during execution of the operation.";
                        }

                        ///global-operations/oam/eth-cfm/loopback/output/info-message
                        leaf-list info-message {
                            type types-operation:operation-message;
                            description "Info messages reported during execution of the operation.";
                        }

                        ///global-operations/oam/eth-cfm/loopback/output/end-time
                        leaf end-time {
                            type types-operation:operation-timestamp;
                            description "Time when the operation ended its execution.";
                        }

                    }
                } // action loopback

            } // container eth-cfm

            ///global-operations/oam/lsp-ping
            container lsp-ping {
                description "Perform a Label Switched Path (LSP) ping test";

                ///global-operations/oam/lsp-ping/bgp-label
                action bgp-label {
                    description "Perform a BGP-label LSP trace test";

                    input {

                        ///global-operations/oam/lsp-ping/bgp-label/input/prefix
                        leaf prefix {
                            type types-sros:ip-prefix;
                            mandatory true;
                            description "Destination IP prefix for the test";
                        }

                        ///global-operations/oam/lsp-ping/bgp-label/input/path-destination
                        leaf path-destination {
                            type types-sros:ip-address;
                            description "IP address of the path destination";
                        }

                        ///global-operations/oam/lsp-ping/bgp-label/input/path-destination-egress
                        choice path-destination-egress {
                            ///global-operations/oam/lsp-ping/bgp-label/input/path-destination-egress/case-interface
                            case case-interface {

                                ///global-operations/oam/lsp-ping/bgp-label/input/path-destination-egress/case-interface/interface
                                leaf interface {
                                    type leafref {
                                        path "/state:state/state:router/state:interface/state:interface-name";
                                        require-instance "false";
                                    }
                                    description "Egress router interface to use with path destination";
                                }

                            }
                            ///global-operations/oam/lsp-ping/bgp-label/input/path-destination-egress/case-next-hop
                            case case-next-hop {

                                ///global-operations/oam/lsp-ping/bgp-label/input/path-destination-egress/case-next-hop/next-hop
                                leaf next-hop {
                                    type types-sros:ip-address;
                                    description "Egress IP next hop address to use with path destination";
                                }

                            }
                        }
                        ///global-operations/oam/lsp-ping/bgp-label/input/source-ip-address
                        leaf source-ip-address {
                            type types-sros:ip-address;
                            description "Source IP address";
                        }

                        ///global-operations/oam/lsp-ping/bgp-label/input/interval
                        leaf interval {
                            type uint32 {
                                range "1..10";
                            }
                            units "seconds";
                            default "1";
                            description "Minimum time to expire before next echo request is sent";
                        }

                        ///global-operations/oam/lsp-ping/bgp-label/input/size
                        leaf size {
                            type uint32 {
                                range "1..9786";
                            }
                            units "bytes";
                            default "1";
                            description "Echo request packet pad size";
                        }

                        ///global-operations/oam/lsp-ping/bgp-label/input/fc
                        leaf fc {
                            type types-sros:fc-name;
                            default "be";
                            description "QoS forwarding class for the sent echo request packet";
                        }

                        ///global-operations/oam/lsp-ping/bgp-label/input/profile
                        leaf profile {
                            type types-qos:profile-in-out;
                            default "out";
                            description "QoS profile for the transmitted echo request packets";
                        }

                        ///global-operations/oam/lsp-ping/bgp-label/input/timeout
                        leaf timeout {
                            type uint32 {
                                range "1..10";
                            }
                            units "seconds";
                            default "5";
                            description "Time to wait for a reply after sending an echo request";
                        }

                        ///global-operations/oam/lsp-ping/bgp-label/input/ttl
                        leaf ttl {
                            type uint32 {
                                range "1..255";
                            }
                            default "255";
                            description "TTL value for the MPLS label of the echo request";
                        }

                        ///global-operations/oam/lsp-ping/bgp-label/input/send-count
                        leaf send-count {
                            type uint32 {
                                range "1..100";
                            }
                            units "packets";
                            default "1";
                            description "Number of echo request packets to send";
                        }

