                                }

                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/path-destination
                                leaf path-destination {
                                    type types-sros:ip-address;
                                    description "The IP address of the path destination";
                                }

                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/path-destination-egress
                                choice path-destination-egress {
                                    ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/path-destination-egress/case-interface
                                    case case-interface {

                                        ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/path-destination-egress/case-interface/interface
                                        leaf interface {
                                            type types-sros:interface-name;
                                            description "The egress router interface, used in conjunction with path-destination";
                                        }

                                    }
                                    ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/path-destination-egress/case-next-hop
                                    case case-next-hop {

                                        ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/path-destination-egress/case-next-hop/next-hop
                                        leaf next-hop {
                                            type types-sros:ip-address;
                                            description "The egress IP next hop address, used in conjunction with path-destination";
                                        }

                                    }
                                }
                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/source-ip-address
                                leaf source-ip-address {
                                    type types-sros:ip-address;
                                    description "Source IP address";
                                }

                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/interval
                                leaf interval {
                                    type uint32 {
                                        range "1..10";
                                    }
                                    units "seconds";
                                    description "Minimum time to expire before the next request is sent";
                                }

                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/size
                                leaf size {
                                    type uint32 {
                                        range "1..9786";
                                    }
                                    units "bytes";
                                    description "Packet pad size of request";
                                }

                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/fc
                                leaf fc {
                                    type types-sros:fc-name;
                                    description "QoS Forwarding Class for the transmitted request packet(s)";
                                }

                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/profile
                                leaf profile {
                                    type types-qos:profile-in-out;
                                    description "QoS profile for the transmitted request packet(s)";
                                }

                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/timeout
                                leaf timeout {
                                    type uint32 {
                                        range "1..10";
                                    }
                                    units "seconds";
                                    description "Time router waits for a reply after sending a request";
                                }

                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/downstream-map-tlv
                                leaf downstream-map-tlv {
                                    type enumeration {
                                        enum dsmap                          { value 1; }
                                        enum ddmap                          { value 2; }
                                        enum none                           { value 3; }
                                    }
                                    description "The type of downstream mapping TLV in MPLS echo request PDU";
                                }

                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/min-ttl
                                leaf min-ttl {
                                    type uint32 {
                                        range "1..255";
                                    }
                                    description "The initial time-to-live value for the test";
                                }

                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/max-ttl
                                leaf max-ttl {
                                    type uint32 {
                                        range "1..255";
                                    }
                                    description "The maximum time-to-live value for the test";
                                }

                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/max-fail
                                leaf max-fail {
                                    type uint32 {
                                        range "1..255";
                                    }
                                    description "The maximum number of consecutive timeouts allowed before terminating test";
                                }

                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/probe-count
                                leaf probe-count {
                                    type uint32 {
                                        range "1..10";
                                    }
                                    description "The number of OAM trace probes to send per TTL";
                                }

