                ///global-operations/oam/lsp-trace/ldp
                action ldp {
                    description "Perform an LDP LSP trace test";

                    input {

                        ///global-operations/oam/lsp-trace/ldp/input/prefix
                        leaf prefix {
                            type types-sros:ip-prefix;
                            mandatory true;
                            description "Destination IP prefix for the test";
                        }

                        ///global-operations/oam/lsp-trace/ldp/input/path-destination
                        leaf path-destination {
                            type types-sros:ip-address;
                            description "IP address of the path destination";
                        }

                        ///global-operations/oam/lsp-trace/ldp/input/path-destination-egress
                        choice path-destination-egress {
                            ///global-operations/oam/lsp-trace/ldp/input/path-destination-egress/case-interface
                            case case-interface {

                                ///global-operations/oam/lsp-trace/ldp/input/path-destination-egress/case-interface/interface
                                leaf interface {
                                    type leafref {
                                        path "/state:state/state:router/state:interface/state:interface-name";
                                        require-instance "false";
                                    }
                                    description "Egress router interface to use with path destination";
                                }

                            }
                            ///global-operations/oam/lsp-trace/ldp/input/path-destination-egress/case-next-hop
                            case case-next-hop {

                                ///global-operations/oam/lsp-trace/ldp/input/path-destination-egress/case-next-hop/next-hop
                                leaf next-hop {
                                    type types-sros:ip-address;
                                    description "Egress IP next hop address to use with path destination";
                                }

                            }
                        }
                        ///global-operations/oam/lsp-trace/ldp/input/source-ip-address
                        leaf source-ip-address {
                            type types-sros:ip-address;
                            description "Source IP address";
                        }

                        ///global-operations/oam/lsp-trace/ldp/input/interval
                        leaf interval {
                            type uint32 {
                                range "1..10";
                            }
                            units "seconds";
                            default "1";
                            description "Minimum time to expire before next echo request is sent";
                        }

                        ///global-operations/oam/lsp-trace/ldp/input/size
                        leaf size {
                            type uint32 {
                                range "1..9786";
                            }
                            units "bytes";
                            default "1";
                            description "Echo request packet pad size";
                        }

                        ///global-operations/oam/lsp-trace/ldp/input/fc
                        leaf fc {
                            type types-sros:fc-name;
                            default "be";
                            description "QoS forwarding class for the sent echo request packet";
                        }

                        ///global-operations/oam/lsp-trace/ldp/input/profile
                        leaf profile {
                            type types-qos:profile-in-out;
                            default "out";
                            description "QoS profile for the transmitted echo request packets";
                        }

                        ///global-operations/oam/lsp-trace/ldp/input/timeout
                        leaf timeout {
                            type uint32 {
                                range "1..10";
                            }
                            units "seconds";
                            default "5";
                            description "Time to wait for a reply after sending an echo request";
                        }

                        ///global-operations/oam/lsp-trace/ldp/input/downstream-map-tlv
                        leaf downstream-map-tlv {
                            type enumeration {
                                enum dsmap {
                                    value 1;
                                    description "Downstream mapping TLV";
                                }
                                enum ddmap {
                                    value 2;
                                    description "Detailed downstream mapping TLV";
                                }
                                enum none {
                                    value 3;
                                    description "No mapping TLV";
                                }
                            }
                            description "Type of downstream mapping TLV in MPLS echo request PDU";
                        }

                        ///global-operations/oam/lsp-trace/ldp/input/min-ttl
                        leaf min-ttl {
                            type uint32 {
                                range "1..255";
                            }
                            default "1";
                            description "Initial TTL for the test";
                        }

                        ///global-operations/oam/lsp-trace/ldp/input/max-ttl
                        leaf max-ttl {
                            type uint32 {
                                range "1..255";
                            }
                            default "30";
                            description "Maximum TTL";
                        }

                        ///global-operations/oam/lsp-trace/ldp/input/max-fail
                        leaf max-fail {
                            type uint32 {
                                range "1..255";
                            }
                            default "5";
                            description "Maximum consecutive timeouts before terminating test";
                        }

                        ///global-operations/oam/lsp-trace/ldp/input/probe-count
                        leaf probe-count {
                            type uint32 {
                                range "1..10";
                            }
                            default "1";
                            description "Number of OAM trace probes to send per TTL";
                        }

                    }
                    output {

                        ///global-operations/oam/lsp-trace/ldp/output/operation-id
                        leaf operation-id {
                            type types-operation:operation-id;
                            description "ID assigned to the operation.";
                        }

                        ///global-operations/oam/lsp-trace/ldp/output/start-time
                        leaf start-time {
                            type types-operation:operation-timestamp;
                            description "Time when the operation started its execution.";
                        }

                        ///global-operations/oam/lsp-trace/ldp/output/results-path
                        leaf results-path {
                            type types-operation:operation-path;
                            description "Path to the results of the operation formatted as xpath.";
                        }

                        ///global-operations/oam/lsp-trace/ldp/output/results
                        container results {
                            description "Enter the results context";

                            ///global-operations/oam/lsp-trace/ldp/output/results/test-parameters
                            container test-parameters {
                                description "Enter the test-parameters context";

                                ///global-operations/oam/lsp-trace/ldp/output/results/test-parameters/prefix
                                leaf prefix {
                                    type types-sros:ip-prefix;
                                    description "The destination IP prefix for the test";
                                }

                                ///global-operations/oam/lsp-trace/ldp/output/results/test-parameters/path-destination
                                leaf path-destination {
                                    type types-sros:ip-address;
