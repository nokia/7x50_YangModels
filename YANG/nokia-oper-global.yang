                            ///global-operations/oam/lsp-trace/sr-policy/input/path-destination-egress/case-next-hop
                            case case-next-hop {

                                ///global-operations/oam/lsp-trace/sr-policy/input/path-destination-egress/case-next-hop/next-hop
