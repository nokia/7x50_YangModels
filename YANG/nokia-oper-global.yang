                                            ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/return-code-string
                                            leaf return-code-string {
                                                type types-oam:lsp-ping-trace-return-code;
                                                description "Description of the return-code";
                                            }

                                            ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/return-subcode
                                            leaf return-subcode {
                                                type uint32;
                                                description
                                                    "Indicates the stack-depth where processing was terminated with
                                                     the value in the return-code";
                                            }

                                            ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv
                                            list downstream-mapping-tlv {
                                                key "mapping-tlv-index";
                                                description
                                                    "Content of the RFC 4379 Downstream Mapping TLV
                                                     (or one RFC 6424 Downstream Detailed Mapping TLV) within
                                                     the reply PDU";

                                                ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/mapping-tlv-index
                                                leaf mapping-tlv-index {
                                                    type uint32;
                                                    description
                                                        "The index value identifies a RFC 4379 Downstream Mapping TLV
                                                         (or one RFC 6424 Downstream Detailed Mapping TLV) within
                                                         the reply PDU";
                                                }

                                                ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/mtu
                                                leaf mtu {
                                                    type uint32;
                                                    description
                                                        "The largest MPLS frame (including the label stack) that fits on the
                                                         interface to the downstream LSR (Label Switching Router).  The value
                                                         from the MTU (Maximum Transmission Unit) field of the specified
                                                         RFC 4379 Downstream Mapping TLV (or RFC 6424 Downstream
                                                         Detailed Mapping TLV)";
                                                }

                                                ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/address-type
                                                leaf address-type {
                                                    type enumeration {
                                                        enum unknown                        { value 0; }
                                                        enum ipv4-numbered                  { value 1; }
                                                        enum ipv6-numbered                  { value 2; }
                                                        enum ipv4-unnumbered                { value 3; }
                                                        enum ipv6-unnumbered                { value 4; }
                                                        enum non-ip                         { value 5; }
                                                    }
                                                    description
                                                        "The value of the Adress Type field of the specified RFC 4379 Downstream
                                                         Mapping TLV or RFC 6424 Downstream Detailed Mapping TLV";
                                                }

                                                ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/ip-address
                                                leaf ip-address {
                                                    type types-sros:ip-address;
                                                    description
                                                        "The Downstream IP Address field in the specified RFC 4379 Downstream Mapping TLV (or the
                                                         Downstream Address field in the specified RFC 6424 Downstream Detailed
                                                         Mapping TLV)";
                                                }

                                                ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/interface-address
                                                leaf interface-address {
                                                    type types-sros:ip-address;
                                                    description
                                                        "The value of the Downstream Interface Address field in the specified
                                                         RFC 4379 Downstream Mapping TLV (or RFC 6424 Downstream Detailed Mapping TLV)";
                                                }

                                                ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/egress-interface
                                                leaf egress-interface {
                                                    type types-sros:interface-name;
                                                    description
                                                        "The egress interface name corresponding to the interface number in the
                                                         Downstream IP Address field of the specified RFC 4379 Downstream Mapping
                                                         TLV (or the RFC 6424 Downstream Address field of the specified RFC 6424
                                                         Downstream Detailed Mapping TLV)";
                                                }

                                                ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/ingress-interface
                                                leaf ingress-interface {
                                                    type types-sros:interface-name;
                                                    description
                                                        "The ingress interface number reported by the replying node.
                                                         This value is copied from the Downstream Interface Address field";
                                                }

                                                ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/mpls-label-stack-entry
                                                list mpls-label-stack-entry {
                                                    key "label-index";
                                                    description "The information of a single downstream label within a returned Downstream L2 Mapping TLV";

                                                    ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/mpls-label-stack-entry/label-index
                                                    leaf label-index {
                                                        type uint32 {
                                                            range "1..max";
                                                        }
                                                        description "The index value identifies a downstream label entry within a specific L2 mapping entry";
                                                    }

                                                    ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/mpls-label-stack-entry/label
                                                    leaf label {
                                                        type int32;
                                                        description
                                                            "The value in the Downstream Label field of the specified RFC 4379 Downstream
                                                             Mapping TLV or RFC 6424 Downstream Detailed Mapping TLV";
                                                    }

                                                    ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/mpls-label-stack-entry/protocol
                                                    leaf protocol {
                                                        type enumeration {
                                                            enum unknown                        { value 0; }
                                                            enum static                         { value 1; }
                                                            enum bgp                            { value 2; }
                                                            enum ldp                            { value 3; }
                                                            enum rsvp-te                        { value 4; }
                                                            enum ospf                           { value 5; }
                                                            enum isis                           { value 6; }
                                                            enum ospf3                          { value 7; }
                                                        }
                                                        description
                                                            "The value in the Protocol field of the specified RFC 4379 Downstream
                                                             Mapping TLV or RFC 6424 Downstream Detailed Mapping TLV";
                                                    }

                                                } // list mpls-label-stack-entry

                                                ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/fec-stack-entry
                                                list fec-stack-entry {
                                                    key "fec-index";
                                                    description
                                                        "Content of RFC 6424 FEC (Forwarding Equivalence Class) Stack
                                                         Change sub-TLV within the specified Downstream Detailed Mapping TLV
                                                         present in the reply PDU";

                                                    ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/fec-stack-entry/fec-index
                                                    leaf fec-index {
                                                        type uint32 {
                                                            range "1..8";
                                                        }
                                                        description "The index value of one RFC 6424 FEC Stack Change sub-TLV";
                                                    }

                                                    ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/fec-stack-entry/operation-type
                                                    leaf operation-type {
                                                        type enumeration {
                                                            enum push                           { value 1; }
                                                            enum pop                            { value 2; }
                                                        }
                                                        description
                                                            "The value of the Operation Type field in
                                                             RFC 6424 FEC Stack Change sub-TLV";
