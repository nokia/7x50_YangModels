                        ///global-operations/oam/lsp-trace/sr-ospf3/output/results
                        container results {
                            description "Enter the results context";

                            ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters
                            container test-parameters {
                                description "Enter the test-parameters context";

                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/prefix
                                leaf prefix {
                                    type types-sros:ip-prefix;
                                    description "The destination IP prefix for the test";
                                }

                                ///global-operations/oam/lsp-trace/sr-ospf3/output/results/test-parameters/igp-instance
                                leaf igp-instance {
                                    type uint32 {
                                        range "0..127";
