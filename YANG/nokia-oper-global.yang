                                                ///global-operations/oam/lsp-trace/sr-ospf/output/results/hop/probe/probe-instance/response-packet/downstream-mapping-tlv/mpls-label-stack-entry
                                                list mpls-label-stack-entry {
                                                    key "label-index";
