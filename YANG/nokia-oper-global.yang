                ///global-operations/oam/lsp-trace/sr-ospf3
                action sr-ospf3 {
                    description "Perform an SR-OSPFv3 LSP trace test";

                    input {

                        ///global-operations/oam/lsp-trace/sr-ospf3/input/prefix
                        leaf prefix {
                            type types-sros:ip-prefix;
                            mandatory true;
                            description "Destination IP prefix for the test";
                        }

                        ///global-operations/oam/lsp-trace/sr-ospf3/input/igp-instance
                        leaf igp-instance {
                            type uint32 {
                                range "0..31|64..95";
                            }
                            default "0";
                            description "IGP instance for the SR-OSPFv3 test";
                        }

                        ///global-operations/oam/lsp-trace/sr-ospf3/input/path-destination
                        leaf path-destination {
                            type types-sros:ip-address;
                            description "IP address of the path destination";
                        }

                        ///global-operations/oam/lsp-trace/sr-ospf3/input/path-destination-egress
                        choice path-destination-egress {
                            ///global-operations/oam/lsp-trace/sr-ospf3/input/path-destination-egress/case-interface
                            case case-interface {

                                ///global-operations/oam/lsp-trace/sr-ospf3/input/path-destination-egress/case-interface/interface
                                leaf interface {
                                    type leafref {
                                        path "/state:state/state:router/state:interface/state:interface-name";
                                        require-instance "false";
                                    }
                                    description "Egress router interface to use with path destination";
                                }

                            }
                            ///global-operations/oam/lsp-trace/sr-ospf3/input/path-destination-egress/case-next-hop
                            case case-next-hop {

                                ///global-operations/oam/lsp-trace/sr-ospf3/input/path-destination-egress/case-next-hop/next-hop
                                leaf next-hop {
                                    type types-sros:ip-address;
                                    description "Egress IP next hop address to use with path destination";
                                }

                            }
                        }
                        ///global-operations/oam/lsp-trace/sr-ospf3/input/source-ip-address
                        leaf source-ip-address {
                            type types-sros:ip-address;
                            description "Source IP address";
                        }

                        ///global-operations/oam/lsp-trace/sr-ospf3/input/interval
                        leaf interval {
                            type uint32 {
                                range "1..10";
                            }
                            units "seconds";
                            default "1";
                            description "Minimum time to expire before next echo request is sent";
                        }

                        ///global-operations/oam/lsp-trace/sr-ospf3/input/size
                        leaf size {
                            type uint32 {
                                range "1..9786";
                            }
                            units "bytes";
                            default "1";
                            description "Echo request packet pad size";
                        }

                        ///global-operations/oam/lsp-trace/sr-ospf3/input/fc
                        leaf fc {
                            type types-sros:fc-name;
                            default "be";
                            description "QoS forwarding class for the sent echo request packet";
                        }

                        ///global-operations/oam/lsp-trace/sr-ospf3/input/profile
                        leaf profile {
                            type types-qos:profile-in-out;
                            default "out";
                            description "QoS profile for the transmitted echo request packets";
                        }

                        ///global-operations/oam/lsp-trace/sr-ospf3/input/timeout
                        leaf timeout {
                            type uint32 {
                                range "1..10";
                            }
                            units "seconds";
                            default "5";
                            description "Time to wait for a reply after sending an echo request";
                        }

                        ///global-operations/oam/lsp-trace/sr-ospf3/input/downstream-map-tlv
                        leaf downstream-map-tlv {
                            type enumeration {
                                enum dsmap {
                                    value 1;
                                    description "Downstream mapping TLV";
                                }
                                enum ddmap {
                                    value 2;
                                    description "Detailed downstream mapping TLV";
                                }
                                enum none {
                                    value 3;
                                    description "No mapping TLV";
                                }
                            }
                            description "Type of downstream mapping TLV in MPLS echo request PDU";
                        }

                        ///global-operations/oam/lsp-trace/sr-ospf3/input/min-ttl
                        leaf min-ttl {
                            type uint32 {
                                range "1..255";
                            }
                            default "1";
                            description "Initial TTL for the test";
                        }

                        ///global-operations/oam/lsp-trace/sr-ospf3/input/max-ttl
                        leaf max-ttl {
                            type uint32 {
                                range "1..255";
                            }
                            default "30";
                            description "Maximum TTL";
                        }

                        ///global-operations/oam/lsp-trace/sr-ospf3/input/max-fail
                        leaf max-fail {
                            type uint32 {
                                range "1..255";
                            }
                            default "5";
                            description "Maximum consecutive timeouts before terminating test";
                        }

                        ///global-operations/oam/lsp-trace/sr-ospf3/input/probe-count
                        leaf probe-count {
                            type uint32 {
                                range "1..10";
                            }
                            default "1";
                            description "Number of OAM trace probes to send per TTL";
                        }

                    }
                    output {

                        ///global-operations/oam/lsp-trace/sr-ospf3/output/operation-id
                        leaf operation-id {
