        ///global-operations/ping
        action ping {
            description "Trigger ping of address or dns-name via machine interface";

            input {

                ///global-operations/ping/input/ping-type
                choice ping-type {
                    mandatory true;
                    ///global-operations/ping/input/ping-type/case-ip-or-dns
                    case case-ip-or-dns {

                        ///global-operations/ping/input/ping-type/case-ip-or-dns/destination
                        leaf destination {
                            type union {
                                type types-sros:ip-address-with-zone;
                                type types-sros:string-not-all-spaces {
                                    length "1..128";
                                }
                            }
                            mandatory true;
                            description "Destination IP address or DNS name";
                        }

                        ///global-operations/ping/input/ping-type/case-ip-or-dns/routing-options
                        choice routing-options {
                            ///global-operations/ping/input/ping-type/case-ip-or-dns/routing-options/case-bypass-routing
                            case case-bypass-routing {

                                ///global-operations/ping/input/ping-type/case-ip-or-dns/routing-options/case-bypass-routing/bypass-routing
                                leaf bypass-routing {
                                    type empty;
                                    description "Bypass routing table when sending ping request to host";
                                }

                            }
                            ///global-operations/ping/input/ping-type/case-ip-or-dns/routing-options/case-interface
                            case case-interface {

                                ///global-operations/ping/input/ping-type/case-ip-or-dns/routing-options/case-interface/interface
                                leaf interface {
                                    type types-sros:interface-name;
                                    description "Sending interface name";
                                }

                            }
                            ///global-operations/ping/input/ping-type/case-ip-or-dns/routing-options/case-next-hop
                            case case-next-hop {

                                ///global-operations/ping/input/ping-type/case-ip-or-dns/routing-options/case-next-hop/next-hop-address
                                leaf next-hop-address {
                                    type types-sros:ip-address;
                                    description "Next-hop address to send packet, ignoring routing table";
                                }

                            }
                            ///global-operations/ping/input/ping-type/case-ip-or-dns/routing-options/case-subscriber
                            case case-subscriber {

                                ///global-operations/ping/input/ping-type/case-ip-or-dns/routing-options/case-subscriber/subscriber
                                leaf subscriber {
                                    type types-subscriber-mgmt:subscriber-id;
                                    description "Subscriber ID for sending ICMP Echo Request packets";
                                }

                            }
                        }
                        ///global-operations/ping/input/ping-type/case-ip-or-dns/router-instance
                        leaf router-instance {
                            type string;
                            default "Base";
                            description "Router name or VPRN service name";
                        }

                        ///global-operations/ping/input/ping-type/case-ip-or-dns/source-address
                        leaf source-address {
                            type types-sros:ip-address;
                            description "Source IP address used in the ICMP Echo Request packet";
                        }

                    }
                    ///global-operations/ping/input/ping-type/case-srv6-policy
                    case case-srv6-policy {

                        ///global-operations/ping/input/ping-type/case-srv6-policy/srv6-policy
                        leaf srv6-policy {
                            type empty;
                            mandatory true;
                            description "Ping a Segment Routing IPv6 policy";
                        }

                        ///global-operations/ping/input/ping-type/case-srv6-policy/color
                        leaf color {
                            type uint32;
                            mandatory true;
                            description "Color identifier of the Segment Routing IPv6 policy";
                        }

                        ///global-operations/ping/input/ping-type/case-srv6-policy/endpoint
                        leaf endpoint {
                            type types-sros:ipv6-unicast-or-zero-address;
                            mandatory true;
                            description "Endpoint identifier of the Segment Routing IPv6 policy to be pinged";
                        }

                        ///global-operations/ping/input/ping-type/case-srv6-policy/segment-list
                        leaf segment-list {
                            type uint32 {
                                range "1..32";
                            }
                            description "Segment Routing IPv6 segment list to be pinged";
                        }

                    }
                }
                ///global-operations/ping/input/count
                leaf count {
                    type uint32 {
                        range "1..100000";
                    }
                    units "packets";
                    default "5";
                    description "Number of ping requests to send to the remote host";
                }

                ///global-operations/ping/input/output-format
                leaf output-format {
                    type enumeration {
                        enum summary                        { value 1; }
                        enum detail                         { value 2; }
                    }
                    default "detail";
                    description "Output format";
                }

                ///global-operations/ping/input/do-not-fragment
                leaf do-not-fragment {
                    type empty;
                    description "Do not fragment the request frame";
                }

                ///global-operations/ping/input/fc
                leaf fc {
                    type types-sros:fc-name;
                    default "nc";
                    description "Forwarding class option for the transmitted packet";
                }

                ///global-operations/ping/input/interval
                leaf interval {
                    type union {
                        type uint32 {
                            range "1..10000";
                        }
                        type decimal64 {
                            range "0.01..100.00";
                            fraction-digits 2;
                        }
                    }
                    units "seconds";
                    default "1";
                    description "Time between consecutive ping requests";
                }

                ///global-operations/ping/input/pattern
                leaf pattern {
                    type union {
                        type enumeration {
                            enum sequential                     { value -1; }
                        }
                        type int32 {
                            range "0..65535";
                        }
                    }
                    default "sequential";
                    description "Pattern string to include in the packet";
                }

                ///global-operations/ping/input/size
                leaf size {
                    type uint32 {
                        range "0..16384";
                    }
                    units "bytes";
                    default "56";
                    description "Request packet size including ICMP header and payload";
                }

                ///global-operations/ping/input/timeout
                leaf timeout {
                    type uint32 {
                        range "1..10";
                    }
                    units "seconds";
                    default "5";
                    description "Wait time for the ICMP Echo Reply packet";
                }

                ///global-operations/ping/input/tos
                leaf tos {
                    type uint32 {
                        range "0..255";
                    }
                    default "0";
                    description "Type of Service (ToS) bits in the IP header";
                }

                ///global-operations/ping/input/ttl
                leaf ttl {
                    type uint32 {
                        range "1..128";
                    }
                    default "64";
                    description "Time to Live (TTL) included in the request packet";
                }

            }
            output {

                ///global-operations/ping/output/operation-id
                leaf operation-id {
                    type types-operation:operation-id;
                    description "ID assigned to the operation.";
                }

                ///global-operations/ping/output/start-time
                leaf start-time {
                    type types-operation:operation-timestamp;
                    description "Time when the operation started its execution.";
                }

                ///global-operations/ping/output/results-path
                leaf results-path {
                    type types-operation:operation-path;
                    description "Path to the results of the operation formatted as xpath.";
                }

                ///global-operations/ping/output/results
                container results {
                    description "Enter the results context";

                    ///global-operations/ping/output/results/test-parameters
                    container test-parameters {
                        description "Enter the test-parameters context";

                        ///global-operations/ping/output/results/test-parameters/ping-type
                        choice ping-type {
                            ///global-operations/ping/output/results/test-parameters/ping-type/case-ip-or-dns
                            case case-ip-or-dns {

                                ///global-operations/ping/output/results/test-parameters/ping-type/case-ip-or-dns/destination
                                leaf destination {
                                    type union {
                                        type types-sros:ip-address-with-zone;
                                        type types-sros:string-not-all-spaces {
                                            length "1..128";
                                        }
                                    }
                                    description "IP address or DNS name that was pinged.";
                                }

                                ///global-operations/ping/output/results/test-parameters/ping-type/case-ip-or-dns/routing-options
                                choice routing-options {
                                    ///global-operations/ping/output/results/test-parameters/ping-type/case-ip-or-dns/routing-options/case-bypass-routing
                                    case case-bypass-routing {

                                        ///global-operations/ping/output/results/test-parameters/ping-type/case-ip-or-dns/routing-options/case-bypass-routing/bypass-routing
                                        leaf bypass-routing {
                                            type boolean;
                                            description
                                                "Bypass the routing table when sending to a host on a directly attached
                                                 network.";
                                        }

                                    }
                                    ///global-operations/ping/output/results/test-parameters/ping-type/case-ip-or-dns/routing-options/case-interface
                                    case case-interface {

                                        ///global-operations/ping/output/results/test-parameters/ping-type/case-ip-or-dns/routing-options/case-interface/interface
                                        leaf interface {
                                            type types-sros:interface-name;
                                            description "Sending interface for the ICMP Echo Request packet(s).";
                                        }

                                    }
                                    ///global-operations/ping/output/results/test-parameters/ping-type/case-ip-or-dns/routing-options/case-next-hop
                                    case case-next-hop {

                                        ///global-operations/ping/output/results/test-parameters/ping-type/case-ip-or-dns/routing-options/case-next-hop/next-hop-address
                                        leaf next-hop-address {
                                            type types-sros:ip-address;
                                            description "Next hop IP address used when sending ICMP Echo Request packet(s).";
                                        }

                                    }
                                    ///global-operations/ping/output/results/test-parameters/ping-type/case-ip-or-dns/routing-options/case-subscriber
                                    case case-subscriber {

                                        ///global-operations/ping/output/results/test-parameters/ping-type/case-ip-or-dns/routing-options/case-subscriber/subscriber
                                        leaf subscriber {
                                            type types-subscriber-mgmt:subscriber-id;
                                            description "Subscriber-id used when sending ICMP Echo Request packet(s).";
                                        }

                                    }
                                }
                                ///global-operations/ping/output/results/test-parameters/ping-type/case-ip-or-dns/router-instance
                                leaf router-instance {
                                    type types-sros:router-instance-base-management-vprn-loose;
                                    description "Router used for the test ('Base', 'management', 'vpls-management', or a VPRN service name).";
                                }

                                ///global-operations/ping/output/results/test-parameters/ping-type/case-ip-or-dns/source-address
                                leaf source-address {
                                    type types-sros:ip-address;
                                    description
                                        "Source IP address that was loaded into the IP header of the ICMP Echo
                                         Request packet(s).";
                                }

                            }
                            ///global-operations/ping/output/results/test-parameters/ping-type/case-srv6-policy
                            case case-srv6-policy {

                                ///global-operations/ping/output/results/test-parameters/ping-type/case-srv6-policy/srv6-policy
                                leaf srv6-policy {
                                    type boolean;
                                    description "ping a Segment Routing IPv6 policy";
                                }

                                ///global-operations/ping/output/results/test-parameters/ping-type/case-srv6-policy/color
                                leaf color {
                                    type uint32;
                                    description "Color identifier of the Segment Routing IPv6 policy to be pinged";
                                }

                                ///global-operations/ping/output/results/test-parameters/ping-type/case-srv6-policy/endpoint
                                leaf endpoint {
                                    type types-sros:ipv6-unicast-or-zero-address;
                                    description "Endpoint identifier of the Segment Routing IPv6 policy to be pinged";
                                }

                                ///global-operations/ping/output/results/test-parameters/ping-type/case-srv6-policy/segment-list
                                leaf segment-list {
                                    type uint32 {
                                        range "0..32";
                                    }
                                    description "The Segment Routing IPv6 segment list to be pinged.  Unspecified: system-selected segment list.";
                                }

                            }
                        }
                        ///global-operations/ping/output/results/test-parameters/count
                        leaf count {
                            type uint32 {
                                range "1..100000";
                            }
                            units "packets";
                            description "Number of ICMP Echo Request packet(s) sent.";
                        }

                        ///global-operations/ping/output/results/test-parameters/output-format
                        leaf output-format {
                            type enumeration {
                                enum summary                        { value 1; }
                                enum detail                         { value 2; }
                            }
                            description
                                "If detail output was active, extra information was returned in error
                                 cases";
                        }

                        ///global-operations/ping/output/results/test-parameters/do-not-fragment
                        leaf do-not-fragment {
                            type boolean;
                            description
                                "ICMP Echo Request packet(s) were not allowed to be fragmented (valid for
                                 IPv4 only).";
                        }

                        ///global-operations/ping/output/results/test-parameters/fc
                        leaf fc {
                            type types-sros:fc-name;
                            description "Forwarding Class for the transmitted ICMP Echo Request packet(s).";
                        }

                        ///global-operations/ping/output/results/test-parameters/interval
                        leaf interval {
                            type union {
                                type uint32 {
                                    range "1..10000";
                                }
                                type decimal64 {
                                    range "0.01..100.00";
                                    fraction-digits 2;
                                }
                            }
                            units "seconds";
                            description "Interval between ICMP Echo Request packets sent.";
                        }

                        ///global-operations/ping/output/results/test-parameters/pattern
                        leaf pattern {
                            type union {
                                type enumeration {
                                    enum sequential                     { value -1; }
                                }
                                type int32 {
                                    range "0..65535";
                                }
                            }
                            description
                                "Two byte value used to pad the ICMP Echo Request packet(s). 'sequential'
                                 indicates bytes increasing by one.";
                        }

                        ///global-operations/ping/output/results/test-parameters/size
                        leaf size {
                            type uint32 {
                                range "0..16384";
                            }
                            units "bytes";
                            description
                                "Number of bytes in the combined ICMP Header (8 bytes) and ICMP Payload
                                 (variable size) of the ICMP Echo Request packet(s).";
                        }

                        ///global-operations/ping/output/results/test-parameters/timeout
                        leaf timeout {
                            type uint32 {
                                range "1..10";
                            }
                            units "seconds";
                            description
                                "Maximum time (in seconds) that was allowed for ICMP Echo Reply packet(s).
                                 The timer was started when the last ICMP Echo Request was sent.";
                        }

                        ///global-operations/ping/output/results/test-parameters/tos
                        leaf tos {
                            type uint32 {
                                range "0..255";
                            }
                            description
                                "Type of Service value to be loaded into the DSCP and ECN fields of the IP
                                 header of the ICMP Echo Request packet(s).";
                        }

                        ///global-operations/ping/output/results/test-parameters/ttl
                        leaf ttl {
                            type uint32 {
                                range "1..128";
                            }
                            description
                                "Time To Live value (IPv4) or Hop Limit value (IPv6) to be loaded into the
                                 IP header of the ICMP Echo Request packet(s).";
                        }

                    } // container test-parameters

                    ///global-operations/ping/output/results/probe
                    list probe {
                        key "probe-index";
                        description "Probe data for the specified test run.";

                        ///global-operations/ping/output/results/probe/probe-index
                        leaf probe-index {
                            type uint32 {
                                range "1..max";
                            }
                            description "Index of probes with the specified test run.";
                        }

                        ///global-operations/ping/output/results/probe/status
                        leaf status {
                            type types-oam:response-status;
                            description "The OAM response status code for the specified test.";
                        }

                        ///global-operations/ping/output/results/probe/round-trip-time
                        leaf round-trip-time {
                            type uint32;
                            units "microseconds";
                            description "Indicates the round trip time for the specified probe.";
                        }

                        ///global-operations/ping/output/results/probe/response-packet
                        container response-packet {
                            description "Data from packet received as a response to transmitted probe.";

                            ///global-operations/ping/output/results/probe/response-packet/size
                            leaf size {
                                type uint32;
                                units "bytes";
                                description
                                    "Indicates the number of bytes in the response packet for the specified
                                     probe.";
                            }

                            ///global-operations/ping/output/results/probe/response-packet/source-address
                            leaf source-address {
                                type types-sros:ip-address-with-zone;
                                description
                                    "Indicates the source address in the response packet for the specified
                                     probe.";
                            }

                            ///global-operations/ping/output/results/probe/response-packet/icmp-sequence-number
                            leaf icmp-sequence-number {
                                type uint32;
                                description
                                    "Indicates the ICMP sequence number in the response packet for the
                                     specified probe.";
                            }

                            ///global-operations/ping/output/results/probe/response-packet/ttl
                            leaf ttl {
                                type uint32;
                                description
                                    "Indicates the time to live in the response packet for the specified
                                     probe.";
                            }

                        } // container response-packet

                    } // list probe

                    ///global-operations/ping/output/results/summary
                    container summary {
                        description "Test run summary data.";

                        ///global-operations/ping/output/results/summary/statistics
                        container statistics {
                            description "Test run summary statistics.";

                            ///global-operations/ping/output/results/summary/statistics/packets
                            container packets {
                                description "Packet statistics for the specified test run.";

                                ///global-operations/ping/output/results/summary/statistics/packets/sent
                                leaf sent {
                                    type uint32;
                                    units "packets";
                                    description "Indicates the number of packets sent for the specified test run.";
                                }

                                ///global-operations/ping/output/results/summary/statistics/packets/received
                                leaf received {
                                    type uint32;
                                    units "packets";
                                    description "Indicates the number of packets received for the specified test run.";
                                }

                                ///global-operations/ping/output/results/summary/statistics/packets/loss
                                leaf loss {
                                    type decimal64 {
                                        range "0.00..100.00";
                                        fraction-digits 2;
                                    }
                                    units "percent";
                                    description
                                        "Indicates the number of packet loss percentage for the specified test
                                         run.";
                                }

                            } // container packets

                            ///global-operations/ping/output/results/summary/statistics/round-trip-time
                            container round-trip-time {
                                description "Test run round trip time statistics.";

                                ///global-operations/ping/output/results/summary/statistics/round-trip-time/minimum
                                leaf minimum {
                                    type uint32;
                                    units "microseconds";
                                    description "Indicates the minimum round trip time for the specified test run.";
                                }

                                ///global-operations/ping/output/results/summary/statistics/round-trip-time/average
                                leaf average {
                                    type uint32;
                                    units "microseconds";
                                    description "Indicates the average round trip time for the specified test run.";
                                }

                                ///global-operations/ping/output/results/summary/statistics/round-trip-time/maximum
                                leaf maximum {
                                    type uint32;
                                    units "microseconds";
                                    description "Indicates the maximum round trip time for the specified test run.";
                                }

                                ///global-operations/ping/output/results/summary/statistics/round-trip-time/standard-deviation
                                leaf standard-deviation {
                                    type uint32;
                                    units "microseconds";
                                    description
                                        "Indicates the standard deviation for the round trip times for the
                                         specified test run.";
                                }

                            } // container round-trip-time

                        } // container statistics

                    } // container summary

                } // container results

                ///global-operations/ping/output/status
                leaf status {
                    type types-operation:operation-status;
                    description "Status of the operation";
                }

                ///global-operations/ping/output/error-message
                leaf-list error-message {
                    type types-operation:operation-message;
                    description "Error messages reported during execution of the operation.";
                }

                ///global-operations/ping/output/warning-message
                leaf-list warning-message {
                    type types-operation:operation-message;
                    description "Warning messages reported during execution of the operation.";
                }

                ///global-operations/ping/output/info-message
                leaf-list info-message {
                    type types-operation:operation-message;
                    description "Info messages reported during execution of the operation.";
                }

                ///global-operations/ping/output/end-time
                leaf end-time {
                    type types-operation:operation-timestamp;
                    description "Time when the operation ended its execution.";
                }

            }
        } // action ping

        ///global-operations/traceroute
        action traceroute {
            description "Trigger traceroute via machine interface";

            input {

                ///global-operations/traceroute/input/traceroute-type
                choice traceroute-type {
                    mandatory true;
                    ///global-operations/traceroute/input/traceroute-type/case-ip-or-dns
                    case case-ip-or-dns {

                        ///global-operations/traceroute/input/traceroute-type/case-ip-or-dns/destination
                        leaf destination {
                            type union {
                                type types-sros:ip-unicast-address;
                                type types-sros:string-not-all-spaces {
                                    length "1..128";
                                }
                            }
                            mandatory true;
                            description "Destination IP address or DNS name";
                        }

                        ///global-operations/traceroute/input/traceroute-type/case-ip-or-dns/router-instance
                        leaf router-instance {
                            type string;
                            default "Base";
                            description "Router name or VPRN service name";
                        }

                        ///global-operations/traceroute/input/traceroute-type/case-ip-or-dns/source-address
                        leaf source-address {
                            type types-sros:ip-address;
                            description "Source address of the probe packets";
                        }

                    }
                    ///global-operations/traceroute/input/traceroute-type/case-srv6-policy
                    case case-srv6-policy {

                        ///global-operations/traceroute/input/traceroute-type/case-srv6-policy/srv6-policy
                        leaf srv6-policy {
                            type empty;
                            mandatory true;
                            description "Trace an SRv6 policy";
                        }

                        ///global-operations/traceroute/input/traceroute-type/case-srv6-policy/color
                        leaf color {
                            type uint32;
                            mandatory true;
                            description "Color ID of the SRv6 policy to trace";
                        }

                        ///global-operations/traceroute/input/traceroute-type/case-srv6-policy/endpoint
                        leaf endpoint {
                            type types-sros:ipv6-unicast-or-zero-address;
                            mandatory true;
                            description "Endpoint of the SRv6 policy to trace";
                        }

                        ///global-operations/traceroute/input/traceroute-type/case-srv6-policy/segment-list
                        leaf segment-list {
                            type uint32 {
                                range "1..32";
                            }
                            description "SR IPv6 segment list to trace";
                        }

                    }
                }
                ///global-operations/traceroute/input/decode
                leaf decode {
                    type enumeration {
                        enum none                           { value 0; }
                        enum original-datagram              { value 1; }
                    }
                    default "none";
                    description "Perform original datagram parsing functions";
                }

                ///global-operations/traceroute/input/dest-port
                leaf dest-port {
                    type uint32 {
                        range "1..65535";
                    }
                    default "33434";
                    description "Transport protocol destination port number";
                }

                ///global-operations/traceroute/input/dest-port-udp-fixed
                leaf dest-port-udp-fixed {
                    type empty;
                    description "Avoid increment of destination port number for UDP test";
                }

                ///global-operations/traceroute/input/detail
                leaf detail {
                    type empty;
                    description "Display MPLS label stack information or TCP port status";
                }

                ///global-operations/traceroute/input/min-ttl
                leaf min-ttl {
                    type uint32 {
                        range "1..255";
                    }
                    default "1";
                    description "First hop traceroute probes using the TTL value";
                }

                ///global-operations/traceroute/input/ttl
                leaf ttl {
                    type uint32 {
                        range "1..255";
                    }
                    default "30";
                    description "Maximum number of hops (TTL) to probe";
                }

                ///global-operations/traceroute/input/numeric
                leaf numeric {
                    type empty;
                    description "Display IP addresses instead of DNS names";
                }

                ///global-operations/traceroute/input/probe-count
                leaf probe-count {
                    type uint32 {
                        range "1..10";
                    }
                    default "3";
                    description "Number of probes to send per hop";
                }

                ///global-operations/traceroute/input/protocol
                leaf protocol {
                    type enumeration {
                        enum udp                            { value 1; }
                        enum tcp                            { value 2; }
                    }
                    default "udp";
                    description "Packet type to send";
                }

                ///global-operations/traceroute/input/size
                leaf size {
                    type uint32 {
                        range "0..9786";
                    }
                    units "bytes";
                    default "0";
                    description "Number of pad bytes in each transmitted packet";
                }

                ///global-operations/traceroute/input/tos
                leaf tos {
                    type uint32 {
                        range "0..255";
                    }
                    default "0";
                    description "Type of Service (ToS) bits in the IP header";
                }

                ///global-operations/traceroute/input/wait
                leaf wait {
                    type uint32 {
                        range "10..60000";
                    }
                    units "milliseconds";
                    default "5000";
                    description "Time to wait for a response to the probe";
                }

            }
            output {

                ///global-operations/traceroute/output/operation-id
                leaf operation-id {
                    type types-operation:operation-id;
                    description "ID assigned to the operation.";
                }

                ///global-operations/traceroute/output/start-time
                leaf start-time {
                    type types-operation:operation-timestamp;
                    description "Time when the operation started its execution.";
                }

                ///global-operations/traceroute/output/results-path
                leaf results-path {
                    type types-operation:operation-path;
                    description "Path to the results of the operation formatted as xpath.";
                }

                ///global-operations/traceroute/output/results
                container results {
                    description "Enter the results context";

                    ///global-operations/traceroute/output/results/test-parameters
                    container test-parameters {
                        description "Enter the test-parameters context";

                        ///global-operations/traceroute/output/results/test-parameters/traceroute-type
                        choice traceroute-type {
                            ///global-operations/traceroute/output/results/test-parameters/traceroute-type/case-ip-or-dns
                            case case-ip-or-dns {

                                ///global-operations/traceroute/output/results/test-parameters/traceroute-type/case-ip-or-dns/destination
                                leaf destination {
                                    type union {
                                        type types-sros:ip-address;
                                        type types-sros:string-not-all-spaces {
                                            length "1..128";
                                        }
                                    }
                                    description "The destination IP address or DNS name";
                                }

                                ///global-operations/traceroute/output/results/test-parameters/traceroute-type/case-ip-or-dns/router-instance
                                leaf router-instance {
                                    type types-sros:router-instance-base-management-vprn-loose;
                                    description "Router instance used for the traceroute (i.e. 'Base', 'management', 'vpls-management' or a VPRN service name)";
                                }

                                ///global-operations/traceroute/output/results/test-parameters/traceroute-type/case-ip-or-dns/source-address
                                leaf source-address {
                                    type types-sros:ip-address;
                                    description "The IP source address to use for the test";
                                }

                            }
                            ///global-operations/traceroute/output/results/test-parameters/traceroute-type/case-srv6-policy
                            case case-srv6-policy {

                                ///global-operations/traceroute/output/results/test-parameters/traceroute-type/case-srv6-policy/srv6-policy
                                leaf srv6-policy {
                                    type boolean;
                                    description "Trace a Segment Routing IPv6 policy";
                                }

                                ///global-operations/traceroute/output/results/test-parameters/traceroute-type/case-srv6-policy/color
                                leaf color {
                                    type uint32;
                                    description "Color identifier of the Segment Routing IPv6 policy to be traced";
                                }

                                ///global-operations/traceroute/output/results/test-parameters/traceroute-type/case-srv6-policy/endpoint
                                leaf endpoint {
                                    type types-sros:ipv6-unicast-or-zero-address;
                                    description "Endpoint identifier of the Segment Routing IPv6 policy to be traced";
                                }

                                ///global-operations/traceroute/output/results/test-parameters/traceroute-type/case-srv6-policy/segment-list
                                leaf segment-list {
                                    type uint32 {
                                        range "0..32";
                                    }
                                    description "The Segment Routing IPv6 segment list to be traced.  Zero: system-selected segment list.";
                                }

                            }
                        }
                        ///global-operations/traceroute/output/results/test-parameters/decode
                        leaf decode {
                            type enumeration {
                                enum none                           { value 0; }
                                enum original-datagram              { value 1; }
                            }
                            description "Decode and display additional fields in replies received";
                        }

                        ///global-operations/traceroute/output/results/test-parameters/dest-port
                        leaf dest-port {
                            type uint32 {
                                range "1..65535";
                            }
                            description
                                "For a TCP test, the destination TCP port number to send.
                                 For a UDP test, the base or only destination UDP port number to send (see dest-port-udp-fixed)";
                        }

                        ///global-operations/traceroute/output/results/test-parameters/dest-port-udp-fixed
                        leaf dest-port-udp-fixed {
                            type boolean;
                            description
                                "For a UDP test, 'true' uses the same destination UDP port number in each
                                 transmitted packet (i.e. the port number is not incremented)";
                        }

                        ///global-operations/traceroute/output/results/test-parameters/detail
                        leaf detail {
                            type boolean;
                            description "Display MPLS label stack information (if available)";
                        }

                        ///global-operations/traceroute/output/results/test-parameters/min-ttl
                        leaf min-ttl {
                            type uint32 {
                                range "1..255";
                            }
                            description "The minimum number of hops to probe";
                        }

                        ///global-operations/traceroute/output/results/test-parameters/ttl
                        leaf ttl {
                            type uint32 {
                                range "1..255";
                            }
                            description "The maximum number of hops to probe";
                        }

                        ///global-operations/traceroute/output/results/test-parameters/numeric
                        leaf numeric {
                            type boolean;
                            description "'true' avoids looking up DNS names when displaying results";
                        }

                        ///global-operations/traceroute/output/results/test-parameters/probe-count
                        leaf probe-count {
                            type uint32 {
                                range "1..10";
                            }
                            description "The number of probes to send per hop";
                        }

                        ///global-operations/traceroute/output/results/test-parameters/protocol
                        leaf protocol {
                            type enumeration {
                                enum udp                            { value 1; }
                                enum tcp                            { value 2; }
                            }
                            description "Send UDP packets or TCP-SYN packets";
                        }

                        ///global-operations/traceroute/output/results/test-parameters/size
                        leaf size {
                            type uint32 {
                                range "0..9786";
                            }
                            units "bytes";
                            description "The number of pad bytes in each transmitted packet";
                        }

                        ///global-operations/traceroute/output/results/test-parameters/tos
                        leaf tos {
                            type uint32 {
                                range "0..255";
                            }
                            description "The IPv4 Type of Service or IPv6 Traffic Class in each transmitted packet";
                        }

                        ///global-operations/traceroute/output/results/test-parameters/wait
                        leaf wait {
                            type uint32 {
                                range "10..60000";
                            }
                            units "milliseconds";
                            description "The time (in milliseconds) to wait for a response to a packet";
                        }

                    } // container test-parameters

                    ///global-operations/traceroute/output/results/hop
                    list hop {
                        key "hop-index";
                        description "Per-hop (also known as per-TTL) test results";

                        ///global-operations/traceroute/output/results/hop/hop-index
                        leaf hop-index {
                            type uint32 {
                                range "1..max";
                            }
                            description "The hop index of the test results";
                        }

                        ///global-operations/traceroute/output/results/hop/probe
                        list probe {
                            key "probe-index";
                            description "Per-probe test results";

                            ///global-operations/traceroute/output/results/hop/probe/probe-index
                            leaf probe-index {
                                type uint32 {
                                    range "1..max";
                                }
                                description "Index of the probe within the specified hop for the test";
                            }

                            ///global-operations/traceroute/output/results/hop/probe/status
                            leaf status {
                                type types-oam:response-status;
                                description "The status of the probe (e.g. response-received, request-timed-out)";
                            }

                            ///global-operations/traceroute/output/results/hop/probe/round-trip-time
                            leaf round-trip-time {
                                type uint32;
                                units "microseconds";
                                description "The round trip time for the specified probe and response";
                            }

                            ///global-operations/traceroute/output/results/hop/probe/size
                            leaf size {
                                type uint32;
                                units "bytes";
                                description "The number of bytes in the transmitted packet (zero if no packet was sent)";
                            }

                            ///global-operations/traceroute/output/results/hop/probe/response-packet
                            container response-packet {
                                description "Test results copied (or derived) from a response packet";

                                ///global-operations/traceroute/output/results/hop/probe/response-packet/icmp-type
                                leaf icmp-type {
                                    type uint32 {
                                        range "0..255";
                                    }
                                    description
                                        "The ICMP or ICMPv6 Type in the received response.
                                         icmp-type = 0 AND icmp-code = 0 indicates no Type and Code
                                         is available";
                                }

                                ///global-operations/traceroute/output/results/hop/probe/response-packet/icmp-code
                                leaf icmp-code {
                                    type uint32 {
                                        range "0..255";
                                    }
                                    description
                                        "The ICMP or ICMPv6 Code in the received response.
                                         icmp-type = 0 AND icmp-code = 0 indicates no Type and Code
                                         is available";
                                }

                                ///global-operations/traceroute/output/results/hop/probe/response-packet/mtu-exceeded
                                leaf mtu-exceeded {
                                    type uint32;
                                    units "bytes";
                                    description
                                        "The MTU (Maximum Transmission Unit) in the received response.
                                         When the MTU is non-zero, it indicates the MTU exceeded by
                                         a transmitted packet .";
                                }

                                ///global-operations/traceroute/output/results/hop/probe/response-packet/source-address
                                leaf source-address {
                                    type types-sros:ip-address;
                                    description
                                        "The IP source address in the response packet (i.e. the address of the
                                         replying node)";
                                }

                                ///global-operations/traceroute/output/results/hop/probe/response-packet/source-host-name
                                leaf source-host-name {
                                    type string {
                                        length "0..255";
                                    }
                                    description "The host name looked up (in DNS) using the IP address of the replying node";
                                }

                                ///global-operations/traceroute/output/results/hop/probe/response-packet/tcp-port-status
                                leaf tcp-port-status {
                                    type enumeration {
                                        enum none                           { value 1; }
                                        enum tcp-port-open                  { value 2; }
                                        enum tcp-port-closed                { value 3; }
                                    }
                                    description
                                        "The operational state of the probed TCP port on the
                                         test's destination node. 'none' is returned if the test's protocol
                                         is UDP, or the replying node is not the test's destination";
                                }

                                ///global-operations/traceroute/output/results/hop/probe/response-packet/mpls-label-stack-entry
                                list mpls-label-stack-entry {
                                    key "index";
                                    description "The information read from an RFC 4950 MPLS Label Stack Object in the response packet";

                                    ///global-operations/traceroute/output/results/hop/probe/response-packet/mpls-label-stack-entry/index
                                    leaf index {
                                        type uint32 {
                                            range "1..max";
                                        }
                                        description "Index of an RFC 4950 MPLS Label Stack Entry within the MPLS Label Stack Object";
                                    }

                                    ///global-operations/traceroute/output/results/hop/probe/response-packet/mpls-label-stack-entry/label
                                    leaf label {
                                        type types-sros:mpls-label-full-range;
                                        description "The Label field within the specified RFC 4950 MPLS Label Stack Entry";
                                    }

                                    ///global-operations/traceroute/output/results/hop/probe/response-packet/mpls-label-stack-entry/traffic-class
                                    leaf traffic-class {
                                        type uint32 {
                                            range "0..7";
                                        }
                                        description "The Experimental field within the specified RFC 4950 MPLS Label Stack Entry (subsequently renamed to Traffic Class)";
                                    }

                                    ///global-operations/traceroute/output/results/hop/probe/response-packet/mpls-label-stack-entry/bottom-of-stack
                                    leaf bottom-of-stack {
                                        type uint32 {
                                            range "0..1";
                                        }
                                        description "The Bottom of Stack field within the specified RFC 4950 MPLS Label Stack Entry";
                                    }

                                    ///global-operations/traceroute/output/results/hop/probe/response-packet/mpls-label-stack-entry/ttl
                                    leaf ttl {
                                        type uint32 {
                                            range "0..255";
                                        }
                                        description "The TTL field within the specified RFC 4950 MPLS Label Stack Entry";
                                    }

                                } // list mpls-label-stack-entry

                                ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram
                                container original-datagram {
                                    description "Information copied from the Original Datagram field within the ICMPv6 response packet";

                                    ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header
                                    list header {
                                        key "header-index";
                                        description "information copied from the headers within the Original Datagram field within the ICMPv6 response packet";

                                        ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header/header-index
                                        leaf header-index {
                                            type uint32 {
                                                range "1..max";
                                            }
                                            description "The sequence number of the header within the Original Datagram field within the ICMPv6 response packet";
                                        }

                                        ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header/header-type-choice
                                        choice header-type-choice {
                                            ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header/header-type-choice/ipv6-case
                                            case ipv6-case {

                                                ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header/header-type-choice/ipv6-case/ipv6-header
                                                container ipv6-header {
                                                    description "An IPv6 header within the Original Datagram field within the ICMPv6 response packet";

                                                    ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header/header-type-choice/ipv6-case/ipv6-header/destination-address
                                                    leaf destination-address {
                                                        type types-sros:ip-address;
                                                        description "The destination address field within the IPv6 header within the Original Datagram field within the ICMPv6 response packet";
                                                    }

                                                    ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header/header-type-choice/ipv6-case/ipv6-header/dscp
                                                    leaf dscp {
                                                        type types-sros:named-item-or-empty;
                                                        description "The Differentiated Services Code Point (DSCP) field within the IPv6 header within the Original Datagram field within the ICMPv6 response packet";
                                                    }

                                                    ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header/header-type-choice/ipv6-case/ipv6-header/hop-limit
                                                    leaf hop-limit {
                                                        type uint32 {
                                                            range "0..255";
                                                        }
                                                        description "The Hop Limit field within the IPv6 header within the Original Datagram field within the ICMPv6 response packet";
                                                    }

                                                    ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header/header-type-choice/ipv6-case/ipv6-header/source-address
                                                    leaf source-address {
                                                        type types-sros:ip-address;
                                                        description "The source address field within the IPv6 header within the Original Datagram field within the ICMPv6 response packet";
                                                    }

                                                } // container ipv6-header

                                            }
                                            ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header/header-type-choice/srv6-case
                                            case srv6-case {

                                                ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header/header-type-choice/srv6-case/srv6-header
                                                container srv6-header {
                                                    description "An SRv6 header within the Original Datagram field within the ICMPv6 response packet";

                                                    ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header/header-type-choice/srv6-case/srv6-header/segments-left
                                                    leaf segments-left {
                                                        type uint32 {
                                                            range "0..255";
                                                        }
                                                        description "The Segments Left field within the SRv6 header within the Original Datagram field within the ICMPv6 response packet";
                                                    }

                                                    ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header/header-type-choice/srv6-case/srv6-header/segment-list
                                                    list segment-list {
                                                        key "segment-index";
                                                        description "Segment List addresses copied from the SRv6 header within the Original Datagram field within the ICMPv6 response packet";

                                                        ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header/header-type-choice/srv6-case/srv6-header/segment-list/segment-index
                                                        leaf segment-index {
                                                            type uint32;
                                                            description "The sequence number of the Segment address within the SRv6 header within the Original Datagram field within the ICMPv6 response packet";
                                                        }

                                                        ///global-operations/traceroute/output/results/hop/probe/response-packet/original-datagram/header/header-type-choice/srv6-case/srv6-header/segment-list/segment-address
                                                        leaf segment-address {
                                                            type types-sros:ip-address;
                                                            description "A Segment address within the SRv6 header within the Original Datagram field within the ICMPv6 response packet";
                                                        }

                                                    } // list segment-list

                                                } // container srv6-header

                                            }
                                        }
                                    } // list header

                                } // container original-datagram

                            } // container response-packet

                        } // list probe

                    } // list hop

                } // container results

                ///global-operations/traceroute/output/status
                leaf status {
                    type types-operation:operation-status;
                    description "Status of the operation";
                }

                ///global-operations/traceroute/output/error-message
                leaf-list error-message {
                    type types-operation:operation-message;
                    description "Error messages reported during execution of the operation.";
                }

                ///global-operations/traceroute/output/warning-message
                leaf-list warning-message {
                    type types-operation:operation-message;
                    description "Warning messages reported during execution of the operation.";
                }

                ///global-operations/traceroute/output/info-message
                leaf-list info-message {
                    type types-operation:operation-message;
                    description "Info messages reported during execution of the operation.";
                }

                ///global-operations/traceroute/output/end-time
                leaf end-time {
                    type types-operation:operation-timestamp;
                    description "Time when the operation ended its execution.";
                }

            }
        } // action traceroute

    } // container global-operations

}
