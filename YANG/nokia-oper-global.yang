                        leaf lsp-name {
                            type leafref {
                                path "/state:state/state:router/state:mpls/state:lsp/state:lsp-name";
                                require-instance "false";
                            }
                            mandatory true;
                            description "Name of the LSP to test";
                        }

                        ///global-operations/oam/lsp-trace/rsvp-te/input/path
                        leaf path {
                            type leafref {
                                path "/state:state/state:router/state:mpls/state:path/state:path-name";
                                require-instance "false";
                            }
                            description "Name of the MPLS path to test";
                        }

                        ///global-operations/oam/lsp-trace/rsvp-te/input/source-ip-address
                        leaf source-ip-address {
                            type types-sros:ip-address;
                            description "Source IP address";
                        }

                        ///global-operations/oam/lsp-trace/rsvp-te/input/interval
                        leaf interval {
                            type uint32 {
                                range "1..10";
                            }
                            units "seconds";
                            default "1";
                            description "Minimum time to expire before next echo request is sent";
                        }

                        ///global-operations/oam/lsp-trace/rsvp-te/input/size
                        leaf size {
                            type uint32 {
                                range "1..9786";
                            }
                            units "bytes";
                            default "1";
                            description "Echo request packet pad size";
                        }

                        ///global-operations/oam/lsp-trace/rsvp-te/input/fc
                        leaf fc {
                            type types-sros:fc-name;
                            default "be";
                            description "QoS forwarding class for the sent echo request packet";
                        }

                        ///global-operations/oam/lsp-trace/rsvp-te/input/profile
                        leaf profile {
                            type types-qos:profile-in-out;
                            default "out";
                            description "QoS profile for the transmitted echo request packets";
                        }

                        ///global-operations/oam/lsp-trace/rsvp-te/input/timeout
                        leaf timeout {
                            type uint32 {
                                range "1..10";
                            }
                            units "seconds";
                            default "5";
                            description "Time to wait for a reply after sending an echo request";
                        }

                        ///global-operations/oam/lsp-trace/rsvp-te/input/downstream-map-tlv
                        leaf downstream-map-tlv {
                            type enumeration {
                                enum dsmap {
                                    value 1;
