                                            description "Indicates the round trip time for the probe";
                                        }

                                        ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/status
                                        leaf status {
                                            type types-oam:response-status;
                                            description "The OAM response status code for the specified test";
                                        }

                                        ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet
                                        container response-packet {
                                            description "Data from packet received as a response to transmitted probe";

                                            ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/size
                                            leaf size {
                                                type uint32;
                                                units "bytes";
                                                description "Indicates the number of bytes in the response packet for the probe";
                                            }

                                            ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/response-packet/source-address
                                            leaf source-address {
                                                type types-sros:ip-address;
