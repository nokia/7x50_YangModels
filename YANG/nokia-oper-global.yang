                                    ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance
                                    list probe-instance {
                                        key "probe-instance-index";
                                        description "Probe instance of a probe, in the contex of a hop";

                                        ///global-operations/oam/lsp-trace/sr-policy/output/results/hop/probe/probe-instance/probe-instance-index
                                        leaf probe-instance-index {
                                            type uint32 {
                                                range "1..4294967295";
